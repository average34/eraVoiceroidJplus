;----------------------------------------------------------------
;【妊娠】(妊娠機能をオンにして膣出しすると一定確率で妊娠。女主人も)
;変数Tに妊娠したキャラ、CFLAG:T:104に妊娠させたキャラ(1:主人、2:助手、3:パートナー)
;恋慕無しで反発感情LV2以上で妊娠させられると壊れる
;----------------------------------------------------------------
IF TFLAG:13 == 20
	;主人→パートナー、助手(助手の時、パートナーはZで参照可能。TARGETでは参照できません)
	IF CFLAG:T:104 == 1
		;相愛
		IF TALENT:相愛
			PRINTFORMW 
		;恋慕
		ELSEIF TALENT:恋慕
			PRINTFORMW 
		;恋人
		ELSEIF TALENT:恋人
			PRINTFORMW 
		;反発感情Lv2以上
		ELSEIF MARK:T:3 >= 2
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
	;助手→パートナー
	ELSEIF CFLAG:T:104 == 2 && ASSI >= 0 && T == TARGET
		;恋慕無しで反発感情Lv2以上
		IF TALENT:恋慕 == 0 && MARK:T:3 >= 2
			PRINTFORMW 
		;恋慕もしくは恋人
		ELSEIF TALENT:恋慕
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
	;パートナー→主人
	ELSEIF CFLAG:T:104 == 3 && T == MASTER
		;恋慕
		IF TALENT:恋慕
			PRINTFORMW 
		;反発感情LV2以上
		ELSEIF MARK:3 >= 2
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
	;パートナー→助手(助手を妊娠させた)
	ELSEIF CFLAG:T:104 == 3 && T == ASSI
		;恋慕無しで反発感情Lv2以上
		IF TALENT:恋慕 == 0 && MARK:T:3 >= 2
			PRINTFORMW 
		;恋慕
		ELSEIF TALENT:恋慕
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF
IF TFLAG:13 == 94
	;助手の時にパートナーに妊娠させられた(パートナーはZで参照可能。TARGETでは参照できません)
	IF CFLAG:T:104 == 3
		;恋慕無しで反発感情Lv2以上
