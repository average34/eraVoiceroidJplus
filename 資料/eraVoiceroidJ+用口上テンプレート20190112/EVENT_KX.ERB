	;デート経験有り&恋慕
	IF EXP:94 && TALENT:85
		PRINTFORMW 
	;デート経験有り
	ELSEIF EXP:94
		PRINTFORMW 
	;恋慕
	ELSEIF TALENT:85
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
ENDIF

