;-------------------------------------------------
;@KOJO_MESSAGE_ORGASM関係
;J用霊夢口上の構文を流用しました。
;-------------------------------------------------
@KOJO_MESSAGE_ORGASM_KX1
;助手が調教した時に口上をスキップする（好みに応じて使う、行頭の;を消すとスキップするようになる）
SIF ASSI > -1 && ASSIPLAY
	RETURN 0 
;ボールギャグ着用時には口上をスキップする（OFFだと口を塞いでるのに喋りまくる）
SIF TEQUIP:45
	RETURN 0
;面倒なので、２回目の口上扱いってことで口上頻度MAX以外はスキップさせてしまう
SIF FLAG:7 < 2
	RETURN 0
;-------------------------------------------------
;初回口上が表示された後かの判別処理
;ここで各CFLAGが1＝初回絶頂の直後なので、2にして後の処理をスキップする
;ついでに、絶頂が１つもなかった場合もスキップ
;-------------------------------------------------
D = 0
IF CFLAG:347 == 1
	CFLAG:347 = 2
	D += 1
ENDIF
IF CFLAG:348 == 1
	CFLAG:348 = 2
	D += 1
ENDIF
IF CFLAG:349 == 1
	CFLAG:349 = 2
	D += 1
ENDIF
IF CFLAG:350 == 1
	CFLAG:350 = 2
	D += 1
ENDIF
SIF D > 0
	RETURN 0

;各部位に絶頂があったかを確認して、CVABに結果を代入（0or1）
C = (NOWEX:0 > 0)
V = (NOWEX:1 > 0)
A = (NOWEX:2 > 0)
B = (NOWEX:3 > 0)

;CVABの合計をDに代入（何重絶頂だったかを表す値になる）
D = C + V + A + B
SIF D == 0
	RETURN 0

;-------------------------------------------------
;フェラ中に絶頂した
;-------------------------------------------------
;フェラ、シックスナイン、イラマチオが該当
IF SELECTCOM == 31 || SELECTCOM == 69 || SELECTCOM == 80 || SELECTCOM == 373
	PRINTL 
	RETURN 0
ENDIF

;-------------------------------------------------
;Ｃ絶頂した
;-------------------------------------------------
IF C && D == 1
	;ウフフ中ではない＝着衣で絶頂
	IF TFLAG:44 == 0
		PRINTFORMW 
	;恋慕
	ELSEIF TALENT:85
		;C強絶頂以上
		IF NOWEX:0 > 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
	PRINTL 

;-------------------------------------------------
;Ｖ絶頂した
;-------------------------------------------------
ELSEIF V && D == 1
	;ウフフ中ではない＝着衣で絶頂
	IF TFLAG:44 == 0
		PRINTFORMW 
	;恋慕
	ELSEIF TALENT:85
		;V強絶頂以上
		IF NOWEX:1 > 1
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
	PRINTL 

;-------------------------------------------------
;Ａ絶頂した
;-------------------------------------------------
ELSEIF A && D == 1
	;ウフフ中ではない＝着衣で絶頂
	IF TFLAG:44 == 0
		PRINTFORMW 
	;恋慕
	ELSEIF TALENT:85
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
	PRINTL 

;-------------------------------------------------
;Ｂ絶頂した
;-------------------------------------------------
ELSEIF B && D == 1
	;ウフフ中ではない＝着衣で絶頂
	IF TFLAG:44 == 0
		PRINTFORMW 
	;恋慕
	ELSEIF TALENT:85
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
	PRINTL 

;-------------------------------------------------
;同時絶頂
;-------------------------------------------------
;四重絶頂
ELSEIF D == 4
	PRINTFORMW 
;ＶＡ込みの三重絶頂
ELSEIF V && A && D == 3
	PRINTFORMW 
;それ以外の三重絶頂
ELSEIF D == 3
	PRINTFORMW 
;二重絶頂：Ｃ＆Ｂ
ELSEIF C && B
	PRINTFORMW 
;二重絶頂：Ｖ＆Ａ
ELSEIF V && A
	PRINTFORMW 
;それ以外の二重絶頂
ELSEIF D == 2
	PRINTFORMW 
ENDIF
RETURN 0


