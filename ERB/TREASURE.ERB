		PRINTFORMW 一時的に羽が生えました
		TEQUIP:40 |= 1p15
		TALENT:羽 = 1
		ITEM:空色の秘薬 -= 1
	ENDIF
ELSEIF RESULT == 30
	;黒猫の秘薬使用　猫になる
	IF TALENT:猫舌 && TALENT:動物耳 && TALENT:しっぽ
		IF TEQUIP:40 & 1p14
			PRINTFORMW %CALLNAME:TARGET%には既に%ITEMNAME:530%を飲ませています
		ELSE
			PRINTFORMW %CALLNAME:TARGET%は元々猫です
		ENDIF
		RESTART
	ELSE
		PRINTFORMW %CALLNAME:TARGET%に%ITEMNAME:530%を飲ませました
		PRINTFORMW 一時的にネコ耳としっぽが生えました
		;猫舌
		TEQUIP:40 |= 1p14
		TALENT:猫舌 = 1
		;動物耳
		TEQUIP:40 |= 32
		TALENT:動物耳 = 1
		;しっぽ
		TEQUIP:40 |= 1p16
		TALENT:しっぽ = 1
		ITEM:黒猫の秘薬 -= 1
	ENDIF
ENDIF
TFLAG:17 = RESULT
TFLAG:600 = 42
CALL KOJO_JUN
