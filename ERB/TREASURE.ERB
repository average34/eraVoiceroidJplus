;---------------------------------------------------------
;宝探し処理
;宝探し能力の判定、アイテム獲得、獲得時のメッセージを独立させて個別に使えるよう変更
;G	宝探し能力の判定に使用
;N	アイテムランクのテーブル
;R	手に入るITEM番号
;LOCAL:2	テーブル内での乱数に使用
;---------------------------------------------------------
@TREASURE
;変数初期化
G = 0
N = 0
R = 0
S = 0

;宝探し能力の判定
CALL TREASURE_CHECK

;アイテム獲得
CALL TREASURE_GET

;アイテム獲得の地の文
CALL TREASURE_MESSAGE

RETURN 1
;---------------------------------------------------------
;宝探し能力の判定
;---------------------------------------------------------
@TREASURE_CHECK
;基本は0～50
G = RAND:50
;技巧*10
G += ABL:COUNT:技巧 * 10
;奉仕精神*5
G += ABL:COUNT:奉仕精神 * 5

;ナンバー参照からABL参照へ
;探索素質を見る
G += ABL:COUNT:探索素質 * 10

;好感度に応じたボーナス
IF CFLAG:COUNT:2 < 100
	G -= 5
ELSEIF CFLAG:COUNT:2 < 500
	G += 1
ELSEIF CFLAG:COUNT:2 < 1500
	G += 5
ELSEIF CFLAG:COUNT:2 < 3000
	G += 10
ELSEIF CFLAG:COUNT:2 < 5000
	G += 15
ELSEIF CFLAG:COUNT:2 < 10000
	G += 20
ELSEIF CFLAG:COUNT:2 < 30000
	G += 30
ELSE
	G += 40
ENDIF

;テーブルを決定
IF G < 50
	N = 0
ELSEIF G < 100
	N = 1
ELSEIF G < 200
	N = 2
ELSEIF G < 300
	N = 3
ELSEIF G < 400
	N = 4
ELSEIF G < 500
	N = 5
ELSE
	N = 6
ENDIF

;10%でテーブルランクアップ
IF RAND:10 == 0
	N += 1
ENDIF

SIF N >= 6
	N = 6

;---------------------------------------------------------
;アイテム獲得
;---------------------------------------------------------
@TREASURE_GET
;テーブルごとに獲得アイテム決定

;テーブルランク0は消費アイテム
;25,ローション,200,
;26,媚薬,2000,
IF N == 0
	;最低ランク
	LOCAL:2 = RAND:3
	IF LOCAL:2 == 0 && ITEM:ローション < 99
		R = 25
	ELSEIF LOCAL:2 == 1 && ITEM:媚薬 < 99
		R = 26
	ELSE
		R = 0
	ENDIF
	;40%で強制的に何もなし。
	IF RAND:100 < 40
		;PRINTFORML %CALLNAME:COUNT%は何も発見できませんでした
		RETURN 0
	ELSEIF R == 0
		;R = 0の場合はお金入手
		R = 100 + RAND:100
		MONEY += R
		;PRINTFORML %CALLNAME:COUNT%は${R}を発見しました
		RETURN 1
	ELSE
		;アイテム入手
		ITEM:R += 1
		;PRINTFORML %CALLNAME:COUNT%は%ITEMNAME:R%を発見しました
		RETURN 2
	ENDIF
ENDIF

;テーブルランク1以上は各ランクのJ用アイテム
VARSET J, 0
VARSET K, 0
;お金を入手する可能性有
K:J = 0
J += 1
;J用アイテムから獲得アイテム決定
FOR LOCAL, 500, 600
	;テーブルのランク以下のJ用アイテムしか手に入らない
	SIF ITEMPRICE:LOCAL > N || ITEMPRICE:LOCAL < 1
		CONTINUE
	;99個以上は手に入らない
	SIF ITEM:LOCAL >= 99
		CONTINUE
	;体力・気力を全回復するアイテムは3つまで
	SIF LOCAL == 517 && ITEM:LOCAL >= 3
		CONTINUE
	
	;ここまで残ったJ用アイテムは獲得アイテム候補
	K:J = LOCAL
	J += 1
NEXT

LOCAL = RAND:J
R = K:LOCAL

;15-N*2%で強制的に何もなし。
IF RAND:100 < (15 - N * 2)
	;PRINTFORML %CALLNAME:COUNT%は何も発見できませんでした
	RETURN 0
ENDIF

IF R == 0
	;R = 0の場合はお金入手
	IF N ==  0
		R = 100 + RAND:100
	ELSEIF N ==  1
		R = 500 + RAND:100
	ELSEIF N ==  2
		R = 1000 + RAND:1000
	ELSEIF N ==  3
		R = 2000 + RAND:1000
	ELSEIF N ==  4
		R = 5000 + RAND:1000
	ELSEIF N ==  5
		R = 10000 + RAND:10000
	ELSE
		R = 50000 + RAND:10000
	ENDIF
	MONEY += R
	;PRINTFORML %CALLNAME:COUNT%は${R}を発見しました
	RETURN 1
ELSE
	ITEM:R += 1
	;PRINTFORML %CALLNAME:COUNT%は%ITEMNAME:R%を発見しました
	RETURN 2
ENDIF

;---------------------------------------------------------
;アイテム獲得の地の文
;---------------------------------------------------------
@TREASURE_MESSAGE
IF FLAG:23 & 1024
	IF RESULT == 0
		PRINTFORML %CALLNAME:COUNT%は何も発見できませんでした
	ELSEIF RESULT == 1
		PRINTFORML %CALLNAME:COUNT%は${R}を発見しました
	ELSEIF RESULT == 2
		PRINTFORML %CALLNAME:COUNT%は%ITEMNAME:R%を発見しました
	ENDIF
ENDIF

;---------------------------------------------------------
;%CALLNAME:MASTER%が宝探し
;---------------------------------------------------------
@TREASURE_MASTER
TFLAG:600 = 43
CALL KOJO_JUN
BASE:MASTER:耐力 -= 500
;COUNTにMASTERを指定して宝探し
COUNT = MASTER
;宝探し能力の判定
CALL TREASURE_CHECK
;レア度ＭＡＸの入手確率は最高10%
IF G >= 500 && RAND:100 < 90
	N = 5
ENDIF
;アイテム獲得
CALL TREASURE_GET
;アイテムとは別にお金入手（宝探し能力の判定変数Gを基準に算出してGに上書き）
G = G * (RAND:11 + 15) * (RAND:5 + 5)
MONEY += G

;アイテム獲得の地の文
IF RESULT == 0
	PRINTFORMW %CALLNAME:COUNT%は遺跡を探索して${G}を発見しました
ELSEIF RESULT == 1
	PRINTFORMW %CALLNAME:COUNT%は遺跡を探索して${G + R}を発見しました
ELSEIF RESULT == 2
	PRINTFORMW %CALLNAME:COUNT%は遺跡を探索して${G}と%ITEMNAME:R%を発見しました
ENDIF
;宝探し詳細報告アリなら、PRINTL 
SIF FLAG:23 & 1024
	PRINTL 

;---------------------------------------------------------
;２人で宝探し
;---------------------------------------------------------
@TREASURE_TWO
TFLAG:600 = 44
CALL KOJO_JUN
BASE:MASTER:耐力 -= 300
BASE:TARGET:体力 -= 200
;MASTERとTARGETをそれぞれ別に宝探し能力の判定
COUNT = MASTER
CALL TREASURE_CHECK
;Iを利用しMASTERの宝探し能力Gを保存
I = G

COUNT = TARGET
CALL TREASURE_CHECK
;パートナーが親愛なら合体
IF TALENT:親愛
	G = (G + I)
;恋人ならMASTERとパートナーで良い方を採用
ELSEIF TALENT:恋人
	SIF G <= I
		G = I
;それ以外は二人の平均
ELSE
	G = (G + I) / 2
ENDIF
;レア度ＭＡＸの入手確率は最高20%
IF G >= 500 && RAND:100 < 80
	N = 5
ENDIF

;アイテム獲得
CALL TREASURE_GET

;アイテムとは別にお金入手（宝探し能力の判定変数Gを基準に算出してGに上書き）
G = G * (RAND:11 + 15) * (RAND:10 + 5)
MONEY += G

;アイテム獲得の地の文
IF RESULT == 0
	PRINTFORMW %CALLNAME:MASTER%と%CALLNAME:TARGET%は２人で遺跡を探索して${G}を発見しました
ELSEIF RESULT == 1
	PRINTFORMW %CALLNAME:MASTER%と%CALLNAME:TARGET%は２人で遺跡を探索して${G + R}を発見しました
ELSEIF RESULT == 2
	PRINTFORMW %CALLNAME:MASTER%と%CALLNAME:TARGET%は２人で遺跡を探索して${G}と%ITEMNAME:R%を発見しました
ENDIF
;宝探し詳細報告アリなら、PRINTL 
SIF FLAG:23 & 1024
	PRINTL 

