;---------------------------------------------------------------------
;全キャラ一括操作用関数
;---------------------------------------------------------------------
@EDIT_ALL_CHARA(ARGS, ARG, ARG:1)
;主人公は除く
FOR LOCAL, 1, CHARANUM
	IF ARGS == "ABL"
		ABL:LOCAL:ARG = ARG:1
	ELSEIF ARGS == "PALAM"
		PALAM:LOCAL:ARG = ARG:1
	ELSEIF ARGS == "TALENT"
		TALENT:LOCAL:ARG = ARG:1
	ELSEIF ARGS == "MARK"
		MARK:LOCAL:ARG = ARG:1
