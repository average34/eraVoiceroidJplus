	CALL PRINTFORMW2T , @"「はぁ、はぁ……これ、すご……」"
;二重絶頂：Ｃ＆Ｂ
ELSEIF C && B
	CALL PRINTFORMW2T , @"「く、ふぁっ……胸と、アソコが、いっぺんに……」"
	CALL PRINTFORMW2T , @"「ダメ、も、我慢、がっ……んっ、ああっ──！」"
	CALL PRINTFORMW2T , @"「はぁ、はぁ……気持ち、良かったぁ……」"
;二重絶頂：Ｖ＆Ａ
ELSEIF V && A
	CALL PRINTFORMW2T , @"「はぅ……こんな、前も、後ろも……！」"
	CALL PRINTFORMW2T , @"「や、ダメです……そんな……く、うぁ……ああっ──！」"
	CALL PRINTFORMW2T , @"「はぁ、はぁ……こんなの、凄すぎる……」"
;それ以外の二重絶頂
ELSEIF D == 2
	CALL PRINTFORMW2T , @"「くぅ……ダメですっ、こんなの……」"
	CALL PRINTFORMW2T , @"「や、あっ……も、我慢できない……んっ、ああっ──！」"
	CALL PRINTFORMW2T , @"「はぁ、はぁ……す、凄かったぁ……」"
ENDIF
RETURN 0

