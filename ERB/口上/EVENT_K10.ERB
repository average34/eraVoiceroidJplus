	ELSEIF RESULT == 6
	CSTR:1 = おねえちゃん
	ELSEIF RESULT == 7
	CSTR:1 = %NAME:MASTER%
	ELSEIF RESULT == 8
	CSTR:1 = %NAME:MASTER%さん
