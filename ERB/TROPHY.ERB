;--------------------------------------------------
;実績一覧
;--------------------------------------------------
;GLOBALについて
;0-154をゲーム開始時の主人公の素質記録
;199を互換用に使用
;900-1000のうちいくつかをコンフィグ等の記録
;1000-1999をクリアチェック関数@GLOBALEVENTで使用
;実績一覧では200～使用。ビット演算で実績取得済みか判定
;200:イベント編
;201:ウフフ編
;202:イベント編-Lunatic-
;203:ウフフ編-Lunatic-
;FLAG:57 どの実績を開いていたかの記録に使用
;--------------------------------------------------
@TROPHY
CALL TROPHY_CHECK_200
CALL TROPHY_CHECK_202

VARSET B, 0
VARSET S, 0
VARSET G, 0
VARSET P, 0
VARSET LOCAL, 0
LOCAL:2 = 200+FLAG:57

$PRINT_LIST
VARSET RESULT, 0
DRAWLINE
PRINTFORM  ■ トロフィー　
IF FLAG:57 == 0
	PRINTFORML イベント編　　
ELSEIF FLAG:57 == 1
	PRINTFORML ウフフ編　　
ELSEIF FLAG:57 == 2
	PRINTFORML イベント編-Lunatic-　　
;ELSEIF FLAG:57 == 3
;	PRINTFORML ウフフ編-Lunatic-　　
ENDIF
DRAWLINE
FOR LOCAL, 0, 50
	TRYCALLFORM TROPHY_{LOCAL:2}_{LOCAL}
	SIF RESULTS:0 == ""
		CONTINUE
	GETBIT GLOBAL:(LOCAL:2), LOCAL
	IF RESULTS:0 == "ブロンズ"
		SETCOLOR 151, 107, 47
		PRINT [B] 
	ELSEIF RESULTS:0 == "シルバー"
		SETCOLOR 158, 158, 158
		PRINT [S] 
	ELSEIF RESULTS:0 == "ゴールド"
		SETCOLOR 180, 158, 83
		PRINT [G] 
	ELSEIF RESULTS:0 == "プラチナ"
		SETCOLOR 217, 217, 140
		PRINT [P] 
	ENDIF
	RESETCOLOR
	SIF RESULT == 0
		SETCOLOR 105, 105, 105
	SIF RESULT == 0 && (RESULTS:0 == "シルバー" || RESULTS:0 == "ゴールド" || RESULTS:0 == "プラチナ")
		RESULTS:1 = ？？？
	SIF RESULT == 0 && (RESULTS:0 == "ゴールド" || RESULTS:0 == "プラチナ")
		RESULTS:2 = ？？？
	;イベント編-Lunatic-は実績名長
	IF FLAG:57 == 2
		PRINTFORM %RESULTS:1,28,LEFT%%RESULTS:2%
	ELSE
		PRINTFORM %RESULTS:1,22,LEFT%%RESULTS:2%
	ENDIF
	;実績によっては現在の達成率をパーセント表示
	IF RESULT:1 && RESULT == 0
		PRINT [ 現在
		PRINTFORM {RESULT:1}
		PRINT % ]
	ENDIF
	PRINTL 
	VARSET RESULT, 0
	VARSET RESULTS, ""
	RESETCOLOR
NEXT

DRAWLINE
;実績数カウント
CALL TROPHY_COUNT(LOCAL:2)
;実績数表示
CALL TROPHY_COUNT_PRINT
DRAWLINE
PRINTFORML 全実績総合
;全実績数カウント
CALL TROPHY_COUNT_ALL
;実績数表示
CALL TROPHY_COUNT_PRINT
DRAWLINE
PRINTFORML [  0] イベント編
PRINTFORML [  1] ウフフ編
SIF GLOBAL:202
	PRINTFORML [  2] イベント編-Lunatic-
;PRINTFORML [  3] ウフフ編-Lunatic-
PRINTFORML [ 99] 実績リセット
PRINTFORML [100] 戻る
$INPUT_LOOP
INPUT
IF RESULT == 100
	RETURN 0
ELSEIF RESULT == 99
	CALL TROPHY_RESET
	CALL TROPHY_CHECK_200
	GOTO PRINT_LIST
ELSEIF RESULT >= 0 && RESULT <= 1
	FLAG:57 = RESULT
	LOCAL:2 = 200+FLAG:57
	GOTO PRINT_LIST
ELSEIF RESULT == 2 && GLOBAL:202
	FLAG:57 = RESULT
	LOCAL:2 = 200+FLAG:57
	GOTO PRINT_LIST
ELSE
	PRINTFORML 正しい値を入力してください
	GOTO INPUT_LOOP
ENDIF

;--------------------------------------------------
;実績リセット
;--------------------------------------------------
@TROPHY_RESET
LOADGLOBAL
PRINTFORML 実績をリセットしますか？
PRINTL 
PRINTFORML [  0] イベント編のみ
PRINTFORML [  1] ウフフ編のみ
PRINTFORML [  2] イベント編-Lunatic-のみ
PRINTFORML [ 99] 全実績
PRINTFORML [100] 戻る

$INPUT_LOOP
INPUT
IF RESULT == 100
	RETURN 0
ELSEIF RESULT == 99
	PRINTFORML 全実績をリセットしますか？
	CALL TROPHY_RESET_CHECK
	IF RESULT == 0
		PRINTFORMW 全実績をリセットしました
		GLOBAL:200 = 0
		GLOBAL:201 = 0
		GLOBAL:202 = 0
	ELSE
		PRINTFORMW 全実績リセットを中止しました
	ENDIF
ELSEIF RESULT == 0
	PRINTFORML イベント編の実績をリセットしますか？
	CALL TROPHY_RESET_CHECK
	IF RESULT == 0
		PRINTFORMW イベント編の実績をリセットしました
		GLOBAL:200 = 0
	ELSE
		PRINTFORMW イベント編の実績リセットを中止しました
	ENDIF
ELSEIF RESULT == 1
	PRINTFORML ウフフ編の実績をリセットしますか？
	CALL TROPHY_RESET_CHECK
	IF RESULT == 0
		PRINTFORMW ウフフ編の実績をリセットしました
		GLOBAL:201 = 0
	ELSE
		PRINTFORMW ウフフ編の実績リセットを中止しました
	ENDIF
ELSEIF RESULT == 2
	PRINTFORML イベント編-Lunatic-の実績をリセットしますか？
	CALL TROPHY_RESET_CHECK
	IF RESULT == 0
		PRINTFORMW イベント編-Lunatic-の実績をリセットしました
		GLOBAL:202 = 0
	ELSE
		PRINTFORMW イベント編-Lunatic-の実績リセットを中止しました
	ENDIF
ELSE
	PRINTFORML 正しい値を入力してください
	GOTO INPUT_LOOP
ENDIF
SAVEGLOBAL
RETURN 0

@TROPHY_RESET_CHECK
PRINTL [0] はい
PRINTL [1] いいえ
$INPUT_LOOP
INPUT
IF RESULT == 0
	RETURN 0
ELSEIF RESULT == 1
	RETURN 1
ELSE
	PRINTFORML 正しい値を入力してください
	GOTO INPUT_LOOP
ENDIF

;--------------------------------------------------
;実績数カウント
;--------------------------------------------------
@TROPHY_COUNT(ARG)
LOADGLOBAL

VARSET B, 0
VARSET S, 0
VARSET G, 0
VARSET P, 0
VARSET LOCAL, 0


FOR LOCAL, 0, 50
	TRYCALLFORM TROPHY_{ARG}_{LOCAL}
	SIF RESULTS:0 == ""
		CONTINUE
	GETBIT GLOBAL:(ARG), LOCAL
	IF RESULTS:0 == "ブロンズ"
		B:0 += 1
		B:1 += RESULT
	ELSEIF RESULTS:0 == "シルバー"
		S:0 += 1
		S:1 += RESULT
	ELSEIF RESULTS:0 == "ゴールド"
		G:0 += 1
		G:1 += RESULT
	ELSEIF RESULTS:0 == "プラチナ"
		P:0 += 1
		P:1 += RESULT
	ENDIF
	VARSET RESULTS, ""
NEXT

;--------------------------------------------------
;全実績数カウント
;--------------------------------------------------
@TROPHY_COUNT_ALL
#DIM LCOUNT, 1
VARSET LOCAL, 0

FOR LCOUNT, 200, 203
	CALL TROPHY_COUNT(LCOUNT)
	LOCAL:0 += B:0
	LOCAL:1 += S:0
	LOCAL:2 += G:0
	LOCAL:3 += P:0
	
	LOCAL:10 += B:1
	LOCAL:11 += S:1
	LOCAL:12 += G:1
	LOCAL:13 += P:1
NEXT
B:0 = LOCAL:0
S:0 = LOCAL:1
G:0 = LOCAL:2
P:0 = LOCAL:3

B:1 = LOCAL:10
S:1 = LOCAL:11
G:1 = LOCAL:12
P:1 = LOCAL:13

;--------------------------------------------------
;実績数表示
;--------------------------------------------------
@TROPHY_COUNT_PRINT
LOCAL:0 = B:0 + S:0 + G:0 + P:0
LOCAL:1 = B:1 + S:1 + G:1 + P:1
PRINTFORM 達成率 {(LOCAL:1)*100/(LOCAL:0)}％　
CALL PRINT_BAR, LOCAL:1, LOCAL:0, 32, UNICODE(0x2585), UNICODE(0x2585), 0x0082FF, 0x002050
IF LOCAL:0 == LOCAL:1
	SETCOLOR 217, 217, 140 
	PRINT [P]
	RESETCOLOR
ENDIF
PRINTL 
PRINTFORML 実績数 {LOCAL:1}/{LOCAL:0}
SETCOLOR 217, 217, 140 
PRINT [P]
RESETCOLOR
PRINTFORM {P:1} 

SETCOLOR 180, 158, 83
PRINT [G]
RESETCOLOR
PRINTFORM {G:1} 

SETCOLOR 158, 158, 158
PRINT [S]
RESETCOLOR
PRINTFORM {S:1} 

SETCOLOR 151, 107, 47
PRINT [B]
RESETCOLOR
PRINTFORM {B:1} 

PRINTL 

;--------------------------------------------------
;判定（GLOBAL:200）
;--------------------------------------------------
@TROPHY_CHECK_200
LOADGLOBAL

VARSET T, 0
VARSET LOCAL, 0
;経験～～以上等の実績はここで一括して判定
FOR LOCAL, 1, CHARANUM
	;好感度
	SIF CFLAG:LOCAL:2 >= 1000
		GLOBAL:200 |= 1p20
	SIF CFLAG:LOCAL:2 >= 10000
		GLOBAL:200 |= 1p21
	SIF CFLAG:LOCAL:2 >= 50000
		GLOBAL:200 |= 1p22
	SIF CFLAG:LOCAL:2 >= 100000
		GLOBAL:200 |= 1p23
	;戦闘力
	T = LOCAL
	CALL GET_BATTLE_POWER
	SIF P >= 10000
		GLOBAL:200 |= 1p24
	SIF P >= 100000
		GLOBAL:200 |= 1p25
	SIF P >= 530000
		GLOBAL:200 |= 1p26
	SIF P >= 1000000
		GLOBAL:200 |= 1p27
	SIF P >= 10000000
		GLOBAL:200 |= 1p28
NEXT

VARSET LOCAL, 0
FOR LOCAL, 0, 50
	;条件をクリアしているか
	TRYCALLFORM TROPHY_200_{LOCAL}
	SIF RESULT
		SETBIT GLOBAL:200, LOCAL
NEXT

SAVEGLOBAL

;全実績取得判定
;実績数カウント
CALL TROPHY_COUNT(200)
LOCAL:0 = B:0 + S:0 + G:0
LOCAL:1 = B:1 + S:1 + G:1
SIF LOCAL:0 == LOCAL:1
	GLOBAL:200 |= 1p0

SAVEGLOBAL

