;--------------------------------------------------
;判定（GLOBAL:201）
;--------------------------------------------------
@TROPHY_CHECK_201(ARG, ARG:1)
LOADGLOBAL
VARSET LOCAL, 0

;ARG　4：四重絶頂、5：五重絶頂
;ARG:1　4：最強四重絶頂、5：最強五重絶頂
IF ARG == 4
	GLOBAL:201 |= 1p1
	LOCAL:10 = 1
ENDIF
IF ARG:1 == 4
	GLOBAL:201 |= 1p2
	LOCAL:10 = 1
ENDIF
IF ARG == 5
	GLOBAL:201 |= 1p3
	LOCAL:10 = 1
ENDIF
IF ARG:1 == 5
	GLOBAL:201 |= 1p4
	LOCAL:10 = 1
ENDIF

FOR LOCAL, 0, 50
	;条件をクリアしているか
	TRYCALLFORM TROPHY_201_{LOCAL}
	IF RESULT
		SETBIT GLOBAL:201, LOCAL
		LOCAL:10 = 1
	ENDIF
NEXT

;SAVEGLOBALを呼び出しすぎると強制終了しやすいので
;最小限呼び出すようにした
SIF LOCAL:10 == 1
	SAVEGLOBAL


VARSET LOCAL, 0
;全実績取得判定
;実績数カウント
CALL TROPHY_COUNT(201)
LOCAL:0 = B:0 + S:0 + G:0
LOCAL:1 = B:1 + S:1 + G:1
IF LOCAL:0 == LOCAL:1
	GLOBAL:201 |= 1p0
	SAVEGLOBAL
ENDIF
	


