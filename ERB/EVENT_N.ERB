;---------------------------------------------------------
;中田氏・妊娠フラグに絡む関数郡
;膣射1～20までは4刻みで確率が上昇する。
;20～は一律。毎回リセット。
;小柄体系は妊娠しづらい。
;---------------------------------------------------------

@IN_VAGINA_M_TO_T
;中田氏による妊娠、非妊娠のチェック
;CFLAG:101使用

IF TARGET != -1
	SIF TEQUIP:87
		RETURN 0
	;奴隷の妊娠チェック
	IF TALENT:TARGET:妊娠 == 0
		T = TARGET
		P = 1
		CALL NAKADASHI_CHECK
	ENDIF
ENDIF

@IN_VAGINA_M_TO_A
;中田氏による妊娠、非妊娠のチェック
;CFLAG:ASSI:101使用

;助手がいるか
IF ASSI >= 1
	SIF TEQUIP:ASSI:87
		RETURN 0
	;助手の妊娠チェック
	IF TALENT:ASSI:妊娠 == 0
		T = ASSI
		P = 1
		CALL NAKADASHI_CHECK
	ENDIF
ENDIF


@IN_VAGINA_A_TO_T
;助手→奴隷への中田氏による妊娠判定
;CFLAG:102使用

;助手とターゲットがいるか
IF ASSI >= 1 && TARGET != -1
	SIF TEQUIP:87
		RETURN 0
	;奴隷が妊娠してるかの判定
	IF TALENT:TARGET:妊娠 == 0
		T = TARGET
		P = 2
		CALL NAKADASHI_CHECK
	ENDIF
ENDIF

@IN_VAGINA_T_TO_A
;奴隷→助手への中田氏による妊娠判定
;CFLAG:ASSI:103使用

;助手がいるか
IF ASSI >= 1
	SIF TEQUIP:ASSI:87
		RETURN 0
	;助手が妊娠してるかの判定
	IF TALENT:ASSI:妊娠 == 0
		T = ASSI
		P = 3
		CALL NAKADASHI_CHECK
	ENDIF
ENDIF

@IN_VAGINA_T_TO_M
;奴隷→主人への中田氏による妊娠判定
;CFLAG:MASTER:103使用


IF TARGET != -1
	SIF TEQUIP:MASTER:87
		RETURN 0
	;主人が妊娠してるかの判定
	IF TALENT:MASTER:妊娠 == 0
		T = MASTER
		P = 3
		CALL NAKADASHI_CHECK
	ENDIF
ENDIF

;---------------------------------------------------------
;中出しされた精子量に応じて確率処理
;チェックするフラグ設定
;CFLAG
;101→マスターによる中田氏カウント用
;102→助手による中田氏カウント用
;103→パートナーによる中田氏カウント用
;---------------------------------------------------------
@NAKADASHI_CHECK
;誰に中出しされたか設定(P=1～3)
F = 100 + P

;そもそも中だしされてないなら戻り
SIF CFLAG:T:F == 0
	RETURN 0

;膣内射精量で分岐するのでその下準備
;危険日である
IF TALENT:T:危険日
	X = CFLAG:T:F 
;危険日でない
ELSE
	X = CFLAG:T:F / 6
ENDIF
SIF X > 5
	X = 5
;妊娠するかの判定
;1~36分の1で妊娠する
;6 - X + (X == 1) + (X == 0) * 2：基本値設定
;(P > 1) * ((X > 0) + (X > 1))：父親が主人以外時の補正
;TALENT:T:小柄体型 * 2：小柄体型の補正
;3 - (TEQUIP:T:86 * 2)：排卵誘発剤による補正←が小柄体型の時しか動いていない…多分不具合だと思うので上記計算結果に掛かる様に修正
SIF RAND:(((6 - X + (X == 1) + (X == 0) * 2) + ((P > 1) * ((X > 0) + (X > 1))) + (TALENT:T:小柄体型 * 2)) * (3 - (TEQUIP:T:86 * 2))) == 0
	CFLAG:T:104 = P

;---------------------------------------------------------
;妊娠処理関数郡
;---------------------------------------------------------

@GET_CHILD_M_TO_T
;マスターの精液でターゲット妊娠発覚時の処理
;CFLAG:108→素質の消滅をカウントする変数
IF TARGET != -1
	IF TALENT:TARGET:妊娠 == 0 && CFLAG:TARGET:104 == 1
		PRINTFORMW %CALLNAME:TARGET%の様子がおかしい…
		PRINTFORML %CALLNAME:TARGET%は子供を身篭った様だ
		PRINTFORMW %NAME:TARGET%は[%TALENTNAME:134%]した
		TALENT:TARGET:妊娠 = 1

		T = TARGET
		CALL CHANGE_N_STATUS

		;妊娠時の口上をイベント口上でやってみるのはいいとして分岐どうしよう…
		;基本的には口上側で分岐してもらう方向で
		TFLAG:13 = 20
		CALL SELF_KOJO
	ENDIF
ENDIF

@GET_CHILD_M_TO_A
;マスターの精液で助手妊娠発覚時の処理
;CFLAG:108→素質の消滅をカウントする変数

;助手がいるか
IF ASSI >= 1
	IF TALENT:ASSI:妊娠 == 0 && CFLAG:ASSI:104 == 1
		PRINTFORMW %CALLNAME:ASSI%の様子がおかしい…
		PRINTFORML %CALLNAME:ASSI%は子供を身篭った様だ
		PRINTFORMW %NAME:ASSI%は[%TALENTNAME:134%]した
		TALENT:ASSI:妊娠 = 1

		T = ASSI
		CALL CHANGE_N_STATUS

		;主人が助手を妊娠させた時のパートナーの口上
		TFLAG:13 = 20
		CALL SELF_KOJO
		;口上用にターゲットを待避してから助手の番号を入れる
		Z = TARGET
		TARGET = ASSI
		TFLAG:13 = 94
		CALL SELF_KOJO
		TARGET = Z
	ENDIF
ENDIF

@GET_CHILD_A_TO_T
;助手の精液で奴隷妊娠発覚時の処理
;CFLAG:108→素質の消滅をカウントする変数

;助手と奴隷がいるか
IF ASSI >= 1 && TARGET != -1
	IF TALENT:TARGET:妊娠 == 0 && CFLAG:TARGET:104 == 2
