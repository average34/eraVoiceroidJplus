;---------------------------------------------------------
;妊娠時のステータスの変化
;基本的に対象が変わるだけで同じなので関数にくくりだした
;体力上限を減らす
;体力が上限超えることもあるので、体力501以上なら500にする
;---------------------------------------------------------
@CHANGE_N_STATUS
MAXBASE:T:体力 -= 500
IF BASE:T:体力 >= 501
	BASE:T:体力 = 500
	IF T == TARGET
		PRINTFORMW 酷く疲れているようだ……休ませた方が良いだろう
		PRINTFORMW 　
	ENDIF
ENDIF

;出産系のフラグ設定
CFLAG:T:110 = DAY:7 + 20
IF CFLAG:T:104 == 1
	CFLAG:T:111 = NO:MASTER
ELSEIF CFLAG:T:104 == 2 && ASSI >= 0
	CFLAG:T:111 = NO:ASSI
ELSE
	CFLAG:T:111 = NO:TARGET
ENDIF

;主人→助手、パートナー
IF CFLAG:T:104 == 1
	;恋慕なら
	IF TALENT:T:恋慕 == 1
		;相愛なら
		IF TALENT:T:相愛 == 1
			PRINTFORMW %NAME:T%は%CALLNAME:MASTER%の子を授かった喜びに表情を緩ませている
		;相愛を持っておらず親愛を持っていれば
		ELSEIF TALENT:T:親愛 == 1
			PRINTFORMW %CALLNAME:MASTER%の子を授かった喜びに表情を緩ませている
		ELSE
			PRINTFORMW %CALLNAME:MASTER%の子を授かった喜びに表情を緩ませている
		ENDIF
		CALL N_TALENT_R
	;反発感情Lv2以上なら壊れる
	ELSEIF MARK:T:反発感情 >= 2
		PRINTFORML %CALLNAME:T%は幽鬼のような表情をしている
		PRINTFORMW %CALLNAME:T%の中で何かが壊れてしまったようだ
		CALL N_TALENT_K
	ELSE
		PRINTFORMW %CALLNAME:TARGET%は困惑しつつも%CALLNAME:MASTER%の子を授かった事を受け入れた
	ENDIF
;助手→パートナー
ELSEIF CFLAG:T:104 == 2 && ASSI >= 0 && T == TARGET
	;恋慕無しで反発感情Lv2以上なら壊れる
	IF TALENT:T:恋慕 == 0 && MARK:T:反発感情 >= 2
		PRINTFORML %CALLNAME:T%は幽鬼のような表情をしている
		PRINTFORMW %CALLNAME:T%の中で何かが壊れてしまったようだ
		CALL N_TALENT_K
	ELSE
		IF TALENT:T:恋慕 == 0
			PRINTFORM %CALLNAME:TARGET%は困惑しつつも
		ELSE
			PRINTFORM %CALLNAME:TARGET%は複雑な表情を浮かべつつも
		ENDIF
		PRINTFORMW %CALLNAME:ASSI%の子を授かった事を受け入れた
	ENDIF
;パートナー→助手
ELSEIF CFLAG:T:104 == 3 && ASSI >= 0 && T == ASSI
	;恋慕無しで反発感情Lv2以上なら壊れる
	IF TALENT:T:恋慕 == 0 && MARK:T:反発感情 >= 2
		PRINTFORML %CALLNAME:T%は幽鬼のような表情をしている
		PRINTFORMW %CALLNAME:T%の中で何かが壊れてしまったようだ
		CALL N_TALENT_K
	ELSE
		IF TALENT:T:恋慕 == 0
			PRINTFORM %CALLNAME:ASSI%は困惑しつつも
		ELSE
			PRINTFORM %CALLNAME:ASSI%は複雑な表情を浮かべつつも
		ENDIF
		PRINTFORMW %CALLNAME:TARGET%の子を授かった事を受け入れた
	ENDIF
ELSEIF CFLAG:T:104 == 3 && T == MASTER
	PRINTFORMW %CALLNAME:TARGET%の子を授かった事を伝えた
	;恋慕なら
	IF TALENT:恋慕 == 1
		PRINTFORM %CALLNAME:TARGET%は
		SIF TALENT:TARGET:オトコ == 0
			PRINT 困惑しながらも
		PRINTFORMW %CALLNAME:MASTER%との間に子供が出来た喜びに表情を緩ませている
	;反発感情LV2以上
	ELSEIF MARK:反発感情 >= 2
		PRINTFORMW しかし、%CALLNAME:TARGET%は興味なさそうだ…
	ELSE
		IF TALENT:TARGET:オトコ
			PRINTFORMW %CALLNAME:TARGET%は困惑しながらもどこか嬉しそうである
		ELSE
			PRINTFORMW %CALLNAME:TARGET%は困惑しながらも事態を受け入れた
		ENDIF
	ENDIF
ENDIF
	
;乳がワンランクパワーアップする
;貧→普　普→巨　巨→そのまま
IF TALENT:T:貧乳 == 1
	PRINTFORMW %CALLNAME:T%の胸が大きくなった
	PRINTFORMW [%TALENTNAME:109%]から普通のサイズになった
	PRINTFORMW 　
	TALENT:T:貧乳 = 0
	MARK:T:89 = 1
ELSEIF TALENT:T:貧乳 == 0 && TALENT:T:巨乳 == 0
	PRINTFORMW %CALLNAME:T%の胸が大きくなった
	PRINTFORMW [%TALENTNAME:110%]になった
	PRINTFORMW 　
	TALENT:T:巨乳 = 1
	MARK:T:89 = 1
ENDIF

;回復遅いをつける(回復早いは一時的に消去)
;CFLAG:RESULT:96に回復遅い等を元々持っていたかを記録
IF TALENT:T:回復早い
	CFLAG:T:96 = 1
ELSEIF TALENT:T:回復遅い
	CFLAG:T:96 = 2
ENDIF
IF TALENT:T:回復遅い == 0
	PRINTFORMW %CALLNAME:T%は[%TALENTNAME:112%]を得た
	PRINTFORMW 　
	TALENT:T:回復早い = 0
	TALENT:T:回復遅い = 1
ENDIF

;---------------------------------------------------------
;妊娠時の素質変化(主人→恋慕付きのコピペ)
;---------------------------------------------------------
@N_TALENT_R
;初めだけ名前表示するためにCFLAG:T:108を使っていたのをLOCAL:99使用に変更
;以下の素質消滅
;反抗的、自制心、無関心、感情乏しい、一線越えない、貞操無頓着、抑圧、抵抗、弱み、献身的、男嫌い、貞操観念
VARSET LOCAL, 0
LOCAL:0 = 11
LOCAL:1 = 20
LOCAL:2 = 21
LOCAL:3 = 22
LOCAL:4 = 27
LOCAL:5 = 31
LOCAL:6 = 32
LOCAL:7 = 34
LOCAL:8 = 37
LOCAL:9 = 63
REPEAT 10
	IF TALENT:T:(LOCAL:COUNT)
		;初めだけ名前表示
		IF LOCAL:99 == 0
			PRINTS NAME:T
			PRINT は
		ENDIF
		;失う素質名表示、素質消滅
		PRINTFORM [%TALENTNAME:(LOCAL:COUNT)%]
		TALENT:T:(LOCAL:COUNT) = 0
		;初めだけ名前表示するためにLOCAL:99使用
		LOCAL:99 = 1
	ENDIF
REND
;以下は特殊なため別枠
;男嫌い
F = CFLAG:T:111
IF TALENT:T:男嫌い && TALENT:F:オトコ
	IF LOCAL:99 == 0
		PRINTS NAME:T
		PRINT は
	ENDIF
	PRINTFORM [%TALENTNAME:82%]
	TALENT:T:男嫌い = 0
	LOCAL:99 = 1
ENDIF
;貞操観念
IF TALENT:T:貞操観念 == 0
	SIF LOCAL:99 != 0
		PRINTW を失い、
	PRINTFORMW [%TALENTNAME:30%]を得た
	TALENT:T:貞操観念 = 1
ELSEIF LOCAL:99 != 0
	PRINTW を失った
ENDIF

;---------------------------------------------------------
;妊娠時の素質変化(壊れたときのコピペ)
;---------------------------------------------------------
@N_TALENT_K
;以下の素質消滅
;反抗的、気丈、プライド高い、自制心、一線越えない、貞操無頓着、抑圧、抵抗、恥じらい、
;献身的、快感に素直、魅惑、謎の魅力、魅力、鼓舞
VARSET LOCAL, 0
LOCAL:0 = 11
LOCAL:1 = 12
LOCAL:2 = 15
LOCAL:3 = 20
LOCAL:4 = 27
LOCAL:5 = 31
LOCAL:6 = 32
LOCAL:7 = 34
LOCAL:8 = 35
LOCAL:9 = 63
LOCAL:10 = 70
LOCAL:11 = 91
LOCAL:12 = 92
LOCAL:13 = 113
LOCAL:14 = 118
REPEAT 15
	IF TALENT:T:(LOCAL:COUNT)
		;初めだけ名前表示
		IF LOCAL:99 == 0
			PRINTS NAME:T
			PRINT は
		ENDIF
		;失う素質名表示、素質消滅
		PRINTFORM [%TALENTNAME:(LOCAL:COUNT)%]
		TALENT:T:(LOCAL:COUNT) = 0
		;初めだけ名前表示するためにLOCAL:99使用
		LOCAL:99 = 1
	ENDIF
REND

;TをLOCAL:98に一時保存
LOCAL:98 = T

;以下の素質取得
;無関心、感情乏しい、男嫌い、狂気
;父親が男の場合男嫌い取得。EとCFLAG:T:111を判定に使用
T = CFLAG:T:111
CALL GET_EXISTANCE
E = E-1
REPEAT 4
	LOCAL:COUNT = 0
REND
LOCAL:0 = 21
LOCAL:1 = 22
SIF TALENT:E:オトコ
	LOCAL:2 = 82
LOCAL:3 = 123

;TをLOCAL:98から復帰
T = LOCAL:98

REPEAT 4
	IF TALENT:T:(LOCAL:COUNT) == 0 && LOCAL:COUNT > 0
		;一回だけ表示
		SIF LOCAL:99 == 1
			PRINTW を失い、
		;失う素質名表示、素質消滅
		PRINTFORM [%TALENTNAME:(LOCAL:COUNT)%]
		TALENT:T:(LOCAL:COUNT) = 1
		;表示のためにLOCAL:99使用
		LOCAL:99 = 2
	ENDIF
REND

IF LOCAL:99 == 2
	PRINTW を得た
ELSEIF LOCAL:99 == 1
	PRINTW を失った
ENDIF

;---------------------------------------------------------
;臨月到達
;---------------------------------------------------------
@REACH_FULL_TERM
MARK:COUNT:90 = 1
CFLAG:COUNT:13 = 0

IF CFLAG:COUNT:21 == 0
	PRINTFORML %CALLNAME:COUNT%は臨月を迎えました
	PRINTFORMW %CALLNAME:COUNT%は出産に備え育児室へ移動します
ENDIF
SIF FLAG:97 == 0
	FLAG:97 = 1

TFLAG:13 = 25
Z = TARGET
TARGET = COUNT
CALL SELF_KOJO
TARGET = Z


IF TARGET == COUNT
	LOCAL = COUNT
	CALL SET_TMP_PARTNER
	COUNT = LOCAL
	;@SET_TMP_PARTNERで好感度マイナスの仲間しかいない等で暫定パートナーが選択されなかった場合パートナーなし
	SIF MARK:TARGET:90
		TARGET = -1
ELSEIF ASSI == COUNT
	ASSI = -1
ELSEIF MASTER == COUNT
	;主人公が臨月の場合@EVENT_NEWDAYで出産まで自動進行
ENDIF

;---------------------------------------------------------
;出産
;---------------------------------------------------------
@CHILD_BIRTH
C = COUNT
T = CFLAG:C:111
CALL GET_EXISTANCE
E = E-1

;口上を呼ぶキャラと分岐を設定
IF C == MASTER
	;父親不在
	IF CFLAG:E:21
		;呼ばない
		Y = -1
		;ノーマル
		TFLAG:17 = 2
	;居て恋慕
	ELSEIF TALENT:E:恋慕
