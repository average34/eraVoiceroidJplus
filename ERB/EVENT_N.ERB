		TFLAG:13 = 20
		CALL SELF_KOJO
		;妊娠させた助手の口上を呼び出す
		;口上用にターゲットを待避してから助手の番号を入れる
		Z = TARGET
		TARGET = ASSI
		TFLAG:13 = 94
		CALL SELF_KOJO
		TARGET = Z
	ENDIF
ENDIF


@GET_CHILD_T_TO_A
;奴隷の精液で助手妊娠発覚時の処理
;CFLAG:108→素質の消滅をカウントする変数

;助手がいるか
IF ASSI >= 1 && TARGET != -1
	IF TALENT:ASSI:妊娠 == 0 && CFLAG:ASSI:104 == 3
		PRINTFORMW %CALLNAME:ASSI%の様子がおかしい…
		PRINTFORML %CALLNAME:ASSI%は%CALLNAME:TARGET%の子供を身篭った様だ
