;追加処理及びサブイベント

;-------------------------------------------------
;絶頂時の追加処理
;-------------------------------------------------
@ECST_CHECK
TFLAG:29 = C + V + A + B + M

;-------------------------------------------------
;主人による調教の経験値
;-------------------------------------------------
@MASTER_FLAG_CHECK
;絶頂
TFLAG:29 += TFLAG:10 + TFLAG:11
Q = 0
SIF NOWEX:0 > 0
	Q += 1
SIF NOWEX:1 > 0
	Q += 1
SIF NOWEX:2 > 0
	Q += 1
SIF NOWEX:3 > 0
	Q += 1
SIF NOWEX:6 > 0
	Q += 1
SIF TFLAG:10 > 0
	Q += 1
SIF TFLAG:11 > 0
	Q += 1
TFLAG:30 += TFLAG:29 * Q

;射精
REPEAT 5
	SIF TFLAG:COUNT > 0 && EXP:20 >= EXPLV:3
		TFLAG:30 += TFLAG:COUNT
REND
SIF TFLAG:9 > 0 && EXP:20 >= EXPLV:3
	TFLAG:30 += TFLAG:9

;主人と対象の能力を計算
IF ASSIPLAY == 0 && TEQUIP:90 == 0
	;ABL:従順をみる
	IF ABL:0 == 0
		R = 0
	ELSEIF ABL:0 <= 3
		R = 1
	ELSEIF ABL:0 <= 7
		R = 2
	ELSEIF ABL:0 <= 10
		R = 3
	ELSEIF ABL:0 <= 15
		R = 4
	ELSE
		R = 5
	ENDIF

	;素質による増減
	;反抗的
	SIF TALENT:11
		R -= 1
	;素直
	SIF TALENT:13
		R += 1
	;自制心
	SIF TALENT:20
		R -= 1
	;無関心
	SIF TALENT:21
		R -= 1
	;感情乏しい
	SIF TALENT:22
		R -= 1
	;抵抗
	SIF TALENT:34
		R -= 1
	;献身的
	SIF TALENT:63
		R += 1
	;快感に素直
	SIF TALENT:70
		R += 1
	;快感の否定
	SIF TALENT:71
		R -= 1
	;男嫌い
	SIF TALENT:82 && TALENT:MASTER:122
		R -= 1
	;恋慕
	SIF TALENT:85
		R += 2
	;親愛
	SIF TALENT:88
		R += 3
	;恋人
	SIF TALENT:153
		R += 2
	;服従
	SIF TALENT:180
		R += 2
	;烙印
	SIF TALENT:181
		R += 1
	;隷属
	SIF TALENT:182
		R += 3
	;妄信
	SIF TALENT:86
		R += 2
	;主人の魅惑
	SIF TALENT:MASTER:91
		R += 1
	;主人の謎の魅力
	SIF TALENT:MASTER:92
		R += 1
	;主人の魅力
	SIF TALENT:MASTER:113
		R += 1
	;主人の人気
	SIF TALENT:MASTER:126
		R += 1
	;幼稚
	;SIF TALENT:132
		;R += 1

	;結果が0以下の場合
	SIF R <= 0
		R = 1
	;結果が10以上の場合
	SIF R >= 10
		R = 10

	IF TFLAG:18 < 0
		;コマンドに失敗している場合、TFLAG:30をそのまま使う
		R = TFLAG:30
	ELSE
		R += TFLAG:30
	ENDIF

	;好感度補正
	IF CFLAG:2 <= 50
		R += 0
	ELSEIF CFLAG:2 <= 250
		R += 1
	ELSEIF CFLAG:2 <= 800
		R += 2
	ELSEIF CFLAG:2 <= 3000
		R += 3
	ELSE
		R += 5
	ENDIF


	;相性による修正
	IF RELATION:(NO:MASTER) != 0
		R *= RELATION:(NO:MASTER)
		R /= 100
	ENDIF

	;パートナー補正
	SIF TALENT:150
		TIMES R , 1.20

	;霊夢様補正
	SIF TALENT:201
		TIMES R , 0.20

	;特殊能力(好感度ボーナス)補正
	IF R > 0
		R += ABL:MASTER:81
	ENDIF

	;「何もしない」は好感度上昇量が一律で+0～3 (見つめあう　はこれの影響を受けない）
	IF SELECTCOM == 7
		R = RAND:4
	ENDIF

	CFLAG:2 += R
PRINTFORML 好感度上昇:{R}
SIF ((FLAG:23 & 1p26) == 0) && FLAG:2 != 1
	PRINTFORMW 好感度合計:{CFLAG:2}
ENDIF
R = 0
TFLAG:30 = 0

;-------------------------------------------------
;主人による調教のボーナス
;-------------------------------------------------
