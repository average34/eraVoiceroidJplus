			PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%に処女を捧げられた事に幸福を感じているようだ…
			TFLAG:150 = 1
		ENDIF
		TIMES SOURCE:3 , 2.00
		;TIMES SOURCE:6 , 0.50
		TIMES SOURCE:15 , 0.30
	ENDIF
ENDIF

;親愛
;主人経験との重複前提で計算
IF TALENT:88 && ASSIPLAY == 0
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:2 , 1.50
	TIMES SOURCE:3 , 2.00
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
	TIMES SOURCE:7 , 1.20
	TIMES SOURCE:17 , 1.50
	TIMES SOURCE:18 , 2.00
	TIMES SOURCE:24 , 1.50
	IF TALENT:0 && TFLAG:20
		L = 0
		R = NO:PLAYER
		SIF (RELATION:R == 0 || RELATION:R >= 100) && SOURCE:6 < 2000 && SOURCE:15 <= 3000
			L += 1
		SIF TEQUIP:11 == 0 && TEQUIP:13 == 0 && TEQUIP:14 == 0 && TEQUIP:15 == 0 && TEQUIP:16 == 0
			L += 1
		SIF TEQUIP:43 == 0 && TEQUIP:44 == 0 && TEQUIP:45 == 0 && TEQUIP:46 == 0 && TEQUIP:53 == 0 && TEQUIP:54 == 0 && TEQUIP:90 == 0
			L += 1
		IF L == 3 && FLAG:6
			PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%が初めての相手である事にこの上ない幸せを感じているようだ…
			TFLAG:150 = 1
		ENDIF
		TIMES SOURCE:3 , 2.00
		;TIMES SOURCE:6 , 0.50
		TIMES SOURCE:15 , 0.10
	ENDIF
ENDIF

