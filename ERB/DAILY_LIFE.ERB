@DAILY_LIFE
#DIM LCOUNT, 1

;表示頻度
SIF FLAG:7 <= 0
	RETURN 1

;調教をまだ行っていなければ引き返す
VARSET LOCALS
FOR LCOUNT, 0, CHARANUM
	;主人公を除く
	SIF LCOUNT == MASTER
		CONTINUE
	;別れていない && (初調教口上が表示済み || 好感度 > 30) && 妊娠していない
	SIF CFLAG:LCOUNT:21 == 0 && (CFLAG:LCOUNT:201 == 1 || CFLAG:LCOUNT:2 > 30) && (MARK:LCOUNT:90 == 0)
		LOCALS += TOSTR(LCOUNT) + "/"
NEXT
SIF STRCOUNT(LOCALS, "/") == 0
	RETURN 1

DRAWLINE

;メインキャラを選ぶ
LOCAL = AUTO_SPLIT_INT(LOCALS, "/", RAND:STRCOUNT(LOCALS, "/"))

;個別
TRYCCALLFORM DAILY_LIFE_MESSAGE_{NO:LOCAL}(LOCAL)
;共通
CATCH
	CALL DAILY_LIFE_MESSAGE(LOCAL)
ENDCATCH

