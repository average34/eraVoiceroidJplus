		PRINTV '(,10,')
		S = 1
	ELSEIF RELATION:R > 0 && RELATION:R < 70
		PRINT  - 
		PRINT 相性悪い
		PRINTV '(,10,')
		S = 1
	ELSEIF RELATION:R > 0 && RELATION:R < 100
		PRINT  - 
		PRINT 相性いまいち
		PRINTV '(,3,')
		S = 1
	ELSEIF RELATION:R >= 100 && RELATION:R < 130
		SIF S
			PRINT  + 
		PRINT 相性ややよい
		PRINTV '(,3,')
		S = 1
	ELSEIF RELATION:R >= 100 && RELATION:R < 170
		SIF S
			PRINT  + 
