	;酔ってる
	IF TFLAG:47 >= 5
		SIF S
			PRINT  + 
		IF TFLAG:47 >= 60
			PRINT 泥酔
		ELSEIF TFLAG:47 >= 30
			PRINT 酩酊
		ELSEIF TFLAG:47 >= 5
			PRINT ほろ酔い
		ENDIF
		PRINTV '(,TFLAG:47 / 5,')
		S = 1
