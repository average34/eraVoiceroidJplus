;-------------------------------------------------
;朝に起こるイベント
;-------------------------------------------------
@EVENT_NEWDAY
;必殺技の名前変更
TRYCALL SPELL_NAME_CHANGE
IF TARGET > -1
	IF TFLAG:121
		;朝イベントスキップ
	ELSEIF MARK:反発感情 >= 3
		;騒音で起こされる
		CALL EVENT_NEWDAY_OTO
	ELSEIF TALENT:恋慕 == 0 && TALENT:淫乱 == 0
		;朝イベントは起きない
	ELSE
		;朝に起こるイベントが発生するかチェック
		CALL EVENT_NEWDAY_CHECK
	ENDIF
ENDIF
;出産関係チェック
CALL NINSSINXTU
;主人公が臨月の場合出産まで自動進行
IF MARK:MASTER:90
	FOR LOCAL, 0, 3
		CALL EVENT_NEXTDAY
		CALL EVENT_DANGERDAY
		DAY = DAY+1
		DAY:7 = DAY:7+1
		PRINTFORMW 
		PRINTFORMW ―――{LOCAL+1}日経過―――
		PRINTFORMW …………
		PRINTFORMW ………
		PRINTFORMW ……
		CALL NINSSINXTU
	NEXT
ENDIF
RETURN 0

@NINSSINXTU
;出産関連
IF (FLAG:62 & 1)
	REPEAT CHARANUM
		;妊娠時
		IF TALENT:COUNT:妊娠
			;出産3日前からは臨月突入で調教不可に
			IF (CFLAG:COUNT:110 - 3) <= DAY:7 && (MARK:COUNT:90 != 1)
				DRAWLINE
				CALL REACH_FULL_TERM
			;出産予定日なら出産
			ELSEIF CFLAG:COUNT:110 <= DAY:7
				IF CFLAG:COUNT:21 == 0
					DRAWLINE
					CALL CHILD_BIRTH
				ELSE
					TALENT:COUNT:妊娠 = 0
					TALENT:COUNT:育児中 = 1
				ENDIF
			ENDIF
		;育児中
		ELSEIF TALENT:COUNT:育児中
			;出産20日で親離れで調教可能に
			IF (CFLAG:COUNT:110 + 20) <= DAY:7
				IF CFLAG:COUNT:21 == 0
					DRAWLINE
					CALL DEPEARENT
				ELSE
					TALENT:COUNT:育児中 = 0
					MARK:COUNT:90 = 0
				ENDIF
			ENDIF
		ENDIF
	REND
ENDIF

@EVENT_NEWDAY_CHECK
;ランダムに発生するかとチェックし、全部やってダメなら終了
;疑似乱数だから収束しない可能性もあるが、まあ多分大丈夫
I = 0
$LOOP_M
R = RAND:8
IF R == 0
	SIF I & 1
		GOTO LOOP_M
	CALL EVENT_NEWDAY_PI
	I |= 1
ELSEIF R == 1
	SIF I & 2
		GOTO LOOP_M
	CALL EVENT_NEWDAY_SEX
	I |= 2
ELSEIF R == 2
	SIF I & 4
		GOTO LOOP_M
	CALL EVENT_NEWDAY_YOME
	I |= 4
ELSEIF R == 3
	SIF I & 8
		GOTO LOOP_M
	CALL EVENT_NEWDAY_KISS
	I |= 8
ELSEIF R == 4
	SIF I & 16
		GOTO LOOP_M
	CALL EVENT_NEWDAY_F
	I |= 16
ELSEIF R == 5
	SIF I & 32
		GOTO LOOP_M
	CALL EVENT_NEWDAY_SOINE
	I |= 32
ELSEIF R == 6
	SIF I & 64
		GOTO LOOP_M
	CALL EVENT_NEWDAY_NEGAO
	I |= 64
ELSEIF R == 7
	SIF I & 128
		GOTO LOOP_M
	CALL EVENT_NEWDAY_FEMALE
	I |= 128
ENDIF
SIF RESULT == 0 && I < 255
	GOTO LOOP_M
RETURN 0

@EVENT_NEWDAY_PI
;カットフラグが立っていたらリターン
SIF CFLAG:97 & 1
	RETURN 0
;パートナーがオトコならリターン
SIF TALENT:オトコ
	RETURN 0
;反発感情があるとダメ
SIF MARK:反発感情 > 0
	RETURN 0
;パイズリで起こされる
;欲望8以上奉仕精神8以上精液中毒1以上で発生、奉仕精神と精液中毒で回数判定
IF ABL:欲望 >= 8 && ABL:奉仕精神 >= 8 && ABL:精液中毒 >= 1 && (TALENT:MASTER:半人半妖 || TALENT:MASTER:ふたなり || TALENT:MASTER:オトコ) && TALENT:TARGET:恋慕
	IF ABL:奉仕精神 + ABL:精液中毒 < 6
		A = 1
	ELSEIF ABL:奉仕精神 + ABL:精液中毒 < 12
		A = 2
	ELSEIF ABL:奉仕精神 + ABL:精液中毒 < 18
		A = 3
	ELSEIF ABL:奉仕精神 + ABL:精液中毒 < 24
		A = 4
	ELSEIF ABL:奉仕精神 + ABL:精液中毒 >= 24
		A = 2 + (ABL:奉仕精神 / 6) + (ABL:精液中毒 / 4)
	ENDIF
	
	;男嫌い
	SIF TALENT:男嫌い && TALENT:MASTER:オトコ
		A -= 1
	;両刀
	SIF TALENT:両刀 && TALENT:MASTER:オトコ == 0
		A += 1
	;汚臭鈍感
	SIF TALENT:汚臭鈍感
		A += 1
	;汚臭敏感
	SIF TALENT:汚臭敏感
		A -= 1
	;献身的
	SIF TALENT:献身的
		A += 1
	;親愛
	SIF TALENT:親愛
		A += 1
	;相愛
	SIF TALENT:相愛
		A += 1
	;淫乱
	SIF TALENT:恋慕
		A += 1
	;B鈍感
	SIF TALENT:Ｂ鈍感
		A -= 1
	;B敏感
	SIF TALENT:Ｂ敏感
		A += 1
	;淫乳
	SIF TALENT:淫乳
		A += 1
	
	A:1 = A
	SIF A >= 64
		A = 64
	
	IF A > 0
		DRAWLINE
		DRAWLINE
		;カットフラグを退避
		LOCAL:100 = CFLAG:99
		;口上開始前の地の文をカットするか
		TFLAG:13 = 8
		CALL SELF_MAEKOJO
		;地の文章カット
		IF (CFLAG:99 & 1) == 0
			PRINTFORML 朝、%CALLNAME:MASTER%が目覚めると下腹部に違和感があるのに気づいた
			;貧乳の場合
			IF TALENT:貧乳
				PRINTFORML 気づかれないように目をやると、%CALLNAME:TARGET%が胸でペニスを弄っていた
				PRINTFORML どうやらパイズリの練習をしているらしい……
			;巨乳の場合	
			ELSEIF TALENT:巨乳
				PRINTFORML 目をやると%CALLNAME:TARGET%が大きな胸でペニスを挟み、擦り上げている
			;それ以外
			ELSE
				
				PRINTFORML 目をやると%CALLNAME:TARGET%が胸でペニスを擦り上げ、出てきた精液を吸っている
			ENDIF
		ENDIF
		;イベント時の口上呼び出し。CALL前にAをLOCALに退避、CALL後戻す
		LOCAL = A
		LOCAL:1 = A:1
		CALL SELF_KOJO
		A = LOCAL
		A:1 = LOCAL:1
		
		EXP:精液経験 += A
		PRINTFORML %EXPNAME:20%＋{A}
		EXP:奉仕快楽経験 += A/2
		PRINTFORML %EXPNAME:21%＋{A/2}
		IF (TALENT:MASTER:オトコ == 0) && (TALENT:オトコ == 0)
			PRINTFORML %EXPNAME:40%＋{A*2}
			EXP:レズ経験 += A*2
			EXP:MASTER:レズ経験 += A*2
		ENDIF
		PRINTFORML %PALAMNAME:4%の珠＋{A:1*100}
		;巨乳の場合のボーナス,欲情が2倍
		IF TALENT:巨乳
			PRINTFORML %PALAMNAME:5%の珠＋{A:1*60}
		ELSE
			PRINTFORML %PALAMNAME:5%の珠＋{A:1*30}
		ENDIF
		PRINTFORMW %PALAMNAME:6%の珠＋{A:1*40}
		;普乳の場合のボーナス追加、習得が少し増える
		IF TALENT:貧乳 == 0 && TALENT:巨乳 == 0
			PRINTFORML %PALAMNAME:7%の珠＋{A:1*60}
		ELSE
			PRINTFORML %PALAMNAME:7%の珠＋{A:1*40}
		ENDIF
		;貧乳の場合のボーナス,恥情が入る
		PRINTFORML %PALAMNAME:8%の珠＋{A:1*30}
		JUEL:恭順 += A:1*100
		IF TALENT:巨乳
			JUEL:欲情 += A:1*60
		ELSE
			JUEL:欲情 += A:1*30
		ENDIF
		JUEL:屈服 += A*40
		IF TALENT:貧乳 == 0 && TALENT:巨乳 == 0
			JUEL:習得 += A:1*60
		ELSE
			JUEL:習得 += A:1*40
		ENDIF
		SIF TALENT:貧乳
			JUEL:恥情 += A:1*30
		;カットフラグを戻す
		CFLAG:99 = LOCAL:100
		RETURN 1
	ELSE
		RETURN 0
	ENDIF
ELSE
	RETURN 0
ENDIF

