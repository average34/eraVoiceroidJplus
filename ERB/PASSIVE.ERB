;----------------------------------------------------------------
;ACTIVECOM
;----------------------------------------------------------------
@PASSIVEMODE_ACTIVECOM(ARG:0, ARG:1)
#DIM LCOUNT, 1

;TFLAG:0-29,100,540毎ターンリセット
REPEAT 30
	TFLAG:COUNT = 0
REND
TFLAG:100 = 0
TFLAG:540 = 0

;PLAYERに男性器がある
IF PENIS(PLAYER)
	SELECTCASE ARG:1
	;快感に耐える（1/4）
	CASE 6
		BASE:PLAYER:射精 -= CALC_EJAC(ARG:0, PLAYER) / 4 * 3
	;快感を受け入れる（3/4）
	CASE 7
		BASE:PLAYER:射精 -= CALC_EJAC(ARG:0, PLAYER) / 4
	;それ以外（1/2）
	CASEELSE
		BASE:PLAYER:射精 -= CALC_EJAC(ARG:0, PLAYER) / 2
	ENDSELECT
ENDIF

SKIPDISP 1
;デートは特殊処理
IF ARG:0 == 323
	CALL PASSIVEMODE_ACTIVECOM_323
;押し倒されるは特殊処理
ELSEIF ARG:0 == 999
	;ウフフ開始
	TFLAG:44 = 1
	;押し倒されフラグON
	TFLAG:51 = 1
;それ以外は対応コマンドを文字表示無しで呼び出してソース取得
ELSE
	TRYCALLFORM COM{ARG:0}
ENDIF
SKIPDISP 0

;胸愛撫選択時に快Bソース無い場合(背面座位の快Bソースを参考）
IF SOURCE:快Ｂ == 0 && ARG:1 == 3
	SOURCE:快Ｂ += (ABL:PLAYER:技巧 * 100) * 2 + (ABL:Ｂ感覚 * 30)
ENDIF

SELECTCASE ARG:1
;消極的にやる（1/4）
CASE 4
	FOR LCOUNT, 0, 25, 1
		TIMES SOURCE:LCOUNT, 0.25
	NEXT
;積極的にやる（3/4）
CASE 5
	FOR LCOUNT, 0, 25, 1
		TIMES SOURCE:LCOUNT, 0.75
	NEXT
;それ以外（1/2）
CASEELSE
	FOR LCOUNT, 0, 25, 1
		TIMES SOURCE:LCOUNT, 0.5
	NEXT
ENDSELECT

;----------------------------------------------------------------
;ACTIVECOM デートは特殊処理
;----------------------------------------------------------------
@PASSIVEMODE_ACTIVECOM_323
CALL PARTNER_SELECT_DATE
LOSEBASE:気力 += 220
;恋慕・恋人・親愛による気力消費軽減（デートが長続きする、の再現だとでも脳内補完）
SIF TALENT:恋慕
	LOSEBASE:気力 -= 40
SIF TALENT:恋人
	LOSEBASE:気力 -= 50
SIF TALENT:親愛
	LOSEBASE:気力 -= 90

;固定で獲得するソース
SOURCE:歓楽 = 100
SOURCE:接触 = 50

;ABL:従順をみる
IF ABL:親密 <= 1
	SOURCE:歓楽 += (ABL:親密 * 20)
	SOURCE:接触 += (ABL:親密 * 10)
ELSEIF ABL:親密 <= 3
	SOURCE:歓楽 += 200 + (ABL:親密 * 40)
	SOURCE:接触 += 100 + (ABL:親密 * 15)
ELSEIF ABL:親密 <= 5
	SOURCE:歓楽 += 500 + (ABL:親密 * 50)
	SOURCE:接触 += 200 + (ABL:親密 * 20)
ELSEIF ABL:親密 <= 8
	SOURCE:歓楽 += 700 + (ABL:親密 * 80)
	SOURCE:接触 += 300 + (ABL:親密 * 30)
ELSEIF ABL:親密 <= 11
	SOURCE:歓楽 += 1200 + (ABL:親密 * 100)
	SOURCE:接触 += 400 + (ABL:親密 * 35)
ELSE
	SOURCE:歓楽 += 2000 + (ABL:親密 * 130)
	SOURCE:接触 += 800 + (ABL:親密 * 40)
ENDIF

;好感度をみる
IF CFLAG:2 <= 500
	SOURCE:歓楽 += CFLAG:2
ELSEIF CFLAG:2 <= 5000
	SOURCE:歓楽 += 500 + (CFLAG:2 - 500) / 3
ELSE
	SOURCE:歓楽 += 2000 + (CFLAG:2 - 5000) / 5
ENDIF
SIF SOURCE:歓楽 < 0
	SOURCE:歓楽 = 0

;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:受動 += 300 + (ABL:親密 * 50)
