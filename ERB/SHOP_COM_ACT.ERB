@TURNEND_COM
VARSET LOCAL, 0
;SHOP.ERBのREPEAT CHARANUM中
;待機中指示コマンド実行
IF COUNT != MASTER && CFLAG:COUNT:17 == 0
	IF CFLAG:COUNT:13 == 0
		;何もしない
		;実行したコマンドを保存
		Y:COUNT = 0
	ELSEIF CFLAG:COUNT:13 == 1
		;修行
		TIMES A , 0.70
		TIMES B , 0.70
		TIMES C , 0.75
		LOCAL = RAND:3 + 1
		EXP:COUNT:95 += LOCAL
		CALL TURNEND_COM_PRINT, "EXP", 95, LOCAL
		Y:COUNT = 1
	ELSEIF CFLAG:COUNT:13 == 2
		;リフレッシュ
		A *= 2
		B *= 2
		C *= 3
		LOCAL = RAND:3 + 1
		CFLAG:COUNT:2 += LOCAL
		CALL TURNEND_COM_PRINT, "CFLAG", 2, LOCAL
		;実行したコマンドを保存
		Y:COUNT = 2
	ELSEIF CFLAG:COUNT:13 == 3
		;自給自足
		TIMES A , 0.10
		TIMES B , 0.10
		TIMES C , 0.10
		LOCAL = (ABL:COUNT:2 * ABL:COUNT:6 + 5) * (RAND:10 + 5) / 10
		JUEL:COUNT:16 += LOCAL
		CALL TURNEND_COM_PRINT, "JUEL", 16, LOCAL
		;実行したコマンドを保存
		Y:COUNT = 3
	ELSEIF CFLAG:COUNT:13 == 4 && H == 1
		;連携を確認する
		TIMES A , 0.80
		TIMES B , 0.80
		C *= 2
		LOCAL = RAND:7 + 5
		EXP:COUNT:95 += LOCAL
		CALL TURNEND_COM_PRINT, "EXP", 95, LOCAL
		LOCAL = RAND:21 + 20
		CFLAG:COUNT:2 += LOCAL
		CALL TURNEND_COM_PRINT, "CFLAG", 2, LOCAL
		BASE:MASTER:4 -= MAXBASE:MASTER:4 / 100
		;実行したコマンドを保存
		Y:COUNT = 4
	ELSEIF CFLAG:COUNT:13 == 5
		;家事
		TIMES A , 0.80
		TIMES B , 0.80
		C /= 2
		LOCAL = SQRT(ABL:COUNT:6 + ABL:COUNT:90) + RAND:6 / 2 + 1
		EXP:COUNT:90 += LOCAL
		CALL TURNEND_COM_PRINT, "EXP", 90, LOCAL
		LOCAL = (ABL:COUNT:6 * SQRT(ABL:COUNT:6) * (ABL:COUNT:90 * 2 + ABL:COUNT:2) / 3 + 5) * (RAND:10 + 5) / 10
		JUEL:COUNT:16 += LOCAL
		CALL TURNEND_COM_PRINT, "JUEL", 16, LOCAL
		LOCAL = RAND:6 + 5
		CFLAG:COUNT:2 += LOCAL
		CALL TURNEND_COM_PRINT, "CFLAG", 2, LOCAL
		;実行したコマンドを保存
		Y:COUNT = 5
	ELSEIF CFLAG:COUNT:13 == 6
		;モンスター退治
		IF BASE:COUNT:4 >= 500
			LOCAL = RAND:5 + 5
			EXP:COUNT:95 += LOCAL
			CALL TURNEND_COM_PRINT, "EXP", 95, LOCAL
			LOCAL = RAND:10 + 6
			CFLAG:COUNT:2 -= LOCAL
			CALL TURNEND_COM_PRINT, "CFLAG", 2, -LOCAL
			BASE:COUNT:4 -= RAND:11 * 30 + 200
			T = COUNT
			Z = COUNT
			CALL GET_BATTLE_POWER
			COUNT = Z

			A = 0
			B = 0

			D = P / 10 * (RAND:10 + 5)

			;稼ぎが高くなり過ぎないように補正をかける
			;7500を超えた分は25%OFF
			SIF D >= 7500
				D = 7500 + (D - 7500) / 4 * 3
			;補正後、15000を超えていたらその分を更に50%OFF
			SIF D >= 15000
				D = 15000 + (D - 15000) / 2
			;補正後、30000を超えていたらその分を更に75%OFF
			SIF D >= 30000
				D = 30000 + (D - 30000) / 4
			;補正後、50000を超えていたらその分を更に90%OFF
			SIF D >= 50000
				D = 50000 + (D - 50000) / 10
			;補正後、100000を超えていたらその分を更に90%OFF
			SIF D >= 100000
				D = 100000 + (D - 100000) / 10

			;実行したコマンドを保存
			Y:COUNT = 6
		ELSE
			PRINTFORML %CALLNAME:COUNT%はかなり疲労しているようだ…
		ENDIF
	ELSEIF CFLAG:COUNT:13 == 7 && H == 1
		;イチャイチャする
		C *= 3
		LOCAL = (ABL:COUNT:0 + 5) * (ABL:COUNT:6 + 3) * (RAND:10 + 10) / 10
		JUEL:COUNT:15 += LOCAL
		CALL TURNEND_COM_PRINT, "JUEL", 15, LOCAL
		LOCAL = RAND:51 + 50
		CFLAG:COUNT:2 += LOCAL
		CALL TURNEND_COM_PRINT, "CFLAG", 2, LOCAL
		BASE:MASTER:4 -= MAXBASE:MASTER:4 / 200
		;実行したコマンドを保存
		Y:COUNT = 7
	ELSEIF CFLAG:COUNT:13 == 8 && H == 1
		;朝フェラ 朝じゃなくてもやってくれます。昼寝の寝起き？
		TIMES A , 0.90
		TIMES B , 0.90
		LOCAL = RAND:11 + 10
		CFLAG:COUNT:2 += LOCAL
		CALL TURNEND_COM_PRINT, "CFLAG", 2, LOCAL
		BASE:MASTER:4 -= MAXBASE:MASTER:4 / 200

		G = 0
		IF ABL:COUNT:11 == 1
			G = 1
		ELSEIF ABL:COUNT:11 == 2
			G = 2
		ELSEIF ABL:COUNT:11 == 3
			G = 3
		ELSEIF ABL:COUNT:11 == 4
			G = 4
		ELSEIF ABL:COUNT:11 >= 5
			G = 3 + ABL:COUNT:11 / 2
		ENDIF

		;汚臭鈍感
		SIF TALENT:COUNT:61
			G += 1
		;汚臭敏感
		SIF TALENT:COUNT:62
			G -= 1
		;献身的
		SIF TALENT:COUNT:63
			G += 1
		;恋慕
		SIF TALENT:COUNT:76
			G += 1
		;淫乱
		SIF TALENT:COUNT:85
			G += 1
		;親愛
		SIF TALENT:COUNT:88
			G += 3
		;恋人
		SIF TALENT:COUNT:153
			G += 1

		;言われてやってるので普段より多い
		G += 1

		SIF G <= 0
			G = 1

		EXP:COUNT:22 += G
		CALL TURNEND_COM_PRINT, "EXP", 22, G
		EXP:COUNT:20 += G*2
		CALL TURNEND_COM_PRINT, "EXP", 20, G*2
		JUEL:COUNT:4 += G*100
		CALL TURNEND_COM_PRINT, "JUEL", 4, G*100
		JUEL:COUNT:6 += G*30
		CALL TURNEND_COM_PRINT, "JUEL", 6, G*30
		JUEL:COUNT:7 += G*40
		CALL TURNEND_COM_PRINT, "JUEL", 7, G*40
		JUEL:COUNT:15 += G*100
		CALL TURNEND_COM_PRINT, "JUEL", 15, G*100
		
		;実行したコマンドを保存
		Y:COUNT = 8
	ELSEIF CFLAG:COUNT:13 == 9
		;自慰
		TIMES A , 0.80
		TIMES B , 0.80
		TIMES C , 1.30
		LOCAL = RAND:11 + 10
		CFLAG:COUNT:2 += LOCAL
		CALL TURNEND_COM_PRINT, "CFLAG", 2, LOCAL

		G = 0

		IF ABL:COUNT:10 == 1
			G = 1
		ELSEIF ABL:COUNT:10 == 2
			G = 2
		ELSEIF ABL:COUNT:10 == 3
			G = 4
		ELSEIF ABL:COUNT:10 == 4
			G = 6
		ELSEIF ABL:COUNT:10 >= 5
			G = (ABL:COUNT:10 * 2) - 1
		ENDIF

		;言われてやってるので普段より多い
		G += 1

		IF ABL:COUNT:1 == 4 && ABL:COUNT:3 >= 4
			G += 1
		ELSEIF ABL:COUNT:1 >= 5 && ABL:COUNT:3 >= 4
			G += ABL:COUNT:1
		ENDIF
		
		;自慰しやすい
		IF TALENT:COUNT:60
			G *= 2
			;欲望ＬＶ３でもGに+1する
			SIF ABL:COUNT:1 >= 3 && ABL:COUNT:3 >= 3
				G += 1
		ENDIF
		
		;快感に素直、快感の否定、淫乱化
		IF TALENT:COUNT:70
			G *= 3/2
		ELSEIF TALENT:COUNT:71
			G /= 2
		ENDIF
		IF TALENT:COUNT:76
			G *= 2
		ENDIF
		
		SIF G <= 0
			G = 1

		EXP:COUNT:10 += G
		CALL TURNEND_COM_PRINT, "EXP", 10, G
		EXP:COUNT:2 += G / 5
		CALL TURNEND_COM_PRINT, "EXP", 2, G/5
		JUEL:COUNT:0 += G*500
		CALL TURNEND_COM_PRINT, "JUEL", 0, G*500
		JUEL:COUNT:4 += G*100
		CALL TURNEND_COM_PRINT, "JUEL", 4, G*100
		JUEL:COUNT:5 += G*250
		CALL TURNEND_COM_PRINT, "JUEL", 5, G*250
		
		IF ABL:COUNT:0+ABL:COUNT:7+ABL:COUNT:8 >= 10
			JUEL:COUNT:8 += G*200
			CALL TURNEND_COM_PRINT, "JUEL", 8, G*200
		ENDIF
		
		;実行したコマンドを保存
		Y:COUNT = 9
	ELSEIF CFLAG:COUNT:13 == 10
		;レズ行為
		TIMES A , 0.80
		TIMES B , 0.80
		TIMES C , 1.30

		N = 0

		IF ABL:COUNT:12 == 1
			N += 1
		ELSEIF ABL:COUNT:12 == 2
			N += 2
		ELSEIF ABL:COUNT:12 == 3
			N += 3
		ELSEIF ABL:COUNT:12 == 4
			N += 5
		ELSEIF ABL:COUNT:12 >= 5
			N += 2 + ABL:COUNT:12
		ENDIF
		G = RAND:100 + 1
		IF G <= 30
			N += 1
		ELSEIF G <= 65
			N += 2
		ELSEIF G <= 85
			N += 5
		ELSEIF G <= 95
			N += 8
		ELSE
			N += 15
		ENDIF
		
		R = RAND:76 + 75
		N *= R
		N /= 100

		EXP:COUNT:40 += N*20
		CALL TURNEND_COM_PRINT, "EXP", 40, N*20
		EXP:COUNT:2 += N*100*ABL:COUNT:0/500
		CALL TURNEND_COM_PRINT, "EXP", 2, N*100*ABL:COUNT:0/500
		
		JUEL:COUNT:6 += N*75*(ABL:COUNT:2+ABL:COUNT:6)
		CALL TURNEND_COM_PRINT, "JUEL", 6, N*75*(ABL:COUNT:2+ABL:COUNT:6)
		JUEL:COUNT:7 += N*75*(ABL:COUNT:2+ABL:COUNT:6)
		CALL TURNEND_COM_PRINT, "JUEL", 7, N*75*(ABL:COUNT:2+ABL:COUNT:6)
		IF TALENT:COUNT:121
			EXP:COUNT:3 += N
			CALL TURNEND_COM_PRINT, "EXP", 3, N
			JUEL:COUNT:8 += N*100
			CALL TURNEND_COM_PRINT, "JUEL", 8, N*100
		ENDIF
		
		;実行したコマンドを保存
		Y:COUNT = 10
	ELSEIF CFLAG:COUNT:13 == 11 && H == 1
		;性行為
		TIMES A , 0.70
		TIMES B , 0.70
		TIMES C , 1.50
		LOCAL = RAND:41 + 30
		CFLAG:COUNT:2 += LOCAL
		BASE:MASTER:4 -= MAXBASE:MASTER:4 / 50

		S = 1
		;V感覚
		IF ABL:COUNT:4 == 1
			S += 1
		ELSEIF ABL:COUNT:4 == 2
			S += 2
		ELSEIF ABL:COUNT:4 == 3
			S += 2
		ELSEIF ABL:COUNT:4 == 4
			S += 3
		ELSEIF ABL:COUNT:4 >= 5
			S += 1 + (ABL:COUNT:4 * 3/4)
		ENDIF
		
		;欲望と奉仕精神によるボーナス
		IF ABL:COUNT:1 == 4 && ABL:COUNT:6 >= 4
			S += 1
		ELSEIF ABL:COUNT:1 >= 5 && ABL:COUNT:6 >= 4
			S += ABL:COUNT:1
		ENDIF
		
		;V感覚が充分にあれば、欲望ＬＶ３でもSに+1する
		IF ABL:COUNT:1 >= 3 && ABL:COUNT:4 >= 3
			S += 1
		ENDIF
		
		;快感に素直、快感の否定、淫乱化
		IF TALENT:COUNT:70
			S *= 3/2
		ELSEIF TALENT:COUNT:71
			S /= 2
		ENDIF
		
		IF TALENT:COUNT:76
			S *= 2
		ENDIF
	
		SIF S <= 0
			S = 1
		EXP:COUNT:0 += S
		CALL TURNEND_COM_PRINT, "EXP", 0, S
		EXP:COUNT:2 += S
		CALL TURNEND_COM_PRINT, "EXP", 2, S
		CFLAG:MASTER:2 += S
		CFLAG:COUNT:2 += S*5
		CALL TURNEND_COM_PRINT, "CFLAG", 2, LOCAL+S*5
		IF TALENT:MASTER:121 || TALENT:MASTER:122
			EXP:COUNT:20 += S * 10
			CALL TURNEND_COM_PRINT, "EXP", 20, S*10
		ENDIF
		JUEL:COUNT:1 += S*800
		CALL TURNEND_COM_PRINT, "JUEL", 1, S*800
		JUEL:COUNT:4 += S*500
		CALL TURNEND_COM_PRINT, "JUEL", 4, S*500
		JUEL:COUNT:5 += S*500
		CALL TURNEND_COM_PRINT, "JUEL", 5, S*500
		JUEL:COUNT:15 += S*500
		CALL TURNEND_COM_PRINT, "JUEL", 15, S*500
		
		;実行したコマンドを保存
		Y:COUNT = 11
	ELSEIF CFLAG:COUNT:13 == 12
		;宝探し
		TIMES A , 0.70
		TIMES B , 0.70
		TIMES C , 1.50
		CALL TREASURE
		
		;実行したコマンドを保存
		Y:COUNT = 12
	ENDIF
	;リング資金稼ぎ
	CALL TURNEND_EARN_MONEY_TM
ENDIF

@TURNEND_COM_PRINT, ARGS, ARG, ARG:1
;待機行動中に入手した経験や宝珠や好感度を表示させる関数
;当該CONFIGがONのときのみ。
SIF (FLAG:23 & 1p25) == 0
	RETURN 0
SIF ARG:1 == 0
	RETURN 0
SELECTCASE ARGS
	CASE "CFLAG"
		PRINTFORML %CALLNAME:COUNT%は%CFLAGNAME:ARG%を\@(ARG:1 > 0) ? {ARG:1}だけ得ることができた。 # {-ARG:1}だけ減らした。\@
	CASE "EXP"
		PRINTFORML %CALLNAME:COUNT%は%EXPNAME:ARG%を{ARG:1}だけ得ることができた。
	CASE "JUEL"
		PRINTFORML %CALLNAME:COUNT%は%PALAMNAME:ARG%の宝珠を{ARG:1}だけ得ることができた。
ENDSELECT
RETURN 1
