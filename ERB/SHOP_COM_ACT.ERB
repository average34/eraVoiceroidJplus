;-------------------------------------------------
;待機中指示変更
;-------------------------------------------------
@CHANGE_ACTION
X = 0
REPEAT CHARANUM
	SIF CFLAG:COUNT:16 == 1
		X = 1
REND

;臨月、育児中キャラは強制的に[何もしない]
REPEAT CHARANUM
	SIF MARK:COUNT:90 == 1
		CFLAG:COUNT:13 = 0
REND

PRINTL 誰に指示を出しますか？
CALL ACTION_LIST
IF CHARANUM <= 100
	PRINTL [100] 戻る
ELSE
	PRINTL [1000][-1] 戻る
ENDIF
IF X == 1 && U == 0
	PRINTFORML [200] 休憩中のキャラも表示する
ELSEIF X == 1 && U == 1
	PRINTFORML [200] 休憩中のキャラを非表示にする
ENDIF
SIF CHARANUM >= 3
	PRINTFORML [300] 全員に指示を出す


$INPUT_LOOP
INPUT
IF RESULT == -1 || (CHARANUM <= 100 && RESULT == 100) || RESULT == 1000
	RETURN 0
ELSEIF RESULT == 200 && X == 1
	IF U == 0
		U = 1
		RESTART
	ELSE
		U = 0
		RESTART
	ENDIF
ELSEIF RESULT == 300 && CHARANUM >= 3
	;全員に指示を出す
	CALL SHOP_ACT_LIST
	$INPUT_LOOP_ACT_TOTAL
	INPUT
	IF RESULT == 100
		RESTART
	ELSEIF RESULT < 0 || RESULT > 100
		PRINTFORML 正しい値を入力してください
		GOTO INPUT_LOOP_ACT_TOTAL
	ELSEIF C:RESULT == 0
		PRINTFORML 正しい値を入力してください
		GOTO INPUT_LOOP_ACT_TOTAL
	ELSE
		;選択された待機中行動を保存
		D = RESULT
		IF RESULT == 20
			;[13] リフレッシュ(耐力○％以下)
			PRINTFORML 耐力が○％以下の仲間にリフレッシュを指示します。
			PRINTFORML 何％以下の仲間に指示しますか？ （1-100、0で戻る）
			$INPUT_LOOP_REFRESH2
			INPUT
			IF RESULT == 0
				CALL SHOP_ACT_LIST
				GOTO INPUT_LOOP_ACT_TOTAL
			ELSEIF RESULT < 0 || RESULT > 100
				PRINTFORML 正しい値を入力してください
				GOTO INPUT_LOOP_REFRESH2
			ELSE
				;キャラごとに選択された待機中行動を判定し、可能なキャラは変更する
				FOR A, 0, CHARANUM
					;MASTERは変更できない
					SIF A == MASTER
						CONTINUE
					;U == 0の時（休憩中のキャラが表示されていない時）は休憩中のキャラの行動を変えない
					SIF U == 0 && CFLAG:A:16
						CONTINUE
					
					;耐力が○(選択された値)％以下ならリフレッシュに変更
					SIF (BASE:A:4 <= MAXBASE:A:4 * RESULT / 100)
						CFLAG:A:13 = 2
				NEXT
			ENDIF
		ELSE
			;キャラごとに選択された待機中行動を判定し、可能なキャラは変更する
			FOR A, 0, CHARANUM
				;MASTERは変更できない
				SIF A == MASTER
					CONTINUE
				;U == 0の時（休憩中のキャラが表示されていない時）は休憩中のキャラの行動を変えない
				SIF U == 0 && CFLAG:A:16
					CONTINUE
				
				CALL SHOP_ACT_ABLE
				;D（選択された待機中行動）が可能（B:D = 1）なら変更
				SIF B:D
					CFLAG:A:13 = D
			NEXT
		ENDIF
		PRINTW 全員に指示しました
		CALL ACTION_LIST
		CALL SHOP_ACT_LIST
		GOTO INPUT_LOOP_ACT_TOTAL
	ENDIF
	RESTART
ELSEIF RESULT < 0 || RESULT >= CHARANUM
	PRINTFORML 正しい値を入力してください
	GOTO INPUT_LOOP
ELSEIF CFLAG:RESULT:16 == 1 && U == 0
	PRINTFORML 正しい値を入力してください
	GOTO INPUT_LOOP
ELSEIF CFLAG:RESULT:21 == 1
	PRINTFORML 正しい値を入力してください
	GOTO INPUT_LOOP
ELSEIF FLAG:62 & 1 && (MARK:RESULT:90 == 1)
	PRINTFORML 正しい値を入力してください
	GOTO INPUT_LOOP
;ELSEIF RESULT == ASSI
	;GOTO INPUT_LOOP
ENDIF

IF CFLAG:RESULT:16 == 0 || U == 1
;内容を指示
	;RESULT退避
	A = RESULT

	IF RESULT == MASTER
		PRINT 新たに加わった仲間へのデフォルトとして
	ENDIF
	PRINTFORML 何を指示しますか？
	
	CALL SHOP_ACT_NAME
	FOR LOCAL, 0, 15
		CALL SHOP_ACT_ABLE
		SIF B:LOCAL
			PRINTFORML [{LOCAL, 3}] %TSTR:LOCAL,16,LEFT% ：%TSTR:(100+LOCAL)%
	NEXT
	PRINTFORML [100] キャンセル

	$INPUT_LOOP_ACT
	INPUT
	IF RESULT == 100
		RESTART
	ELSEIF RESULT < 0 || RESULT > 100
		PRINTFORML 正しい値を入力してください
		GOTO INPUT_LOOP_ACT
	ELSEIF B:RESULT == 0
		PRINTFORML 正しい値を入力してください
		GOTO INPUT_LOOP_ACT
	ELSE
		CFLAG:A:13 = RESULT
		PRINTFORMW 指示を変更しました
	ENDIF

	RESTART
ENDIF

@SHOP_ACT_LIST
IF U == 0
	PRINTFORML 何を指示しますか？(休憩中のキャラには指示を出しません)
ELSEIF U == 1
	PRINTFORML 何を指示しますか？(休憩中のキャラも含めて全員に指示を出します)
ENDIF
;全キャラに対して指示可能な待機中行動(B:1～)を判定し、指示可能なキャラが1人でもいる待機中行動(C:1～に記録)を表示する
VARSET C, 0
FOR A, 0, CHARANUM
	CALL SHOP_ACT_ABLE
	FOR LOCAL, 0, 15
		SIF B:LOCAL
			C:LOCAL = 1
	NEXT
NEXT
CALL SHOP_ACT_NAME
FOR LOCAL, 0, 15
	SIF C:LOCAL
		PRINTFORML [{LOCAL, 3}] %TSTR:LOCAL,16,LEFT% ：%TSTR:(100+LOCAL)%
NEXT
PRINTFORML [ 20] リフレッシュ(耐力○％以下)
C:13 = 1
PRINTFORML [100] キャンセル

@SHOP_ACT_NAME
VARSET TSTR, ""
TSTR:0  = 何もさせない
TSTR:1  = 修行
TSTR:2  = リフレッシュ
TSTR:3  = 自給自足
TSTR:4  = 連携を確認する
TSTR:5  = 家事
TSTR:6  = モンスター退治
TSTR:7  = イチャイチャする
TSTR:8  = 朝フェラ
TSTR:9  = 自慰
TSTR:10 = レズ行為
TSTR:11 = 性行為
TSTR:12 = 宝探し
;TSTR:20 = リフレッシュ(耐力○％以下)
TSTR:100 = 変化なし
TSTR:101 = 回復量↓   生活費↓   戦闘経験↑
TSTR:102 = 回復量↑↑ 生活費↑↑
TSTR:103 = 回復量↓↓ 生活費↓↓ 宝珠↑
TSTR:104 = 回復量↓   生活費↑   戦闘経験↑ 好感度↑
TSTR:105 = 回復量↓   生活費↓   宝珠↑
TSTR:106 = 回復量０   耐力  ↓   戦闘経験↑ 金↑
TSTR:107 = 生活費↑↑ 好感度↑
TSTR:108 = 回復量↓   経験  ↑   宝珠↑
TSTR:109 = 回復量↓   生活費↑   経験↑ 宝珠↑
TSTR:110 = 回復量↓   生活費↑   経験↑ 宝珠↑
TSTR:111 = 回復量↓   生活費↑   経験↑ 宝珠↑ 好感度↑
TSTR:112 = 回復量↓   生活費↑   アイテム発見

@SHOP_ACT_ABLE
;事前にAにTARGET等キャラを指定しておき、Aが各待機中行動をできるか判定する
;各待機中行動が可能な場合B:○が1になる
VARSET B, 0
;[0] 何もさせない
B:0 = 1
;[1] 修行
B:1 = 1
;[2] リフレッシュ
B:2 = 1
;[3] 自給自足
;ゆかり様には親密30以上ないと自給自足を指示できない
SIF TALENT:A:201 == 0 || ABL:A:0 >= 30 || A == MASTER
	B:3 = 1
;[4] 連携を確認する
SIF A != MASTER && (ABL:A:0 >= 5 || CFLAG:A:2 >= 500 || TALENT:A:153)
	B:4 = 1
;[5] 家事
;ゆかり様には親密20以上ないと指示できない
SIF A != MASTER && (TALENT:A:201 == 0 && (ABL:A:0 >= 5 || CFLAG:A:2 >= 500 || TALENT:A:153) || ABL:A:0 >= 20)
	B:5 = 1
;[6] モンスター退治
SIF A != MASTER && A != TARGET && A != ASSI && BASE:A:4 >= 500 && (ABL:A:0 >= 5 || CFLAG:A:2 >= 500 || TALENT:A:153)
	B:6 = 1
;[7] イチャイチャする
SIF A != MASTER && TALENT:A:153
	B:7 = 1
;[8] 朝フェラ
SIF TALENT:A:54 == 0 && A != MASTER && (TALENT:A:152 || TALENT:A:153) && (ABL:A:0 >= 10 || ABL:A:1 >= 10 || ABL:A:6 >= 10 || ABL:A:11 >= 1) && (TALENT:MASTER:119 || TALENT:MASTER:121 || TALENT:MASTER:122)
	B:8 = 1
;[9] 自慰
SIF A != MASTER && (TALENT:A:152 || TALENT:A:153) && (ABL:A:0 >= 10 || ABL:A:1 >= 10 || ABL:A:10 >= 1)
	B:9 = 1
;[10] レズ行為
SIF A != MASTER && ABL:A:1 >= 5 && ABL:A:9 >= 3 && (ABL:A:0 >= 10 || ABL:A:1 >= 10 || ABL:A:9 >= 10 || ABL:A:12 >= 1) && TALENT:MASTER:122 == 0 && TALENT:A:122 == 0
	B:10 = 1
;[11] 性行為
SIF A != MASTER && TALENT:MASTER:154 == 0 && TALENT:A:88 && TALENT:A:0 == 0 && (ABL:A:0 + ABL:A:1 + ABL:A:4 + ABL:A:6 >= 55) && (TALENT:MASTER:119 || TALENT:MASTER:121 || TALENT:MASTER:122)
	B:11 = 1
;[12] 宝探し
B:12 = 1

@ALL_ACT_ABLE
;全キャラ指定中の待機中行動をできるか判定し、できなくなっている場合は何もさせないに変更
FOR A, 0, CHARANUM
	CALL SHOP_ACT_ABLE
	SIF B:(CFLAG:A:13) == 0
		CFLAG:A:13 = 0
NEXT

@ACTION_LIST
DRAWLINE
REPEAT CHARANUM
	;主人公は特別表示
	SIF COUNT == MASTER
		PRINTFORM                                     [ 0]デフォルト指示    
	;預かり中のキャラは非表示だと選べない
	SIF CFLAG:COUNT:16 == 1 && U == 0
		CONTINUE
	;別れたキャラも選べない
	SIF CFLAG:COUNT:21
		CONTINUE
	;臨月、育児中のキャラも選べない
	SIF FLAG:62 & 1 && (MARK:COUNT:90 == 1)
		CONTINUE
	IF COUNT != MASTER
		PRINT 体力
		A = BASE:COUNT:0
		SIF BASE:COUNT:0 < 0
			A = 0
		CALL PRINT_BAR_0, A, MAXBASE:COUNT:0, 16
		PRINTFORM ({BASE:COUNT:0,4}/{MAXBASE:COUNT:0,4})
		IF COUNT == TARGET
			PRINT  ☆
		ELSEIF COUNT == ASSI
			PRINT  ★
		ELSE
			PRINT  　
		ENDIF
		PRINTFORM [{COUNT,2}]%NAME:COUNT,16,LEFT%　
		;CALL ARRANGE_CHARANAME
	ENDIF

	CALL SHOP_ACT_NAME
	PRINTFORML （%TSTR:(CFLAG:COUNT:13)%）
REND
PRINTL 　　　　　　☆：パートナー　★：助手　を表しています

