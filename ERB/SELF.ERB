@SELF_CHECK
;合意持ち、かつ、ウフフに突入していたなら終了後にハッスルする可能性アリ(主人公が童貞だと発生しない)
SIF TALENT:合意 && TFLAG:44 && TALENT:MASTER:童貞 == 0
	CALL AFTER_V_CHECK
;[キス魔]などの淫系素質チェック
CALL AFTER_TALENT_CHECK
;恋慕持ちか、極端に欲望が高いと自慰をする可能性アリ
SIF TALENT:恋慕 || ABL:欲望 >= 10
	CALL AFTER_S_CHECK
;レズプレイチェック
CALL AFTER_L_CHECK
;晩酌チェック
CALL SELF_SAKE_CHECK
;合意持ちなら夜這いに来る可能性アリ(主人公が童貞だと発生しない)
SIF TALENT:合意 && TALENT:MASTER:童貞 == 0
	CALL SELF_CHECK2

WAIT

@AFTER_V_CHECK
;恋慕、非処女による調教後性交渉チェック

;体力余って、[恋慕]がないとダメ
SIF BASE:体力 < 500 || TALENT:TARGET:恋慕 == 0 || TFLAG:301
	RETURN 0
;カットフラグが立っていたらリターン
SIF CFLAG:98 & 1
	RETURN 0

;TFLAG:15を借りますよ
TFLAG:15 = 0

;ターゲットに挿入
SIF (TALENT:MASTER:ふたなり || TALENT:MASTER:オトコ) && TALENT:処女 == 0 && TALENT:オトコ == 0
	TFLAG:15 |= 1
;マスターに挿入
SIF (TALENT:ふたなり || TALENT:オトコ) && TALENT:MASTER:処女 == 0 && TALENT:MASTER:オトコ == 0
	TFLAG:15 |= 2
;両方出来るならランダムで決定
SIF TFLAG:15 == 3
	TFLAG:15 -= 1 + RAND:2

VARSET S, 0

;ターゲットに挿入する場合はV感覚
IF TFLAG:15 & 1
	A = 4
;ターゲットが挿入、もしくは非挿入の場合はC感覚
ELSE
	A = 3
ENDIF

;C/V感覚
IF ABL:A == 1
	S = 1
ELSEIF ABL:A == 2
	S = 2
ELSEIF ABL:A == 3
	S = 2
ELSEIF ABL:A == 4
	S = 3
ELSEIF ABL:A >= 5
	S = 1 + (ABL:A / 2)
ENDIF

;欲望と奉仕精神によるボーナス
IF ABL:欲望 == 4 && ABL:奉仕精神 >= 4 && PALAM:欲情 >= PALAMLV:4
	S += 1
ELSEIF ABL:欲望 >= 5 && ABL:奉仕精神 >= 4 && PALAM:欲情 >= PALAMLV:4
	S += ABL:欲望 / 2
ENDIF

;C/V感覚が充分にあれば、欲望ＬＶ３でもSに+1する
IF ABL:欲望 >= 3 && ABL:A >= 3 && PALAM:欲情 >= PALAMLV:3
	S += 1
ENDIF

;快感に素直、快感の否定、淫乱化
IF TALENT:快感に素直 && S
	S += 1
ELSEIF TALENT:快感の否定 && S
	S -= 2
ENDIF

;淫乱
IF TALENT:淫乱 && S
	S += 1
ENDIF

;淫核/淫壷
IF ((TALENT:淫壷 && A == 4) || (TALENT:淫核 && A == 3)) && S
	S += 1
ENDIF

;回数と経験は上限9
S:1 = S
SIF S >= 9
	S = 9

;変数退避
LOCAL = S
LOCAL:1 = S:1
IF S > 0
	DRAWLINE
	;カットフラグを退避
	LOCAL:100 = CFLAG:99
	;口上開始前の地の文をカットするか
	;ターゲットが挿入されるなら元々の調教後セックスを呼び出す
	IF TFLAG:15 & 1
		TFLAG:13 = 4
	;そうでないなら、独自仕様なので、別の番号を呼び出す。
	ELSE
		TFLAG:13 = 92
	ENDIF
	CALL SELF_MAEKOJO
	;地の文章カット
	IF (CFLAG:99 & 1) == 0
		PRINTFORM %CALLNAME:MASTER%と%CALLNAME:TARGET%はまだ冷めやらぬ昂奮を抑えきれず、
		PRINTV S
		PRINTL 回ベッドの上でまぐわった…
	ENDIF
	;ターゲットが挿入されるなら元々の調教後セックスを呼び出す
;	IF TFLAG:15 & 1
;		TFLAG:13 = 4
	;そうでないなら、独自仕様なので、別の番号を呼び出す。
;	ELSE
;		TFLAG:13 = 92
;	ENDIF
	CALL SELF_KOJO
	;変数を戻す
	S = LOCAL
	S:1 = LOCAL:1
	;ターゲットが挿入されるならV経験が付く
	IF TFLAG:15 & 1
		PRINTFORML %EXPNAME:0%＋{S}
		EXP:Ｖ経験 += S
	ENDIF
	;精液経験が付くか
	IF TALENT:MASTER:ふたなり || TALENT:MASTER:オトコ
		PRINTFORML %EXPNAME:20%＋{S*5}
		EXP:精液経験 += S * 5
	ENDIF
	PRINTFORML %EXPNAME:2%＋{S}
	EXP:絶頂経験 += S
	;射精経験
	IF TALENT:オトコ || TALENT:ふたなり
		PRINTFORML %EXPNAME:3%＋{S}
		EXP:射精経験 += S
	ENDIF
	;レズ経験
	IF (TALENT:MASTER:オトコ == 0) && (TALENT:オトコ == 0)
		EXP:レズ経験 += S * 20
		EXP:MASTER:レズ経験 += S * 20
		PRINTFORML %EXPNAME:40%＋{S*20}
	ENDIF
	
	;ターゲットが挿入されるときはV
	IF TFLAG:15 & 1
		PRINTFORML %PALAMNAME:1%の珠＋{S:1*200}
		GOTJUEL:快Ｖ += S:1*200
	;ターゲットが挿入する時はC
	ELSEIF TFLAG:15 & 2
		PRINTFORML %PALAMNAME:0%の珠＋{S:1*200}
		GOTJUEL:快Ｃ += S:1*200
	;非挿入の時はBC
	ELSE
		PRINTFORML %PALAMNAME:0%の珠＋{S:1*150}
		GOTJUEL:快Ｃ += S:1*150
		PRINTFORML %PALAMNAME:14%の珠＋{S:1*100}
		GOTJUEL:快Ｂ += S:1*100
	ENDIF
	PRINTFORML %PALAMNAME:4%の珠＋{S:1*100}
	PRINTFORML %PALAMNAME:5%の珠＋{S:1*250}
	IF TALENT:オトコ || TALENT:ふたなり
		T = TARGET
		PLAYER = MASTER
		CALL GET_INITIATIVE_RATE
		;ターゲットが挿入し、強気だと優越の珠
		IF TFLAG:15 & 2 && R > 0
			PRINTFORML %PALAMNAME:17%の珠＋{S:1*100}
			GOTJUEL:優越 += S:1*100
		;そうでなければ恥情の珠
		ELSE
			PRINTFORML %PALAMNAME:8%の珠＋{S:1*100}
			GOTJUEL:恥情 += S:1*100
		ENDIF
	ENDIF
	GOTJUEL:恭順 += S:1*100
	GOTJUEL:欲情 += S:1*250
	IF ABL:親密+ABL:Ｖ感覚+ABL:奉仕精神 >= 13 && TALENT:恋人 && (CFLAG:99 & 1) == 0
		PRINTFORM 我に返り、痴態を省みて赤面する%CALLNAME:TARGET%に苦笑いしながらそっとキスをすると、
		IF TIME == 1
			PRINTFORML お休みを告げて部屋を出た
		ELSE
			PRINTFORML 別れを告げて部屋を出た
		ENDIF
	ENDIF
	;カットフラグを戻す
	CFLAG:99 = LOCAL:100
ENDIF
TFLAG:15 = 0
RETURN 0

@AFTER_S_CHECK
;カットフラグが立っていたらリターン
SIF CFLAG:98 & 2
	RETURN 0
;自慰中毒による自慰チェック
VARSET A, 0
VARSET B, 0
VARSET C, 0

IF ABL:自慰中毒 == 1
	A = 1
ELSEIF ABL:自慰中毒 == 2
	A = 2
ELSEIF ABL:自慰中毒 == 3
	A = 4
ELSEIF ABL:自慰中毒 == 4
	A = 6
ELSEIF ABL:自慰中毒 >= 5
	A = (ABL:自慰中毒 * 2) - 1
ENDIF

IF ABL:欲望 == 4 && ABL:Ｃ感覚 >= 4 && PALAM:欲情 >= PALAMLV:4
	A += 1
ELSEIF ABL:欲望 >= 5 && ABL:Ｃ感覚 >= 4 && PALAM:欲情 >= PALAMLV:4
	A += ABL:欲望 / 2
ENDIF

;自慰しやすい
IF TALENT:自慰しやすい
	A *= 2
	;欲望ＬＶ３でもAに+1する
	SIF ABL:欲望 >= 3 && ABL:Ｃ感覚 >= 3 && PALAM:欲情 >= PALAMLV:3
		A += 1
ENDIF

;快感に素直、快感の否定、淫乱化
IF TALENT:快感に素直 && A
	A += 1
ELSEIF TALENT:快感の否定 && A
	A -= 2
ENDIF
IF TALENT:淫乱 && A
	A += 1
ENDIF

;鼓舞
IF ASSI >= 0
	SIF (TALENT:ASSI:鼓舞 || TALENT:MASTER:鼓舞) && A
		A += 1
ELSE
	SIF TALENT:MASTER:鼓舞 && A
		A += 1
ENDIF

;自慰、アナニー判定。ポイントが同じ場合は自慰優先(Ｃ感覚が０の場合はアナニーのみ)
;Ａ感覚見る
IF ABL:Ａ感覚 == 1
	B += 1
ELSEIF ABL:Ａ感覚 == 2
	B += 2
ELSEIF ABL:Ａ感覚 == 3
	B += 3
ELSEIF ABL:Ａ感覚 == 4
	B += 4
ELSEIF ABL:Ａ感覚 >= 5
	B += ABL:Ａ感覚
ENDIF
;Ｃ感覚見る
IF ABL:Ｃ感覚 == 0
	C = 0
ELSEIF ABL:Ｃ感覚 == 1
	C += 1
ELSEIF ABL:Ｃ感覚 == 2
	C += 2
ELSEIF ABL:Ｃ感覚 == 3
	C += 3
ELSEIF ABL:Ｃ感覚 == 4
	C += 4
ELSEIF ABL:Ｃ感覚 >= 5
	C += ABL:Ｃ感覚
ENDIF

;調教時にＡ快楽、Ｃ快楽をより稼いだほうに+1
IF PALAM:快Ａ > PALAM:快Ｃ
	B += 1
ELSEIF PALAM:快Ｃ > PALAM:快Ａ
	C += 1
ENDIF

;尻穴狂いだとアナニーしやすい+
IF TALENT:尻穴狂い
	B += 1
ENDIF

;淫核だと通常オナニーしやすい
IF TALENT:淫核
	C += 1
ENDIF

;体力が500未満だと半分になる
IF BASE:体力 <= 500
	A /= 2
	;Aが6回以上だったら5回以下にする
	SIF A > 6
		A %= 6
	;Aが0だった場合1にする
	SIF A < 1
		A = 1
ENDIF

;回数と経験は上限9
A:1 = A
SIF A >= 9
	A = 9

;変数退避
LOCAL = A
LOCAL:1 = A:1

IF A > 0 && (C >= B)
	DRAWLINE
	;カットフラグを退避
	LOCAL:100 = CFLAG:99
	;口上開始前の地の文をカットするか
	TFLAG:13 = 1
	CALL SELF_MAEKOJO
	;地の文章カット
	IF CFLAG:99 & 1
;		TFLAG:13 = 1
		CALL SELF_KOJO
	ELSE
		PRINTFORM %CALLNAME:MASTER%が出て行った後、%CALLNAME:TARGET%は
		PRINTV A
		PRINTL 回自慰をしたようだ。
;		TFLAG:13 = 1
		CALL SELF_KOJO
	ENDIF
	
	;変数を戻す
	A = LOCAL
	A:1 = LOCAL:1
	
	PRINTFORML %EXPNAME:10%＋{A}
	SIF A / 5 > 0
		PRINTFORML %EXPNAME:2%＋{A/5}
	PRINTFORML %PALAMNAME:0%の珠＋{A:1*500}
	PRINTFORML %PALAMNAME:4%の珠＋{A:1*100}
	PRINTFORMW %PALAMNAME:5%の珠＋{A:1*250}
	EXP:自慰経験 += A
	EXP:絶頂経験 += A / 5
	GOTJUEL:快Ｃ += A:1*500
	GOTJUEL:恭順 += A:1*100
	GOTJUEL:欲情 += A:1*250

	;カットフラグを戻す
	CFLAG:99 = LOCAL:100
ELSEIF A > 0 && (B > C)
	DRAWLINE
	;カットフラグを退避
	LOCAL:100 = CFLAG:99
	;口上開始前の地の文をカットするか
	TFLAG:13 = 1
	CALL SELF_MAEKOJO
	;地の文章カット
	IF CFLAG:99 & 1
;		TFLAG:13 = 1
		CALL SELF_KOJO
	ELSE
		PRINTFORM %CALLNAME:MASTER%が出て行った後、%CALLNAME:TARGET%は
		PRINTV A
		PRINTL 回後ろで自慰をしたようだ。
;		TFLAG:13 = 1
		CALL SELF_KOJO
	ENDIF
	
	;変数を戻す
	A = LOCAL
	A:1 = LOCAL:1
	
	PRINTFORML %EXPNAME:10%＋{A}
	PRINTFORML %EXPNAME:1%＋{A}
	SIF A / 5 > 0
		PRINTFORML %EXPNAME:2%＋{A/5}
	PRINTFORML %PALAMNAME:2%の珠＋{A:1*500}
	PRINTFORML %PALAMNAME:4%の珠＋{A:1*100}
	PRINTFORMW %PALAMNAME:5%の珠＋{A:1*250}
	EXP:自慰経験 += A
	EXP:Ａ経験 += A
	EXP:絶頂経験 += A / 5
	GOTJUEL:快Ａ += A:1*500
	GOTJUEL:恭順 += A:1*100
	GOTJUEL:欲情 += A:1*250
	
	IF ABL:親密 + ABL:露出癖 * 3 + ABL:マゾっ気 * 3 >= 30 && TIME == 0 && TALENT:合意
		SIF (CFLAG:99 & 1) == 0
			PRINTFORML そのことを%NAME:TARGET%は報告してきた。
		PRINTFORML %PALAMNAME:8%の珠＋{A:1*200}
		GOTJUEL:恥情 += A:1*200
	ENDIF
	
	IF (ABL:親密 >= 10 || ABL:欲望 >= 10) && TALENT:恋慕 && (CFLAG:99 & 1) == 0
		PRINTFORML しかし何度自分を慰めてみても、%CALLNAME:MASTER%への欲望は満たされなかった。
	ENDIF
	;カットフラグを戻す
	CFLAG:99 = LOCAL:100
	RETURN 0
ENDIF

;レズプレイ
@AFTER_L_CHECK
;カットフラグが立っていたらリターン
SIF CFLAG:98 & 4
	RETURN 0
;レズ中毒によるレズチェック
SIF ASSI < 0
	RETURN 0

;オトコはだめ
SIF TALENT:オトコ || TALENT:ASSI:オトコ
	RETURN 0

;一定能力必要
SIF ABL:レズっ気 < 2 || ABL:Ｃ感覚 < 3 || ABL:親密 < 2 || ABL:欲望 < 5
	RETURN 0

;レズ中毒必要
SIF ABL:レズ中毒 == 0 && ABL:ASSI:レズ中毒 == 0
	RETURN 0

;体力必要
SIF BASE:体力 < 500 || TFLAG:301
	RETURN 0

VARSET N, 0

IF ABL:レズ中毒 == 1
	N += 1
ELSEIF ABL:レズ中毒 == 2
	N += 2
ELSEIF ABL:レズ中毒 == 3
	N += 3
ELSEIF ABL:レズ中毒 == 4
	N += 5
ELSEIF ABL:レズ中毒 >= 5
	N += 2 + ABL:レズ中毒
ENDIF
IF ABL:ASSI:レズ中毒 == 1
	N += 1
ELSEIF ABL:ASSI:レズ中毒 == 2
	N += 2
ELSEIF ABL:ASSI:レズ中毒 == 3
	N += 5
ELSEIF ABL:ASSI:レズ中毒 == 4
	N += 8
ELSEIF ABL:ASSI:レズ中毒 >= 5
	N += 3 + (ABL:ASSI:レズ中毒 * 2)
ENDIF

R = NO:ASSI
IF RELATION:R > 0
	N *= RELATION:R
	N /= 100
ENDIF

;0回レズプレイの報告がスレであったので、弾いておく
SIF N <= 0
	RETURN 0

;回数と経験は上限9
N:1 = N
SIF N >= 9
	N = 9

;変数退避
LOCAL = N
LOCAL:1 = N:1

;カットフラグを退避
LOCAL:100 = CFLAG:99
;口上開始前の地の文をカットするか
TFLAG:13 = 2
CALL SELF_MAEKOJO
;地の文章カット
IF CFLAG:99 ==1
;	TFLAG:13 = 2
	CALL SELF_KOJO
ELSE
	PRINTFORML %CALLNAME:MASTER%と別れた後、{N}回レズプレイをしたようだ。
;	TFLAG:13 = 2
	CALL SELF_KOJO
ENDIF

;変数を戻す
N = LOCAL
N:1 = LOCAL:1

PRINTFORML %EXPNAME:40%＋{N*20}
SIF N*100*ABL:親密/500 > 0
	PRINTFORML %EXPNAME:2%＋{N*100*ABL:親密/500}
PRINTFORML %PALAMNAME:0%の珠＋{N:1*100*ABL:親密}
PRINTFORML %PALAMNAME:5%の珠＋{N:1*200}

EXP:レズ経験 += N*20
EXP:ASSI:レズ経験 += N*20
EXP:絶頂経験 += N*100*ABL:親密/500

GOTJUEL:快Ｃ += N:1*100*ABL:親密
GOTJUEL:欲情 += N:1*200
IF TALENT:ASSI:ふたなり
	PRINTFORML %EXPNAME:20%＋{N}
	PRINTFORML %PALAMNAME:6%の珠＋{N:1*100*(ABL:技巧+ABL:奉仕精神)}
	PRINTFORML %PALAMNAME:7%の珠＋{N:1*100*(ABL:技巧+ABL:奉仕精神)}

	EXP:精液経験 += N
	GOTJUEL:屈服 += N:1*100*(ABL:技巧+ABL:奉仕精神)
	GOTJUEL:習得 += N:1*100*(ABL:技巧+ABL:奉仕精神)
ELSE
	PRINTFORML %PALAMNAME:6%の珠＋{N:1*50*(ABL:技巧+ABL:奉仕精神)}
	PRINTFORML %PALAMNAME:7%の珠＋{N:1*50*(ABL:技巧+ABL:奉仕精神)}

	GOTJUEL:屈服 += N:1*50*(ABL:技巧+ABL:奉仕精神)
	GOTJUEL:習得 += N:1*50*(ABL:技巧+ABL:奉仕精神)
ENDIF
IF TALENT:ASSI:サド
	PRINTFORML %EXPNAME:30%＋{N}
	SIF N*100*ABL:マゾっ気 > 0
		PRINTFORML %PALAMNAME:9%の珠＋{N:1*100*ABL:マゾっ気}

	GOTJUEL:苦痛 += N:1*100*ABL:マゾっ気
	EXP:苦痛快楽経験 += N
ENDIF
IF TALENT:ふたなり
	PRINTFORML %EXPNAME:3%＋{N}
	PRINTFORML %PALAMNAME:8%の珠＋{N:1*100}

	EXP:射精経験 += N
	GOTJUEL:恥情 += N:1*100
ENDIF
IF TALENT:ふたなり && TALENT:ASSI:ふたなり && ABL:奉仕精神 >= 8 && ABL:精液中毒 >= 3
	IF (CFLAG:99 & 1) == 0
		PRINTFORML %NAME:ASSI%と%NAME:TARGET%は
		IF TIME == 0
			PRINTFORML 日が暮れるまで互いのペニスをしゃぶりあった。
		ELSE
			PRINTFORML 一晩中互いのペニスをしゃぶりあった。
		ENDIF
	ENDIF
	PRINTFORML %EXPNAME:3%＋{N}
	PRINTFORML %EXPNAME:20%＋{N}
	PRINTFORML %EXPNAME:21%＋{N}
	PRINTFORML %EXPNAME:22%＋{N}
	PRINTFORML %PALAMNAME:5%の珠＋{N:1*100}
	PRINTFORML %PALAMNAME:6%の珠＋{N:1*100}
	PRINTFORML %PALAMNAME:8%の珠＋{N:1*100}

	EXP:射精経験 += N
	EXP:精液経験 += N
	EXP:奉仕快楽経験 += N
	EXP:フェラ経験 += N
	GOTJUEL:欲情 += N:1*100
	GOTJUEL:屈服 += N:1*100
	GOTJUEL:恥情 += N:1*100
ENDIF
;カットフラグを戻す
CFLAG:99 = LOCAL:100
RETURN 0

@SELF_SAKE_CHECK
;晩酌イベントチェック
;カットフラグが立っていたらリターン
SIF CFLAG:98 & 8
	RETURN 0
;反発感情あり、夜でない、体力５００以下の場合はだめ
SIF MARK:TARGET:反発感情 >= 1 || TIME == 0 || BASE:体力 < 500
	RETURN 0
IF (MARK:TARGET:既成事実 >= 1 && CFLAG:2 >= 500) || (TALENT:TARGET:酒豪 == 1 && CFLAG:2 >= 200)
	S = 10
	;酒豪なら効果上昇
	SIF TALENT:TARGET:酒豪
		S += 10
	;マスターが酒豪なら効果上昇
	SIF TALENT:MASTER:酒豪
		S += 10
	;恋慕なら気を許しているので効果上昇
	SIF TALENT:TARGET:恋慕
		S += 5
	;親愛の場合さらに気を許しているので効果上昇
	SIF TALENT:TARGET:親愛
		S += 5
	;自制心の場合、飲み過ぎないように気をつけてしまうので効果半減
	SIF TALENT:TARGET:自制心
		TIMES S , 0.50
	
	;好感度上昇
	CFLAG:2 += S
	;変数退避
	LOCAL = S
	DRAWLINE
	;カットフラグを退避
	LOCAL:100 = CFLAG:99
	;口上開始前の地の文をカットするか
	TFLAG:13 = 82
	CALL SELF_MAEKOJO
	;地の文章カット
	IF CFLAG:99 ==1
;		TFLAG:13 = 82
		CALL SELF_KOJO
	ELSE
		PRINTFORML %CALLNAME:MASTER%が寝る準備をしていると%CALLNAME:TARGET%が晩酌に誘いに来た
;		TFLAG:13 = 82
		CALL SELF_KOJO
		PRINTFORML それからしばらく%CALLNAME:TARGET%と%CALLNAME:MASTER%は楽しく酒を飲み交わした
	ENDIF
	;変数を戻す
	S = LOCAL
	PRINTFORML 
