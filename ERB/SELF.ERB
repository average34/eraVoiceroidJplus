;IF TALENT:TARGET:0 == 0 && (ABL:1 * 3 + ABL:4 * 2 + ABL:0 + ABL:6) > 50 && TALENT:76 == 1
@SELF_YOBAI
;TFLAG:15を借りますよ
TFLAG:15 = 0

;ターゲットに挿入
SIF (TALENT:MASTER:121 || TALENT:MASTER:122) && TALENT:0 == 0
	TFLAG:15 |= 1
;マスターに挿入
SIF (TALENT:121 || TALENT:122) && TALENT:MASTER:0 == 0
	TFLAG:15 |= 2
;両方出来るならランダムで決定
SIF TFLAG:15 == 3
	TFLAG:15 -= 1 + RAND:2

VARSET S, 0

;ターゲットに挿入する場合はV感覚
IF TFLAG:15 & 1
	A = 4
;ターゲットが挿入、もしくは非挿入の場合はC感覚
ELSE
	A = 3
ENDIF

;C/V感覚
IF ABL:A == 4
	S = 2
ELSEIF ABL:A >= 5
	S = ABL:A - 1
ENDIF
;淫核/淫壷
IF (TALENT:74 && A == 4) || (TALENT:75 && A == 3)
	S += 1
ENDIF

;回数と経験は上限9
S:1 = S
SIF S >= 9
	S = 9

;変数退避
LOCAL = S
LOCAL:1 = S:1
DRAWLINE
IF S > 0
	;カットフラグを退避
	LOCAL:100 = CFLAG:99
	;口上開始前の地の文をカットするか
	;ターゲットが挿入されるなら元々の夜這いを呼び出す
	IF TFLAG:15 & 1
		TFLAG:13 = 5
	;そうでないなら、独自仕様なので、別の番号を呼び出す。
	ELSE
		TFLAG:13 = 104
	ENDIF
	CALL SELF_MAEKOJO
	;地の文章カット
	IF CFLAG:99 & 1
;		TFLAG:13 = 5
		CALL SELF_KOJO
	ELSE
		PRINTFORML %CALLNAME:TARGET%と別れ、%CALLNAME:MASTER%が床に就こうとしたとき、突然%CALLNAME:TARGET%が部屋を訪ねてきた
		PRINTFORM そのまま彼女は%CALLNAME:MASTER%を押し倒し、
		IF TFLAG:15 & 1
			PRINTFORML 蜜壷でペニスを咥え込み激しく腰を打ち付け始めた
		ELSEIF TFLAG:15 & 2
			;濡らす描写を入れようか迷った
			PRINTFORML 漲る怒張を膣口に押し当て一息に貫いた
		ELSE
			PRINTFORML 体をまさぐりながら唇を奪った
		ENDIF
;		TFLAG:13 = 5
		CALL SELF_KOJO
		IF TFLAG:15 & 1
			PRINTFORML 飢えた獣の口は涎を滴らせて怒張を貪り、子宮の渇きを癒している…
		ELSEIF TFLAG:15 & 2
			PRINTFORML 衰えることを知らぬ怒張で子宮を突き上げ、膣内を貪っている…
		ELSE
			PRINTFORML 体を絡ませ攻め立てながら自らの秘所に指を這わせ、疼き鎮めている…
		ENDIF
	ENDIF
	;変数を戻す
	S = LOCAL
	S:1 = LOCAL
	;ターゲットが挿入されるならV経験が付く
	IF TFLAG:15 & 1
		PRINTFORML %EXPNAME:0%＋{S}
		EXP:0 += S
	ENDIF
	;精液経験が付くか
	IF TALENT:MASTER:121 || TALENT:MASTER:122
		PRINTFORML %EXPNAME:20%＋{S*5}
		EXP:20 += S * 5
	ENDIF
	PRINTFORML %EXPNAME:2%＋{S}
	EXP:2 += S
	;射精経験
	IF TALENT:122 || TALENT:121
		PRINTFORML %EXPNAME:3%＋{S}
		EXP:3 += S
	ENDIF
	;レズ経験
	IF (TALENT:MASTER:122 == 0) && (TALENT:122 == 0)
		EXP:40 += S * 20
		EXP:MASTER:40 += S * 20
		PRINTFORML %EXPNAME:40%＋{S*20}
	ENDIF
	
	;ターゲットが挿入されるときはV
	IF TFLAG:15 & 1
		PRINTFORML %PALAMNAME:1%の珠＋{S:1*200}
		GOTJUEL:1 += S:1*200
	;ターゲットが挿入する時はC
	ELSEIF TFLAG:15 & 2
		PRINTFORML %PALAMNAME:0%の珠＋{S:1*200}
		GOTJUEL:0 += S:1*200
	;非挿入の時はBC
	ELSE
		PRINTFORML %PALAMNAME:0%の珠＋{S:1*150}
		GOTJUEL:0 += S:1*150
		PRINTFORML %PALAMNAME:14%の珠＋{S:1*100}
		GOTJUEL:14 += S:1*100
	ENDIF
	PRINTFORML %PALAMNAME:4%の珠＋{S:1*100}
	PRINTFORML %PALAMNAME:5%の珠＋{S:1*250}
	IF TALENT:122 || TALENT:121
		T = TARGET
		PLAYER = MASTER
		CALL GET_INITIATIVE_RATE
		;ターゲットが挿入し、強気だと優越の珠
		IF TFLAG:15 & 2 && R > 0
			PRINTFORML %PALAMNAME:17%の珠＋{S:1*100}
			GOTJUEL:17 += S:1*100
		;そうでなければ恥情の珠
		ELSE
			PRINTFORML %PALAMNAME:8%の珠＋{S:1*100}
			GOTJUEL:8 += S:1*100
		ENDIF
	ENDIF
	GOTJUEL:4 += S:1*100
	GOTJUEL:5 += S:1*250
	IF (CFLAG:99 & 1) == 0
		PRINTFORM %CALLNAME:TARGET%の
		PRINTV S
		PRINT 回目の絶頂の後、想いを遂げた二人は
		IF TFLAG:15
			PRINT 繋がったまま
		ELSE
			PRINT 体を絡ませ
		ENDIF
		PRINTL 抱き合い、深い眠りに落ちた
	ENDIF
	;カットフラグを戻す
	CFLAG:99 = LOCAL:100
ENDIF

;キス夜這い
@SELF_KISSYOBAI
S = 0

;M感覚
IF ABL:17 == 4
	S = 2
ELSEIF ABL:17 >= 5
	S = ABL:17 - 1
ENDIF
;恋慕あり
SIF TALENT:TARGET:85
	S += 1
;親愛あり
SIF TALENT:TARGET:88
	S += 1
;相愛あり
SIF TALENT:TARGET:89
	S += 1
;恋人あり
SIF TALENT:TARGET:153
	S += 1
;童貞あり
SIF TALENT:TARGET:154
	S += 1
;キス魔あり
SIF TALENT:TARGET:155
	S += 1

;回数と経験は上限9
S:1 = S
SIF S >= 9
	S = 9

;変数退避
LOCAL = S
LOCAL:1 = S:1

DRAWLINE
IF S > 0
	;カットフラグを退避
	LOCAL:100 = CFLAG:99
	;口上開始前の地の文をカットするか
	TFLAG:13 = 76
	CALL SELF_MAEKOJO
	;地の文章カット
	IF CFLAG:99 & 1
;		TFLAG:13 = 76
		CALL SELF_KOJO
	ELSE
		;親愛または相愛
		IF TALENT:88 || TALENT:89
			PRINTFORML %CALLNAME:MASTER%が%CALLNAME:TARGET%と床に就き、%CALLNAME:MASTER%が寝付いた後
		ELSE
			PRINTFORMW %CALLNAME:MASTER%が床に就くと、%CALLNAME:TARGET%はこっそり部屋に忍び込んだ
		ENDIF
		PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の寝顔をのぞき込み、想いを抑えきれず唇を奪った
;		TFLAG:13 = 76
		CALL SELF_KOJO
		;地の文章カット2　口上内でCFLAG:99に1を入れればカットされず目を覚ますか決められる…と思う
		SIF (CFLAG:99 & 1) == 0
			PRINTFORMW %CALLNAME:MASTER%が目を醒まさない事を良い事に、何度も貪るように口付けを重ねている……
;		CFLAG:99 = 0
	ENDIF
	;変数を戻す
	S = LOCAL
	S:1 = LOCAL:1
	PRINTFORML %EXPNAME:23%＋{S}
	PRINTFORML %EXPNAME:96%＋{S*2}
	IF (TALENT:MASTER:122 == 0) && (TALENT:122 == 0)
		EXP:40 += S * 10
		EXP:MASTER:40 += S * 10
		PRINTFORML %EXPNAME:40%＋{S*10}
	ENDIF
	PRINTFORML %PALAMNAME:4%の珠＋{S:1*400}
	PRINTFORML %PALAMNAME:7%の珠＋{S:1*200}
	PRINTFORMW %PALAMNAME:8%の珠＋{S:1*200}
	
	CFLAG:2 += S*5
	CFLAG:MASTER:2 += S
	EXP:23 += S
	EXP:96 += S*2
	GOTJUEL:4 += S:1*400
	GOTJUEL:7 += S:1*200
	GOTJUEL:8 += S:1*200
	;Ｍ感覚を設定している場合快Ｍの珠取得
	IF FLAG:62 & 4194304
		PRINTFORMW %PALAMNAME:18%の珠＋{S:1*200}
		GOTJUEL:18 += S:1*200
	ENDIF
	IF (CFLAG:99 & 1) == 0
		PRINTFORML {S*5}分もの密かな行為を終え、唇に残る感触を思い出しながら
		;親愛または相愛
		IF TALENT:88 || TALENT:89
			PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%に寄り添い眠りについた…
		ELSE
			PRINTFORML %CALLNAME:TARGET%は名残惜しそうに部屋を後にした
		ENDIF
	ENDIF
	;カットフラグを戻す
	CFLAG:99 = LOCAL:100
ENDIF

