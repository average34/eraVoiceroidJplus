@COM113
;アナル注射
;アナルは鈍感なので痛みも少ないですが、A経験は増えないので
;そういった意味では一番使えないかもしれません

PRINTL アナル注射
STR:0 =アナル注射
CALL TRAIN_MESSAGE_B

;調合知識があれば消費を抑える
D = 0
IF TALENT:MASTER:調合知識
	D = 1
ELSEIF ASSI >= 0
	SIF TALENT:ASSI:調合知識
		D = 1
ENDIF

IF D
	LOSEBASE:体力 += 500
	LOSEBASE:気力 += 200
ELSE
	LOSEBASE:体力 += 700
	LOSEBASE:気力 += 300
ENDIF


SOURCE:快Ａ = 3000
SOURCE:中毒充足 = 700
SOURCE:不安 = 4000

;触手でない場合は媚薬を1個使用
SIF TEQUIP:90 == 0
	ITEM:媚薬 -= 1

;ABL:従順をみる
IF ABL:親密 == 0
	TIMES SOURCE:不安 , 2.00
ELSEIF ABL:親密 == 1
	TIMES SOURCE:不安 , 1.80
ELSEIF ABL:親密 == 2
	TIMES SOURCE:不安 , 1.50
ELSEIF ABL:親密 == 3
	TIMES SOURCE:不安 , 1.20
ELSEIF ABL:親密 == 4
	TIMES SOURCE:不安 , 1.00
ELSE
	TIMES SOURCE:不安 , 0.80
ENDIF

;ABL:A感覚を見る
IF ABL:Ａ感覚 == 0
	TIMES SOURCE:快Ａ , 0.50
ELSEIF ABL:Ａ感覚 == 1
	TIMES SOURCE:快Ａ , 2.00
ELSEIF ABL:Ａ感覚 == 2
	TIMES SOURCE:快Ａ , 3.00
ELSEIF ABL:Ａ感覚 == 3
	TIMES SOURCE:快Ａ , 4.00
	EX:Ａ絶頂 += 1
ELSEIF ABL:Ａ感覚 == 4
	TIMES SOURCE:快Ａ , 5.00
	EX:Ａ絶頂 += 3
ELSE
	TIMES SOURCE:快Ａ , 6.00
	EX:Ａ絶頂 += 5
ENDIF


;ABL:マゾっ気をみる
IF ABL:マゾっ気 == 0
	TIMES SOURCE:中毒充足 , 0.80
	TIMES LOSEBASE:体力 , 1.20
ELSEIF ABL:マゾっ気 == 1
	TIMES SOURCE:中毒充足 , 1.10
	TIMES LOSEBASE:体力 , 1.10
ELSEIF ABL:マゾっ気 == 2
	TIMES SOURCE:中毒充足 , 1.30
	TIMES LOSEBASE:体力 , 1.00
ELSEIF ABL:マゾっ気 == 3
	TIMES SOURCE:中毒充足 , 1.50
	TIMES LOSEBASE:体力 , 0.90
ELSEIF ABL:マゾっ気 == 4
	TIMES SOURCE:中毒充足 , 1.70
	TIMES LOSEBASE:体力 , 0.80
ELSE
	TIMES SOURCE:中毒充足 , 2.10
	TIMES LOSEBASE:体力 , 0.70
ENDIF


;PALAM:苦痛をみる
IF PALAM:苦痛 < PALAMLV:1
	SOURCE:痛み = 2700
ELSEIF PALAM:苦痛 < PALAMLV:2
	SOURCE:痛み = 3000
ELSEIF PALAM:苦痛 < PALAMLV:3
	SOURCE:痛み = 3300
ELSEIF PALAM:苦痛 < PALAMLV:4
	SOURCE:痛み = 3700
ELSEIF PALAM:苦痛 >= PALAMLV:4
	SOURCE:痛み = 4000
ENDIF

;媚薬効果を発揮
TEQUIP:21 = 1

;反抗的
SIF TALENT:反抗的
	TIMES SOURCE:痛み , 1.10

;抵抗
SIF TALENT:抵抗
	TIMES SOURCE:痛み , 1.10

;触手補正
IF TEQUIP:90
	TIMES SOURCE:痛み , 0.30
	TIMES SOURCE:逸脱 , 0.80
	LOSEBASE:体力 -= 900
	LOSEBASE:気力 -= 400
ENDIF

;A鈍感
IF TALENT:Ａ鈍感
	IF ABL:Ｖ感覚 == 0
		TIMES SOURCE:快Ａ , 0.75
	ELSEIF ABL:Ｖ感覚 == 1
		TIMES SOURCE:快Ａ , 0.75
	ELSEIF ABL:Ｖ感覚 == 2
		TIMES SOURCE:快Ａ , 0.75
	ELSEIF ABL:Ｖ感覚 == 3
		TIMES SOURCE:快Ａ , 0.75
		EX:Ａ絶頂 -= 1
	ELSEIF ABL:Ｖ感覚 == 4
		TIMES SOURCE:快Ａ , 0.75
		EX:Ａ絶頂 -= 1
	ELSE
		TIMES SOURCE:快Ａ , 0.75
		EX:Ａ絶頂 -= 1
	ENDIF
ENDIF

;A敏感
IF TALENT:Ａ敏感
	IF ABL:Ｖ感覚 == 0
		TIMES SOURCE:快Ａ , 2.00
	ELSEIF ABL:Ｖ感覚 == 1
		TIMES SOURCE:快Ａ , 2.00
	ELSEIF ABL:Ｖ感覚 == 2
		TIMES SOURCE:快Ａ , 2.00
	ELSEIF ABL:Ｖ感覚 == 3
		TIMES SOURCE:快Ａ , 2.00
		EX:Ａ絶頂 += 1
	ELSEIF ABL:Ｖ感覚 == 4
		TIMES SOURCE:快Ａ , 2.00
		EX:Ａ絶頂 += 2
	ELSE
		TIMES SOURCE:快Ａ , 2.00
		EX:Ａ絶頂 += 3
	ENDIF
ENDIF


;調教者に針さばき
SIF TALENT:PLAYER:針さばき
	TIMES SOURCE:痛み , 0.90

;調合知識、治療、針さばきがあれば痛覚半減
;調合知識、治療があれば痛覚減
IF TEQUIP:90 == 0
	IF TALENT:PLAYER:調合知識 && TALENT:PLAYER:治療 && TALENT:PLAYER:針さばき
		TIMES SOURCE:痛み , 0.50
	ELSEIF TALENT:PLAYER:調合知識 && TALENT:PLAYER:治療
		TIMES SOURCE:痛み , 0.70
	ENDIF
ENDIF

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +2
	EXP:レズ経験 += 2
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +2
	EXP:ＢＬ経験 += 2
ENDIF

SIF ASSIPLAY == 0 && ABL:マゾっ気 >= 3
	TFLAG:30 += 1

RETURN 1
