@COM23
;逆レイプ

PRINTL 逆レイプ

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = 逆レイプ
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
SIF TALENT:PLAYER:半人半妖 || TALENT:PLAYER:ふたなり || TALENT:PLAYER:オトコ
	BASE:PLAYER:射精 += CALC_EJAC(SELECTCOM, PLAYER)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 40
LOSEBASE:気力 += 220

SOURCE:屈従 = 1500
SOURCE:逸脱 = 800

;ABL:Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	SOURCE:快Ｃ = 800
	SOURCE:性行動 = 1600
ELSEIF ABL:Ｃ感覚 == 1
	SOURCE:快Ｃ = 1400
	SOURCE:性行動 = 1900
ELSEIF ABL:Ｃ感覚 == 2
	SOURCE:快Ｃ = 2000
	SOURCE:性行動 = 2300
ELSEIF ABL:Ｃ感覚 == 3
	SOURCE:快Ｃ = 2500
	SOURCE:性行動 = 2700
ELSEIF ABL:Ｃ感覚 == 4
	SOURCE:快Ｃ = 2900
	SOURCE:性行動 = 3100
ELSE
	SOURCE:快Ｃ = 2450 + (ABL:Ｃ感覚 * 150)
	SOURCE:性行動 = 2500 + (ABL:Ｃ感覚 * 200)
ENDIF


;ABL:技巧をみる
IF ABL:PLAYER:技巧 == 0
	TIMES SOURCE:快Ｃ , 0.50
	TIMES SOURCE:性行動 , 0.50
ELSEIF ABL:PLAYER:技巧 == 1
	TIMES SOURCE:快Ｃ , 0.80
	TIMES SOURCE:性行動 , 0.80
ELSEIF ABL:PLAYER:技巧 == 2
	TIMES SOURCE:快Ｃ , 1.00
	TIMES SOURCE:性行動 , 1.00
ELSEIF ABL:PLAYER:技巧 == 3
	TIMES SOURCE:快Ｃ , 1.50
	TIMES SOURCE:性行動 , 1.50
ELSEIF ABL:PLAYER:技巧 == 4
	TIMES SOURCE:快Ｃ , 2.50
	TIMES SOURCE:性行動 , 2.50
ELSE
	SOURCE:快Ｃ = SOURCE:快Ｃ * (ABL:PLAYER:技巧 * 75 + 25) / 100
	SOURCE:性行動 = SOURCE:性行動 * (ABL:PLAYER:技巧 * 75 + 25) / 100
ENDIF

;ABL:サドっ気をみる
IF ABL:PLAYER:サドっ気 == 0
	TIMES SOURCE:快Ｃ , 1.00
	TIMES SOURCE:性行動 , 1.00
ELSEIF ABL:PLAYER:サドっ気 == 1
	TIMES SOURCE:快Ｃ , 1.05
	TIMES SOURCE:性行動 , 1.05
ELSEIF ABL:PLAYER:サドっ気 == 2
	TIMES SOURCE:快Ｃ , 1.10
	TIMES SOURCE:性行動 , 1.10
ELSEIF ABL:PLAYER:サドっ気 == 3
	TIMES SOURCE:快Ｃ , 1.20
	TIMES SOURCE:性行動 , 1.20
ELSEIF ABL:PLAYER:サドっ気 == 4
	TIMES SOURCE:快Ｃ , 1.30
	TIMES SOURCE:性行動 , 1.30
ELSE
	SOURCE:快Ｃ = SOURCE:快Ｃ * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
	SOURCE:性行動 = SOURCE:性行動 * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
ENDIF

;調教者が処女
IF TALENT:PLAYER:処女 == 1
	TIMES SOURCE:屈従 , 20.00
	TIMES SOURCE:逸脱 , 3.00
	TALENT:PLAYER:処女 = 0

	PRINTFORML ＜%CALLNAME:PLAYER%が処女＞

	EXP:異常経験 += 5
	PRINTFORML %EXPNAME:50%＋５

	IF ABL:親密 < 1
		PRINTFORML そして%ABLNAME:0%が1になった
		ABL:親密 = 1
	ENDIF
ENDIF
EXP:PLAYER:Ｖ経験 += 1


;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:受動 += 300 + (ABL:親密 * 80)
	ELSEIF ABL:親密 <= 3
		SOURCE:受動 += 600 + (ABL:親密 * 130)
	ELSEIF ABL:親密 <= 5
		SOURCE:受動 += 800 + (ABL:親密 * 150)
	ELSEIF ABL:親密 <= 8
		SOURCE:受動 += 1000 + (ABL:親密 * 170)
	ELSEIF ABL:親密 <= 11
		SOURCE:受動 += 1200 + (ABL:親密 * 190)
	ELSE
		SOURCE:受動 += 1500 + (ABL:親密 * 210)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:征服 = 600
	ELSEIF ABL:サドっ気 == 1
		SOURCE:征服 = 900
	ELSEIF ABL:サドっ気 == 2
		SOURCE:征服 = 1200
	ELSEIF ABL:サドっ気 == 3
		SOURCE:征服 = 1600
	ELSEIF ABL:サドっ気 == 4
		SOURCE:征服 = 2000
	ELSE
		SOURCE:征服 = 1000 + (ABL:サドっ気 * 300)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
SIF TALENT:恋慕 || TALENT:恋人
	A += 5
SIF TALENT:親愛
	A += 5
;合意がないと好感度が下がる
IF TALENT:合意 == 0 && TFLAG:48 == 0
	A -= 10
	SOURCE:反感追加 += 500
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;既成事実3に相当
TFLAG:200 = 3

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK

;射精時の処理
IF E >= 1
	;コマンドごとに固有の処理はこの下に書く
	TIMES SOURCE:性行動 , 3.00

	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		SOURCE:中毒充足 = 0
		TIMES SOURCE:快Ｃ , 2.00
		TIMES SOURCE:屈従, 2.00
	ELSEIF ABL:精液中毒 == 1
		SOURCE:中毒充足 = 50
		TIMES SOURCE:快Ｃ , 2.50
		TIMES SOURCE:屈従, 1.60
	ELSEIF ABL:精液中毒 == 2
		SOURCE:中毒充足 = 150
		TIMES SOURCE:快Ｃ , 3.00
		TIMES SOURCE:屈従, 1.00
	ELSEIF ABL:精液中毒 == 3
		SOURCE:中毒充足 = 300
		TIMES SOURCE:快Ｃ , 4.50
		TIMES SOURCE:屈従, 0.70
	ELSEIF ABL:精液中毒 == 4
		SOURCE:中毒充足 = 600
		TIMES SOURCE:快Ｃ , 6.00
		TIMES SOURCE:屈従, 0.40
	ELSE
		SOURCE:中毒充足 = (ABL:精液中毒 * 300) - 300
		SOURCE:快Ｃ = SOURCE:快Ｃ * (ABL:精液中毒 + 3)
		TIMES SOURCE:屈従, 0.10
	ENDIF

	;射精先タイプの設定
	TFLAG:703 = 9
	;射精先選択の可・不可
	;TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

IF TALENT:半人半妖 || TALENT:ふたなり || TALENT:オトコ
	;奴隷のＰ⇔調教者のＶの汚れが移動
	STAIN:ペニス |= STAIN:PLAYER:ヴァギナ
	STAIN:PLAYER:ヴァギナ |= STAIN:ペニス
ENDIF

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +4
	EXP:レズ経験 += 4
	EXP:PLAYER:レズ経験 += 4
ENDIF

TFLAG:100 = 1

RETURN 1

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_23
A = 0
S = 0
V = 15

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:従順
IF ABL:親密
	A += ABL:親密 * 4
ENDIF

;ABL:欲望
IF ABL:欲望
	A += ABL:欲望 * 2
ENDIF

IF MARK:快楽依存
	A += MARK:快楽依存 * 2
ENDIF

;PALAM:欲情
IF PALAM:欲情 < PALAMLV:1
	L = 0
ELSEIF PALAM:欲情 < PALAMLV:2
	L = 1
ELSEIF PALAM:欲情 < PALAMLV:3
	L = 2
ELSEIF PALAM:欲情 < PALAMLV:4
	L = 3
ELSEIF PALAM:欲情 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 2
ENDIF

;快感に素直
IF TALENT:快感に素直
	A += 2
ENDIF
;快感の否定
IF TALENT:快感の否定
	A -= 2
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	A += 5
ENDIF

;調教者が処女
IF EXP:PLAYER:Ｖ経験 == 0
	A -= 15
ENDIF

;調教者が貞操観念
IF TALENT:PLAYER:貞操観念
	A -= 10
ENDIF

;調教者が貞操無頓着
IF TALENT:PLAYER:貞操観念
	A += 5
ENDIF

;調教者のABL:技巧
IF ABL:PLAYER:技巧
	A += ABL:PLAYER:技巧 * 4
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:従順
	IF ABL:親密
		SIF S
			PRINT  + 
		PRINTS ABLNAME:0
		PRINTV 'LV,ABL:親密,'(,ABL:親密 * 4,')
		S = 1
	ENDIF

	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 2,')
		S = 1
	ENDIF

	IF MARK:快楽依存
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:快楽依存,'(,MARK:快楽依存 * 2,')
		S = 1
	ENDIF

	;PALAM:欲情
	IF PALAM:欲情 < PALAMLV:1
		L = 0
	ELSEIF PALAM:欲情 < PALAMLV:2
		L = 1
	ELSEIF PALAM:欲情 < PALAMLV:3
		L = 2
	ELSEIF PALAM:欲情 < PALAMLV:4
		L = 3
	ELSEIF PALAM:欲情 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 2,')
		S = 1
	ENDIF

	;快感に素直
	IF TALENT:快感に素直
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:70
		PRINTV '(,2,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:快感の否定
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,2,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:恋慕 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 1
	ENDIF

	;調教者が処女
	IF EXP:PLAYER:Ｖ経験 == 0
		PRINT  - 
		PRINTFORM %NAME:PLAYER%が処女
		PRINTV '(,15,')
		S = 1
	ENDIF

	;調教者が貞操観念
	IF TALENT:PLAYER:貞操観念
		PRINT  - 
		PRINTFORM %NAME:PLAYER%が貞操観念
		PRINTV '(,10,')
		S = 1
	ENDIF

	;調教者が貞操無頓着
	IF TALENT:PLAYER:貞操観念
		PRINT  + 
		PRINTFORM %NAME:PLAYER%が貞操無頓着
		PRINTV '(,5,')
		S = 1
	ENDIF

	;調教者のABL:技巧
	IF ABL:PLAYER:技巧
		SIF S
			PRINT  + 
		PRINTFORM %NAME:PLAYER%の
		PRINTS ABLNAME:2
		PRINTV 'LV,ABL:PLAYER:技巧,'(,ABL:PLAYER:技巧 * 4,')
		S = 1
	ENDIF

	;合計を表示(15以上で実行)
	PRINT  = 
	PRINTV A


	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF
