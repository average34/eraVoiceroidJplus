SIF TALENT:85 || TALENT:153
	A += 5
SIF TALENT:88
	A += 5
;欲望11以上か好感度8000以上でないと好感度が下がる
IF ABL:1 < 11 && CFLAG:2 < 8000
	A -= 12
	;恋慕で少し反感
	IF TALENT:85
		SOURCE:15 += 200
	ELSE
	;恋慕なしだと反感増加
		SOURCE:15 += 600
	ENDIF
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;既成事実3に相当
TFLAG:200 = 3

;-------------------------------------------------
;射精チェック
;-------------------------------------------------

CALL SAMEN_CHECK
;;オーバーフロー対策に、値が大きすぎる場合と、負の場合は調整を加える
