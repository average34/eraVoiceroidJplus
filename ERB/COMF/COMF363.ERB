	SOURCE:3 = 150
ELSEIF ABL:4 == 1
	SOURCE:1 = 150
	SOURCE:3 = 250
ELSEIF ABL:4 == 2
	SOURCE:1 = 400
	SOURCE:3 = 350
ELSEIF ABL:4 == 3
	SOURCE:1 = 1000
	SOURCE:3 = 500
ELSEIF ABL:4 == 4
	SOURCE:1 = 1700
	SOURCE:3 = 1700
ELSE
	SOURCE:1 = 300 + (ABL:4 * 300)
	SOURCE:3 = 250 + (ABL:4 * 350)
ENDIF

;着衣補正でV快感2割減
TIMES SOURCE:1 , 0.80

;EXP:Ｖ経験をみる
IF EXP:0 < EXPLV:1
