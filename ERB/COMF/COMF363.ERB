IF CFLAG:2 < 5000
	B = 0
	IF CFLAG:2 < 2000
		B = 50
	ELSEIF CFLAG:2 < 3000
		B = 20
	ELSEIF CFLAG:2 < 4000
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 7000
	B = 0
	IF CFLAG:2 < 10000
		B = 5
	ELSE
		B = CFLAG:2 / 500
	ENDIF
	A += B
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:1
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:1,'(,ABL:1 * 3,')
		S = 1
	ENDIF
	;ABL:Ｖ感覚
	IF ABL:4
		SIF S
			PRINT  + 
		PRINTS ABLNAME:4
		PRINTV 'LV,ABL:4,'(,ABL:4 * 2,')
		S = 1
	ENDIF
	;ABL:奉仕精神
	IF ABL:6
		SIF S
			PRINT  + 
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:6,'(,ABL:6 * 4,')
		S = 1
	ENDIF
	;ABL:精液中毒
	IF ABL:11
		SIF S
			PRINT  + 
		PRINTS ABLNAME:11
		PRINTV 'LV,ABL:11,'(,ABL:11 * 1,')
		S = 1
	ENDIF

	IF MARK:1
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:1,'(,MARK:1 * 3,')
		S = 1
	ENDIF

	;PALAM:欲情
	IF PALAM:5 < 15000
		L = 0
	ELSEIF PALAM:5 < 20000
		L = 1
	ELSEIF PALAM:5 < 30000
		L = 2
	ELSEIF PALAM:5 < 40000
		L = 3
	ELSEIF PALAM:5 < 60000
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 3,')
		S = 1
	ENDIF

	;恥じらい
	IF TALENT:35
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,5,')
		S = 1
	ENDIF
	;快感の否定
