@COM_ORDER_363
A = 0
S = 0
V = 150

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	A += ABL:欲望 * 3
ENDIF
;ABL:Ｖ感覚
IF ABL:Ｖ感覚
	A += ABL:Ｖ感覚 * 2
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	A += ABL:奉仕精神 * 4
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	A += ABL:精液中毒 * 1
ENDIF

IF MARK:快楽依存
	A += MARK:快楽依存 * 3
ENDIF

;PALAM:欲情
IF PALAM:欲情 < 15000
	L = 0
ELSEIF PALAM:欲情 < 20000
	L = 1
ELSEIF PALAM:欲情 < 30000
	L = 2
ELSEIF PALAM:欲情 < 40000
	L = 3
ELSEIF PALAM:欲情 < 60000
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 3
ENDIF

;恥じらい
IF TALENT:恥じらい
	A -= 5
ENDIF
;快感の否定
IF TALENT:快感の否定
	A -= 5
ENDIF
;男嫌い
IF TALENT:男嫌い && TALENT:PLAYER:オトコ
	A -= 12
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	A += 8
	S = 5
ENDIF

;処女
IF TALENT:処女
	A -= 30
ELSEIF EXP:Ｖ経験 < EXPLV:2
	A -= 10
ENDIF

;一線越えない（処女のみ）
IF TALENT:一線越えない && TALENT:処女
	A -= 20
ENDIF

;媚薬
IF TEQUIP:21
	A += 6
ENDIF

;童貞
IF TALENT:PLAYER:童貞
	A += 3
ENDIF

;ムードボーナス
B = 0
IF TFLAG:37 < 10
	B -= 40
ELSEIF TFLAG:37 < 30
	B -= 30
ELSEIF TFLAG:37 < 50
	B -= 20
ELSEIF TFLAG:37 < 70
	B -= 5
ELSEIF TFLAG:37 < 100
	B += TFLAG:37 / 15
ELSEIF TFLAG:37 < 300
	B += TFLAG:37 / 5
ELSE
	B += TFLAG:37 / 3
ENDIF
A += B
IF B < 0
	B = B * -1
ELSE
ENDIF


;好感度
IF CFLAG:2 < 5000
	B = 0
	IF CFLAG:2 < 2000
		B = 50
	ELSEIF CFLAG:2 < 3000
		B = 20
	ELSEIF CFLAG:2 < 4000
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 7000
	B = 0
	IF CFLAG:2 < 10000
		B = 5
	ELSE
		B = CFLAG:2 / 500
	ENDIF
	A += B
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 3,')
		S = 1
	ENDIF
	;ABL:Ｖ感覚
	IF ABL:Ｖ感覚
		SIF S
			PRINT  + 
		PRINTS ABLNAME:4
		PRINTV 'LV,ABL:Ｖ感覚,'(,ABL:Ｖ感覚 * 2,')
		S = 1
	ENDIF
	;ABL:奉仕精神
	IF ABL:奉仕精神
		SIF S
			PRINT  + 
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
		S = 1
	ENDIF
	;ABL:精液中毒
	IF ABL:精液中毒
		SIF S
			PRINT  + 
		PRINTS ABLNAME:11
		PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 1,')
		S = 1
	ENDIF

	IF MARK:快楽依存
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:快楽依存,'(,MARK:快楽依存 * 3,')
		S = 1
	ENDIF

	;PALAM:欲情
	IF PALAM:欲情 < 15000
		L = 0
	ELSEIF PALAM:欲情 < 20000
		L = 1
	ELSEIF PALAM:欲情 < 30000
		L = 2
	ELSEIF PALAM:欲情 < 40000
		L = 3
	ELSEIF PALAM:欲情 < 60000
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 3,')
		S = 1
	ENDIF

	;恥じらい
	IF TALENT:恥じらい
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,5,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:快感の否定
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,5,')
		S = 1
	ENDIF
	;男嫌い
	IF TALENT:男嫌い && TALENT:PLAYER:オトコ
		PRINT  - 
		PRINTS TALENTNAME:82
		PRINTV '(,12,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:恋慕 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,8,')
		S = 5
	ENDIF

	;処女
	IF TALENT:処女
		PRINT  - 
		PRINTS TALENTNAME:0
		PRINTV '(,30,')
		S = 1
	ELSEIF EXP:Ｖ経験 < EXPLV:2
		PRINT  - 
		PRINTS EXPNAME:0
		PRINT 不足
		PRINTV '(,10,')
		S = 1
	ENDIF

	;一線越えない（処女のみ）
	IF TALENT:一線越えない && TALENT:処女
		PRINT  - 
		PRINTS TALENTNAME:27
		PRINTV '(,20,')
		S = 1
	ENDIF

	;媚薬
	IF TEQUIP:21
		SIF S
			PRINT  + 
		PRINTS ITEMNAME:26
		PRINTV '(,6,')
		S = 1
	ENDIF

	;童貞
	IF TALENT:PLAYER:童貞
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:154
		PRINTV '(,3,')
		S = 1
	ENDIF

	;ムードボーナス
	B = 0
	IF TFLAG:37 < 10
		B -= 40
	ELSEIF TFLAG:37 < 30
		B -= 30
	ELSEIF TFLAG:37 < 50
		B -= 20
	ELSEIF TFLAG:37 < 70
		B -= 5
	ELSEIF TFLAG:37 < 100
		B += TFLAG:37 / 15
	ELSEIF TFLAG:37 < 300
		B += TFLAG:37 / 5
	ELSE
		B += TFLAG:37 / 3
	ENDIF
	IF B < 0
		B = B * -1
		PRINT  - 
		PRINT ムード悪い
		PRINTV '(,B,')
		S = 1
	ELSE
		PRINT  + 
		PRINT ムード良い
		PRINTV '(,B,')
		S = 1
	ENDIF


	;好感度
	IF CFLAG:2 < 5000
		B = 0
		PRINT  - 
		IF CFLAG:2 < 2000
			B = 50
		ELSEIF CFLAG:2 < 3000
			B = 20
		ELSEIF CFLAG:2 < 4000
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 >= 7000
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 10000
			B = 5
		ELSE
			B = CFLAG:2 / 500
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合計を表示(150以上で実行)
	PRINT  = 
	PRINTV A

	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF

