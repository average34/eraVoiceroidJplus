	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:6 , 0.10
ENDIF

;調教者がふたなり
SIF TALENT:PLAYER:121
	TIMES SOURCE:1 , 2.50

;小柄体形
SIF TALENT:100
	TIMES SOURCE:6 , 2.00

;貞操観念
IF TALENT:30 && TALENT:85 == 0
	IF EXP:0 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 13000
	ELSE
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 1300
	ENDIF
;貞操無頓着
ELSEIF TALENT:31 || TALENT:88 && ASSIPLAY == 0
