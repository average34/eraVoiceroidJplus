	RETURN 0
;潤滑10000以上かつ、欲情10000以上が必要
SIF PALAM:3 < 10000 || PALAM:5 < 10000
	RETURN 0
;欲望9以上が必要
SIF ABL:1 < 9
	RETURN 0
;親密13以上or好感度6000以上が必要
SIF ABL:0 < 13 && CFLAG:2 < 6000
	RETURN 0
;合意が必要
SIF TALENT:152 == 0
	RETURN 0
;淫乱が必要
SIF TALENT:76 == 0
	RETURN 0
;ムードか酒の勢いが必要
SIF TFLAG:37 < 50 && TFLAG:47 < 30
	RETURN 0
RETURN 1

