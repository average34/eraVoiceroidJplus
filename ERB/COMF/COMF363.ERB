	B += TFLAG:37 / 15
ELSEIF TFLAG:37 < 300
	B += TFLAG:37 / 5
ELSE
	B += TFLAG:37 / 3
ENDIF
A += B
IF B < 0
	B = B * -1
ELSE
ENDIF


