@COM_ABLE201
;指フェラ実行判定
;COM_ABLE共通判定
CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中じゃないと無理
SIF TFLAG:44 == 0
	RETURN 0
;[猫舌]だとダメ
SIF TALENT:猫舌
	RETURN 0
;乗馬中はダメ
SIF TEQUIP:41
	RETURN 0
;蜘蛛の巣にかけているときはダメ
SIF TEQUIP:44 == 7
	RETURN 0
;ボールギャグ使用中は不可
SIF TEQUIP:45
	RETURN 0
;女体盛り中はダメ
SIF TEQUIP:69
	RETURN 0
;時止め中は無理
SIF TEQUIP:81
	RETURN 0
;触手調教中はダメ
SIF TEQUIP:90
	RETURN 0
RETURN 1

@COM201
;指フェラ
PRINTL 指フェラ

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = 指フェラ
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 5
LOSEBASE:気力 += 50

SOURCE:屈従 = 500
SOURCE:逸脱 = 200

;上のほうで計算した汚れデータ
SOURCE:不潔 = Y*80 + 50

;Ｍ感覚を設定している場合快Ｍソース等取得
SIF FLAG:62 & 4194304
	CALL SOURCE_M_COM201

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:性行動 = 210
	SOURCE:達成感 = 100
	TIMES SOURCE:不潔 , 4.00
ELSEIF ABL:奉仕精神 == 1
	SOURCE:性行動 = 250
	SOURCE:達成感 = 200
	TIMES SOURCE:不潔 , 2.50
ELSEIF ABL:奉仕精神 == 2
	SOURCE:性行動 = 290
	SOURCE:達成感 = 400
	TIMES SOURCE:不潔 , 1.50
ELSEIF ABL:奉仕精神 == 3
	SOURCE:性行動 = 330
	SOURCE:達成感 = 600
	TIMES SOURCE:不潔 , 1.00
ELSEIF ABL:奉仕精神 == 4
	SOURCE:性行動 = 370
	SOURCE:達成感 = 1000
	TIMES SOURCE:不潔 , 0.50
ELSE
	SOURCE:性行動 = 310 + (ABL:奉仕精神 * 20)
	SOURCE:達成感 = 300 + (ABL:奉仕精神 * 240)
	TIMES SOURCE:不潔 , 0.10
ENDIF

;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:性行動 , 0.50
	TIMES SOURCE:達成感 , 0.50
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:性行動 , 0.80
	TIMES SOURCE:達成感 , 0.80
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:性行動 , 1.00
	TIMES SOURCE:達成感 , 1.00
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:性行動 , 1.50
	TIMES SOURCE:達成感 , 1.50
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:性行動 , 2.50
	TIMES SOURCE:達成感 , 2.50
ELSE
	SOURCE:性行動 = SOURCE:性行動 * (ABL:技巧 * 75 + 25) / 100
	SOURCE:達成感 = SOURCE:達成感 * (ABL:技巧 * 75 + 25) / 100
ENDIF


;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:受動 += 300 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:受動 += 600 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:受動 += 800 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:受動 += 1000 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:受動 += 1200 + (ABL:親密 * 160)
	ELSE
		SOURCE:受動 += 1500 + (ABL:親密 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:征服 = 600
	ELSEIF ABL:サドっ気 == 1
		SOURCE:征服 = 900
	ELSEIF ABL:サドっ気 == 2
		SOURCE:征服 = 1200
	ELSEIF ABL:サドっ気 == 3
		SOURCE:征服 = 1600
	ELSEIF ABL:サドっ気 == 4
		SOURCE:征服 = 2000
	ELSE
		SOURCE:征服 = 1000 + (ABL:サドっ気 * 300)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
;合意がないと好感度が下がる
IF TALENT:合意 == 0 && TFLAG:48 == 0
	A -= 9
	SOURCE:反感追加 += 400
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;奴隷の口⇔調教者の手の汚れが移動
STAIN:口 |= STAIN:PLAYER:手
STAIN:PLAYER:手 |= STAIN:口

PRINTFORML %EXPNAME:22%＋１
EXP:フェラ経験 += 1

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTFORML %EXPNAME:40%＋3
	EXP:レズ経験 += 3
ENDIF

TFLAG:100 = 1

;既成事実１に相当
TFLAG:200 = 1

RETURN 1

;Ｍ感覚を設定している場合快Ｍソース等取得
@SOURCE_M_COM201
;快Mのソース・M感覚を見る
IF ABL:Ｍ感覚 == 1
	SOURCE:快Ｍ = 20
ELSEIF ABL:Ｍ感覚 == 2
	SOURCE:快Ｍ = 50
ELSEIF ABL:Ｍ感覚 == 3
	SOURCE:快Ｍ = 100
ELSEIF ABL:Ｍ感覚 == 4
	SOURCE:快Ｍ = 200
ELSEIF ABL:Ｍ感覚 >= 5
	SOURCE:快Ｍ = 200 + (100 * (ABL:Ｍ感覚 - 4))
ENDIF

;TALENT:恋慕をみる
SIF TALENT:恋慕
	TIMES SOURCE:快Ｍ , 1.50
;TALENT:恋人をみる
SIF TALENT:恋人
	TIMES SOURCE:快Ｍ , 1.50
;TALENT:親愛をみる
SIF TALENT:親愛
	TIMES SOURCE:快Ｍ , 2.00
;TALENT:相愛をみる
SIF TALENT:相愛
	TIMES SOURCE:快Ｍ , 2.00

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	TIMES SOURCE:快Ｍ , 0.00
ELSEIF ABL:奉仕精神 == 1
	TIMES SOURCE:快Ｍ , 0.00
ELSEIF ABL:奉仕精神 == 2
	TIMES SOURCE:快Ｍ , 0.00
ELSEIF ABL:奉仕精神 == 3
	TIMES SOURCE:快Ｍ , 0.85
ELSEIF ABL:奉仕精神 == 4
	TIMES SOURCE:快Ｍ , 1.00
ELSE
	TIMES SOURCE:快Ｍ , 1.15
ENDIF

;指フェラ
@TRAIN_MESSAGE_COM201
PRINTFORM %CALLNAME:TARGET%は
SIF TALENT:恋慕
	PRINT 慈しむような表情で、
PRINTFORMW %CALLNAME:PLAYER%の指を咥えて舐め回している…

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_201
A = 0
S = 0
V = 60

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	S = ABL:欲望
	A += S
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	S = ABL:奉仕精神
	A += S * 4
ENDIF

;快楽刻印
IF MARK:快楽依存
	S = MARK:快楽依存
	A += S * 2
ENDIF

;PALAM:欲情
IF PALAM:欲情 < PALAMLV:1
	L = 0
ELSEIF PALAM:欲情 < PALAMLV:2
	L = 1
ELSEIF PALAM:欲情 < PALAMLV:3
	L = 2
ELSEIF PALAM:欲情 < PALAMLV:4
	L = 3
ELSEIF PALAM:欲情 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	S = L
	A += S
ENDIF

;恥じらい
IF TALENT:恥じらい
	A -= S
ENDIF
;汚臭鈍感
IF TALENT:汚臭鈍感
	A += 3
ENDIF
;汚臭敏感
IF TALENT:汚臭敏感
	A -= 3
ENDIF
;献身的
IF TALENT:献身的
	S = 6
	A += S
ENDIF
;快感の否定
IF TALENT:快感の否定
	A -= S
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	S = 5
	A += S
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:手 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:手 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:手 & 8
	Y += 7
SIF STAIN:PLAYER:手 & 16
	Y += 15

SIF TALENT:汚臭鈍感
	Y /= 3
SIF TALENT:汚臭敏感
	Y *= 2

;汚れあり
IF Y
	A -= Y
ENDIF

;好感度
IF CFLAG:2 < 1000
	B = 0
	IF CFLAG:2 < 500
		B = 50
	ELSEIF CFLAG:2 < 750
		B = 20
	ELSEIF CFLAG:2 < 900
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 1500
	B = 0
	IF CFLAG:2 < 2000
		B = 5
	ELSE
		B = CFLAG:2 / 200
	ENDIF
	A += B
ENDIF

;合意
IF TALENT:合意 == 0
	A -= 10
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		S = ABL:欲望
		PRINTFORM %ABLNAME:1%LV{S}({S})
	ENDIF
	;ABL:奉仕精神
	IF ABL:奉仕精神
		SIF S
			PRINT  + 
		S = ABL:奉仕精神
		PRINTFORM %ABLNAME:6%LV{S}({S * 4})
	ENDIF

	;快楽刻印
	IF MARK:快楽依存
		SIF S
			PRINT  + 
		S = MARK:快楽依存
		PRINTFORM %MARKNAME:1%LV{S}({S * 2})
	ENDIF

	;PALAM:欲情
	IF PALAM:欲情 < PALAMLV:1
		L = 0
	ELSEIF PALAM:欲情 < PALAMLV:2
		L = 1
	ELSEIF PALAM:欲情 < PALAMLV:3
		L = 2
	ELSEIF PALAM:欲情 < PALAMLV:4
		L = 3
	ELSEIF PALAM:欲情 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		S = L
		PRINTFORM %PALAMNAME:5%LV{L}({S})
	ENDIF

	;恥じらい
	IF TALENT:恥じらい
		PRINT  - 
		S = 1
		PRINTFORM %TALENTNAME:35%({S})
	ENDIF
	;汚臭鈍感
	IF TALENT:汚臭鈍感
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:61
		PRINTV '(,3,')
		S = 1
	ENDIF
	;汚臭敏感
	IF TALENT:汚臭敏感
		PRINT  - 
		PRINTS TALENTNAME:62
		PRINTV '(,3,')
		S = 1
	ENDIF
	;献身的
	IF TALENT:献身的
		SIF S
			PRINT  + 
		S = 6
		PRINTFORM %TALENTNAME:63%({S})
	ENDIF
	;快感の否定
	IF TALENT:快感の否定
		PRINT  - 
		S = 1
		PRINTFORM %TALENTNAME:71%({S})
	ENDIF
	;恋慕
	IF TALENT:恋慕 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		S = 5
		PRINTFORM %TALENTNAME:85%({S})
	ENDIF

	Y = 0
	;愛液の汚れ
	SIF STAIN:PLAYER:手 & 1
		Y += 1
	;精液の汚れ
	SIF STAIN:PLAYER:手 & 4
		Y += 3
	;アナルの汚れ
	SIF STAIN:PLAYER:手 & 8
		Y += 7
	SIF STAIN:PLAYER:手 & 16
		Y += 15

	SIF TALENT:汚臭鈍感
		Y /= 3
	SIF TALENT:汚臭敏感
		Y *= 2

	;汚れあり
	IF Y
		PRINT  - 
		PRINT 汚れあり
		;汚臭鈍感
		IF TALENT:汚臭鈍感
			PRINTFORM 、%TALENTNAME:61%
		ELSEIF TALENT:汚臭敏感
			PRINTFORM 、%TALENTNAME:62%
		ENDIF
		PRINTFORM ({Y})
		S = 1
	ENDIF

	;好感度
	IF CFLAG:2 < 1000
		B = 0
		PRINT  - 
		IF CFLAG:2 < 500
			B = 50
		ELSEIF CFLAG:2 < 750
			B = 20
		ELSEIF CFLAG:2 < 900
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 >= 1500
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 2000
			B = 5
		ELSE
			B = CFLAG:2 / 200
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合意
	IF TALENT:合意 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,10,')
		S = 1
	ENDIF

	;合計を表示(60以上で実行)
	PRINT  = 
	PRINTV A


	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF

