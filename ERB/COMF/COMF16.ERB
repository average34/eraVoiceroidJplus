@COM16
;搾乳器
IF TEQUIP:90
	PRINTL 触手搾乳
	STR:0 = 触手搾乳
ELSE
	;実行者が[淫具知識]を持っているなら効果増
	SIF TALENT:PLAYER:58
		PRINT 高性能
	PRINTL 搾乳器
	STR:0 = 搾乳器
ENDIF

LOSEBASE:0 += 50
LOSEBASE:1 += 100

SOURCE:4 = 100
SOURCE:5 = 100
SOURCE:6 = 100
SOURCE:7 = 100
SOURCE:12 = 100
SOURCE:13 = 100
SOURCE:16 = 100


;ABL:Ｂ感覚をみる
IF ABL:14 == 0
	A = 200
ELSEIF ABL:14 == 1
	A = 400
ELSEIF ABL:14== 2
	A = 900
ELSEIF ABL:14 == 3
	A = 1600
ELSEIF ABL:14 == 4
	A = 2400
ELSE
	A = 1500 + (ABL:14 * 300)
ENDIF

IF TALENT:110
	TIMES A , 1.50
ENDIF
IF TALENT:108
	TIMES A , 1.50
ELSEIF TALENT:107
	TIMES A , 0.60
ENDIF

;貧乳
SIF TALENT:109
	TIMES SOURCE:6 , 1.50

SOURCE:17 += A

;実行者が[淫具知識]を持っているなら効果増
SIF TALENT:PLAYER:58
	TIMES SOURCE:17 , 1.20

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:40 += 1
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:41 += 1
ENDIF

CALL TRAIN_MESSAGE_B

;搾乳器の着脱
IF TEQUIP:90
	IF TEQUIP:16
		TEQUIP:16 = 0
	ELSE
		TEQUIP:16 = 1
		STAIN:5 |= 2
		STAIN:5 |= 4
	ENDIF
	T = 0
ELSE
	IF TEQUIP:16
		TEQUIP:16 = 0
	ELSE
		TEQUIP:16 = 1
	ENDIF
ENDIF

RETURN 1

@TRAIN_MESSAGE_COM16
;搾乳器の着脱
IF TEQUIP:90
	IF TEQUIP:16
		PRINTFORML 触手は%CALLNAME:TARGET%の乳首を解放した
	ELSE
		PRINTFORML 吸盤状の触手が%CALLNAME:TARGET%の乳首を吸いあげはじめた
	ENDIF
ELSE
	IF TEQUIP:16
		PRINTFORML %CALLNAME:TARGET%から搾乳器を取り外した
	ELSE
		PRINTFORML %CALLNAME:TARGET%に搾乳器を取り付けた
	ENDIF
ENDIF

@EQUIP_COM16
;搾乳器装備中
IF TEQUIP:90
	PRINTL ＜触手搾乳中＞
ELSE
	PRINTL ＜搾乳器装備中＞
ENDIF

LOSEBASE:0 += 15
LOSEBASE:1 += 15

;ABL:B感覚をみる
IF ABL:14 == 0
	A = 40
ELSEIF ABL:14 == 1
	A = 120
ELSEIF ABL:14 == 2
	A = 250
ELSEIF ABL:14 == 3
	A = 450
ELSEIF ABL:14 == 4
	A = 600
ELSE
	A = 375 + (ABL:14 * 75)
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:5 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:5 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES A , 1.10
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES A , 1.20
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES A , 0.80
ELSEIF ABL:0 == 1
	TIMES A , 0.90
ELSEIF ABL:0 == 2
	TIMES A , 1.00
ELSEIF ABL:0 == 3
	TIMES A , 1.10
ELSEIF ABL:0 == 4
	TIMES A , 1.20
ELSE
	A = A * (ABL:0 * 5 + 105) / 100
ENDIF

;EXP:墳乳経験をみる
IF EXP:54 <= EXPLV:0
	TIMES A , 1.00
ELSEIF EXP:54 <= EXPLV:1
	TIMES A , 1.20
ELSEIF EXP:54 <= EXPLV:2
	TIMES A , 1.40
ELSEIF EXP:54 <= EXPLV:3
	TIMES A , 1.80
ELSEIF EXP:54 <= EXPLV:4
	TIMES A , 2.25
ELSE
	TIMES A , 4.00
ENDIF

IF TALENT:110
	TIMES A , 1.3
ENDIF
IF TALENT:108
	TIMES A , 1.4
ELSEIF TALENT:107
	TIMES A , 0.6
ENDIF

;貧乳
IF TALENT:109
	TIMES SOURCE:6 , 1.50
ENDIF

SOURCE:17 += A
SOURCE:4 += 150
SOURCE:6 += 100
SOURCE:7 += 100
SOURCE:11 += 50
SOURCE:12 += 50
SOURCE:13 += 50
SOURCE:14 += 50
SOURCE:16 += 50

;実行者が[淫具知識]を持っているなら効果増
SIF TALENT:PLAYER:58
	TIMES A , 1.20

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:40 += 1
	EXP:PLAYER:40 += 1
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:41 += 1
	EXP:PLAYER:41 += 1
ENDIF

IF TEQUIP:90
	T += 1
	CALL SYOKUSYU_MILK
ENDIF

RETURN 1
