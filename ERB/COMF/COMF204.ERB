@COM_ABLE204
;実行判定
;COM_ABLE共通判定
CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中じゃないと無理
SIF TFLAG:44 == 0
	RETURN 0
;異常経験フィルタ
SIF (FLAG:61 & 1) == 0
	RETURN 0
;小人体型持ちは、双方が調教者と調教対象ならOK
;それ以外の場合は主人が禁断の知識持ちじゃないとダメ
SIF TALENT:小人体型 && (TALENT:MASTER:禁断の知識 == 0 && TALENT:PLAYER:小人体型 == 0)
	RETURN 0
;オトコ、半人半妖、ふたなりでない場合、ペニスバンドがないとダメ
IF (TALENT:TARGET:半人半妖 || TALENT:TARGET:ふたなり || TALENT:TARGET:オトコ) == 0
	SIF ITEM:ペニスバンド == 0 && NOITEM == 0
		RETURN 0
ENDIF
;オナホール使用中はダメ
SIF TEQUIP:17
	RETURN 0
;触手調教中はダメ
SIF TEQUIP:90
	RETURN 0
;お風呂場プレイ中はプレイマットが無いとダメ
IF TEQUIP:58
SIF (ITEM:プレイマット == 0) && (NOITEM == 0)
	RETURN 0
ENDIF
;新妻プレイ中はダメ
SIF TEQUIP:59
	RETURN 0
;時止め中は無理
SIF TEQUIP:81
	RETURN 0
RETURN 1

@COM204
;アナルに入れさせる

PRINTL アナルに入れさせる

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = アナルに入れさせる
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック(パートナー)
;-------------------------------------------------
SIF TALENT:TARGET:半人半妖 || TALENT:TARGET:ふたなり || TALENT:TARGET:オトコ
	BASE:TARGET:射精 += CALC_EJAC(SELECTCOM, TARGET)
;-------------------------------------------------
;射精ゲージチェック(実行者が射精できる場合)
;-------------------------------------------------
SIF TALENT:PLAYER:半人半妖 || TALENT:PLAYER:オトコ  || TALENT:PLAYER:ふたなり
	BASE:PLAYER:射精 += CALC_EJAC(SELECTCOM, PLAYER)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 60
LOSEBASE:気力 += 150

SOURCE:情愛 = 1000
SOURCE:中毒充足 = 800
SOURCE:接触 = 1000
SOURCE:屈従 = 1500
SOURCE:逸脱 = 1800

;ABL:Ｃ感覚をみる
IF ABL:TARGET:Ｃ感覚 == 0
	SOURCE:快Ｃ = 800
	SOURCE:性行動 = 1600
ELSEIF ABL:Ｃ感覚 == 1
	SOURCE:快Ｃ = 1400
	SOURCE:性行動 = 1900
ELSEIF ABL:Ｃ感覚 == 2
	SOURCE:快Ｃ = 2000
	SOURCE:性行動 = 2300
ELSEIF ABL:Ｃ感覚 == 3
	SOURCE:快Ｃ = 2500
	SOURCE:性行動 = 2700
ELSEIF ABL:Ｃ感覚 == 4
	SOURCE:快Ｃ = 2900
	SOURCE:性行動 = 3100
ELSE
	SOURCE:快Ｃ = 2450 + (ABL:Ｃ感覚 * 150)
	SOURCE:性行動 = 2500 + (ABL:Ｃ感覚 * 200)
ENDIF


;ABL:技巧をみる
IF ABL:PLAYER:技巧 == 0
	TIMES SOURCE:快Ｃ , 0.50
	TIMES SOURCE:性行動 , 0.50
ELSEIF ABL:PLAYER:技巧 == 1
	TIMES SOURCE:快Ｃ , 0.80
	TIMES SOURCE:性行動 , 0.80
ELSEIF ABL:PLAYER:技巧 == 2
	TIMES SOURCE:快Ｃ , 1.00
	TIMES SOURCE:性行動 , 1.00
ELSEIF ABL:PLAYER:技巧 == 3
	TIMES SOURCE:快Ｃ , 1.50
	TIMES SOURCE:性行動 , 1.50
ELSEIF ABL:PLAYER:技巧 == 4
	TIMES SOURCE:快Ｃ , 2.50
	TIMES SOURCE:性行動 , 2.50
ELSE
	SOURCE:快Ｃ = SOURCE:快Ｃ * (ABL:PLAYER:技巧 * 75 + 25) / 100
	SOURCE:性行動 = SOURCE:性行動 * (ABL:PLAYER:技巧 * 75 + 25) / 100
ENDIF

;ABL:サドっ気をみる
IF ABL:PLAYER:サドっ気 == 0
	TIMES SOURCE:快Ｃ , 1.00
	TIMES SOURCE:性行動 , 1.00
ELSEIF ABL:PLAYER:サドっ気 == 1
	TIMES SOURCE:快Ｃ , 1.05
	TIMES SOURCE:性行動 , 1.05
ELSEIF ABL:PLAYER:サドっ気 == 2
	TIMES SOURCE:快Ｃ , 1.10
	TIMES SOURCE:性行動 , 1.10
ELSEIF ABL:PLAYER:サドっ気 == 3
	TIMES SOURCE:快Ｃ , 1.20
	TIMES SOURCE:性行動 , 1.20
ELSEIF ABL:PLAYER:サドっ気 == 4
	TIMES SOURCE:快Ｃ , 1.30
	TIMES SOURCE:性行動 , 1.30
ELSE
	SOURCE:快Ｃ = SOURCE:快Ｃ * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
	SOURCE:性行動 = SOURCE:性行動 * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
ENDIF

;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:受動 += 300 + (ABL:親密 * 80)
	ELSEIF ABL:親密 <= 3
		SOURCE:受動 += 600 + (ABL:親密 * 130)
	ELSEIF ABL:親密 <= 5
		SOURCE:受動 += 800 + (ABL:親密 * 150)
	ELSEIF ABL:親密 <= 8
		SOURCE:受動 += 1000 + (ABL:親密 * 170)
	ELSEIF ABL:親密 <= 11
		SOURCE:受動 += 1200 + (ABL:親密 * 190)
	ELSE
		SOURCE:受動 += 1500 + (ABL:親密 * 210)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:征服 = 600
	ELSEIF ABL:サドっ気 == 1
		SOURCE:征服 = 900
	ELSEIF ABL:サドっ気 == 2
		SOURCE:征服 = 1200
	ELSEIF ABL:サドっ気 == 3
		SOURCE:征服 = 1600
	ELSEIF ABL:サドっ気 == 4
		SOURCE:征服 = 2000
	ELSE
		SOURCE:征服 = 1000 + (ABL:サドっ気 * 300)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
SIF TALENT:恋慕 || TALENT:恋人
	A += 5
SIF TALENT:親愛
	A += 5
;合意がないと好感度が下がる
IF TALENT:合意 == 0 && TFLAG:48 == 0
	A -= 10
	SOURCE:反感追加 += 800
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;異常経験増加
EXP:異常経験 += 3
PRINTFORML %EXPNAME:50%＋3

;既成事実2に相当
TFLAG:200 = 2

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK

;射精時の処理
IF E >= 1
	;射精先タイプの設定
	TFLAG:703 = 13
	;射精先選択の可・不可
	;TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

IF TALENT:半人半妖 || TALENT:ふたなり || TALENT:オトコ
	;奴隷のＰ⇔調教者のAの汚れが移動
	STAIN:ペニス |= STAIN:PLAYER:8
	STAIN:PLAYER:8 |= STAIN:ペニス
ENDIF

;将来オトコ持ちキャラが追加された時の為の保険
IF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +4
	EXP:ＢＬ経験 += 4
	EXP:PLAYER:ＢＬ経験 += 4
ENDIF

TFLAG:100 = 1

RETURN 1

;アナルに挿入させる
@TRAIN_MESSAGE_COM204
;前回と今回の調教者が同じで、前回の調教がアナルに挿入させるであり、抜かないフラグがある場合
IF TFLAG:60 && PREVCOM == 204 && ((ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0))
	PRINTFORMW %CALLNAME:TARGET%は、再び腰を動かし始めた…
;同様に前回の調教が正常位させるか後背位させるか騎乗位するの場合
ELSEIF TFLAG:60 && (PREVCOM == 193 || PREVCOM == 194 || PREVCOM == 195) && ((ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0))
	PRINTFORM %CALLNAME:TARGET%は、膣内からペニス\@ TALENT:半人半妖 || TALENT:ふたなり || TALENT:オトコ ? # バンド \@を抜き去ると%CALLNAME:PLAYER%のアナルに挿入して、再び腰を動かし始めた…
ELSE
	PRINTFORMW %CALLNAME:PLAYER%は%CALLNAME:TARGET%に自らのアナルに入れるように言った…
ENDIF
;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_204
A = 0
S = 0
V = 15

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:従順
IF ABL:親密
	A += ABL:親密 * 4
ENDIF

;ABL:欲望
IF ABL:欲望
	A += ABL:欲望 * 2
ENDIF

IF MARK:快楽依存
	A += MARK:快楽依存 * 2
ENDIF

;PALAM:欲情
IF PALAM:欲情 < PALAMLV:1
	L = 0
ELSEIF PALAM:欲情 < PALAMLV:2
	L = 1
ELSEIF PALAM:欲情 < PALAMLV:3
	L = 2
ELSEIF PALAM:欲情 < PALAMLV:4
	L = 3
ELSEIF PALAM:欲情 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 2
ENDIF

;快感に素直
IF TALENT:快感に素直
	A += 2
ENDIF
;快感の否定
IF TALENT:快感の否定
	A -= 2
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	A += 5
ENDIF

;調教者が処女
IF EXP:PLAYER:Ｖ経験 == 0
	A -= 15
ENDIF

;調教者が貞操観念
IF TALENT:PLAYER:貞操観念
	A -= 10
ENDIF

;調教者が貞操無頓着
IF TALENT:PLAYER:貞操観念
	A += 5
ENDIF

;調教者のABL:技巧
IF ABL:PLAYER:技巧
	A += ABL:PLAYER:技巧 * 4
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:従順
	IF ABL:親密
		SIF S
			PRINT  + 
		PRINTS ABLNAME:0
		PRINTV 'LV,ABL:親密,'(,ABL:親密 * 4,')
		S = 1
	ENDIF

	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 2,')
		S = 1
	ENDIF

	IF MARK:快楽依存
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:快楽依存,'(,MARK:快楽依存 * 2,')
		S = 1
	ENDIF

	;PALAM:欲情
	IF PALAM:欲情 < PALAMLV:1
		L = 0
	ELSEIF PALAM:欲情 < PALAMLV:2
		L = 1
	ELSEIF PALAM:欲情 < PALAMLV:3
		L = 2
	ELSEIF PALAM:欲情 < PALAMLV:4
		L = 3
	ELSEIF PALAM:欲情 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 2,')
		S = 1
	ENDIF

	;快感に素直
	IF TALENT:快感に素直
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:70
		PRINTV '(,2,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:快感の否定
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,2,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:恋慕 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 1
	ENDIF


	;調教者が貞操観念
	IF TALENT:PLAYER:貞操観念
		PRINT  - 
		PRINTFORM %NAME:PLAYER%が貞操観念
		PRINTV '(,10,')
		S = 1
	ENDIF

	;調教者が貞操無頓着
	IF TALENT:PLAYER:貞操観念
		PRINT  + 
		PRINTFORM %NAME:PLAYER%が貞操無頓着
		PRINTV '(,5,')
		S = 1
	ENDIF

	;調教者のABL:技巧
	IF ABL:PLAYER:技巧
		SIF S
			PRINT  + 
		PRINTFORM %NAME:PLAYER%の
		PRINTS ABLNAME:2
		PRINTV 'LV,ABL:PLAYER:技巧,'(,ABL:PLAYER:技巧 * 4,')
		S = 1
	ENDIF

	;合計を表示(15以上で実行)
	PRINT  = 
	PRINTV A


	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF
