@COM56
;助手を犯す

PRINTL 助手を犯す

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = 助手を犯す
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
SIF TALENT:MASTER:119 || TALENT:MASTER:121 || TALENT:MASTER:122
	BASE:PLAYER:2 += CALC_EJAC(SELECTCOM, MASTER)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:1 += 40
LOSEBASE:1 += 220

SOURCE:3 = 1500
SOURCE:14 = 800

;ABL:欲望をみる
IF ABL:欲望 == 0
	SOURCE:10 = 200
	SOURCE:13 = 3200
ELSEIF ABL:欲望 == 1
	SOURCE:10 = 400
	SOURCE:13 = 3800
ELSEIF ABL:欲望 == 2
	SOURCE:10 = 750
	SOURCE:13 = 4600
ELSEIF ABL:欲望 == 3
	SOURCE:10 = 1200
	SOURCE:13 = 5400
ELSEIF ABL:欲望 == 4
	SOURCE:10 = 1700
	SOURCE:13 = 6200
ELSE
	SOURCE:10 = 500 + (ABL:欲望 * 400)
	SOURCE:13 = 5000 + (ABL:欲望 * 400)
ENDIF


;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	TIMES SOURCE:3 , 0.10
	TIMES SOURCE:10 , 0.50
ELSEIF ABL:奉仕精神 == 1
	TIMES SOURCE:3 , 0.40
	TIMES SOURCE:10 , 0.80
ELSEIF ABL:奉仕精神 == 2
	TIMES SOURCE:3 , 0.70
	TIMES SOURCE:10 , 1.00
ELSEIF ABL:奉仕精神 == 3
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:10 , 1.50
ELSEIF ABL:奉仕精神 == 4
	TIMES SOURCE:3 , 1.60
	TIMES SOURCE:10 , 2.00
ELSE
	SOURCE:3 = SOURCE:3 * (ABL:奉仕精神 * 20 + 100) / 100
	SOURCE:10 = SOURCE:10 * (ABL:奉仕精神 * 25 + 125) / 100
ENDIF

;助手が処女
IF TALENT:ASSI:0 == 1
	TIMES SOURCE:13 , 20.00
	TIMES SOURCE:14 , 3.00
	TALENT:ASSI:0 = 0

	PRINTL ＜助手が処女＞

	EXP:50 += 3
	PRINTFORML %EXPNAME:50%＋３

	IF ABL:親密 < 2
		PRINTFORML そして%ABLNAME:0%が2になった
		ABL:親密 = 2
	ENDIF
ENDIF
EXP:ASSI:0 += 1


;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:21 += 300 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:21 += 600 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:21 += 800 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:21 += 1000 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:21 += 1200 + (ABL:親密 * 160)
	ELSE
		SOURCE:21 += 1500 + (ABL:親密 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:20 = 800
	ELSEIF ABL:サドっ気 == 1
		SOURCE:20 = 1100
	ELSEIF ABL:サドっ気 == 2
		SOURCE:20 = 1400
	ELSEIF ABL:サドっ気 == 3
		SOURCE:20 = 1800
	ELSEIF ABL:サドっ気 == 4
		SOURCE:20 = 2300
	ELSE
		SOURCE:20 = 1000 + (ABL:サドっ気 * 350)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
;合意がないと好感度が下がる
IF TALENT:152 == 0 && TFLAG:48 == 0
	A -= 9
	SOURCE:15 += 400
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK

;汎用処理に移行
;S = BASE:MASTER:2
;EJAC = MAXBASE:MASTER:2

;IF  S > EJAC * 2
;	E = 2
;ELSEIF S > EJAC
;	E = 1
;ELSE
;	E = 0
;ENDIF

IF E
;射精している
	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		TIMES SOURCE:13, 1.00
	ELSEIF ABL:精液中毒 == 1
		TIMES SOURCE:13, 1.20
	ELSEIF ABL:精液中毒 == 2
		TIMES SOURCE:13, 1.50
	ELSEIF ABL:精液中毒 == 3
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:精液中毒 == 4
		TIMES SOURCE:13, 2.50
	ELSE
		SOURCE:13 = SOURCE:13 * (ABL:精液中毒 * 25 + 175) / 100
	ENDIF

	;射精先タイプの設定(助手)
	TFLAG:703 = 6
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;汎用処理に移行
;大量射精
;IF E == 2
;	TIMES SOURCE:10 , 1.50
;
;	EXP:MASTER:3 += 2
;	EXP:20 += 1
;	PRINTL 大量射精
;	PRINTL 精液経験＋１
	;精液汚れ
;	STAIN:MASTER:2 |= 4

;	BASE:MASTER:2 -= EJAC*2
;	SIF BASE:MASTER:2 >= EJAC
;		BASE:MASTER:2 = EJAC-1

	;助手を犯すで射精したフラグ
;	TFLAG:7 = 2
;通常の射精
;ELSEIF E == 1
;	EXP:MASTER:3 += 1
;	PRINTL 射精
	;精液汚れ
;	STAIN:MASTER:2 |= 4

;	BASE:MASTER:2 -= EJAC
;	SIF BASE:MASTER:2 >= EJAC
;		BASE:MASTER:2 = EJAC-1

	;助手を犯すで射精したフラグ
;	TFLAG:7 = 1
;ENDIF

IF TALENT:MASTER:119 || TALENT:MASTER:122 || TALENT:MASTER:121
	;主人のＰ⇔助手のＶの汚れが移動
	STAIN:MASTER:2 |= STAIN:ASSI:3
	STAIN:ASSI:3 |= STAIN:MASTER:2
ENDIF

IF TALENT:MASTER:122 == 0 && TALENT:ASSI:122 == 0
	PRINTS EXPNAME:40
	PRINT +10
	PRINTFORML (%CALLNAME:ASSI%) 
	EXP:ASSI:40 += 10
ELSEIF TALENT:MASTER:122 == 1 && TALENT:ASSI:122 == 1
	PRINTS EXPNAME:41
	PRINT +10
	PRINTFORML (%CALLNAME:ASSI%) 
	EXP:ASSI:41 += 10
ENDIF

TFLAG:100 = 1

;既成事実２に相当
TFLAG:200 = 2

RETURN 1

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_56
A = 0
S = 0
V = 140

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	A += ABL:欲望 * 2
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	A += ABL:奉仕精神 * 4
ENDIF

IF MARK:1
	A += MARK:1 * 2
ENDIF


;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 2
ENDIF

;恥じらい
IF TALENT:35
	A -= 1
ENDIF
;献身的
IF TALENT:63
	A += 6
ENDIF

;恋慕
IF TALENT:85 && ASSIPLAY == 0
	A += 5
ENDIF

;助手が処女
IF EXP:ASSI:0 == 0
	A -= 15
ENDIF


;好感度
IF CFLAG:2 < 2500
	B = 0
	IF CFLAG:2 < 1000
		B = 50
	ELSEIF CFLAG:2 < 1500
		B = 20
	ELSEIF CFLAG:2 < 2000
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 3000
	B = 0
	IF CFLAG:2 < 4000
		B = 5
	ELSE
		B = CFLAG:2 / 250
	ENDIF
	A += B
ENDIF

;合意
IF TALENT:152 == 0
	A -= 20
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 2,')
		S = 1
	ENDIF
	;ABL:奉仕精神
	IF ABL:奉仕精神
		SIF S
			PRINT  + 
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
		S = 1
	ENDIF

	IF MARK:1
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:1,'(,MARK:1 * 2,')
		S = 1
	ENDIF


	;PALAM:欲情
	IF PALAM:5 < PALAMLV:1
		L = 0
	ELSEIF PALAM:5 < PALAMLV:2
		L = 1
	ELSEIF PALAM:5 < PALAMLV:3
		L = 2
	ELSEIF PALAM:5 < PALAMLV:4
		L = 3
	ELSEIF PALAM:5 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 2,')
		S = 1
	ENDIF

	;恥じらい
	IF TALENT:35
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,1,')
		S = 1
	ENDIF
	;献身的
	IF TALENT:63
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:63
		PRINTV '(,6,')
		S = 1
	ENDIF

	;恋慕
	IF TALENT:85 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 1
	ENDIF

	;助手が処女
	IF EXP:ASSI:0 == 0
		PRINT  - 
		PRINT 助手が処女
		PRINTV '(,15,')
		S = 1
	ENDIF


	;好感度
	IF CFLAG:2 < 2500
		B = 0
		PRINT  - 
		IF CFLAG:2 < 1000
			B = 50
		ELSEIF CFLAG:2 < 1500
			B = 20
		ELSEIF CFLAG:2 < 2000
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 >= 3000
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 4000
			B = 5
		ELSE
			B = CFLAG:2 / 250
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合意
	IF TALENT:152 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,20,')
		S = 1
	ENDIF

	;合計を表示(140以上で実行)
	PRINT  = 
	PRINTV A

	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF
