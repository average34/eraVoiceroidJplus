@COM47
;空気浣腸

PRINTL 空気浣腸
STR:0 = 空気浣腸

LOSEBASE:体力 += 200
LOSEBASE:気力 += 300

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:快Ａ = 0
	SOURCE:屈従 = 300
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:快Ａ = 10
	SOURCE:屈従 = 800
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:快Ａ = 450
	SOURCE:屈従 = 1400
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:快Ａ = 80
	SOURCE:屈従 = 1800
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:快Ａ = 120
	SOURCE:屈従 = 2100
ELSE
	SOURCE:快Ａ = 60 + (ABL:Ａ感覚 * 20)
	SOURCE:屈従 = 1650 + (ABL:Ａ感覚 * 150)
ENDIF

;ABL8,マゾっ気をみる
IF ABL:マゾっ気 < 1
	SOURCE:痛み = 2000
	SOURCE:不潔 = 8000
	SOURCE:屈従 += 200
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 2000
	UP:恐怖 += 8000
ELSEIF ABL:マゾっ気 < 2
	SOURCE:痛み = 2000
	SOURCE:不潔 = 6000
	SOURCE:屈従 += 500
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 1000
	UP:恐怖 += 5000
ELSEIF ABL:マゾっ気 < 3
	SOURCE:痛み = 2000
	SOURCE:不潔 = 5000
	SOURCE:屈従 += 800
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 500
	UP:恐怖 += 3000
ELSEIF ABL:マゾっ気 < 4
	SOURCE:痛み = 1800
	SOURCE:不潔 = 4000
	SOURCE:屈従 += 1200
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 100
	UP:恐怖 += 1200
ELSEIF ABL:マゾっ気 < 5
	SOURCE:痛み = 1800
	SOURCE:不潔 = 3000
	SOURCE:屈従 += 1500
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 0
	UP:恐怖 += 500
ELSE
	SOURCE:痛み = 1800
	IF ABL:マゾっ気 <= 10
		SOURCE:不潔 = 3000 - (ABL:マゾっ気 * 200)
	ELSE
		SOURCE:不潔 = 1000
	ENDIF
	SOURCE:屈従 += 750 + (ABL:マゾっ気 * 250)
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 0
	UP:恐怖 += 200
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.40
	SOURCE:痛み += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.80
	SOURCE:痛み += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
	SOURCE:痛み += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.40
	SOURCE:痛み += 120
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:快Ａ , 1.80
	SOURCE:痛み += 100
ENDIF

;ABL:従順をみる
IF ABL:親密 == 0
	TIMES SOURCE:快Ａ , 0.80
ELSEIF ABL:親密 == 1
	TIMES SOURCE:快Ａ , 0.90
ELSEIF ABL:親密 == 2
	TIMES SOURCE:快Ａ , 1.00
ELSEIF ABL:親密 == 3
	TIMES SOURCE:快Ａ , 1.10
ELSEIF ABL:親密 == 4
	TIMES SOURCE:快Ａ , 1.20
ELSE
	SOURCE:快Ａ = SOURCE:快Ａ * (ABL:親密 * 5 + 105) / 100
ENDIF

;ABL:サドっ気をみる
IF ABL:PLAYER:サドっ気 == 0
	TIMES SOURCE:痛み , 1.00
ELSEIF ABL:PLAYER:サドっ気 == 1
	TIMES SOURCE:痛み , 1.05
ELSEIF ABL:PLAYER:サドっ気 == 2
	TIMES SOURCE:痛み , 1.10
ELSEIF ABL:PLAYER:サドっ気 == 3
	TIMES SOURCE:痛み , 1.20
ELSEIF ABL:PLAYER:サドっ気 == 4
	TIMES SOURCE:痛み , 1.30
ELSE
	SOURCE:痛み = SOURCE:痛み * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
ENDIF

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES SOURCE:痛み , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES SOURCE:痛み , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

IF TEQUIP:47 == 0
SOURCE:痛み += 1500
SOURCE:屈従 += 2500
ELSEIF TEQUIP:47 == 1
UP:屈服 += 1000
UP:恥情 += 2000
ENDIF



EXP:Ａ経験 += 1
PRINTL Ａ経験＋１

EXP:異常経験 += 1
PRINTFORML %EXPNAME:50%＋１

CALL TRAIN_MESSAGE_B

;浣腸プラグの着脱
IF TEQUIP:47
	TEQUIP:47 = 0
ELSE
	TEQUIP:47 = 1
ENDIF

RETURN 1

@TRAIN_MESSAGE_COM47
;浣腸プラグの着脱
IF TEQUIP:47
	PRINTFORML 栓を抜くと、%CALLNAME:TARGET%の菊穴からけたたましい放屁の音が響いた…
ELSE
	PRINTFORML %CALLNAME:TARGET%の菊穴に、腹が膨れるくらいポンプで空気を送り込み、栓をした
ENDIF

@EQUIP_COM47
;空気浣腸＋アナルプラグ

PRINTL ＜空気浣腸＋プラグ挿入中＞

LOSEBASE:体力 += 180
LOSEBASE:気力 += 300

SOURCE:痛み += 2000
SOURCE:屈従 += 3000

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	A = 0
	B = 300
ELSEIF ABL:Ａ感覚 == 1
	A = 10
	B = 800
ELSEIF ABL:Ａ感覚 == 2
	A = 30
	B = 1400
ELSEIF ABL:Ａ感覚 == 3
	A = 80
	B = 1800
ELSEIF ABL:Ａ感覚 == 4
	A = 120
	B = 2100
ELSE
	A = 60 + (ABL:Ａ感覚 * 20)
	B = 1650 + (ABL:Ａ感覚 * 150)
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES A , 0.50
	C = 2000
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES A , 1.00
	C = 300
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES A , 1.10
	C = 50
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES A , 1.20
	C = 10
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES A , 1.40
	C = 0
ELSE
	TIMES A , 1.60
	C = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES A , 0.40
	C += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES A , 0.80
	C += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES A , 1.00
	C += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES A , 1.40
	C += 120
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES A , 1.80
	C += 100
ENDIF

;ABL:従順をみる
IF ABL:親密 == 0
	TIMES A , 0.80
ELSEIF ABL:親密 == 1
	TIMES A , 0.90
ELSEIF ABL:親密 == 2
	TIMES A , 1.00
ELSEIF ABL:親密 == 3
	TIMES A , 1.10
ELSEIF ABL:親密 == 4
	TIMES A , 1.20
ELSE
	A = A * (ABL:親密 * 5 + 105) / 100
ENDIF

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES B , 1.50
	TIMES C , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES B , 0.60
	TIMES C , 0.60
ENDIF

SOURCE:逸脱 += B

;処女で貞操観念
IF EXP:Ｖ経験 == 0 && TALENT:貞操観念
	B /= 3
ENDIF

SOURCE:快Ａ += A
SOURCE:屈従 += B
SOURCE:痛み += C

EXP:Ａ経験 += 1
PRINTL Ａ経験＋１

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:レズ経験 += 1
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:ＢＬ経験 += 1
ENDIF

EXP:異常経験 += 1
PRINTFORML %EXPNAME:50%＋１

RETURN 1
