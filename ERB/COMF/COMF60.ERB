@COM60
;助手にキス

PRINTL 助手にキスさせる

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = 助手にキス
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック（ふたなり・オトコの場合）
;-------------------------------------------------
SIF TALENT:PLAYER:119 || TALENT:PLAYER:122  || TALENT:PLAYER:121
	BASE:PLAYER:射精 += CALC_EJAC(SELECTCOM, PLAYER)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------

;キス経験で気力消費軽減（何度もキスしたがる、の再現だとでも脳内補完）
IF EXP:キス経験 < 100
	LOSEBASE:気力 += 50
ELSEIF EXP:キス経験 < 500
	LOSEBASE:気力 += 40
ELSE
	LOSEBASE:気力 += 25
ENDIF

;Ｍ感覚を設定している場合快Ｍソース等取得
SIF FLAG:62 & 4194304
	CALL SOURCE_M_COM60

;上のほうで計算した汚れデータ
SOURCE:8 = Y*20 + 10

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:4 = 50
	SOURCE:5 = 0
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:奉仕精神 == 1
	SOURCE:4 = 150
	SOURCE:5 = 50
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:奉仕精神 == 2
	SOURCE:4 = 200
	SOURCE:5 = 100
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:奉仕精神 == 3
	SOURCE:4 = 250
	SOURCE:5 = 180
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:奉仕精神 == 4
	SOURCE:4 = 300
	SOURCE:5 = 300
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:4 = 225 + (ABL:奉仕精神 * 25)
	SOURCE:5 = ABL:奉仕精神 * 100
	TIMES SOURCE:8 , 0.10
ENDIF


;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:4 , 2.50
	TIMES SOURCE:5 , 2.50
ELSE
	SOURCE:4 = SOURCE:4 * (ABL:技巧 * 75 + 25) / 100
	SOURCE:5 = SOURCE:5 * (ABL:技巧 * 75 + 25) / 100
ENDIF


PRINTFORML %EXPNAME:96%＋１
EXP:キス経験 += 1
EXP:PLAYER:キス経験 += 1
IF TALENT:PLAYER:151
	PRINTFORMW %CALLNAME:PLAYER%は【キス未経験】を失いました
	TALENT:PLAYER:151 = 0
ENDIF

IF TALENT:151
	TFLAG:600 = 1
	CALL KOJO_JUN
	TALENT:151 = 0
	PRINTFORMW %CALLNAME:TARGET%は【キス未経験】を失いました
	TFLAG:38 = 1
ENDIF

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK
;;オーバーフロー対策に、値が大きすぎる場合と、負の場合は調整を加える
;IF BASE:PLAYER:射精 > 100000000 || BASE:PLAYER:射精 < 0
;	BASE:PLAYER:射精 = 100000000
;ENDIF
;S = BASE:PLAYER:射精
;EJAC = MAXBASE:PLAYER:射精
;
;IF  S > EJAC * 2
;	E = 2
;ELSEIF S > EJAC
;	E = 1
;ELSE
;	E = 0
;ENDIF

IF E
;射精している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:精液中毒 == 1
		SOURCE:7 = 100
		TIMES SOURCE:5 , 2.50
		TIMES SOURCE:13, 1.60
	ELSEIF ABL:精液中毒 == 2
		SOURCE:7 = 300
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 1.00
	ELSEIF ABL:精液中毒 == 3
		SOURCE:7 = 700
		TIMES SOURCE:5 , 4.00
		TIMES SOURCE:13, 0.70
	ELSEIF ABL:精液中毒 == 4
		SOURCE:7 = 1500
		TIMES SOURCE:5 , 5.00
		TIMES SOURCE:13, 0.40
	ELSE
		SOURCE:7 = (ABL:精液中毒 * 2000) - 4000
		SOURCE:5 = SOURCE:5 * (ABL:精液中毒 * 50 + 350) / 100
		TIMES SOURCE:13, 0.10
	ENDIF

	;射精先タイプの設定
	TFLAG:703 = 11
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;;大量射精
;IF E == 2
;	TIMES SOURCE:7 , 2.00
;	TIMES SOURCE:5 , 1.50
;
;	EXP:PLAYER:射精経験 += 2
;	EXP:精液経験 += 1
;	PRINTL 大量射精
;	PRINTL 精液経験＋１
;	;精液汚れ
;	STAIN:PLAYER:2 |= 4
;
;	BASE:PLAYER:射精 -= EJAC*2
;	SIF BASE:PLAYER:射精 >= EJAC
;		BASE:PLAYER:射精 = EJAC-1
;
;	;キスで射精させたフラグ
;	TFLAG:4 = 2
;;通常の射精
;ELSEIF E == 1
;	EXP:PLAYER:射精経験 += 1
;	PRINTL 射精
;	;精液汚れ
;	STAIN:PLAYER:2 |= 4
;
;	BASE:PLAYER:射精 -= EJAC
;	SIF BASE:PLAYER:射精 >= EJAC
;		BASE:PLAYER:射精 = EJAC-1
;
;	;キスで射精させたフラグ
;	TFLAG:4 = 1
;ENDIF

;奴隷の口⇔調教者の口の汚れが移動
STAIN:0 |= STAIN:PLAYER:0
STAIN:PLAYER:0 |= STAIN:0

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +3
	EXP:レズ経験 += 3
	EXP:PLAYER:レズ経験 += 3
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +3
	EXP:ＢＬ経験 += 3
	EXP:PLAYER:ＢＬ経験 += 3
ENDIF

TFLAG:100 = 1

;既成事実１に相当
TFLAG:200 = 1

RETURN 1

;Ｍ感覚を設定している場合快Ｍソース等取得
@SOURCE_M_COM60
;屈従・反感ソース
SOURCE:13 = 100
SOURCE:14 = 10

;ABL:Ｍ感覚をみる
IF ABL:Ｍ感覚 == 0
	SOURCE:24 = 10
ELSEIF ABL:Ｍ感覚 == 1
	SOURCE:24 = 50
ELSEIF ABL:Ｍ感覚 == 2
	SOURCE:24 = 200
ELSEIF ABL:Ｍ感覚 == 3
	SOURCE:24 = 600
ELSEIF ABL:Ｍ感覚 == 4
	SOURCE:24 = 1000
ELSE
	SOURCE:24 = 400 + (ABL:Ｍ感覚 * 200)
ENDIF

;PLAYERの技巧を見る
SIF ABL:PLAYER:技巧 >= 1
	SOURCE:24 += SOURCE:24 * (ABL:PLAYER:技巧 * 10) / 250

;EXP:キス経験をみる
IF EXP:キス経験 < EXPLV:1
	TIMES SOURCE:24 , 1.00
ELSEIF EXP:キス経験 < EXPLV:2
	TIMES SOURCE:14 , 0.95
	TIMES SOURCE:24 , 1.05
ELSEIF EXP:キス経験 < EXPLV:3
	TIMES SOURCE:14 , 0.90
	TIMES SOURCE:24 , 1.10
ELSEIF EXP:キス経験 < EXPLV:4
	TIMES SOURCE:14 , 0.85
	TIMES SOURCE:24 , 1.15
ELSEIF EXP:キス経験 < EXPLV:5
	TIMES SOURCE:14 , 0.80
	TIMES SOURCE:24 , 1.20
ELSE
	TIMES SOURCE:14 , 0.75
	TIMES SOURCE:24 , 1.20
ENDIF

IF MARK:3 == 1
	TIMES SOURCE:24 , 0.50
	TIMES SOURCE:13 , 1.15
ELSEIF MARK:3 == 2
	TIMES SOURCE:24 , 0.25
	TIMES SOURCE:13 , 1.45
ELSEIF MARK:3 == 3
	TIMES SOURCE:24 , 0.10
	TIMES SOURCE:13 , 1.90
ENDIF

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_60
A = 0
S = 0
V = 55
;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	A += ABL:欲望 * 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	A += ABL:奉仕精神 * 4
ENDIF

IF MARK:1
	A += MARK:1 * 2
ENDIF


;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 1
ENDIF

;恥じらい
IF TALENT:35
	A -= 1
ENDIF
;汚臭鈍感
IF TALENT:61
	A += 1
ENDIF
;汚臭敏感
IF TALENT:62
	A -= 1
ENDIF
;献身的
IF TALENT:63
	A += 6
ENDIF
;快感の否定
IF TALENT:71
	A -= 1
ENDIF
;恋慕
IF TALENT:85 && ASSIPLAY == 0
	A += 5
	S = 5
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:0 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:0 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:0 & 8
	Y += 7
SIF STAIN:PLAYER:0 & 16
	Y += 15

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2

;キスでは汚れがそれほど影響しない
Y /= 2

;汚れあり
IF Y
	A -= Y
	;汚臭鈍感
	IF TALENT:61
	ELSEIF TALENT:62
	ELSE
	ENDIF
ENDIF


;キス未経験
IF TALENT:151
	A -= 30
ENDIF

;一線越えない（キス未経験時のみ）
IF TALENT:151 && TALENT:27
	A -= 15
ENDIF


;合意
IF TALENT:152 == 0
	A -= 10
ENDIF

;デート中
IF TEQUIP:2
	A += 3
ENDIF

;ムードボーナス
B = 0
IF TFLAG:37 < 10
	B -= 10
ELSEIF TFLAG:37 < 30
	B -= 1
ELSEIF TFLAG:37 < 50
	B += 1
ELSEIF TFLAG:37 < 100
	B += TFLAG:37 / 10
ELSEIF TFLAG:37 < 300
	B += TFLAG:37 / 5
ELSE
	B += TFLAG:37 / 4
ENDIF
A += B
IF B < 0
	B = B * -1
ELSE
ENDIF


;好感度
IF CFLAG:2 < 500
	B = 0
	IF CFLAG:2 < 100
		B = 20
	ELSEIF CFLAG:2 < 300
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF

;好感度
IF CFLAG:2 >= 700
	B = 0
	B = CFLAG:2 / 80
	A += B
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
		S = 1
	ENDIF
	;ABL:奉仕精神
	IF ABL:奉仕精神
		SIF S
			PRINT  + 
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
		S = 1
	ENDIF

	IF MARK:1
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:1,'(,MARK:1 * 2,')
		S = 1
	ENDIF


	;PALAM:欲情
	IF PALAM:5 < PALAMLV:1
		L = 0
	ELSEIF PALAM:5 < PALAMLV:2
		L = 1
	ELSEIF PALAM:5 < PALAMLV:3
		L = 2
	ELSEIF PALAM:5 < PALAMLV:4
		L = 3
	ELSEIF PALAM:5 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 1,')
		S = 1
	ENDIF

	;恥じらい
	IF TALENT:35
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,1,')
		S = 1
	ENDIF
	;汚臭鈍感
	IF TALENT:61
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:61
		PRINTV '(,1,')
		S = 1
	ENDIF
	;汚臭敏感
	IF TALENT:62
		PRINT  - 
		PRINTS TALENTNAME:62
		PRINTV '(,1,')
		S = 1
	ENDIF
	;献身的
	IF TALENT:63
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:63
		PRINTV '(,6,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:71
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,1,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:85 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 5
	ENDIF

	Y = 0
	;愛液の汚れ
	SIF STAIN:PLAYER:0 & 1
		Y += 1
	;精液の汚れ
	SIF STAIN:PLAYER:0 & 4
		Y += 3
	;アナルの汚れ
	SIF STAIN:PLAYER:0 & 8
		Y += 7
	SIF STAIN:PLAYER:0 & 16
		Y += 15

	SIF TALENT:61
		Y /= 3
	SIF TALENT:62
		Y *= 2

	;キスでは汚れがそれほど影響しない
	Y /= 2

	;汚れあり
	IF Y
		PRINT  - 
		;汚臭鈍感
		IF TALENT:61
			PRINT 汚れあり、
			PRINTS TALENTNAME:61
		ELSEIF TALENT:62
			PRINT 汚れあり、
			PRINTS TALENTNAME:62
		ELSE
			PRINT 汚れあり
		ENDIF
		PRINTV '(,Y,')
		S = 1
	ENDIF


	;キス未経験
	IF TALENT:151
		PRINT  - 
		PRINTS TALENTNAME:151
		PRINTV '(,30,')
		S = 1
	ENDIF

	;一線越えない（キス未経験時のみ）
	IF TALENT:151 && TALENT:27
		PRINT  - 
		PRINTS TALENTNAME:27
		PRINTV '(,15,')
		S = 1
	ENDIF


	;合意
	IF TALENT:152 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,10,')
		S = 1
	ENDIF

	;デート中
	IF TEQUIP:2
		PRINT  + 
		PRINT デート中
		PRINTV '(,5,')
		S = 1
	ENDIF

	;ムードボーナス
	B = 0
	IF TFLAG:37 < 10
		B -= 10
	ELSEIF TFLAG:37 < 30
		B -= 1
	ELSEIF TFLAG:37 < 50
		B += 1
	ELSEIF TFLAG:37 < 100
		B += TFLAG:37 / 10
	ELSEIF TFLAG:37 < 300
		B += TFLAG:37 / 5
	ELSE
		B += TFLAG:37 / 4
	ENDIF
	IF B < 0
		B = B * -1
		PRINT  - 
		PRINT ムード悪い
		PRINTV '(,B,')
		S = 1
	ELSE
		PRINT  + 
		PRINT ムード良い
		PRINTV '(,B,')
		S = 1
	ENDIF


	;好感度
	IF CFLAG:2 < 500
		B = 0
		PRINT  - 
		IF CFLAG:2 < 100
			B = 20
		ELSEIF CFLAG:2 < 300
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF

	;好感度
	IF CFLAG:2 >= 700
		B = 0
		SIF S
			PRINT  + 
		B = CFLAG:2 / 80
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合計を表示(55以上で実行)
	PRINT  = 
	PRINTV A

	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF

