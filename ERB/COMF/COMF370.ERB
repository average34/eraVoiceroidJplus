; 地の文
;------------------------------
@TRAIN_MESSAGE_COM370
;主導権がパートナー
IF TFLAG:45
	PRINTFORM %CALLNAME:TARGET%は
	IF TEQUIP:2
		IF (TALENT:目立ちたがり && TALENT:解放 && TALENT:恥薄い && TALENT:淫核 && TALENT:淫乱 && TALENT:淫乳) == 0
			PRINT 周囲を気にしつつも
		ELSEIF (TALENT:臆病 == 0 && TALENT:恥じらい == 0 && (TALENT:目立ちたがり || TALENT:解放) && TALENT:恋慕) && CFLAG:2 > 3500 && ABL:欲望 >= 8
			PRINT 人目もはばからず
		ENDIF
	ENDIF
	SIF TALENT:恋慕 == 0
		PRINT 遠慮がちな様子で
	PRINTFORM %CALLNAME:PLAYER%
	IF TALENT:恋慕
		;温泉以外のデートスポット且つ幼児退行or幼稚
		IF TEQUIP:2 != 7 && (TALENT:幼児 || TALENT:幼稚)
			PRINT の袖を引っ張り、
		ELSE
			PRINT の背中に手を廻すと、
		ENDIF
		;好感度3500より大きい
		IF CFLAG:2 > 3500
			;欲望Lv12以上+いつもの+欲情100000以上
			IF ABL:欲望 >= 12 && TFLAG:17 == 2 && PALAM:欲情 > PALAMLV:7
				PRINT 蕩けきった表情で
			;欲望Lv10以上、淫乱、(サドor小悪魔)+いつもの
			ELSEIF ABL:欲望 >= 10 && TALENT:淫乱 && (TALENT:サド || TALENT:小悪魔) && TFLAG:17 == 2
				PRINT 妖しい笑みを浮かべながら
			;(臆病or恥じらい)、欲望Lv8以上、デート中
			ELSEIF (TALENT:臆病 || TALENT:恥じらい) && ABL:欲望 >= 8 && TEQUIP:2
				PRINT 羞恥に頬を染めながらも
			ENDIF
		ELSEIF TFLAG:17 == 1
			PRINT 優しく微笑みながら
		ENDIF
		PRINTL 
		;欲望Lv12以上+いつもの
		IF ABL:欲望 >= 12 && TFLAG:17 == 2
			;淫核or淫乳
			IF TALENT:淫核 && TALENT:淫乳
				PRINT 身をよじりながら絡み付くように
			;淫乳
			ELSEIF TALENT:淫乳
				;貧乳
				IF TALENT:貧乳
					PRINT 控えめな
				;巨乳
				ELSEIF TALENT:巨乳
					PRINT 豊かな
				ELSE
					PRINT 形の良い
				ENDIF
				PRINT 胸をぐいぐいと押し付けるようにして
			;淫核
			ELSEIF TALENT:淫核
				PRINT 下半身を擦り付けるように
			ELSE
				PRINT 身体を密着させるようにして
			ENDIF
		ELSE
			IF CFLAG:2 >= 3500
				;楽観的
				IF TALENT:楽観的
					PRINT 嬉しそうに
				;恥じらい
				ELSEIF TALENT:恥じらい
					PRINT 顔を埋めるようにして
				;臆病
				ELSEIF TALENT:臆病
					PRINT おずおずと
				;幼児退行or幼稚
				ELSEIF TALENT:幼児 || TALENT:幼稚
					PRINT しがみつくように
				ELSE
					PRINT ぎゅっと
				ENDIF
			ELSE
				PRINT 軽く
			ENDIF
		ENDIF
	ELSE
		PRINT に
	ENDIF
	PRINTW 抱きついてきた…
ELSE
	PRINTFORM %CALLNAME:PLAYER%は
	SIF TALENT:恋慕 == 0
		PRINT 戸惑い気味な
	PRINTFORM %CALLNAME:TARGET%の腰に腕を廻して
	SIF ABL:Ｃ感覚 < 3 && ABL:Ｂ感覚 < 3
		PRINT 優しく
	IF TALENT:恋慕 == 0
		PRINTW 抱き寄せた…
	ELSE
		PRINTL 抱き寄せると
		SIF (TALENT:目立ちたがり && TALENT:解放 && TALENT:恥薄い && TALENT:淫核 && TALENT:淫乱 && TALENT:淫乳) == 0 && TEQUIP:2 != 0
			PRINT 周囲を気にしつつ、
		PRINTFORM %CALLNAME:TARGET%も
		IF TFLAG:17 == 2
			IF TALENT:淫核 && TALENT:淫乳
				PRINT 身をよじりながら、絡み付くように
			ELSEIF TALENT:淫核
				PRINT 下半身を擦り付けるように
			ELSEIF TALENT:淫乳
				IF TALENT:貧乳
					PRINT 控えめな
				ELSEIF TALENT:巨乳
					PRINT 豊かな
				ELSE
					PRINT 形の良い
				ENDIF
				PRINT 胸をぐいぐいと押し付けるようにして
			ELSE
				PRINT 身体を密着させるようにして
			ENDIF
		ELSE
			IF TALENT:楽観的 && CFLAG:2 > 3500
				PRINT 嬉しそうに
			ELSEIF TALENT:恥じらい && CFLAG:2 > 3500
				PRINT 顔を埋めるようにして
			ELSEIF TALENT:臆病
				PRINT おずおずと
			ELSEIF TALENT:幼児 && CFLAG:2 > 3500
				PRINT しがみつくように
			ELSEIF CFLAG:2 < 3500
				PRINT ぎゅっと
			ELSE
				PRINT 軽く
			ENDIF
		ENDIF
		PRINTW 抱き返してきた…
	ENDIF
ENDIF

