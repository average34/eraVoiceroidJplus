	ELSE
		J:1 += 1 + J
	ENDIF
	J = 0
ELSE
	J = 0
ENDIF

;レズ、BL経験
IF J > 0
	EXP:40 += J
	EXP:PLAYER:40 += J
	PRINTFORML %EXPNAME:40%+{J}
ELSEIF J:1 > 0
	EXP:41 += J:1
	EXP:PLAYER:41 += J:1
	PRINTFORML %EXPNAME:41%+{J:1}
ENDIF

;好感度ランダム変化
A = RAND:6 - 1
TFLAG:30 += A

;成功度合いボーナス
IF TFLAG:18 == -1
	TIMES SOURCE:18 , 0.10
	TIMES SOURCE:9 , 0.50
	TIMES SOURCE:20 , 0.50
	TIMES SOURCE:21 , 0.50
	TFLAG:30 -= 10
	TFLAG:37 -= 5
ELSEIF TFLAG:18 == 0
	TIMES SOURCE:18 , 1.00
	TIMES SOURCE:9 , 1.00
	TIMES SOURCE:20 , 1.00
	TIMES SOURCE:21 , 1.00
