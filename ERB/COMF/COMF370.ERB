	ELSE
		;精液汚れ←温泉時は汚れリセットされるので温泉以外のみ実行
		STAIN:PLAYER:2 |= 4
	ENDIF
	PRINTL 

	BASE:PLAYER:2 -= EJAC * E
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC - 1
ENDIF

J = 0
J:1 = 0
