	TFLAG:37 += 5 + A
	;愛情経験追加
	IF TALENT:85 || TALENT:153
		EXP:23 += 6
		PRINTFORML %EXPNAME:23%+6
	ELSE
		EXP:23 += 2
		PRINTFORML %EXPNAME:23%+2
	ENDIF
ENDIF

J = 0
IF TFLAG:17 == 2
	IF (ABL:Ｃ感覚 + ABL:Ｂ感覚) > 2
		J = (ABL:Ｃ感覚 + ABL:Ｂ感覚) - 2
		;淫乳
		SIF TALENT:75
			J += 1
		;淫核
		SIF TALENT:78
			J += 1
		PRINTFORML %EXPNAME:21%+{J}
