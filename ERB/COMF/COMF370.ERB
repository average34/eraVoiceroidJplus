	SOURCE:18 += ((ABL:親密 * 20) / 3)
	SOURCE:9 += (ABL:親密 * 90)
ELSEIF ABL:親密 <= 3
	SOURCE:18 += 100 + ((ABL:親密 * 20) / 3)
	SOURCE:9 += 200 + (ABL:親密 * 95)
ELSEIF ABL:親密 <= 5
	SOURCE:18 += 200 + ((ABL:親密 * 20) / 3)
	SOURCE:9 += 500 + (ABL:親密 * 100)
ELSEIF ABL:親密 <= 8
	SOURCE:18 += 300 + ((ABL:親密 * 30) / 3)
	SOURCE:9 += 700 + (ABL:親密 * 110)
ELSEIF ABL:親密 <= 11
	SOURCE:18 += 400 + ((ABL:親密 * 30) / 3)
	SOURCE:9 += 1200 + (ABL:親密 * 125)
