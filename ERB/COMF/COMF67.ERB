@COM67
;足コキ

PRINTL 足コキ

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = 足コキ
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 30
LOSEBASE:気力 += 150

SOURCE:14 = 100

;ABL:Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	SOURCE:0 = 40
	SOURCE:13 = 40
ELSEIF ABL:Ｃ感覚 == 1
	SOURCE:0 = 160
	SOURCE:13 = 160
ELSEIF ABL:Ｃ感覚 == 2
	SOURCE:0 = 700
	SOURCE:13 = 700
ELSEIF ABL:Ｃ感覚 == 3
	SOURCE:0 = 1500
	SOURCE:13 = 1500
ELSEIF ABL:Ｃ感覚 == 4
	SOURCE:0 = 2400
	SOURCE:13 = 2400
ELSE
	SOURCE:0 = 1050 + (ABL:Ｃ感覚 * 450)
	SOURCE:13 = 1050 + (ABL:Ｃ感覚 * 450)
ENDIF

;ABL:マゾっ気をみる
IF ABL:マゾっ気 == 0
	SOURCE:5 = 150
ELSEIF ABL:マゾっ気 == 1
	SOURCE:5 = 300
ELSEIF ABL:マゾっ気 == 2
	SOURCE:5 = 600
ELSEIF ABL:マゾっ気 == 3
	SOURCE:5 = 900
ELSEIF ABL:マゾっ気 == 4
	SOURCE:5 = 1500
ELSE
	SOURCE:5 = 450 + (ABL:マゾっ気 * 350)
ENDIF

;プレイヤーのABL:技巧をみる
IF ABL:PLAYER:技巧 == 0
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:13 , 0.50
ELSEIF ABL:PLAYER:技巧 == 1
	TIMES SOURCE:0 , 0.80
	TIMES SOURCE:13 , 0.80
ELSEIF ABL:PLAYER:技巧 <= 3
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:13 , 1.00
ELSEIF ABL:PLAYER:技巧 <= 5
	TIMES SOURCE:0 , 1.20
	TIMES SOURCE:13 , 1.50
ELSEIF ABL:PLAYER:技巧 <= 8
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:13 , 2.50
ELSE
	SOURCE:0 = SOURCE:0 * (ABL:PLAYER:技巧 * 10 + 100) / 100
	SOURCE:13 = SOURCE:13 * (ABL:PLAYER:技巧 * 30 + 100) / 100
ENDIF

;ABL:サドっ気をみる
IF ABL:PLAYER:サドっ気 == 0
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:13 , 1.00
ELSEIF ABL:PLAYER:サドっ気 == 1
	TIMES SOURCE:0 , 1.05
	TIMES SOURCE:13 , 1.05
ELSEIF ABL:PLAYER:サドっ気 == 2
	TIMES SOURCE:0 , 1.10
	TIMES SOURCE:13 , 1.10
ELSEIF ABL:PLAYER:サドっ気 == 3
	TIMES SOURCE:0 , 1.20
	TIMES SOURCE:13 , 1.20
ELSEIF ABL:PLAYER:サドっ気 == 4
	TIMES SOURCE:0 , 1.30
	TIMES SOURCE:13 , 1.30
ELSE
	SOURCE:0 = SOURCE:0 * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
	SOURCE:13 = SOURCE:13 * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
ENDIF

;倒錯的
SIF TALENT:倒錯的
	TIMES SOURCE:5 , 1.80

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +4
	EXP:レズ経験 += 4
	EXP:PLAYER:レズ経験 += 4
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +4
	EXP:ＢＬ経験 += 4
	EXP:PLAYER:ＢＬ経験 += 4
ENDIF

TFLAG:100 = 1

;調教者がふたなり
IF TALENT:PLAYER:ふたなり
	SOURCE:13 /= 2
ENDIF



;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:21 += 300 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:21 += 600 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:21 += 800 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:21 += 1000 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:21 += 1200 + (ABL:親密 * 160)
	ELSE
		SOURCE:21 += 1500 + (ABL:親密 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:20 = 600
	ELSEIF ABL:サドっ気 == 1
		SOURCE:20 = 900
	ELSEIF ABL:サドっ気 == 2
		SOURCE:20 = 1200
	ELSEIF ABL:サドっ気 == 3
		SOURCE:20 = 1600
	ELSEIF ABL:サドっ気 == 4
		SOURCE:20 = 2000
	ELSE
		SOURCE:20 = 1000 + (ABL:サドっ気 * 300)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
;合意がないと好感度が下がる
IF TALENT:合意 == 0 && TFLAG:48 == 0
	A -= 7
	SOURCE:15 += 200
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2


RETURN 1

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_67
A = 0
S = 0
V = 75
;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	A += ABL:欲望 * 1
ENDIF
;ABL:マゾっ気
IF ABL:マゾっ気
	A += ABL:マゾっ気 * 4
ENDIF

IF MARK:1
	A += MARK:1 * 1
ENDIF

;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 1
ENDIF

;保守的
IF TALENT:保守的
	A -= 3
ENDIF
;恥じらい
IF TALENT:恥じらい
	A -= 1
ENDIF

;快感に素直
IF TALENT:快感に素直
	A += 3
ENDIF
;快感の否定
IF TALENT:快感の否定
	A -= 1
ENDIF

;ABL:精液中毒
IF ABL:PLAYER:精液中毒
	A += ABL:PLAYER:精液中毒 * 1
ENDIF

;サド
IF TALENT:PLAYER:サド
	A += 6
ENDIF
;小悪魔
IF TALENT:PLAYER:小悪魔
	A += 3
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	A += 5
ENDIF

;好感度
IF CFLAG:2 < 1000
	B = 0
	IF CFLAG:2 < 500
		B = 50
	ELSEIF CFLAG:2 < 750
		B = 20
	ELSEIF CFLAG:2 < 900
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 1500
	B = 0
	IF CFLAG:2 < 2000
		B = 5
	ELSE
		B = CFLAG:2 / 200
	ENDIF
	A += B
ENDIF

;合意
IF TALENT:合意 == 0
	A -= 5
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
		S = 1
	ENDIF
	;ABL:マゾっ気
	IF ABL:マゾっ気
		SIF S
			PRINT  + 
		PRINTS ABLNAME:8
		PRINTV 'LV,ABL:マゾっ気,'(,ABL:マゾっ気 * 4,')
		S = 1
	ENDIF

	IF MARK:1
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:1,'(,MARK:1 * 1,')
		S = 1
	ENDIF

	;PALAM:欲情
	IF PALAM:5 < PALAMLV:1
		L = 0
	ELSEIF PALAM:5 < PALAMLV:2
		L = 1
	ELSEIF PALAM:5 < PALAMLV:3
		L = 2
	ELSEIF PALAM:5 < PALAMLV:4
		L = 3
	ELSEIF PALAM:5 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 1,')
		S = 1
	ENDIF

	;保守的
	IF TALENT:保守的
		SIF S
			PRINT  - 
		PRINTS TALENTNAME:24
		PRINTV '(,3,')
		S = 1
	ENDIF
	;恥じらい
	IF TALENT:恥じらい
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,1,')
		S = 1
	ENDIF

	;快感に素直
	IF TALENT:快感に素直
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:70
		PRINTV '(,3,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:快感の否定
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,1,')
		S = 1
	ENDIF

	;ABL:精液中毒
	IF ABL:PLAYER:精液中毒
		SIF S
			PRINT  + 
		PRINTFORM %NAME:PLAYER%の
		PRINTS ABLNAME:11
		PRINTV 'LV,ABL:PLAYER:精液中毒,'(,ABL:PLAYER:精液中毒 * 1,')
		S = 1
	ENDIF

	;サド
	IF TALENT:PLAYER:サド
		SIF S
			PRINT  + 
		PRINTFORM %NAME:PLAYER%が
		PRINTS TALENTNAME:83
		PRINTV '(,6,')
		S = 1
	ENDIF
	;小悪魔
	IF TALENT:PLAYER:小悪魔
		SIF S
			PRINT  + 
		PRINTFORM %NAME:PLAYER%が
		PRINTS TALENTNAME:87
		PRINTV '(,3,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:恋慕 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 5
	ENDIF

	;好感度
	IF CFLAG:2 < 1000
		B = 0
		PRINT  - 
		IF CFLAG:2 < 500
			B = 50
		ELSEIF CFLAG:2 < 750
			B = 20
		ELSEIF CFLAG:2 < 900
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 >= 1500
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 2000
			B = 5
		ELSE
			B = CFLAG:2 / 200
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合意
	IF TALENT:合意 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,5,')
		S = 1
	ENDIF

	;合計を表示(75以上で実行)
	PRINT  = 
	PRINTV A

	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF

