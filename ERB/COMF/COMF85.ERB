@COM85
;放尿

;おもらし癖なしでも、従順４以上欲望３以上、マゾっ気３以上は
SIF TALENT:57 == 0 && ABL:0 >= 4 && ABL:1 >= 3 && ABL:8 >= 3
	JUMP COM86
;おもらし癖ありで利尿剤が無い
SIF TALENT:57 && TEQUIP:22 == 0
	JUMP COM86

PRINTL 放尿
STR:0 = 放尿
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 10
LOSEBASE:1 += 200

SOURCE:14 = 5000
SOURCE:5 = 3000

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	SOURCE:12 = 1800
ELSEIF PALAM:5 < PALAMLV:2
	SOURCE:12 = 1900
ELSEIF PALAM:5 < PALAMLV:3
	SOURCE:12 = 2000
ELSEIF PALAM:5 < PALAMLV:4
	SOURCE:12 = 2100
ELSEIF PALAM:5 >= PALAMLV:4
	SOURCE:12 = 2200
ENDIF

;露出癖をみる
IF ABL:7 == 0
	SOURCE:13 = 6000
ELSEIF ABL:7 == 1
	SOURCE:13 = 5000
ELSEIF ABL:7 == 2
	SOURCE:13 = 4000
ELSEIF ABL:7 == 3
	SOURCE:13 = 3000
ELSEIF ABL:7 == 4
	SOURCE:13 = 2000
ELSE
	SOURCE:13 = 1000
ENDIF

;ABL:マゾっ気をみる
IF ABL:8 == 0
	TIMES SOURCE:3 , 0.80
	TIMES SOURCE:12 , 0.80
	SOURCE:10 = 100
	SOURCE:11 = 100
ELSEIF ABL:8 == 1
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:12 , 1.00
	SOURCE:10 = 200
	SOURCE:11 = 300
ELSEIF ABL:8 == 2
	TIMES SOURCE:3 , 1.30
	TIMES SOURCE:12 , 1.20
	SOURCE:10 = 400
	SOURCE:11 = 700
ELSEIF ABL:8 == 3
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:12 , 1.40
	SOURCE:10 = 700
	SOURCE:11 = 1200
ELSEIF ABL:8 == 4
	TIMES SOURCE:3 , 1.70
	TIMES SOURCE:12 , 1.50
	SOURCE:10 = 1100
	SOURCE:11 = 1800
ELSE
	SOURCE:3 = SOURCE:3 * (ABL:8 * 15 + 125) / 100
	SOURCE:12 = SOURCE:12 * (ABL:8 * 10 + 120) / 100
	SOURCE:10 = 500 + (ABL:8 * 200)
	SOURCE:11 = 750 + (ABL:8 * 350)
ENDIF

;場所をみる
IF TEQUIP:54
	TIMES SOURCE:14 , 2.50
ELSEIF TEQUIP:57
	TIMES SOURCE:14 , 1.50
ELSEIF TEQUIP:58
	TIMES SOURCE:14 , 0.50
ENDIF


	A = 100
	IF EXP:31 < EXPLV:1
		TIMES A , 3.00
	ELSEIF EXP:31 < EXPLV:2
		TIMES A , 2.50
	ELSEIF EXP:31 < EXPLV:3
		TIMES A , 2.00
	ELSEIF EXP:31 < EXPLV:4
		TIMES A , 1.00
	ELSEIF EXP:31 < EXPLV:5
		TIMES A , 0.80
	ELSE
		TIMES A , 0.60
	ENDIF

	;臆病
	SIF TALENT:10
		TIMES A , 1.20

	;反抗的
	SIF TALENT:11
		TIMES A , 2.00

	;プライド高い
	SIF TALENT:15
		TIMES A , 2.00

	;プライド低い
	SIF TALENT:17
		TIMES A , 0.80

	;自制心
	SIF TALENT:20
		TIMES A , 1.50

	;感情乏しい
	SIF TALENT:22
		TIMES A , 0.60

	;目立ちたがり
	SIF TALENT:28
		TIMES A , 0.80

	;抑圧
	SIF TALENT:32
		TIMES A , 3.00

	;抵抗
	SIF TALENT:34
		TIMES A , 3.00

	;恥じらい
	SIF TALENT:35
		TIMES A , 3.50

	;恥薄い
	SIF TALENT:36
		TIMES A , 0.50

	UP:10 += A * 3
	UP:11 += A * 2
	SOURCE:14 += A * 5

;焼き芋弁当パッチ専用イベント(黄金水入手)
;CALL SWEET_POTATO_GW

PRINTFORML %EXPNAME:31%＋２
EXP:31 += 2

;既成事実２に相当
TFLAG:200 = 2

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:40 += 1
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:41 += 1
ENDIF

SIF TEQUIP:90
	T += 1

SIF PALAM:5 >= PALAMLV:3
	TFLAG:200 = 1

IF TEQUIP:22
	TEQUIP:22 = 0
ENDIF

RETURN 1
