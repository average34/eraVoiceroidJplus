@COM87
;岩清水

PRINTL 岩清水
STR:0 = 岩清水
SELECTCOM = 87

CALL TRAIN_MESSAGE_B

LOSEBASE:0 += 20
LOSEBASE:1 += 150

SOURCE:10 += 1000
SOURCE:12 += 300
SOURCE:14 += 500

A = 500

;ABL:Ｃ感覚をみる
IF ABL:3 == 0
	SOURCE:0 += 50
ELSEIF ABL:3 == 1
	SOURCE:0 += 260
ELSEIF ABL:3 == 2
	SOURCE:0 += 800
ELSEIF ABL:3 == 3
	SOURCE:0 += 1600
ELSEIF ABL:3 == 4
	SOURCE:0 += 2500
ELSE
	SOURCE:0 += 1150 + (ABL:3 * 450)
ENDIF

;ABL:露出癖をみる
IF ABL:7 == 0
	SOURCE:0 += 10
	SOURCE:10 += 100
	SOURCE:13 += 800
	TIMES A, 1.00
ELSEIF ABL:7 == 1
	SOURCE:0 += 150
	SOURCE:10 += 500
	SOURCE:13 += 700
	TIMES A, 1.60
ELSEIF ABL:7 == 2
	SOURCE:0 += 300
	SOURCE:10 += 1000
	SOURCE:13 += 600
	TIMES A, 2.40
ELSEIF ABL:7 == 3
	SOURCE:0 += 700
	SOURCE:10 += 1800
	SOURCE:13 += 500
	TIMES A, 3.60
ELSEIF ABL:7 == 4
	SOURCE:0 += 1200
	SOURCE:10 += 3000
	SOURCE:13 += 400
	TIMES A, 4.80
ELSE
	SOURCE:0 += 300 + (ABL:7 * 300)
	SOURCE:10 += ABL:7 * 1000
	SOURCE:13 += 300
	A = A * (ABL:7 * 60 + 300) / 100
ENDIF

SOURCE:12 += A
SOURCE:13 += A
SOURCE:16 += A/2

UP:8 += A
UP:10 += A

;調教者のTALENT:舌使いをみる
IF TALENT:PLAYER:52
	TIMES SOURCE:0 , 2.00
	SOURCE:16 += SOURCE:0/20
ENDIF

;奴隷のＶ⇔調教者の口の汚れが移動
STAIN:3 |= STAIN:PLAYER:0
STAIN:PLAYER:0 |= STAIN:3

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +3
	EXP:40 += 3
ENDIF

RETURN 1
