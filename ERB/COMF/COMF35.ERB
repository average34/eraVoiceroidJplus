@COM35
;泡踊り

PRINTL 泡踊り

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = 泡踊り
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
SIF TALENT:PLAYER:121 || TALENT:PLAYER:122
	BASE:PLAYER:2 += CALC_EJAC(SELECTCOM, PLAYER)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 50
LOSEBASE:1 += 180

SOURCE:13 = 2000
SOURCE:14 = 1500

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:4 = 500
	SOURCE:5 = 300
	SOURCE:8 = 200
ELSEIF ABL:奉仕精神 == 1
	SOURCE:4 = 700
	SOURCE:5 = 500
	SOURCE:8 = 150
ELSEIF ABL:奉仕精神 == 2
	SOURCE:4 = 900
	SOURCE:5 = 800
	SOURCE:8 = 100
ELSEIF ABL:奉仕精神 == 3
	SOURCE:4 = 1100
	SOURCE:5 = 1200
	SOURCE:8 = 50
ELSEIF ABL:奉仕精神 == 4
	SOURCE:4 = 1300
	SOURCE:5 = 1800
	SOURCE:8 = 20
ELSE
	SOURCE:4 = 1000 + (ABL:奉仕精神 * 100)
	SOURCE:5 = 750 + (ABL:奉仕精神 * 350)
	SOURCE:8 = 0
ENDIF

;ABL:Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	SOURCE:0 = 0
ELSEIF ABL:Ｃ感覚 == 1
	SOURCE:0 = 50
ELSEIF ABL:Ｃ感覚 == 2
	SOURCE:0 = 200
ELSEIF ABL:Ｃ感覚 == 3
	SOURCE:0 = 400
ELSEIF ABL:Ｃ感覚 == 4
	SOURCE:0 = 1000
ELSE
	SOURCE:0 = (ABL:Ｃ感覚 * 500) - 500
ENDIF

;ABL:Ｂ感覚をみる
IF ABL:Ｂ感覚 == 0
	A = 200
ELSEIF ABL:Ｂ感覚 == 1
	A = 300
ELSEIF ABL:Ｂ感覚== 2
	A = 500
ELSEIF ABL:Ｂ感覚 == 3
	A = 1000
ELSEIF ABL:Ｂ感覚 == 4
	A = 1500
ELSE
	A = 1050 + (ABL:Ｂ感覚 * 150)
ENDIF

IF TALENT:110
	TIMES A , 1.20
ENDIF
IF TALENT:108
	TIMES A , 1.20
ELSEIF TALENT:107
	TIMES A , 0.70
ENDIF

SOURCE:17 += A
;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:4 , 1.30
	TIMES SOURCE:5 , 1.30
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:4 , 1.60
	TIMES SOURCE:5 , 1.60
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:4 , 1.90
	TIMES SOURCE:5 , 1.90
ELSE
	SOURCE:4 = SOURCE:4 * (ABL:技巧 * 20 + 130) / 100
	SOURCE:5 = SOURCE:5 * (ABL:技巧 * 20 + 130) / 100
ENDIF


;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:21 += 300 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:21 += 600 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:21 += 800 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:21 += 1000 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:21 += 1200 + (ABL:親密 * 160)
	ELSE
		SOURCE:21 += 1500 + (ABL:親密 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:20 = 800
	ELSEIF ABL:サドっ気 == 1
		SOURCE:20 = 1100
	ELSEIF ABL:サドっ気 == 2
		SOURCE:20 = 1400
	ELSEIF ABL:サドっ気 == 3
		SOURCE:20 = 1800
	ELSEIF ABL:サドっ気 == 4
		SOURCE:20 = 2300
	ELSE
		SOURCE:20 = 1000 + (ABL:サドっ気 * 350)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
SIF TALENT:85 || TALENT:153
	A += 2
SIF TALENT:88
	A += 3
;合意がないと好感度が下がる
IF TALENT:152 == 0 && TFLAG:48 == 0
	A -= 8
	SOURCE:15 += 300
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK
;;オーバーフロー対策に、値が大きすぎる場合と、負の場合は調整を加える
;IF BASE:PLAYER:2 > 100000000 || BASE:PLAYER:2 < 0
;	BASE:PLAYER:2 = 100000000
;ENDIF
;S = BASE:PLAYER:2
;EJAC = MAXBASE:PLAYER:2
;
;IF  S > EJAC * 2
;	E = 2
;ELSEIF S > EJAC
;	E = 1
;ELSE
;	E = 0
;ENDIF

IF E
;射精している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:精液中毒 == 1
		SOURCE:7 = 200
		TIMES SOURCE:5 , 2.50
		TIMES SOURCE:13, 1.60
	ELSEIF ABL:精液中毒 == 2
		SOURCE:7 = 600
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 1.00
	ELSEIF ABL:精液中毒 == 3
		SOURCE:7 = 1500
		TIMES SOURCE:5 , 4.00
		TIMES SOURCE:13, 0.70
	ELSEIF ABL:精液中毒 == 4
		SOURCE:7 = 3000
		TIMES SOURCE:5 , 5.00
		TIMES SOURCE:13, 0.40
	ELSE
		SOURCE:7 = (ABL:精液中毒 * 1500) - 1500
		SOURCE:5 = SOURCE:5 * (ABL:精液中毒 * 50 + 350) / 100
		TIMES SOURCE:13, 0.10
	ENDIF

	;射精先タイプの設定
	TFLAG:703 = 3
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;;大量射精
;IF E == 2
;	TIMES SOURCE:7 , 2.00
;	TIMES SOURCE:5 , 1.50
;
;	EXP:PLAYER:3 += 2
;	EXP:20 += 3
;	PRINTL 大量射精
;	PRINTL 精液経験＋３
;	;精液汚れ
;	STAIN:PLAYER:2 |= 4
;
;	BASE:PLAYER:2 -= EJAC*2
;	SIF BASE:PLAYER:2 >= EJAC
;		BASE:PLAYER:2 = EJAC-1
;
;	;手で射精させたフラグ
;	TFLAG:1 = 2
;
;;通常の射精
;ELSEIF E == 1
;	EXP:PLAYER:3 += 1
;	EXP:20 += 1
;	PRINTL 射精
;	PRINTL 精液経験＋１
;
;	;精液汚れ
;	STAIN:PLAYER:2 |= 4
;
;	BASE:PLAYER:2 -= EJAC
;	SIF BASE:PLAYER:2 >= EJAC
;		BASE:PLAYER:2 = EJAC-1
;
;	;手で射精させたフラグ
;	TFLAG:1 = 1
;ENDIF

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +7
	EXP:40 += 7
	EXP:PLAYER:40 += 7
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +7
	EXP:41 += 7
	EXP:PLAYER:41 += 7
ENDIF

STAIN:0 = 0
STAIN:1 = 0
STAIN:2 = 2
STAIN:3 = 1
STAIN:4 = 8
STAIN:5 = 0

IF ASSIPLAY == 0
	STAIN:MASTER:0 = 0
	STAIN:MASTER:1 = 0
	STAIN:MASTER:2 = 2
	STAIN:MASTER:3 = 1
	STAIN:MASTER:4 = 8
	STAIN:MASTER:0 = 0
ELSEIF ASSIPLAY
	STAIN:ASSI:0 = 0
	STAIN:ASSI:1 = 0
	STAIN:ASSI:2 = 2
	STAIN:ASSI:3 = 1
	STAIN:ASSI:4 = 8
	STAIN:ASSI:0 = 0
ENDIF

SIF ASSIPLAY == 0 && EXP:22 >= EXPLV:3
	TFLAG:30 += 1

TFLAG:100 = 1

;既成事実２に相当
TFLAG:200 = 2

;調教者がふたなり
IF TALENT:PLAYER:121
	SOURCE:13 /= 2
ENDIF

RETURN 1

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_35
A = 0
S = 0
V = 100
;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	A += ABL:欲望 * 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	A += ABL:奉仕精神 * 4
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	A += ABL:精液中毒 * 3
ENDIF

IF MARK:1
	A += MARK:1 * 1
ENDIF


;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 1
ENDIF

;恥じらい
IF TALENT:35
	A -= 3
ENDIF
;汚臭鈍感
IF TALENT:61
	A += 1
ENDIF
;汚臭敏感
IF TALENT:62
	A -= 3
ENDIF
;献身的
IF TALENT:63
	A += 6
ENDIF
;快感の否定
IF TALENT:71
	A -= 3
ENDIF
;男嫌い
IF TALENT:82 && TALENT:PLAYER:122
	A -= 12
ENDIF
;恋慕
IF TALENT:85 && ASSIPLAY == 0
	A += 5
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:121
	A += 8
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2
;汚れあり
IF Y
	A -= Y
ENDIF
;好感度
IF CFLAG:2 < 1000
	B = 0
	IF CFLAG:2 < 500
		B = 50
	ELSEIF CFLAG:2 < 750
		B = 20
	ELSEIF CFLAG:2 < 900
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 1500
	B = 0
	IF CFLAG:2 < 2000
		B = 5
	ELSE
		B = CFLAG:2 / 200
	ENDIF
	A += B
ENDIF

;合意
IF TALENT:152 == 0
	A -= 15
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
		S = 1
	ENDIF
	;ABL:奉仕精神
	IF ABL:奉仕精神
		SIF S
			PRINT  + 
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
		S = 1
	ENDIF
	;ABL:精液中毒
	IF ABL:精液中毒
		SIF S
			PRINT  + 
		PRINTS ABLNAME:11
		PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 3,')
		S = 1
	ENDIF

	IF MARK:1
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:1,'(,MARK:1 * 1,')
		S = 1
	ENDIF


	;PALAM:欲情
	IF PALAM:5 < PALAMLV:1
		L = 0
	ELSEIF PALAM:5 < PALAMLV:2
		L = 1
	ELSEIF PALAM:5 < PALAMLV:3
		L = 2
	ELSEIF PALAM:5 < PALAMLV:4
		L = 3
	ELSEIF PALAM:5 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 1,')
		S = 1
	ENDIF

	;恥じらい
	IF TALENT:35
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,3,')
		S = 1
	ENDIF
	;汚臭鈍感
	IF TALENT:61
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:61
		PRINTV '(,1,')
		S = 1
	ENDIF
	;汚臭敏感
	IF TALENT:62
		PRINT  - 
		PRINTS TALENTNAME:62
		PRINTV '(,3,')
		S = 1
	ENDIF
	;献身的
	IF TALENT:63
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:63
		PRINTV '(,6,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:71
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,3,')
		S = 1
	ENDIF
	;男嫌い
	IF TALENT:82 && TALENT:PLAYER:122
		PRINT  - 
		PRINTS TALENTNAME:82
		PRINTV '(,12,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:85 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 1
	ENDIF

	;調教者がふたなり
	IF TALENT:PLAYER:121
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:121
		PRINTV '(,8,')
		S = 1
	ENDIF

	Y = 0
	;愛液の汚れ
	SIF STAIN:PLAYER:2 & 1
		Y += 1
	;精液の汚れ
	SIF STAIN:PLAYER:2 & 4
		Y += 3
	;アナルの汚れ
	SIF STAIN:PLAYER:2 & 8
		Y += 7
	SIF STAIN:PLAYER:2 & 16
		Y += 15

	SIF TALENT:61
		Y /= 3
	SIF TALENT:62
		Y *= 2
	;汚れあり
	IF Y
		PRINT  - 
		;汚臭鈍感
		IF TALENT:61
			PRINT 汚れあり、
			PRINTS TALENTNAME:61
		ELSEIF TALENT:62
			PRINT 汚れあり、
			PRINTS TALENTNAME:62
		ELSE
			PRINT 汚れあり
		ENDIF
		PRINTV '(,Y,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 < 1000
		B = 0
		PRINT  - 
		IF CFLAG:2 < 500
			B = 50
		ELSEIF CFLAG:2 < 750
			B = 20
		ELSEIF CFLAG:2 < 900
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 >= 1500
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 2000
			B = 5
		ELSE
			B = CFLAG:2 / 200
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合意
	IF TALENT:152 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,15,')
		S = 1
	ENDIF

	;合計を表示(100以上で実行)
	PRINT  = 
	PRINTV A


	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF
