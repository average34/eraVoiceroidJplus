@COM5
;胸愛撫

SIF TEQUIP:65
	PRINT 着衣
PRINTL 胸愛撫
STR:0 = 胸愛撫
CALL TRAIN_MESSAGE_B

LOSEBASE:0 += 5
LOSEBASE:1 += 50

SOURCE:4 = 60
SOURCE:8 = 20
SOURCE:12 = 100

;ABL:Ｂ感覚をみる
IF ABL:Ｂ感覚 == 0
	SOURCE:17 = 20
	SOURCE:3 = 50
ELSEIF ABL:Ｂ感覚 == 1
	SOURCE:17 = 100
	SOURCE:3 = 100
ELSEIF ABL:Ｂ感覚 == 2
	SOURCE:17 = 500
	SOURCE:3 = 160
ELSEIF ABL:Ｂ感覚 == 3
	SOURCE:17 = 1200
	SOURCE:3 = 200
ELSEIF ABL:Ｂ感覚 == 4
	SOURCE:17 = 2000
	SOURCE:3 = 230
ELSE
	SOURCE:17 = 800 + (ABL:Ｂ感覚 * 400)
	SOURCE:3 = 200 + (ABL:Ｂ感覚 * 10)
ENDIF

;調教者が［幼児退行］
IF TALENT:PLAYER:131
	TIMES SOURCE:17 , 1.20
	TIMES SOURCE:3 , 1.20
ENDIF

;調教者が［幼稚］
IF TALENT:PLAYER:132
	TIMES SOURCE:17 , 1.20
	TIMES SOURCE:3 , 1.20
ENDIF

IF TEQUIP:90
	;奴隷のＢに触手の汚れがつく
	STAIN:5 |= 2
	STAIN:5 |= 4
ELSE
	;着衣中でかつコンフィグがONだと性能低下
	SIF TEQUIP:65
		TIMES SOURCE:17 , 0.80
	;奴隷の胸の汚れがなければ口をつかう（愛液と母乳よごれはＯＫ、汚れ無視か助手調教時は問題なし）
	IF STAIN:5 < 2 || STAIN:5 == 16 || STAIN:5 == 17 || ASSIPLAY == 1 || TALENT:MASTER:64 == 1
		;調教者のTALENT:舌使いをみる
		IF TALENT:PLAYER:52
			TIMES SOURCE:17 , 1.40
			SOURCE:16 += SOURCE:17/20
		ENDIF

		;奴隷のＢ⇔調教者の口の汚れが移動
		STAIN:5 |= STAIN:PLAYER:0
		STAIN:PLAYER:0 |= STAIN:5
	ENDIF

	;奴隷のＢ⇔調教者の指の汚れが移動
	STAIN:5 |= STAIN:PLAYER:1
	STAIN:PLAYER:1 |= STAIN:5
ENDIF

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +5
	EXP:40 += 5
	EXP:PLAYER:40 += 5
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +5
	EXP:41 += 5
	EXP:PLAYER:41 += 5
ENDIF


;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:21 += 300 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:21 += 600 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:21 += 800 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:21 += 1000 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:21 += 1200 + (ABL:親密 * 160)
	ELSE
		SOURCE:21 += 1500 + (ABL:親密 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:20 = 600
	ELSEIF ABL:サドっ気 == 1
		SOURCE:20 = 900
	ELSEIF ABL:サドっ気 == 2
		SOURCE:20 = 1200
	ELSEIF ABL:サドっ気 == 3
		SOURCE:20 = 1600
	ELSEIF ABL:サドっ気 == 4
		SOURCE:20 = 2000
	ELSE
		SOURCE:20 = 1000 + (ABL:サドっ気 * 300)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
;合意がないと好感度が下がる
IF TALENT:152 == 0 && TFLAG:48 == 0
	A -= 7
	SOURCE:15 += 200
ENDIF
TFLAG:30 += A
TFLAG:37 += A


;［Ｂ感度］３以下では発生しない
;［母乳体質］無しかつ［小柄体系］で［貧乳］無しかつ［オトコ］無し
;触手調教は発生しない
SIF ABL:TARGET:Ｂ感覚 <= 3 || TEQUIP:90
	RETURN 1
IF TALENT:130 == 0 && (TALENT:100 == 0 || TALENT:109 == 0 ) && TALENT:122 == 0 
	CALL YOUJI
ENDIF

RETURN 1

