@COM68
;ダブルフェラ
;奴隷と助手に舐めさせる

PRINTL ダブルフェラ

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = ダブルフェラ
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
SIF TALENT:MASTER:半人半妖 || TALENT:MASTER:オトコ  || TALENT:MASTER:ふたなり
	BASE:MASTER:射精 += CALC_EJAC(SELECTCOM, MASTER)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 10
LOSEBASE:気力 += 100

SOURCE:13 = 1500
SOURCE:14 = 500

;上のほうで計算した汚れデータ
SOURCE:8 = Y*40 + 100

;Ｍ感覚を設定している場合快Ｍソース等取得
;フェラチオの快Ｍソース計算を使用
SIF FLAG:62 & 4194304
	CALL SOURCE_M_COM31

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:3 = 300
	SOURCE:4 = 420
	SOURCE:5 = 150
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:奉仕精神 == 1
	SOURCE:3 = 400
	SOURCE:4 = 500
	SOURCE:5 = 300
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:奉仕精神 == 2
	SOURCE:3 = 550
	SOURCE:4 = 580
	SOURCE:5 = 600
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:奉仕精神 == 3
	SOURCE:3 = 700
	SOURCE:4 = 660
	SOURCE:5 = 900
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:奉仕精神 == 4
	SOURCE:3 = 900
	SOURCE:4 = 740
	SOURCE:5 = 1500
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:3 = 750 + (ABL:奉仕精神 * 50)
	SOURCE:4 = 620 + (ABL:奉仕精神 * 40)
	SOURCE:5 = 450 + (ABL:奉仕精神 * 350)
	TIMES SOURCE:8 , 0.10
ENDIF

;ABL:従順をみる
IF ABL:親密 == 0
	TIMES SOURCE:3 , 0.50
ELSEIF ABL:親密 == 1
	TIMES SOURCE:3 , 0.80
ELSEIF ABL:親密 == 2
	TIMES SOURCE:3 , 1.00
ELSEIF ABL:親密 == 3
	TIMES SOURCE:3 , 1.20
ELSEIF ABL:親密 == 4
	TIMES SOURCE:3 , 1.50
ELSE
	SOURCE:3 = SOURCE:3 * (ABL:親密 * 25 + 75) / 100
ENDIF

;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSE
	SOURCE:4 = SOURCE:4 * (ABL:技巧 * 25 + 75) / 100
	SOURCE:5 = SOURCE:5 * (ABL:技巧 * 25 + 75) / 100
ENDIF


;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:21 += 300 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:21 += 600 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:21 += 800 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:21 += 1000 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:21 += 1200 + (ABL:親密 * 160)
	ELSE
		SOURCE:21 += 1500 + (ABL:親密 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:20 = 800
	ELSEIF ABL:サドっ気 == 1
		SOURCE:20 = 1100
	ELSEIF ABL:サドっ気 == 2
		SOURCE:20 = 1400
	ELSEIF ABL:サドっ気 == 3
		SOURCE:20 = 1800
	ELSEIF ABL:サドっ気 == 4
		SOURCE:20 = 2300
	ELSE
		SOURCE:20 = 1000 + (ABL:サドっ気 * 350)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
SIF TALENT:恋慕 || TALENT:恋人
	A += 2
SIF TALENT:親愛
	A += 3
;合意がないと好感度が下がる
IF TALENT:合意 == 0 && TFLAG:48 == 0
	A -= 8
	SOURCE:15 += 300
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
;射精するのは主人の為、ASSIPLAYを一時的に0にする
VARSET LOCAL, 0
IF ASSIPLAY > 0
	LOCAL = 1
	ASSIPLAY = 0
	PLAYER = MASTER
ENDIF
CALL SAMEN_CHECK

;S = BASE:MASTER:射精
;EJAC = MAXBASE:MASTER:射精
;
;IF  S > EJAC * 2
;	E = 2
;ELSEIF S > EJAC
;	E = 1
;ELSE
;	E = 0
;ENDIF

;射精している
IF E >= 1
	;射精先タイプの設定
	TFLAG:703 = 4
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT

	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 4.00
	ELSEIF ABL:精液中毒 == 1
		SOURCE:7 = 500
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 3.00
	ELSEIF ABL:精液中毒 == 2
		SOURCE:7 = 1200
		TIMES SOURCE:5 , 4.00
		TIMES SOURCE:13, 2.50
	ELSEIF ABL:精液中毒 == 3
		SOURCE:7 = 3000
		TIMES SOURCE:5 , 6.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:精液中毒 == 4
		SOURCE:7 = 6000
		TIMES SOURCE:5 , 9.00
		TIMES SOURCE:13, 1.50
	ELSE
		SOURCE:7 = (ABL:精液中毒 * 3000) - 3000
		SOURCE:5 = SOURCE:5 * (ABL:親密 * 3)
		TIMES SOURCE:13, 1.00
	ENDIF
ENDIF

;;大量射精
;IF E == 2
;	TIMES SOURCE:7 , 2.00
;	TIMES SOURCE:5 , 1.50
;
;	EXP:MASTER:射精経験 += 2
;	EXP:精液経験 += 9
;	PRINTL 大量射精
;	PRINTL 精液経験+9
;	;精液汚れ
;	STAIN:MASTER:2 |= 4
;
;	BASE:MASTER:射精 -= EJAC*2
;	SIF BASE:MASTER:射精 >= EJAC
;		BASE:MASTER:射精 = EJAC-1
;
;	;口で射精させたフラグ
;	TFLAG:0 = 2
;;通常の射精
;ELSEIF E == 1
;	EXP:MASTER:射精経験 += 1
;	EXP:精液経験 += 3
;	PRINTL 射精
;	PRINTL 精液経験+3
;	;精液汚れ
;	STAIN:MASTER:2 |= 4
;
;	BASE:MASTER:射精 -= EJAC
;	SIF BASE:MASTER:射精 >= EJAC
;		BASE:MASTER:射精 = EJAC-1
;
;	;口で射精させたフラグ
;	TFLAG:0 = 1
;ENDIF
;助手コマンドから選択されるので、ASSIPLAYを戻す
IF LOCAL
	ASSIPLAY = 1
	PLAYER = ASSI
ENDIF

;;コンドーム処理(暫定)
;SIF TEQUIP:MASTER:85
;	TEQUIP:MASTER:85 = 0
;SIF TEQUIP:ASSI:85
;	TEQUIP:ASSI:85 = 0

PRINTFORML %EXPNAME:22%+1
EXP:フェラ経験 += 1

;奴隷の口⇔調教者のＰの汚れが移動
STAIN:0 |= STAIN:MASTER:2
STAIN:MASTER:2 |= STAIN:0

;助手の口⇔調教者のＰの汚れが移動
STAIN:ASSI:0 |= STAIN:MASTER:2
STAIN:MASTER:2 |= STAIN:ASSI:0


;奉仕精神ＬＶ２以上、技巧ＬＶ２以上なら汚れをなめ取る
IF ABL:奉仕精神 >= 2 && ABL:技巧 >= 2
	STAIN:MASTER:2 = 2
	SIF E >= 1
		TFLAG:8 = 2
ENDIF

;奴隷と助手が共に女性でどちらかが精液中毒を持っているとき、奴隷の口⇔助手の口の汚れが移動
IF TALENT:オトコ == 0 && TALENT:ASSI:オトコ == 0 && (ABL:精液中毒 >= 1 || ABL:ASSI:精液中毒 >= 1)
	STAIN:0 |= STAIN:ASSI:2
	STAIN:ASSI:0 |= STAIN:0
	SIF E >= 1
		TFLAG:8 = 3
ENDIF

;マスターと奴隷のレズorBL
IF TALENT:オトコ == 0 && TALENT:MASTER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +7
	EXP:レズ経験 += 7
	EXP:MASTER:レズ経験 += 7
ELSEIF TALENT:オトコ == 1 && TALENT:MASTER:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +7
	EXP:ＢＬ経験 += 7
	EXP:MASTER:ＢＬ経験 += 7
ENDIF

;助手とマスターのレズorBL
IF TALENT:MASTER:オトコ == 0 && TALENT:ASSI:オトコ == 0
	PRINTFORML %EXPNAME:40%+7(助手)
	EXP:MASTER:レズ経験 += 7
	EXP:ASSI:レズ経験 += 7
ELSEIF TALENT:MASTER:オトコ == 1 && TALENT:ASSI:オトコ == 1
	PRINTFORML %EXPNAME:41%+7(助手)
	EXP:MASTER:ＢＬ経験 += 7
	EXP:ASSI:ＢＬ経験 += 7
ENDIF

;調教者がふたなり
IF TALENT:MASTER:ふたなり
	SOURCE:13 /= 2
ENDIF

TFLAG:100 = 1

;既成事実２に相当
TFLAG:200 = 2

RETURN 1

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_68
A = 0
S = 0
V = 105
;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	A += ABL:欲望 * 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	A += ABL:奉仕精神 * 4
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	A += ABL:精液中毒 * 3
ENDIF

IF MARK:1
	A += MARK:1 * 1
ENDIF


;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 1
ENDIF

;恥じらい
IF TALENT:恥じらい
	A -= 1
ENDIF
;汚臭鈍感
IF TALENT:汚臭鈍感
	A += 1
ENDIF
;汚臭敏感
IF TALENT:汚臭敏感
	A -= 3
ENDIF
;献身的
IF TALENT:献身的
	A += 6
ENDIF
;快感の否定
IF TALENT:快感の否定
	A -= 1
ENDIF
;男嫌い
IF TALENT:男嫌い && TALENT:MASTER:オトコ
	A -= 12
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	A += 5
ENDIF

;調教者がふたなり
IF TALENT:MASTER:ふたなり
	A += 8
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:MASTER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:MASTER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:MASTER:2 & 8
	Y += 7
SIF STAIN:MASTER:2 & 16
	Y += 15

SIF TALENT:汚臭鈍感
	Y /= 3
SIF TALENT:汚臭敏感
	Y *= 2



;汚れあり
IF Y
	A -= Y
	;汚臭鈍感
	IF TALENT:汚臭鈍感
	ELSEIF TALENT:汚臭敏感
	ELSE
	ENDIF
ENDIF

;好感度
IF CFLAG:2 < 1000
	B = 0
	IF CFLAG:2 < 500
		B = 50
	ELSEIF CFLAG:2 < 750
		B = 20
	ELSEIF CFLAG:2 < 900
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 1500
	B = 0
	IF CFLAG:2 < 2000
		B = 5
	ELSE
		B = CFLAG:2 / 200
	ENDIF
	A += B
ENDIF

;合意
SIF TALENT:合意 == 0
	A -= 10

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
		S = 1
	ENDIF
	;ABL:奉仕精神
	IF ABL:奉仕精神
		SIF S
			PRINT  + 
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
		S = 1
	ENDIF
	;ABL:精液中毒
	IF ABL:精液中毒
		SIF S
			PRINT  + 
		PRINTS ABLNAME:11
		PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 3,')
		S = 1
	ENDIF

	IF MARK:1
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:1,'(,MARK:1 * 1,')
		S = 1
	ENDIF


	;PALAM:欲情
	IF PALAM:5 < PALAMLV:1
		L = 0
	ELSEIF PALAM:5 < PALAMLV:2
		L = 1
	ELSEIF PALAM:5 < PALAMLV:3
		L = 2
	ELSEIF PALAM:5 < PALAMLV:4
		L = 3
	ELSEIF PALAM:5 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 1,')
		S = 1
	ENDIF

	;恥じらい'
	IF TALENT:恥じらい
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,1,')
		S = 1
	ENDIF
	;汚臭鈍感
	IF TALENT:汚臭鈍感
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:61
		PRINTV '(,1,')
		S = 1
	ENDIF
	;汚臭敏感
	IF TALENT:汚臭敏感
		PRINT  - 
		PRINTS TALENTNAME:62
		PRINTV '(,3,')
		S = 1
	ENDIF
	;献身的
	IF TALENT:献身的
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:63
		PRINTV '(,6,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:快感の否定
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,1,')
		S = 1
	ENDIF
	;男嫌い
	IF TALENT:男嫌い && TALENT:ASSI:オトコ
		PRINT  - 
		PRINTS TALENTNAME:82
		PRINTV '(,12,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:恋慕 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 1
	ENDIF

	;調教者がふたなり
	IF TALENT:ASSI:ふたなり
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:121
		PRINTV '(,8,')
		S = 1
	ENDIF

	Y = 0
	;愛液の汚れ
	SIF STAIN:MASTER:2 & 1
		Y += 1
	;精液の汚れ
	SIF STAIN:MASTER:2 & 4
		Y += 3
	;アナルの汚れ
	SIF STAIN:MASTER:2 & 8
		Y += 7
	SIF STAIN:MASTER:2 & 16
		Y += 15

	SIF TALENT:汚臭鈍感
		Y /= 3
	SIF TALENT:汚臭敏感
		Y *= 2



	;汚れあり
	IF Y
		PRINT  - 
		;汚臭鈍感
		IF TALENT:汚臭鈍感
			PRINT 汚れあり、
			PRINTS TALENTNAME:61
		ELSEIF TALENT:汚臭敏感
			PRINT 汚れあり、
			PRINTS TALENTNAME:62
		ELSE
			PRINT 汚れあり
		ENDIF
		PRINTV '(,Y,')
		S = 1
	ENDIF

	;好感度
	IF CFLAG:2 < 1000
		B = 0
		PRINT  - 
		IF CFLAG:2 < 500
			B = 50
		ELSEIF CFLAG:2 < 750
			B = 20
		ELSEIF CFLAG:2 < 900
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 >= 1500
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 2000
			B = 5
		ELSE
			B = CFLAG:2 / 200
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合意
	IF TALENT:合意 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,10,')
		S = 1
	ENDIF

	;合計を表示(105以上で実行)
	PRINT  = 
	PRINTV A

	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF

