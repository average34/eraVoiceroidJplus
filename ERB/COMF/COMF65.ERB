@COM65
;助手を犯させる

PRINTL 助手を犯させる

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = 助手を犯させる
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック（パートナーがふたなりの場合）
;-------------------------------------------------
SIF TALENT:TARGET:119 || TALENT:TARGET:121 || TALENT:TARGET:122
	BASE:TARGET:2 += CALC_EJAC(SELECTCOM, TARGET)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 40
LOSEBASE:1 += 220

SOURCE:13 = 1500
SOURCE:14 = 800

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:0 = 800
	SOURCE:4 = 3200
	SOURCE:5 = 400
ELSEIF ABL:奉仕精神 == 1
	SOURCE:0 = 1400
	SOURCE:4 = 3800
	SOURCE:5 = 800
ELSEIF ABL:奉仕精神 == 2
	SOURCE:0 = 2000
	SOURCE:4 = 4600
	SOURCE:5 = 1500
ELSEIF ABL:奉仕精神 == 3
	SOURCE:0 = 2500
	SOURCE:4 = 5400
	SOURCE:5 = 2300
ELSEIF ABL:奉仕精神 == 4
	SOURCE:0 = 2900
	SOURCE:4 = 6200
	SOURCE:5 = 3500
ELSE
	SOURCE:0 = 2450 + (ABL:奉仕精神 * 150)
	SOURCE:4 = 5000 + (ABL:奉仕精神 * 400)
	SOURCE:5 = 1250 + (ABL:奉仕精神 * 750)
ENDIF


;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:4 , 2.50
	TIMES SOURCE:5 , 2.50
ELSE
	TIMES SOURCE:0 , 0.50
	SOURCE:4 = SOURCE:4 * (ABL:技巧 * 75 + 25) / 100
	SOURCE:5 = SOURCE:5 * (ABL:技巧 * 75 + 25) / 100
ENDIF

;助手が処女
IF TALENT:ASSI:0 == 1
	TIMES SOURCE:13 , 20.00
	TIMES SOURCE:14 , 3.00
	TALENT:ASSI:0 = 0

	PRINTFORML ＜%CALLNAME:ASSI%が処女＞

	EXP:50 += 3
	PRINTFORML %EXPNAME:50%＋３

	IF ABL:親密 < 3 && PLAYER != ASSI
		PRINTFORML そして%ABLNAME:0%が3になった
		ABL:親密 = 3
	ENDIF
ENDIF
EXP:ASSI:0 += 1


;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:21 += 300 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:21 += 600 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:21 += 800 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:21 += 1000 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:21 += 1200 + (ABL:親密 * 160)
	ELSE
		SOURCE:21 += 1500 + (ABL:親密 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:20 = 600
	ELSEIF ABL:サドっ気 == 1
		SOURCE:20 = 900
	ELSEIF ABL:サドっ気 == 2
		SOURCE:20 = 1200
	ELSEIF ABL:サドっ気 == 3
		SOURCE:20 = 1600
	ELSEIF ABL:サドっ気 == 4
		SOURCE:20 = 2000
	ELSE
		SOURCE:20 = 1000 + (ABL:サドっ気 * 300)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
;合意がないと好感度が下がる
IF TALENT:152 == 0 && TFLAG:48 == 0
	A -= 9
	SOURCE:15 += 400
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

EXP:50 += 1
PRINTFORML %EXPNAME:50%＋１

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK
;;オーバーフロー対策に、値が大きすぎる場合と、負の場合は調整を加える
;IF BASE:PLAYER:2 > 100000000 || BASE:PLAYER:2 < 0
;	BASE:PLAYER:2 = 100000000
;ENDIF
;S = BASE:PLAYER:2
;EJAC = MAXBASE:PLAYER:2
;
;IF  S > EJAC * 2
;	E = 2
;ELSEIF S > EJAC
;	E = 1
;ELSE
;	E = 0
;ENDIF

IF E
	;射精している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:精液中毒 == 1
		SOURCE:7 = 50
		TIMES SOURCE:5 , 2.50
		TIMES SOURCE:13, 1.60
	ELSEIF ABL:精液中毒 == 2
		SOURCE:7 = 150
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 1.00
	ELSEIF ABL:精液中毒 == 3
		SOURCE:7 = 300
		TIMES SOURCE:5 , 4.50
		TIMES SOURCE:13, 0.70
	ELSEIF ABL:精液中毒 == 4
		SOURCE:7 = 600
		TIMES SOURCE:5 , 6.00
		TIMES SOURCE:13, 0.40
	ELSE
		SOURCE:7 = (ABL:精液中毒 * 300) - 300
		SOURCE:5 = SOURCE:5 * (ABL:精液中毒 + 3)
		TIMES SOURCE:13, 0.10
	ENDIF

	;射精先タイプの設定
	TFLAG:703 = 8
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;;大量射精
;IF E == 2
;	TIMES SOURCE:7 , 2.00
;	TIMES SOURCE:5 , 1.50
;
;	EXP:PLAYER:3 += 2
;	EXP:20 += 1
;	PRINTL 大量射精
;	PRINTL 精液経験＋１
;	;精液汚れ
;	STAIN:PLAYER:2 |= 4
;
;	BASE:PLAYER:2 -= EJAC*2
;	SIF BASE:PLAYER:2 >= EJAC
;		BASE:PLAYER:2 = EJAC-1
;
;	;助手が射精フラグ
;	TFLAG:6 = 2
;;通常の射精
;ELSEIF E == 1
;	EXP:PLAYER:3 += 1
;	PRINTL 射精
;	;精液汚れ
;	STAIN:PLAYER:2 |= 4
;
;	BASE:PLAYER:2 -= EJAC
;	SIF BASE:PLAYER:2 >= EJAC
;		BASE:PLAYER:2 = EJAC-1
;
;	;助手が射精フラグ
;	TFLAG:6 = 1
;ENDIF

IF TALENT:119 || TALENT:121 || TALENT:122
	;パートナーのＰ⇔助手のＶの汚れが移動
	STAIN:2 |= STAIN:ASSI:3
	STAIN:ASSI:3 |= STAIN:2
ENDIF

IF TALENT:122 == 0 && TALENT:ASSI:122 == 0
	PRINTS EXPNAME:40
	PRINTL +10
	EXP:40 += 10
	EXP:ASSI:40 += 10
ELSEIF TALENT:122 == 1 && TALENT:ASSI:122 == 1
	PRINTS EXPNAME:41
	PRINTL +10
	EXP:41 += 10
	EXP:ASSI:41 += 10
ENDIF

TFLAG:100 = 1

;既成事実２に相当
TFLAG:200 = 2

RETURN 1

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_65
A = 0
S = 0
V = 130
;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	A += ABL:欲望 * 2
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	A += ABL:奉仕精神 * 4
ENDIF

IF MARK:1
	A += MARK:1 * 2
ENDIF


;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 2
ENDIF

;恥じらい
IF TALENT:35
	A -= 1
ENDIF
;献身的
IF TALENT:63
	A += 6
ENDIF
;快感に素直
IF TALENT:70
	A += 2
ENDIF
;快感の否定
IF TALENT:71
	A -= 2
ENDIF
;恋慕
IF TALENT:85 && ASSIPLAY == 0
	A += 5
ENDIF

;助手が処女
IF EXP:ASSI:0 == 0
	A -= 15
ENDIF


;好感度
IF CFLAG:2 < 2500
	B = 0
	IF CFLAG:2 < 1000
		B = 50
	ELSEIF CFLAG:2 < 1500
		B = 20
	ELSEIF CFLAG:2 < 2000
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 3000
	B = 0
	IF CFLAG:2 < 4000
		B = 5
	ELSE
		B = CFLAG:2 / 250
	ENDIF
	A += B
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 2,')
		S = 1
	ENDIF
	;ABL:奉仕精神
	IF ABL:奉仕精神
		SIF S
			PRINT  + 
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
		S = 1
	ENDIF

	IF MARK:1
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:1,'(,MARK:1 * 2,')
		S = 1
	ENDIF


	;PALAM:欲情
	IF PALAM:5 < PALAMLV:1
		L = 0
	ELSEIF PALAM:5 < PALAMLV:2
		L = 1
	ELSEIF PALAM:5 < PALAMLV:3
		L = 2
	ELSEIF PALAM:5 < PALAMLV:4
		L = 3
	ELSEIF PALAM:5 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 2,')
		S = 1
	ENDIF

	;恥じらい
	IF TALENT:35
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,1,')
		S = 1
	ENDIF
	;献身的
	IF TALENT:63
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:63
		PRINTV '(,6,')
		S = 1
	ENDIF
	;快感に素直
	IF TALENT:70
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:70
		PRINTV '(,2,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:71
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,2,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:85 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 1
	ENDIF

	;助手が処女
	IF EXP:ASSI:0 == 0
		PRINT  - 
		PRINT 助手が処女
		PRINTV '(,15,')
		S = 1
	ENDIF


	;好感度
	IF CFLAG:2 < 2500
		B = 0
		PRINT  - 
		IF CFLAG:2 < 1000
			B = 50
		ELSEIF CFLAG:2 < 1500
			B = 20
		ELSEIF CFLAG:2 < 2000
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 >= 3000
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 4000
			B = 5
		ELSE
			B = CFLAG:2 / 250
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合計を表示(130以上で実行)
	PRINT  = 
	PRINTV A

	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF

