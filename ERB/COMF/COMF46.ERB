@COM46
;浣腸器＋プラグ

;外す時
IF TEQUIP:46
	IF TEQUIP:65
		PRINTL 着衣排泄
		STR:0 = 着衣排泄
	ELSE
		PRINTL 排泄
		STR:0 = 排泄
	ENDIF
;装着する時
ELSE
	IF TEQUIP:90
		PRINTL 触手浣腸
		STR:0 = 触手浣腸
	ELSE
		PRINTL 浣腸
		STR:0 = 浣腸
	ENDIF
ENDIF

LOSEBASE:体力 += 60
LOSEBASE:気力 += 150

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:快Ａ = 80
	SOURCE:屈従 = 300
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:快Ａ = 250
	SOURCE:屈従 = 800
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:快Ａ = 600
	SOURCE:屈従 = 1400
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:快Ａ = 1000
	SOURCE:屈従 = 1800
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:快Ａ = 1300
	SOURCE:屈従 = 2100
ELSE
	SOURCE:快Ａ = 700 + (ABL:Ａ感覚 * 200)
	SOURCE:屈従 = 1650 + (ABL:Ａ感覚 * 150)
ENDIF

;ABL8,マゾっ気をみる
IF ABL:マゾっ気 < 1
	SOURCE:痛み = 2000
	SOURCE:不潔 = 1000
	SOURCE:屈従 += 200
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 2000
ELSEIF ABL:マゾっ気 < 2
	SOURCE:痛み = 1600
	SOURCE:不潔 = 2000
	SOURCE:屈従 += 500
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 1000
ELSEIF ABL:マゾっ気 < 3
	SOURCE:痛み = 1200
	SOURCE:不潔 = 1000
	SOURCE:屈従 += 800
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 500
ELSEIF ABL:マゾっ気 < 4
	SOURCE:痛み = 800
	SOURCE:不潔 = 1000
	SOURCE:屈従 += 1200
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 100
ELSEIF ABL:マゾっ気 < 5
	SOURCE:痛み = 600
	SOURCE:不潔 = 1000
	SOURCE:屈従 += 1500
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 0
ELSE
	SOURCE:痛み = 400
	SOURCE:不潔 = 1000
	SOURCE:屈従 += 750 + (ABL:マゾっ気 * 250)
	SOURCE:逸脱 = 1000
	SOURCE:反感追加 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.40
	SOURCE:痛み += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.80
	SOURCE:痛み += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
	SOURCE:痛み += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.40
	SOURCE:痛み += 120
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:快Ａ , 1.80
	SOURCE:痛み += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.10
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:快Ａ , 1.20
ENDIF

;ABL:従順をみる
IF ABL:親密 == 0
	TIMES SOURCE:快Ａ , 0.80
ELSEIF ABL:親密 == 1
	TIMES SOURCE:快Ａ , 0.90
ELSEIF ABL:親密 == 2
	TIMES SOURCE:快Ａ , 1.00
ELSEIF ABL:親密 == 3
	TIMES SOURCE:快Ａ , 1.10
ELSEIF ABL:親密 == 4
	TIMES SOURCE:快Ａ , 1.20
ELSE
	SOURCE:快Ａ = SOURCE:快Ａ * (ABL:親密 * 5 + 105) / 100
ENDIF

;ABL:サドっ気をみる
IF ABL:PLAYER:サドっ気 == 0
	TIMES SOURCE:痛み , 1.00
ELSEIF ABL:PLAYER:サドっ気 == 1
	TIMES SOURCE:痛み , 1.05
ELSEIF ABL:PLAYER:サドっ気 == 2
	TIMES SOURCE:痛み , 1.10
ELSEIF ABL:PLAYER:サドっ気 == 3
	TIMES SOURCE:痛み , 1.20
ELSEIF ABL:PLAYER:サドっ気 == 4
	TIMES SOURCE:痛み , 1.30
ELSE
	SOURCE:痛み = SOURCE:痛み * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
ENDIF

;小柄体形
SIF TALENT:小柄体型
	TIMES SOURCE:痛み , 2.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES SOURCE:痛み , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES SOURCE:痛み , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

IF EXP:Ｖ経験 == 0 && TALENT:貞操観念
	SOURCE:屈従 /= 3
ENDIF

CALL TRAIN_MESSAGE_B

EXP:Ａ経験 += 5
PRINTL Ａ経験＋5

;異常経験増加
;排泄初経験時経験2倍
IF TEQUIP:46 && CFLAG:4 == 0
	EXP:異常経験 += 10
	PRINTFORML %EXPNAME:50%＋10
	;排泄経験フラグ
	CFLAG:4 = 1
ELSE
	EXP:異常経験 += 5
	PRINTFORML %EXPNAME:50%＋5
ENDIF

;浣腸プラグの着脱
IF TEQUIP:46
	TEQUIP:46 = 0
ELSE
	TEQUIP:46 = 1
	;触手浣腸の場合汚れ追加
	IF TEQUIP:90
		STAIN:アナル |= 2
		STAIN:アナル |= 4
	ENDIF
ENDIF
;触手浣腸用の変数
T = 0

RETURN 1

@TRAIN_MESSAGE_COM46
;浣腸プラグの着脱
IF TEQUIP:46
	IF UNDERWARED()
		PRINTFORML %CALLNAME:TARGET%の%GET_UNDERWARE_NAME(TEQUIP:65)%に手を入れ、肛門栓を抜くと、腸内の汚物が%GET_UNDERWARE_NAME(TEQUIP:65)%の中に溢れ出した。
		IF TFLAG:263
			PRINTFORML 汚らしい音と共に%CALLNAME:TARGET%の%GET_CLOTH_SHORT_NAME(TEQUIP:65)%の染みがさらに広がっていく…
			PRINTFORML 収まりきらなくなった排泄物が%GET_UNDERWARE_NAME(TEQUIP:65)%から溢れ、足下にこぼれた。
		ELSE
			IF TEQUIP:65 == 3
				PRINTFORML 汚らしい音と共に%CALLNAME:TARGET%のブルマが膨らみ、汚物色に染まっていく…
			ELSE
				PRINTFORML 汚らしい音と共に%CALLNAME:TARGET%の%GET_CLOTH_SHORT_NAME(TEQUIP:65)%のお尻が膨らみ、汚物色に染まっていく…
			ENDIF
		ENDIF
		TFLAG:263 = 1
	ELSE
		PRINTFORML %CALLNAME:TARGET%は肛門栓を抜かれ、腸内の汚物を肛門から撒き散らした。
	ENDIF
	;マゾっ気
	IF ABL:マゾっ気 == 0
		PRINTFORMW %CALLNAME:TARGET%は苦悶と恥辱の表情を浮かべている
	ELSEIF ABL:マゾっ気 == 1
		PRINTFORMW %CALLNAME:TARGET%は脂汗を浮かべ、いまだ下腹に残る排泄感に堪えている。
	ELSEIF ABL:マゾっ気 == 2
		PRINTFORMW %CALLNAME:TARGET%は恥ずかしがりながらも、開放感にほっとした表情を浮かべている。
	ELSEIF ABL:マゾっ気 == 3
		IF TEQUIP:65
			PRINTFORMW %CALLNAME:TARGET%は、%GET_UNDERWARE_NAME(TEQUIP:65)%の中に広がる排泄物の感覚に時折艶っぽい喘ぎ声を上げている。
		ELSE
			PRINTFORMW %CALLNAME:TARGET%は、肛門を通り過ぎる排泄物の感覚に時折艶っぽい喘ぎ声を上げている。
		ENDIF
	ELSEIF ABL:マゾっ気 == 4
		PRINTFORMW %CALLNAME:TARGET%は、うっとりした表情を浮かべ、排泄感を愉しんでいる。
	ELSEIF ABL:マゾっ気 >= 5
		PRINTFORMW %CALLNAME:TARGET%は、排泄感と恥辱を心ゆくまで味わっている。
		IF NO_UNDERWARED()
			PRINTFORMW %CALLNAME:TARGET%の表情は快感にだらしなく緩み、肛門から汚物をひりだすと同時に、膣口からも愛液をボタボタと滴らせている。
		ENDIF
	ENDIF
ELSE
	IF TEQUIP:90
		IF TEQUIP:65
			PRINTFORM %GET_CLOTH_SHORT_NAME(TEQUIP:65)%の中に侵入した
		ENDIF
		PRINTFORMW 極太の触手が%CALLNAME:TARGET%の菊穴をふさぎ、体液を流し込んだ
	ELSE
		IF TFLAG:263
			PRINTFORM 汚物が詰まった
		ENDIF
		PRINTFORM %CALLNAME:TARGET%の
		IF TEQUIP:65
			PRINTFORM %GET_UNDERWARE_NAME(TEQUIP:65)%をずらして
			IF TFLAG:263
				PRINTFORM 再び
			ENDIF
		ENDIF
		PRINTFORMW 菊穴に浣腸液を流し込み、栓をした
	ENDIF
ENDIF

@EQUIP_COM46
;浣腸＋アナルプラグ
IF TEQUIP:90
	PRINTL ＜触手浣腸中＞
ELSE
	PRINTL ＜浣腸＋アナルプラグ挿入中＞
ENDIF

LOSEBASE:体力 += 100
LOSEBASE:気力 += 80

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	A = 80
	B = 300
ELSEIF ABL:Ａ感覚 == 1
	A = 250
	B = 800
ELSEIF ABL:Ａ感覚 == 2
	A = 600
	B = 1400
ELSEIF ABL:Ａ感覚 == 3
	A = 1000
	B = 1800
ELSEIF ABL:Ａ感覚 == 4
	A = 1300
	B = 2100
ELSE
	A = 700 + (ABL:Ａ感覚 * 200)
	B = 1650 + (ABL:Ａ感覚 * 150)
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES A , 0.50
	C = 2000
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES A , 1.00
	C = 300
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES A , 1.10
	C = 50
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES A , 1.20
	C = 10
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES A , 1.40
	C = 0
ELSE
	TIMES A , 1.60
	C = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES A , 0.40
	C += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES A , 0.80
	C += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES A , 1.00
	C += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES A , 1.40
	C += 120
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES A , 1.80
	C += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES A , 1.10
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES A , 1.20
ENDIF

;ABL:従順をみる
IF ABL:親密 == 0
	TIMES A , 0.80
ELSEIF ABL:親密 == 1
	TIMES A , 0.90
ELSEIF ABL:親密 == 2
	TIMES A , 1.00
ELSEIF ABL:親密 == 3
	TIMES A , 1.10
ELSEIF ABL:親密 == 4
	TIMES A , 1.20
ELSE
	A = A * (ABL:親密 * 5 + 105) / 100
ENDIF

;小柄体形
SIF TALENT:小柄体型
	TIMES C , 2.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES B , 1.50
	TIMES C , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES B , 0.60
	TIMES C , 0.60
ENDIF

SOURCE:逸脱 += B

;処女で貞操観念
IF EXP:Ｖ経験 == 0 && TALENT:貞操観念
	B /= 3
ENDIF

SOURCE:快Ａ += A
SOURCE:屈従 += B
SOURCE:痛み += C

EXP:Ａ経験 += 3
PRINTL Ａ経験＋３

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:レズ経験 += 1
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:ＢＬ経験 += 1
ENDIF

SIF TEQUIP:90
	T += 1

PRINTFORML %CALLNAME:TARGET%は大量に浣腸液を注がれ、肛門に栓をされたまま責められ続けている
;マゾっ気
IF ABL:マゾっ気 == 0
	PRINTFORML %CALLNAME:TARGET%は苦悶の表情を浮かべている
ELSEIF ABL:マゾっ気 == 1
	PRINTFORML %CALLNAME:TARGET%は脂汗を浮かべ、排泄感に堪えている
ELSEIF ABL:マゾっ気 == 2
	PRINTFORML %CALLNAME:TARGET%は苦しそうな表情をしながらも、顔を紅潮させもじもじとお尻を振っている
ELSEIF ABL:マゾっ気 == 3
	PRINTFORML %CALLNAME:TARGET%は排泄感に苛まれながらも、時折恍惚とした表情を浮かべている
ELSEIF ABL:マゾっ気 == 4
	PRINTFORML %CALLNAME:TARGET%はうっとりした表情を浮かべ、排泄感と腹痛を愉しんでいる
ELSEIF ABL:マゾっ気 >= 5
	PRINTFORML %CALLNAME:TARGET%は、排泄感を心ゆくまで味わっている
	SIF NO_UNDERWARED()
		PRINTFORML %CALLNAME:TARGET%の表情は快感にだらしなく緩み、その口と膣口からはポタポタと涎を滴らせている
ENDIF

RETURN 1

