@COM9

PRINTL アナル舐め
STR:0 = アナル舐め
CALL TRAIN_MESSAGE_B

;アナル舐め
LOSEBASE:体力 += 5
LOSEBASE:気力 += 50

SOURCE:10 = 50
SOURCE:12 = 300
SOURCE:14 = 500

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:2 = 5
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:2 = 50
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:2 = 200
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:2 = 500
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:2 = 1000
ELSE
	SOURCE:2 = (ABL:Ａ感覚 * 400) - 200
ENDIF

;調教者のTALENT:舌使いをみる
IF TALENT:PLAYER:舌使い
	TIMES SOURCE:2 , 2.00
	SOURCE:16 += SOURCE:0/20
ENDIF

EXP:Ａ経験 += 1
PRINTL Ａ経験＋１

;奴隷のＡ⇔調教者の口の汚れが移動
STAIN:アナル |= STAIN:PLAYER:口
STAIN:PLAYER:口 |= STAIN:アナル

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +3
	EXP:レズ経験 += 3
	EXP:PLAYER:レズ経験 += 3
ENDIF

RETURN 1
