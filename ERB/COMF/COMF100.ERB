;---------------------------------------------------------
;触手調教中
;---------------------------------------------------------
@EQUIP_COM100
;触手調教中
IF TEQUIP:89
	PRINTL ＜触手変身中＞
ELSE
	PRINTL ＜触手召喚中＞
ENDIF

;触手の共通ソース
CALL T_SOURCE

SOURCE:不潔 += A * 10
SOURCE:液体追加 += 2000

TIMES SOURCE:快Ｃ , 2.00
TIMES SOURCE:快Ｖ , 2.00
TIMES SOURCE:快Ａ , 2.00
TIMES SOURCE:快Ｂ , 2.00
TIMES SOURCE:快Ｍ , 2.00
TIMES SOURCE:屈従 , 1.80

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
IF MAXBASE:PLAYER:触手射精 != 0
	B = 0
	;ABL:技巧をみる
	IF ABL:技巧 == 0
		B = 500
	ELSEIF ABL:技巧 == 1
		B = 600
	ELSEIF ABL:技巧 == 2
		B = 800
	ELSEIF ABL:技巧 == 3
		B = 1000
	ELSEIF ABL:技巧 == 4
		B = 1400
	ELSE
		B = 2000
	ENDIF

	;ABL:従順をみる
	IF ABL:親密 == 0
		TIMES B , 0.80
	ELSEIF ABL:親密 == 1
		TIMES B , 0.90
	ELSEIF ABL:親密 == 2
		TIMES B , 1.00
	ELSEIF ABL:親密 == 3
		TIMES B , 1.10
	ELSEIF ABL:親密 == 4
		TIMES B , 1.20
	ELSE
		B = B * (ABL:親密 * 5 + 105) / 100
	ENDIF

	;PALAM:欲情をみる
	IF PALAM:欲情 < PALAMLV:1
		TIMES B , 1.00
	ELSEIF PALAM:欲情 < PALAMLV:2
		TIMES B , 1.10
	ELSEIF PALAM:欲情 < PALAMLV:3
		TIMES B , 1.20
	ELSEIF PALAM:欲情 < PALAMLV:4
		TIMES B , 1.30
	ELSEIF PALAM:欲情 < PALAMLV:5
		TIMES B , 1.40
	ELSE
		TIMES B , 1.50
	ENDIF

	;触手挿入
	SIF TEQUIP:11
		TIMES B , 1.50
	;アナル触手
	SIF TEQUIP:13
		TIMES B , 1.50
	;触手クリ責め
	SIF TEQUIP:14
		TIMES B , 1.20
	;触手乳首責め
	SIF TEQUIP:15
		TIMES B , 1.20
	;触手搾乳
	SIF TEQUIP:16
		TIMES B , 1.30
	;触手ペニス責め
	SIF TEQUIP:17
		TIMES B , 1.10
	;触手緊縛
	SIF TEQUIP:44
		TIMES B , 1.20
	;触手浣腸
	SIF TEQUIP:46
		TIMES B , 1.30
	;触手口辱
	SIF TEQUIP:98
		TIMES B , 1.50

	BASE:PLAYER:触手射精 += B

	S = BASE:PLAYER:触手射精
	EJAC = MAXBASE:PLAYER:触手射精

	IF  S > EJAC * 2
		E = 2
	ELSEIF S > EJAC
		E = 1
	ELSE
		E = 0
	ENDIF

	;Tは触手経験加算に利用
	;各@EQUIPでTに加算していたが、どこでTを初期化されるか分からないのでここでまとめて処理
	;各@EQUIPのT += 1は意味がないがあっても問題ないので修正せず
	T = 0
	;触手挿入(バイブ)
	SIF TEQUIP:11
		T += 1
	;アナル触手(アナルバイブ)
	SIF TEQUIP:13
		T += 1
	;触手クリ責め(クリキャップ)
	SIF TEQUIP:14
		T += 1
	;触手乳首責め(二プルキャップ)
	SIF TEQUIP:15
		T += 1
	;触手搾乳(搾乳器)
	SIF TEQUIP:16
		T += 1
	;触手ペニス責め(オナホ)
	SIF TEQUIP:17
		T += 1
	;触手緊縛(縄)
	SIF TEQUIP:44
		T += 1
	;触手浣腸(浣腸器)
	SIF TEQUIP:46
		T += 1
	;触手口辱
	SIF TEQUIP:98
		T += 1

	;大量射精
	IF E == 2
		EXP:精液経験 += 3
		PRINTL 触手大量射精
		PRINTL 精液経験＋３
		TFLAG:15 = 2
		T += 1

		BASE:PLAYER:触手射精 -= EJAC*2
		SIF BASE:PLAYER:触手射精 >= EJAC
			BASE:PLAYER:触手射精 = EJAC-1

	;通常の射精
	ELSEIF E == 1
		EXP:精液経験 += 1
		PRINTL 触手射精
		PRINTL 精液経験＋１
		TFLAG:15 = 1
		T += 1

		BASE:PLAYER:触手射精 -= EJAC
		SIF BASE:PLAYER:触手射精 >= EJAC
			BASE:PLAYER:触手射精 = EJAC-1
	ENDIF
ENDIF

EXP:異常経験 += 3
PRINTL 異常経験＋3

T += 1
PRINT 触手経験＋
PRINTVL T
EXP:触手経験 += T
T = 0
RETURN 1


