;---------------------------------------------------------
@T_SOURCE
A = 100
;触手変身中
IF TEQUIP:89
	;触手変身中は好感度と触手経験が影響
	IF EXP:触手経験 < EXPLV:1
		TIMES A , 1.80
	ELSEIF EXP:触手経験 < EXPLV:2
		TIMES A , 1.75
	ELSEIF EXP:触手経験 < EXPLV:3
		TIMES A , 1.60
	ELSEIF EXP:触手経験 < EXPLV:4
		TIMES A , 0.90
	ELSEIF EXP:触手経験 < EXPLV:5
		TIMES A , 0.80
	ELSE
		TIMES A , 0.70
	ENDIF

	IF CFLAG:2 < 10000
		TIMES A , 1.80
	ELSEIF CFLAG:2 < 10400
		TIMES A , 1.75
	ELSEIF CFLAG:2 < 12000
		TIMES A , 1.60
	ELSEIF CFLAG:2 < 15000
		TIMES A , 0.90
	ELSEIF CFLAG:2 < 30000
		TIMES A , 0.80
	ELSE
		TIMES A , 0.70
	ENDIF
ELSE
	;触手召喚中は触手経験が影響
	IF EXP:触手経験 < EXPLV:1
		TIMES A , 3.00
	ELSEIF EXP:触手経験 < EXPLV:2
		TIMES A , 2.50
	ELSEIF EXP:触手経験 < EXPLV:3
		TIMES A , 2.00
	ELSEIF EXP:触手経験 < EXPLV:4
		TIMES A , 1.00
	ELSEIF EXP:触手経験 < EXPLV:5
		TIMES A , 0.80
	ELSE
		TIMES A , 0.60
	ENDIF
ENDIF

;触手変身中かつ好感度30000以上なら臆病の影響無し
IF TEQUIP:89 && CFLAG:2 >= 30000
	;臆病
	SIF TALENT:臆病
		TIMES A , 2.00
ELSE
	;臆病
	SIF TALENT:臆病
		TIMES A , 2.00
ENDIF

;感情乏しい
SIF TALENT:感情乏しい
	TIMES A , 0.60

LOSEBASE:体力 += A
LOSEBASE:気力 += A * 2

UP:恐怖 += A * 20
SOURCE:逸脱 += A * 5

