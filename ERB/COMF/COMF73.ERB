@COM73
;スク水プレイ

PRINTL スク水プレイ

;スク水を脱ぐのはいつでも
IF TEQUIP:60
	STR:0 = スク水プレイ終了
	CALL TRAIN_MESSAGE_B
	TEQUIP:60 = 0
	RETURN 1
ENDIF
;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

;-------------------------------------------------
;実行決定
;-------------------------------------------------
STR:0 = スク水プレイ
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 5
LOSEBASE:気力 += 40

A = 500
B = 500

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES A , 1.10
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES A , 1.20
ENDIF
;ABL:露出癖をみる
IF ABL:露出癖 == 0
	SOURCE:中毒充足 += 0
	SOURCE:液体追加 += 0
	TIMES A, 1.00
ELSEIF ABL:露出癖 == 1
	SOURCE:中毒充足 += 50
	SOURCE:液体追加 += 50
	TIMES A, 1.20
ELSEIF ABL:露出癖 == 2
	SOURCE:中毒充足 += 100
	SOURCE:液体追加 += 100
	TIMES A, 1.40
ELSEIF ABL:露出癖 == 3
	SOURCE:中毒充足 += 200
	SOURCE:液体追加 += 200
	TIMES A, 1.60
ELSEIF ABL:露出癖 == 4
	SOURCE:中毒充足 += 400
	SOURCE:液体追加 += 400
	TIMES A, 2.00
ELSE
	SOURCE:中毒充足 += 800
	SOURCE:液体追加 += 800
	TIMES A, 3.00
ENDIF
;ABL:マゾっ気をみる
IF ABL:マゾっ気 == 0
	TIMES A , 0.80
ELSEIF ABL:マゾっ気 == 1
	TIMES A , 1.00
ELSEIF ABL:マゾっ気 == 2
	TIMES A , 1.30
ELSEIF ABL:マゾっ気 == 3
	TIMES A , 1.60
ELSEIF ABL:マゾっ気 == 4
	TIMES A , 2.00
ELSE
	TIMES A , 3.00
ENDIF

;目立ちたがり
SIF TALENT:目立ちたがり
	TIMES A , 1.50
;解放
SIF TALENT:解放
	TIMES A , 1.50

;臆病
SIF TALENT:臆病
	TIMES B , 2.00
;恥じらい
SIF TALENT:恥じらい
	TIMES B , 2.00
;小柄体型
IF TALENT:小柄体型
	TIMES A , 1.50
	TIMES B , 1.50
ENDIF

;巨乳
SIF TALENT:巨乳
	SOURCE:痛み = 500

;幼児／幼児退行、幼稚
SIF TALENT:幼児 || TALENT:幼稚
	TIMES A , 2.00

SOURCE:露出 += A
SOURCE:屈従 += A/2
SOURCE:逸脱 += B

UP:恥情 += A/3
UP:恐怖 += SOURCE:逸脱/2
;貧乳
SIF TALENT:貧乳
	UP:恥情 += A/6
;幼児／幼児退行、幼稚
SIF TALENT:幼児 || TALENT:幼稚
	UP:恐怖 += SOURCE:逸脱/2

;スク水の着用
TEQUIP:60 = 1

RETURN 1

@TRAIN_MESSAGE_COM73
IF TEQUIP:60
	PRINTFORML %CALLNAME:TARGET%が着けているスクール水着を脱がせた……
	PRINTL 　
ELSE
	PRINTFORML %CALLNAME:TARGET%にスクール水着を着けるよう頼んだ
	PRINTFORM %CALLNAME:TARGET%は
	IF TALENT:恥じらい
		PRINTFORML 顔を真っ赤にしつつ%CALLNAME:PLAYER%の要望を実行している……
	ELSEIF TALENT:貧乳
		PRINTL 水着を着けたまではよかったものの、
		PRINTL 自分の胸の貧相さを意識しているのかしきりに胸を隠す動作を繰り返している……
	ELSEIF TALENT:巨乳
		PRINTL 水着を着けたまではよかったものの、
		PRINTL 胸回りが窮屈に感じるのか胸元に手を伸ばしては布地を引っ張る動作を頻繁に繰り返している……
	ELSEIF TALENT:幼児 || TALENT:幼稚
		PRINTL 水着を着けると子供のようにはしゃぎ始めた
	ELSEIF TALENT:淫乱 || TALENT:倒錯的
		PRINTL 一瞬躊躇するそぶりを見せたものの、
		PRINTL 早くも新たな快感に目覚めたのかいそいそと着替え始めた……
	ELSE
		PRINTL 心底呆れたような表情を浮かべながらも要望通り水着を着けていく……
	ENDIF
ENDIF

