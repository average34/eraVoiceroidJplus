;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_73
A = 0
S = 0

;難易度上昇
;ビデオまたは野外で+10、羞恥プレイ中で+7、クリキャップまたはオナホール装着で＋3、
;バイブ系やアナル系の道具で+5、拡張系道具装着でさらに＋3
V = 27
SIF TEQUIP:53 || TEQUIP:54
	V += 10
SIF TEQUIP:57
	V += 7
SIF TEQUIP:14 || TEQUIP:17
	V += 3
SIF TEQUIP:11 || TEQUIP:61 || TEQUIP:71
	V += 5
SIF TEQUIP:13 || TEQUIP:19 || TEQUIP:46 || TEQUIP:47 || TEQUIP:48 || TEQUIP:49 || TEQUIP:70 || TEQUIP:79
	V += 5
SIF TEQUIP:14 || TEQUIP:48 || TEQUIP:70 || TEQUIP:71 || TEQUIP:79 > 2
	V += 3

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	A += ABL:欲望 * 3
ENDIF
;ABL:露出癖
IF ABL:露出癖
	A += ABL:露出癖 * 4
ENDIF
;ABL:自慰中毒
IF ABL:自慰中毒
	A += ABL:自慰中毒 * 3
ENDIF

IF MARK:快楽依存
	A += MARK:快楽依存 * 3
ENDIF


;PALAM:欲情
IF PALAM:欲情 < PALAMLV:1
	L = 0
ELSEIF PALAM:欲情 < PALAMLV:2
	L = 1
ELSEIF PALAM:欲情 < PALAMLV:3
	L = 2
ELSEIF PALAM:欲情 < PALAMLV:4
	L = 3
ELSEIF PALAM:欲情 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 3
ENDIF

;自制心
IF TALENT:自制心
	A -= 5
ENDIF
;恥じらい
IF TALENT:恥じらい
	A -= 5
ENDIF
;恥薄い
IF TALENT:恥薄い
	A += 2
ENDIF
;自慰しやすい
IF TALENT:自慰しやすい
	A += 5
ENDIF
;快感に素直
IF TALENT:快感に素直
	A += 5
ENDIF
;快感の否定
IF TALENT:快感の否定
	A -= 5
ENDIF

;媚薬
IF TEQUIP:21
	A += 8
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 3,')
		S = 1
	ENDIF
	;ABL:露出癖
	IF ABL:露出癖
		SIF S
			PRINT  + 
		PRINTS ABLNAME:7
		PRINTV 'LV,ABL:露出癖,'(,ABL:露出癖 * 4,')
		S = 1
	ENDIF
	;ABL:自慰中毒
	IF ABL:自慰中毒
		SIF S
			PRINT  + 
		PRINTS ABLNAME:10
		PRINTV 'LV,ABL:自慰中毒,'(,ABL:自慰中毒 * 3,')
		S = 1
	ENDIF

	IF MARK:快楽依存
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:快楽依存,'(,MARK:快楽依存 * 3,')
		S = 1
	ENDIF


	;PALAM:欲情
	IF PALAM:欲情 < PALAMLV:1
		L = 0
	ELSEIF PALAM:欲情 < PALAMLV:2
		L = 1
	ELSEIF PALAM:欲情 < PALAMLV:3
		L = 2
	ELSEIF PALAM:欲情 < PALAMLV:4
		L = 3
	ELSEIF PALAM:欲情 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 3,')
		S = 1
	ENDIF

	;自制心
	IF TALENT:自制心
		PRINT  - 
		PRINTS TALENTNAME:20
		PRINTV '(,5,')
		S = 1
	ENDIF
	;恥じらい
	IF TALENT:恥じらい
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,5,')
		S = 1
	ENDIF
	;恥薄い
	IF TALENT:恥薄い
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:36
		PRINTV '(,2,')
		S = 1
	ENDIF
	;自慰しやすい
	IF TALENT:自慰しやすい
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:60
		PRINTV '(,5,')
		S = 1
	ENDIF
	;快感に素直
	IF TALENT:快感に素直
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:70
		PRINTV '(,5,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:快感の否定
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,5,')
		S = 1
	ENDIF

	;媚薬
	IF TEQUIP:21
		SIF S
			PRINT  + 
		PRINTS ITEMNAME:26
		PRINTV '(,8,')
		S = 1
	ENDIF

	;'合計を表示(27以上で実行)
	PRINT  = 
	PRINTV A

	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF

