@COM74
;公開野外ライブ

PRINTL 公開野外ライブ

;-------------------------------------------------
;成功できるかの判定
;-------------------------------------------------
A = 0
S = 0

;ABL:欲望
SIF ABL:欲望
	A += ABL:欲望
;ABL:露出癖
SIF ABL:露出癖
	A += ABL:露出癖 * 2
;ABL:歌唱技能
SIF ABL:歌唱技能
	A += ABL:歌唱技能 * 3

SIF MARK:快楽依存
	A += MARK:快楽依存 * 2
SIF MARK:反発感情
	A -= MARK:反発感情 * 5

;PALAM:欲情
IF PALAM:欲情 < PALAMLV:1
	L = 0
ELSEIF PALAM:欲情 < PALAMLV:2
	L = 1
ELSEIF PALAM:欲情 < PALAMLV:3
	L = 2
ELSEIF PALAM:欲情 < PALAMLV:4
	L = 3
ELSEIF PALAM:欲情 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
SIF L
	A += L * 2

;自制心
SIF TALENT:自制心
	A -= 5
;目立ちたがり
SIF TALENT:目立ちたがり
	A += 5
;恥じらい
SIF TALENT:恥じらい
	A -= 5
;恥薄い
SIF TALENT:恥薄い
	A += 2

;合計を表示(37以上で成功、未満で失敗イベントへ)
;難易度上昇
;ビデオで+10、クリキャップまたはオナホール装着で＋4、
;バイブ系やアナル系の道具で+5、拡張系道具装着でさらに＋3
V = 37
SIF TEQUIP:53
	V += 10
SIF TEQUIP:14 || TEQUIP:17
	V += 4
SIF TEQUIP:11 || TEQUIP:61 || TEQUIP:71
	V += 5
SIF TEQUIP:13 || TEQUIP:19 || TEQUIP:46 || TEQUIP:47 || TEQUIP:48 || TEQUIP:49 || TEQUIP:70 || TEQUIP:79
	V += 5
SIF TEQUIP:14 || TEQUIP:48 || TEQUIP:70 || TEQUIP:71 || TEQUIP:79 > 2
	V += 3

;ランダム要素
V += RAND:41
B = 1 + RAND:3
A /= B

;成功、失敗
IF A < V
	TFLAG:18 = -1
	CALL COM74_FAIL
ELSE
	TFLAG:18 = 0
	CALL COM74_SUCCESS
ENDIF

RETURN 1

@COM74_SUCCESS
;-------------------------------------------------
;実行決定
;-------------------------------------------------
STR:0 = 公開野外ライブ

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 500
LOSEBASE:気力 += 500

A = 750
B = 900
C = 0

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES A , 0.80
	C += 0
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES A , 0.90
	C += 1
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES A , 1.00
	C += 2
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES A , 1.10
	C += 4
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES A , 1.20
	C += 8
ENDIF
;ABL:露出癖をみる
IF ABL:露出癖 == 0
	SOURCE:中毒充足 += 0
	SOURCE:液体追加 += 0
	C += 0
	TIMES A, 1.00
ELSEIF ABL:露出癖 == 1
	SOURCE:中毒充足 += 50
	SOURCE:液体追加 += 50
	C += 1
	TIMES A, 1.20
ELSEIF ABL:露出癖 == 2
	SOURCE:中毒充足 += 100
	SOURCE:液体追加 += 100
	C += 2
	TIMES A, 1.40
ELSEIF ABL:露出癖 == 3
	SOURCE:中毒充足 += 200
	SOURCE:液体追加 += 200
	C += 3
	TIMES A, 1.60
ELSEIF ABL:露出癖 == 4
	SOURCE:中毒充足 += 400
	SOURCE:液体追加 += 400
	C += 4
	TIMES A, 2.00
ELSE
	SOURCE:中毒充足 += 800
	SOURCE:液体追加 += 800
	C += 5
	TIMES A, 3.00
ENDIF
;ABL:歌唱技能をみる
IF ABL:歌唱技能 == 0
	C += 0
	TIMES A , 0.80
ELSEIF ABL:歌唱技能 == 1
	C += 1
	TIMES A , 1.00
ELSEIF ABL:歌唱技能 == 2
	C += 2
	TIMES A , 1.30
ELSEIF ABL:歌唱技能 == 3
	C += 3
	TIMES A , 1.60
ELSEIF ABL:歌唱技能 == 4
	C += 4
	TIMES A , 2.00
ELSE
	C += 5
	TIMES A , 3.00
ENDIF

;目立ちたがり
IF TALENT:目立ちたがり
	TIMES A , 1.50
	C += 10
ENDIF
;解放
IF TALENT:解放
	TIMES A , 1.50
	C += 5
ENDIF
;臆病
IF TALENT:臆病
	TIMES B , 2.00
	C -= 10
ENDIF
;恥じらい
IF TALENT:恥じらい
	TIMES B , 2.00
	C -= 7
ENDIF
;恥じ薄い
IF TALENT:恥薄い
	TIMES B , 0.80
	C += 7
ENDIF

;魅惑
SIF TALENT:魅惑
	C += 10
;謎の魅力
SIF TALENT:謎の魅力
	C += 25
;魅力
SIF TALENT:魅力
	C += 20
;鼓舞
SIF TALENT:鼓舞
	C += 50
;人気
SIF TALENT:人気
	C += 30

SOURCE:露出 += A/2
SOURCE:屈従 += A/4
SOURCE:逸脱 += B

UP:習得 += A/3
UP:恥情 += A/4
UP:恐怖 += SOURCE:逸脱/2

;TEQUIPごとにC加算し、レイプ派生判定や経験加算に使用する
IF TEQUIP:43
	C += 10
ENDIF
IF TEQUIP:14
	C += 13
ENDIF
IF TEQUIP:15 || TEQUIP:16
	C += 15
ENDIF
IF TEQUIP:11
	C += 25
ELSEIF TEQUIP:71
	C += 29
ENDIF
IF TEQUIP:43
	IF TEQUIP:13
	ELSEIF TEQUIP:46 || TEQUIP:47
		C += 5
	ELSEIF TEQUIP:48
		C += 5
	ELSEIF TEQUIP:70
		C += 5
	ELSEIF TEQUIP:79
		C += 5
	ENDIF
	C += 20
ENDIF
IF TEQUIP:60
	C += 20
ELSEIF TEQUIP:76
	C += 15
ENDIF
T = C/5

C /= 1 + RAND:3
IF RAND:3 != 0
	C += RAND:21
ELSE
	C -= RAND:21
	SIF C < 0
		C = 0
ENDIF

CALL TRAIN_MESSAGE_B

IF C > 60
	S = 1
	TFLAG:18 = 1
	CALL COM74_CHAOS
ELSE
	SOURCE:達成感 += 7500
ENDIF

T += 1 + RAND:5

PRINTS EXPNAME:93
PRINTFORML +{T}
EXP:歌唱経験 += T

RETURN 1



