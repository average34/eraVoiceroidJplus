@COM74_CHAOS
A = 0
B = 0
C = 0
V = 0

D = 0
E = 0
F = 3 + RAND:5
M = RAND:16
N = 2 + RAND:9
O = 0

IF M > 6
;Ａ感覚をみる
	IF ABL:Ａ感覚 == 0
		A = 40
	ELSEIF ABL:Ａ感覚 == 1
		A = 60
	ELSEIF ABL:Ａ感覚 == 2
		A = 90
	ELSEIF ABL:Ａ感覚 == 3
		A = 130
	ELSEIF ABL:Ａ感覚 == 4
		A = 180
	ELSE
		A = 240
	ENDIF
	D = 2 + RAND:5
	A *= D
ENDIF

;Ｂ感覚をみる
IF ABL:Ｂ感覚 == 0
	B = 50
ELSEIF ABL:Ｂ感覚 == 1
	B = 80
ELSEIF ABL:Ｂ感覚 == 2
	B = 160
ELSEIF ABL:Ｂ感覚 == 3
	B = 240
ELSEIF ABL:Ｂ感覚 == 4
	B = 320
ELSE
	B = 480
ENDIF
B *= N

;Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	C = 100
ELSEIF ABL:Ｃ感覚 == 1
	C = 130
ELSEIF ABL:Ｃ感覚 == 2
	C = 170
ELSEIF ABL:Ｃ感覚 == 3
	C = 250
ELSEIF ABL:Ｃ感覚 == 4
	C = 360
ELSE
	C = 500
ENDIF
C *= N

IF M < 10
;V経験を伴うコマンドのフラグ(処女膜再生対応)
	TFLAG:19 = 1
;Ｖ感覚をみる
	IF ABL:Ｖ感覚 == 0
		V = 100
	ELSEIF ABL:Ｖ感覚 == 1
		V = 120
	ELSEIF ABL:Ｖ感覚 == 2
		V = 150
	ELSEIF ABL:Ｖ感覚 == 3
		V = 180
	ELSEIF ABL:Ｖ感覚 == 4
		V = 200
	ELSE
		V = 250
	ENDIF
	E = 2 + RAND:5
	SIF TALENT:処女
		V *= E
ENDIF

O = D+E+F+(N/2)

CALL TRAIN_MESSAGE_B

IF E > 0
	EXP:Ｖ経験 += E
	IF TALENT:処女 && EXP:Ｖ経験 == 0
		EXP:異常経験 += 3
		PRINTFORML 異常経験＋3
	ENDIF
	PRINTFORML Ｖ経験＋{E}
	STAIN:ヴァギナ |= 2
	STAIN:ヴァギナ |= 4
ENDIF
IF D > 0
	EXP:Ａ経験 += D
	PRINTFORML Ａ経験＋{D}
	STAIN:アナル |= 2
	STAIN:アナル |= 4
ENDIF
EXP:フェラ経験 += F
PRINTFORML フェラ経験＋{F}
SOURCE:痛み += 150 * N
SOURCE:逸脱 += 150 * N
SOURCE:恭順追加 += 150 * N
SOURCE:性行動 += 310 * N
SOURCE:達成感 += 370 * N
STAIN:口 |= 2
STAIN:口 |= 4
IF (D+E+F)/2 > 0
	EXP:精液経験 += (D+E+F)/2
	PRINTFORML 精液経験＋{(D+E+F)/2}
ENDIF
SOURCE:快Ｃ += C
SOURCE:快Ｖ += V
SOURCE:快Ａ += A
SOURCE:快Ｂ += B
SOURCE:屈従 += 750 * O
UP:恐怖 += 330 * O

RETURN 1

