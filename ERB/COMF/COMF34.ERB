			A = 4
		ELSEIF PALAM:5 >= PALAMLV:4
			A = 5
		ENDIF
		B = RAND:11
		A *= B
		B = RAND:11
		A += B*ABL:Ｖ感覚
		;騎乗位だと50以上で子宮口
		IF A >= 50
			JUMP COM256
		ELSE
			JUMP COM255
		ENDIF
	ENDIF
