@COM205
;自慰見せつけ

;対象が恋慕さとりだと変化
IF NAME_CSV(NO:TARGET, 0) == "さとり" && TALENT:85
	PRINTL 読心自慰見せつけ
ELSE
	PRINTL 自慰見せつけ
ENDIF

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = 自慰見せつけ
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
SIF TALENT:PLAYER:119 || TALENT:PLAYER:122 || TALENT:PLAYER:121
	BASE:PLAYER:2 += CALC_EJAC(SELECTCOM, PLAYER)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 5
LOSEBASE:1 += 20

;SOURCE:8(不潔)、SOURCE:12(露出) SOURCE:14(逸脱)
SOURCE:8 = 100
SOURCE:12 = 500
SOURCE:14 = 450

;上のほうで計算した汚れデータ
SOURCE:8 = Y*10 + 60

;ABL:10(自慰中毒)をみる
; SOURCE:4(性行動)は自慰中毒が高くなるにつれ上がりやすくSOURCE:13(屈従)とSOURCE:8(不潔)は上がりにくくなる
IF ABL:10 == 0
	SOURCE:4 = 250
	SOURCE:13 = 100
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:10 == 1
	SOURCE:4 = 300
	SOURCE:13 = 80
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:10 == 2
	SOURCE:4 = 350
	SOURCE:13 = 0
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:10 == 3
	SOURCE:4 = 400
	SOURCE:13 = 60
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:10 == 4
	SOURCE:4 = 450
	SOURCE:13 = 50
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:4 = 375 + (ABL:6 * 25)
	SOURCE:13 = 0
	TIMES SOURCE:8 , 0.10
ENDIF

;PALAM:5(欲情)をみる
IF PALAM:5 < PALAMLV:1
	SOURCE:11 = 25
ELSEIF PALAM:5 < PALAMLV:2
	SOURCE:11 = 50
ELSEIF PALAM:5 < PALAMLV:3
	SOURCE:11 = 100
ELSEIF PALAM:5 < PALAMLV:4
	SOURCE:11 = 200
ELSE
	SOURCE:11 = 400
ENDIF

;-------------------------------------------------
;読心自慰見せつけの場合効果アップ（2倍）
;-------------------------------------------------
;対象が恋慕さとりだと変化(性行動と欲情追加の上昇に補正)
IF NAME_CSV(NO:TARGET, 0) == "さとり" && TALENT:85
	TIMES SOURCE:4 , 2.00
	TIMES SOURCE:11 , 2.00
ENDIF


;好感度ランダム変化
A = RAND:10 + 5
SIF TALENT:85 || TALENT:153
	A += 2
SIF TALENT:88
	A += 3
;合意がないと好感度が下がる
IF TALENT:152 == 0 && TFLAG:48 == 0
	A -= 8
	SOURCE:15 += 300
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK
;;オーバーフロー対策に、値が大きすぎる場合と、負の場合は調整を加える
;IF BASE:PLAYER:2 > 100000000 || BASE:PLAYER:2 < 0
;	BASE:PLAYER:2 = 100000000
;ENDIF
;S = BASE:PLAYER:2
;EJAC = MAXBASE:PLAYER:2
;
;IF  S > EJAC * 2
;	E = 2
;ELSEIF S > EJAC
;	E = 1
;ELSE
;	E = 0
;ENDIF

IF E >= 1
;射精している
	TIMES SOURCE:4 , 3.00

	;ABL:11(精液中毒)をみる　SOURCE:7(中毒充足)　SOURCE:13(屈従)
	IF ABL:11 == 0
		SOURCE:7 = 0
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:11 == 1
		SOURCE:7 = 200
		TIMES SOURCE:13, 1.60
	ELSEIF ABL:11 == 2
		SOURCE:7 = 600
		TIMES SOURCE:13, 1.00
	ELSEIF ABL:11 == 3
		SOURCE:7 = 1500
		TIMES SOURCE:13, 0.70
	ELSEIF ABL:11 == 4
		SOURCE:7 = 3000
		TIMES SOURCE:13, 0.40
	ELSE
		SOURCE:7 = (ABL:11 * 1500) - 1000
		TIMES SOURCE:13, 0.10
	ENDIF
	
	;射精先タイプの設定
	TFLAG:703 = 3
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;調教者の指⇔調教者のＰの汚れが移動
IF TALENT:PLAYER:119 || TALENT:PLAYER:122  || TALENT:PLAYER:121
	STAIN:PLAYER:1 |= STAIN:PLAYER:2
	STAIN:PLAYER:2 |= STAIN:PLAYER:1
;調教者の指⇔調教者のＶの汚れが移動
ELSE
	STAIN:PLAYER:3 |= STAIN:PLAYER:2
	STAIN:PLAYER:2 |= STAIN:PLAYER:3
ENDIF

;レズ・BL経験関連
;女性同士
IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTFORML +1 
	PRINTS EXPNAME:40
	PRINTFORML +1 (%CALLNAME:PLAYER%)
	EXP:40 += 1
	EXP:PLAYER:40 += 1
;男同士
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTFORML +1
	PRINTS EXPNAME:40
	PRINTFORML +1 (%CALLNAME:PLAYER%)
	EXP:41 += 1
	EXP:PLAYER:41 += 1
ENDIF

;自慰経験の追加
PRINTS EXPNAME:10
PRINTFORML +1 (%CALLNAME:PLAYER%)
EXP:PLAYER:10 += 1


TFLAG:100 = 1

;既成事実2に相当
TFLAG:200 = 2

RETURN 1

