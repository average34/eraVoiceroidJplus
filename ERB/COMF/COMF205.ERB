	ENDIF

	;恥じらい
	IF TALENT:35
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,1,')
		S = 1
	ENDIF
	;汚臭鈍感
	IF TALENT:61
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:61
		PRINTV '(,1,')
		S = 1
	ENDIF
	;汚臭敏感
