ELSE
	SIF SELECTCOM == 120 || SELECTCOM == 121 || SELECTCOM == 122 || SELECTCOM == 123
		A = 1
	SIF SELECTCOM == 22 || SELECTCOM == 26 || SELECTCOM == 27 || SELECTCOM == 28 || SELECTCOM == 29
		A = 2
	SIF PREVCOM == 120 || PREVCOM == 121 || PREVCOM == 122 || PREVCOM == 123
		B = 1
	SIF PREVCOM == 22 || PREVCOM == 26 || PREVCOM == 27 || PREVCOM == 28 || PREVCOM == 29
		B = 2
	;前回コマンドが3Pでない
	IF TFLAG:42 == 0
		;今回調教者が助手=前回調教者が主人
		IF ASSIPLAY
			TFLAG:40 = B
			TFLAG:41 = A
		;今回調教者が主人=前回調教者が助手
		ELSE
			TFLAG:40 = A
			TFLAG:41 = B
		ENDIF
	;前回コマンドが3Pである
	ELSE
		;今回調教者が助手=前回調教者が主人
		IF ASSIPLAY
			;助手の使用部位が前回の主人と同じ場合
			IF A == TFLAG:40
				;部位入れ替えする+今回キスはめしない(Aコマンド)場合は3Pへ
				IF A == 2
					TFLAG:42 = 1
					JUMP COM64
				ENDIF 
				TFLAG:40 = TFLAG:41
			ENDIF
			TFLAG:41 = A
		ELSE
			;主人の使用部位が前回の助手と同じ場合
			IF A == TFLAG:41
				;部位入れ替えする+今回キスはめしない(Aコマンド)場合は3Pへ
				IF A == 2
					TFLAG:42 = 1
					JUMP COM64
				ENDIF 
				TFLAG:41 = TFLAG:40
			ENDIF
			TFLAG:40 = A
		ENDIF
	ENDIF
ENDIF

