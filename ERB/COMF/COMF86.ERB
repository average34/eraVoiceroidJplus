@COM86
;強制放尿
IF TEQUIP:65
	PRINTL 強制着衣放尿
	STR:0 = 強制着衣放尿
ELSE
	PRINTL 強制放尿
	STR:0 = 強制放尿
ENDIF

SELECTCOM = 86
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 10
LOSEBASE:気力 += 150

SOURCE:逸脱 = 5000
SOURCE:反感追加 = 500
SOURCE:達成感 = 3000

;ABL:Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	SOURCE:快Ｃ = 10
ELSEIF ABL:Ｃ感覚 == 1
	SOURCE:快Ｃ = 120
ELSEIF ABL:Ｃ感覚 == 2
	SOURCE:快Ｃ = 500
ELSEIF ABL:Ｃ感覚 == 3
	SOURCE:快Ｃ = 1200
ELSEIF ABL:Ｃ感覚 == 4
	SOURCE:快Ｃ = 2000
ELSE
	SOURCE:快Ｃ = 500 + (ABL:Ｃ感覚 * 500)
ENDIF

;ABL:プレイヤーの技巧をみる
IF ABL:PLAYER:技巧 == 0
	SOURCE:快Ｃ = 10
	SOURCE:情愛 = 0
	SOURCE:性行動 = 20
	SOURCE:欲情追加 = 30
ELSEIF ABL:PLAYER:技巧 == 1
	SOURCE:快Ｃ = 100
	SOURCE:情愛 = 50
	SOURCE:性行動 = 150
	SOURCE:欲情追加 = 100
ELSEIF ABL:PLAYER:技巧 == 2
	SOURCE:快Ｃ = 200
	SOURCE:情愛 = 100
	SOURCE:性行動 = 400
	SOURCE:欲情追加 = 300
ELSEIF ABL:PLAYER:技巧 == 3
	SOURCE:快Ｃ = 300
	SOURCE:情愛 = 150
	SOURCE:性行動 = 700
	SOURCE:欲情追加 = 700
ELSEIF ABL:PLAYER:技巧 == 4
	SOURCE:快Ｃ = 400
	SOURCE:情愛 = 300
	SOURCE:性行動 = 1200
	SOURCE:欲情追加 = 1500
ELSE
	SOURCE:快Ｃ = 250 + (ABL:PLAYER:技巧 * 50)
	SOURCE:情愛 = ABL:PLAYER:技巧 * 100
	SOURCE:性行動 = 600 + (ABL:PLAYER:技巧 * 200)
	SOURCE:欲情追加 = 150 + (ABL:PLAYER:技巧 * 450)
ENDIF

;ABL:サドっ気をみる
IF ABL:PLAYER:サドっ気 == 0
	TIMES SOURCE:快Ｃ , 1.00
	TIMES SOURCE:情愛 , 1.00
	TIMES SOURCE:性行動 , 1.00
	TIMES SOURCE:欲情追加 , 1.00
ELSEIF ABL:PLAYER:サドっ気 == 1
	TIMES SOURCE:快Ｃ , 1.05
	TIMES SOURCE:情愛 , 1.05
	TIMES SOURCE:性行動 , 1.05
	TIMES SOURCE:欲情追加 , 1.05
ELSEIF ABL:PLAYER:サドっ気 == 2
	TIMES SOURCE:快Ｃ , 1.10
	TIMES SOURCE:情愛 , 1.10
	TIMES SOURCE:性行動 , 1.10
	TIMES SOURCE:欲情追加 , 1.10
ELSEIF ABL:PLAYER:サドっ気 == 3
	TIMES SOURCE:快Ｃ , 1.20
	TIMES SOURCE:情愛 , 1.20
	TIMES SOURCE:性行動 , 1.20
	TIMES SOURCE:欲情追加 , 1.20
ELSEIF ABL:PLAYER:サドっ気 == 4
	TIMES SOURCE:快Ｃ , 1.30
	TIMES SOURCE:情愛 , 1.30
	TIMES SOURCE:性行動 , 1.30
	TIMES SOURCE:欲情追加 , 1.30
ELSE
	SOURCE:快Ｃ = SOURCE:快Ｃ * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
	SOURCE:情愛 = SOURCE:情愛 * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
	SOURCE:性行動 = SOURCE:性行動 * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
	SOURCE:欲情追加 = SOURCE:欲情追加 * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	SOURCE:露出 = 1800
ELSEIF PALAM:欲情 < PALAMLV:2
	SOURCE:露出 = 2000
ELSEIF PALAM:欲情 < PALAMLV:3
	SOURCE:露出 = 2400
ELSEIF PALAM:欲情 < PALAMLV:4
	SOURCE:露出 = 2800
ELSEIF PALAM:欲情 >= PALAMLV:4
	SOURCE:露出 = 3600
ENDIF

;露出癖をみる
IF ABL:露出癖 == 0
	SOURCE:屈従 = 6000
	SOURCE:性行動 = 100
	SOURCE:不潔 = 1000
ELSEIF ABL:露出癖 == 1
	SOURCE:屈従 = 5000
	SOURCE:性行動 = 200
	SOURCE:不潔 = 800
ELSEIF ABL:露出癖 == 2
	SOURCE:屈従 = 4000
	SOURCE:性行動 = 300
	SOURCE:不潔 = 600
ELSEIF ABL:露出癖 == 3
	SOURCE:屈従 = 3000
	SOURCE:性行動 = 400
	SOURCE:不潔 = 400
ELSEIF ABL:露出癖 == 4
	SOURCE:屈従 = 2000
	SOURCE:性行動 = 500
	SOURCE:不潔 = 200
ELSE
	SOURCE:屈従 = 1000
	SOURCE:性行動 = 350 + (ABL:露出癖 * 50)
	SOURCE:不潔 = 50
ENDIF

;ABL:マゾっ気をみる
IF ABL:マゾっ気 == 0
	TIMES SOURCE:情愛 , 0.60
	TIMES SOURCE:露出 , 0.80
	SOURCE:液体追加 = 100
	SOURCE:欲情追加 = 100
ELSEIF ABL:マゾっ気 == 1
	TIMES SOURCE:情愛 , 1.00
	TIMES SOURCE:露出 , 1.00
	SOURCE:液体追加 = 200
	SOURCE:欲情追加 = 300
ELSEIF ABL:マゾっ気 == 2
	TIMES SOURCE:情愛 , 1.50
	TIMES SOURCE:露出 , 1.20
	SOURCE:液体追加 = 400
	SOURCE:欲情追加 = 700
ELSEIF ABL:マゾっ気 == 3
	TIMES SOURCE:情愛 , 2.00
	TIMES SOURCE:露出 , 1.40
	SOURCE:液体追加 = 700
	SOURCE:欲情追加 = 1200
ELSEIF ABL:マゾっ気 == 4
	TIMES SOURCE:情愛 , 2.70
	TIMES SOURCE:露出 , 1.50
	SOURCE:液体追加 = 1100
	SOURCE:欲情追加 = 1800
ELSE
	SOURCE:情愛 = SOURCE:情愛 * (ABL:マゾっ気 * 45 + 135) / 100
	SOURCE:露出 = SOURCE:露出 * (ABL:マゾっ気 * 10 + 120) / 100
	SOURCE:液体追加 = 500 + (ABL:マゾっ気 * 200)
	SOURCE:欲情追加 = 750 + (ABL:マゾっ気 * 350)
ENDIF

;場所をみる
IF TEQUIP:54
	TIMES SOURCE:逸脱 , 2.50
ELSEIF TEQUIP:57
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TEQUIP:58
	TIMES SOURCE:逸脱 , 0.50
ENDIF

A = 100
IF EXP:放尿経験 < EXPLV:1
	TIMES A , 3.00
ELSEIF EXP:放尿経験 < EXPLV:2
	TIMES A , 2.50
ELSEIF EXP:放尿経験 < EXPLV:3
	TIMES A , 2.00
ELSEIF EXP:放尿経験 < EXPLV:4
	TIMES A , 1.00
ELSEIF EXP:放尿経験 < EXPLV:5
	TIMES A , 0.80
ELSE
	TIMES A , 0.60
ENDIF

;臆病
SIF TALENT:臆病
	TIMES A , 1.20

;反抗的
SIF TALENT:反抗的
	TIMES A , 2.00

;プライド高い
SIF TALENT:プライド高い
	TIMES A , 2.00

;プライド低い
SIF TALENT:プライド低い
	TIMES A , 0.80

;自制心
SIF TALENT:自制心
	TIMES A , 1.50

;感情乏しい
SIF TALENT:感情乏しい
	TIMES A , 0.60

;目立ちたがり
SIF TALENT:目立ちたがり
	TIMES A , 0.80

;抑圧
SIF TALENT:抑圧
	TIMES A , 3.00

;抵抗
SIF TALENT:抵抗
	TIMES A , 3.00

;恥じらい
SIF TALENT:恥じらい
	TIMES A , 3.50

;恥薄い
SIF TALENT:恥薄い
	TIMES A , 0.50

UP:恐怖 += A * 3
UP:反感 += A * 2
SOURCE:逸脱 += A * 5

;焼き芋弁当パッチ専用イベント(黄金水入手)
;CALL SWEET_POTATO_GW

PRINTFORML %EXPNAME:31%＋２
EXP:放尿経験 += 2

;既成事実２に相当
TFLAG:200 = 2

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:レズ経験 += 1
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:ＢＬ経験 += 1
ENDIF

SIF TEQUIP:90
	T += 1

SIF PALAM:欲情 >= PALAMLV:3
	TFLAG:200 = 1

IF TEQUIP:22
	TEQUIP:22 = 0
ENDIF


RETURN 1
