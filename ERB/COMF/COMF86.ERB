@COM86
;強制放尿
IF TEQUIP:65
	PRINTL 強制着衣放尿
	STR:0 = 強制着衣放尿
ELSE
	PRINTL 強制放尿
	STR:0 = 強制放尿
ENDIF

SELECTCOM = 86
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 10
LOSEBASE:気力 += 150

SOURCE:14 = 5000
SOURCE:15 = 500
SOURCE:5 = 3000

;ABL:Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	SOURCE:0 = 10
ELSEIF ABL:Ｃ感覚 == 1
	SOURCE:0 = 120
ELSEIF ABL:Ｃ感覚 == 2
	SOURCE:0 = 500
ELSEIF ABL:Ｃ感覚 == 3
	SOURCE:0 = 1200
ELSEIF ABL:Ｃ感覚 == 4
	SOURCE:0 = 2000
ELSE
	SOURCE:0 = 500 + (ABL:Ｃ感覚 * 500)
ENDIF

;ABL:プレイヤーの技巧をみる
IF ABL:PLAYER:技巧 == 0
	SOURCE:0 = 10
	SOURCE:3 = 0
	SOURCE:4 = 20
	SOURCE:11 = 30
ELSEIF ABL:PLAYER:技巧 == 1
	SOURCE:0 = 100
	SOURCE:3 = 50
	SOURCE:4 = 150
	SOURCE:11 = 100
ELSEIF ABL:PLAYER:技巧 == 2
	SOURCE:0 = 200
	SOURCE:3 = 100
	SOURCE:4 = 400
	SOURCE:11 = 300
ELSEIF ABL:PLAYER:技巧 == 3
	SOURCE:0 = 300
	SOURCE:3 = 150
	SOURCE:4 = 700
	SOURCE:11 = 700
ELSEIF ABL:PLAYER:技巧 == 4
	SOURCE:0 = 400
	SOURCE:3 = 300
	SOURCE:4 = 1200
	SOURCE:11 = 1500
ELSE
	SOURCE:0 = 250 + (ABL:PLAYER:技巧 * 50)
	SOURCE:3 = ABL:PLAYER:技巧 * 100
	SOURCE:4 = 600 + (ABL:PLAYER:技巧 * 200)
	SOURCE:11 = 150 + (ABL:PLAYER:技巧 * 450)
ENDIF

;ABL:サドっ気をみる
IF ABL:PLAYER:サドっ気 == 0
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:11 , 1.00
ELSEIF ABL:PLAYER:サドっ気 == 1
	TIMES SOURCE:0 , 1.05
	TIMES SOURCE:3 , 1.05
	TIMES SOURCE:4 , 1.05
	TIMES SOURCE:11 , 1.05
ELSEIF ABL:PLAYER:サドっ気 == 2
	TIMES SOURCE:0 , 1.10
	TIMES SOURCE:3 , 1.10
	TIMES SOURCE:4 , 1.10
	TIMES SOURCE:11 , 1.10
ELSEIF ABL:PLAYER:サドっ気 == 3
	TIMES SOURCE:0 , 1.20
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:11 , 1.20
ELSEIF ABL:PLAYER:サドっ気 == 4
	TIMES SOURCE:0 , 1.30
	TIMES SOURCE:3 , 1.30
	TIMES SOURCE:4 , 1.30
	TIMES SOURCE:11 , 1.30
ELSE
	SOURCE:0 = SOURCE:0 * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
	SOURCE:3 = SOURCE:3 * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
	SOURCE:4 = SOURCE:4 * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
	SOURCE:11 = SOURCE:11 * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	SOURCE:12 = 1800
ELSEIF PALAM:5 < PALAMLV:2
	SOURCE:12 = 2000
ELSEIF PALAM:5 < PALAMLV:3
	SOURCE:12 = 2400
ELSEIF PALAM:5 < PALAMLV:4
	SOURCE:12 = 2800
ELSEIF PALAM:5 >= PALAMLV:4
	SOURCE:12 = 3600
ENDIF

;露出癖をみる
IF ABL:露出癖 == 0
	SOURCE:13 = 6000
	SOURCE:4 = 100
	SOURCE:8 = 1000
ELSEIF ABL:露出癖 == 1
	SOURCE:13 = 5000
	SOURCE:4 = 200
	SOURCE:8 = 800
ELSEIF ABL:露出癖 == 2
	SOURCE:13 = 4000
	SOURCE:4 = 300
	SOURCE:8 = 600
ELSEIF ABL:露出癖 == 3
	SOURCE:13 = 3000
	SOURCE:4 = 400
	SOURCE:8 = 400
ELSEIF ABL:露出癖 == 4
	SOURCE:13 = 2000
	SOURCE:4 = 500
	SOURCE:8 = 200
ELSE
	SOURCE:13 = 1000
	SOURCE:4 = 350 + (ABL:露出癖 * 50)
	SOURCE:8 = 50
ENDIF

;ABL:マゾっ気をみる
IF ABL:マゾっ気 == 0
	TIMES SOURCE:3 , 0.60
	TIMES SOURCE:12 , 0.80
	SOURCE:10 = 100
	SOURCE:11 = 100
ELSEIF ABL:マゾっ気 == 1
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:12 , 1.00
	SOURCE:10 = 200
	SOURCE:11 = 300
ELSEIF ABL:マゾっ気 == 2
	TIMES SOURCE:3 , 1.50
	TIMES SOURCE:12 , 1.20
	SOURCE:10 = 400
	SOURCE:11 = 700
ELSEIF ABL:マゾっ気 == 3
	TIMES SOURCE:3 , 2.00
	TIMES SOURCE:12 , 1.40
	SOURCE:10 = 700
	SOURCE:11 = 1200
ELSEIF ABL:マゾっ気 == 4
	TIMES SOURCE:3 , 2.70
	TIMES SOURCE:12 , 1.50
	SOURCE:10 = 1100
	SOURCE:11 = 1800
ELSE
	SOURCE:3 = SOURCE:3 * (ABL:マゾっ気 * 45 + 135) / 100
	SOURCE:12 = SOURCE:12 * (ABL:マゾっ気 * 10 + 120) / 100
	SOURCE:10 = 500 + (ABL:マゾっ気 * 200)
	SOURCE:11 = 750 + (ABL:マゾっ気 * 350)
ENDIF

;場所をみる
IF TEQUIP:54
	TIMES SOURCE:14 , 2.50
ELSEIF TEQUIP:57
	TIMES SOURCE:14 , 1.50
ELSEIF TEQUIP:58
	TIMES SOURCE:14 , 0.50
ENDIF

A = 100
IF EXP:放尿経験 < EXPLV:1
	TIMES A , 3.00
ELSEIF EXP:放尿経験 < EXPLV:2
	TIMES A , 2.50
ELSEIF EXP:放尿経験 < EXPLV:3
	TIMES A , 2.00
ELSEIF EXP:放尿経験 < EXPLV:4
	TIMES A , 1.00
ELSEIF EXP:放尿経験 < EXPLV:5
	TIMES A , 0.80
ELSE
	TIMES A , 0.60
ENDIF

;臆病
SIF TALENT:10
	TIMES A , 1.20

;反抗的
SIF TALENT:11
	TIMES A , 2.00

;プライド高い
SIF TALENT:15
	TIMES A , 2.00

;プライド低い
SIF TALENT:17
	TIMES A , 0.80

;自制心
SIF TALENT:20
	TIMES A , 1.50

;感情乏しい
SIF TALENT:22
	TIMES A , 0.60

;目立ちたがり
SIF TALENT:28
	TIMES A , 0.80

;抑圧
SIF TALENT:32
	TIMES A , 3.00

;抵抗
SIF TALENT:34
	TIMES A , 3.00

;恥じらい
SIF TALENT:35
	TIMES A , 3.50

;恥薄い
SIF TALENT:36
	TIMES A , 0.50

UP:10 += A * 3
UP:11 += A * 2
SOURCE:14 += A * 5

;焼き芋弁当パッチ専用イベント(黄金水入手)
;CALL SWEET_POTATO_GW

PRINTFORML %EXPNAME:31%＋２
EXP:放尿経験 += 2

;既成事実２に相当
TFLAG:200 = 2

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:レズ経験 += 1
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:ＢＬ経験 += 1
ENDIF

SIF TEQUIP:90
	T += 1

SIF PALAM:5 >= PALAMLV:3
	TFLAG:200 = 1

IF TEQUIP:22
	TEQUIP:22 = 0
ENDIF


RETURN 1
