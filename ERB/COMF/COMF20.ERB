		;欲情
		IF PALAM:5 < PALAMLV:1
			A = 1
		ELSEIF PALAM:5 < PALAMLV:2
			A = 2
		ELSEIF PALAM:5 < PALAMLV:3
			A = 3
		ELSEIF PALAM:5 < PALAMLV:4
			A = 4
		ELSEIF PALAM:5 >= PALAMLV:4
			A = 5
		ENDIF
		B = RAND:11
		A *= B
		B = RAND:11
		A += B*ABL:Ｖ感覚
		;正常位だと40以上で子宮口
		IF A >= 40
			JUMP COM256
		ELSE
			JUMP COM255
		ENDIF
	ENDIF
ENDIF

