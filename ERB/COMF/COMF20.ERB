	SOURCE:3 = 250
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:1 = 400
	SOURCE:3 = 350
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:1 = 1000
	SOURCE:3 = 500
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:1 = 1700
	SOURCE:3 = 700
ELSE
	SOURCE:1 = 950 + (ABL:Ｖ感覚 * 250)
	SOURCE:3 = 250 + (ABL:Ｖ感覚 * 150)
ENDIF

