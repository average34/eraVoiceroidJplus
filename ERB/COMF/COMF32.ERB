@COM32
;パイズリ
IF TEQUIP:65
	PRINTL 着衣パイズリ
ELSE
	PRINTL パイズリ
ENDIF

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

IF TEQUIP:65
	STR:0 = 着衣パイズリ
ELSE
	STR:0 = パイズリ
ENDIF
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
SIF TALENT:PLAYER:ふたなり || TALENT:PLAYER:オトコ
	BASE:PLAYER:射精 += CALC_EJAC(SELECTCOM, PLAYER)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 10
LOSEBASE:気力 += 150

SOURCE:13 = 1800
SOURCE:14 = 900

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:4 = 420
	SOURCE:5 = 150
	SOURCE:8 = 400
ELSEIF ABL:奉仕精神 == 1
	SOURCE:4 = 500
	SOURCE:5 = 300
	SOURCE:8 = 300
ELSEIF ABL:奉仕精神 == 2
	SOURCE:4 = 580
	SOURCE:5 = 600
	SOURCE:8 = 150
ELSEIF ABL:奉仕精神 == 3
	SOURCE:4 = 660
	SOURCE:5 = 900
	SOURCE:8 = 50
ELSEIF ABL:奉仕精神 == 4
	SOURCE:4 = 740
	SOURCE:5 = 1500
	SOURCE:8 = 20
ELSE
	SOURCE:4 = 620 + (ABL:奉仕精神 * 40)
	SOURCE:5 = 450 + (ABL:奉仕精神 * 350)
	SOURCE:8 = 0
ENDIF

;尿道刺激、ボールギャグ使用中は不可
;Ｍ感覚発生もこちらに
;Ｍ感覚を設定している場合快Ｍソース等取得
IF TEQUIP:45 == 0 && (FLAG:62 & 4194304)
	LOSEBASE:気力 += 10

	IF ABL:サドっ気 == 3
		SOURCE:4 += 50
		SOURCE:5 += 10
	ELSEIF ABL:サドっ気 == 4
		SOURCE:4 += 80
		SOURCE:5 += 30
	ELSEIF ABL:サドっ気 >= 5
		SOURCE:4 += 100
		SOURCE:5 += 50
	ENDIF

	;Ｍ感覚について
	;快Ｍのソース・Ｍ感覚を見る
	IF ABL:Ｍ感覚 == 1
		SOURCE:24 = 10
	ELSEIF ABL:Ｍ感覚 == 2
		SOURCE:24 = 30
	ELSEIF ABL:Ｍ感覚 == 3
		SOURCE:24 = 70
	ELSEIF ABL:Ｍ感覚 == 4
		SOURCE:24 = 100
	ELSEIF ABL:Ｍ感覚 >= 5
		SOURCE:24 = 100 +(50 * (ABL:Ｍ感覚 - 4))
	ENDIF

	;好感度（既成事実で代用）を見る
	IF MARK:既成事実 == 1
		TIMES SOURCE:24 , 1.00
	ELSEIF MARK:既成事実 == 2
		TIMES SOURCE:24 , 1.20
	ELSEIF TALENT:恋慕== 0 && MARK:既成事実 == 3
		TIMES SOURCE:24 , 1.40
	ELSEIF TALENT:恋慕 && CFLAG:2 < 3500
		TIMES SOURCE:24 , 1.60
	ELSEIF TALENT:恋慕 && CFLAG:2 >= 3500
		TIMES SOURCE:24 , 1.80
	ENDIF

	;ABL:精液中毒をみる
	IF ABL:精液中毒 <= 2
		TIMES SOURCE:24 , 0.00
	ELSEIF ABL:精液中毒 == 3
		TIMES SOURCE:24 , 0.50
	ELSEIF ABL:精液中毒 == 4
		TIMES SOURCE:24 , 1.00
	ELSE
		TIMES SOURCE:24 , 1.25
	ENDIF
ENDIF

;ABL:Ｂ感覚をみる
IF ABL:Ｂ感覚 == 0
	A = 100
ELSEIF ABL:Ｂ感覚 == 1
	A = 200
ELSEIF ABL:Ｂ感覚== 2
	A = 400
ELSEIF ABL:Ｂ感覚 == 3
	A = 800
ELSEIF ABL:Ｂ感覚 == 4
	A = 1200
ELSE
	A = 750 + (ABL:Ｂ感覚 * 150)
ENDIF

IF TALENT:巨乳
	TIMES A , 1.20
ENDIF
IF TALENT:Ｂ敏感
	TIMES A , 1.20
ELSEIF TALENT:Ｂ鈍感
	TIMES A , 0.70
ENDIF

SOURCE:17 += A
;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSE
	SOURCE:4 = SOURCE:4 * (ABL:技巧 * 25 + 75) / 100
	SOURCE:5 = SOURCE:5 * (ABL:技巧 * 25 + 75) / 100
ENDIF


;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:21 += 300 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:21 += 600 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:21 += 800 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:21 += 1000 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:21 += 1200 + (ABL:親密 * 160)
	ELSE
		SOURCE:21 += 1500 + (ABL:親密 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:20 = 800
	ELSEIF ABL:サドっ気 == 1
		SOURCE:20 = 1100
	ELSEIF ABL:サドっ気 == 2
		SOURCE:20 = 1400
	ELSEIF ABL:サドっ気 == 3
		SOURCE:20 = 1800
	ELSEIF ABL:サドっ気 == 4
		SOURCE:20 = 2300
	ELSE
		SOURCE:20 = 1000 + (ABL:サドっ気 * 350)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
SIF TALENT:恋慕 || TALENT:恋人
	A += 2
SIF TALENT:親愛
	A += 3
;合意がないと好感度が下がる
IF TALENT:合意 == 0 && TFLAG:48 == 0
	A -= 8
	SOURCE:15 += 300
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK
;;オーバーフロー対策に、値が大きすぎる場合と、負の場合は調整を加える
;IF BASE:PLAYER:射精 > 100000000 || BASE:PLAYER:射精 < 0
;	BASE:PLAYER:射精 = 100000000
;ENDIF
;S = BASE:PLAYER:射精
;EJAC = MAXBASE:PLAYER:射精
;
;IF  S > EJAC * 2
;	E = 2
;ELSEIF S > EJAC
;	E = 1
;ELSE
;	E = 0
;ENDIF

IF E
;射精している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 6.00
	ELSEIF ABL:精液中毒 == 1
		SOURCE:7 = 500
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 4.50
	ELSEIF ABL:精液中毒 == 2
		SOURCE:7 = 1200
		TIMES SOURCE:5 , 4.00
		TIMES SOURCE:13, 3.50
	ELSEIF ABL:精液中毒 == 3
		SOURCE:7 = 3000
		TIMES SOURCE:5 , 6.00
		TIMES SOURCE:13, 3.00
	ELSEIF ABL:精液中毒 == 4
		SOURCE:7 = 6000
		TIMES SOURCE:5 , 9.00
		TIMES SOURCE:13, 2.00
	ELSE
		SOURCE:7 = (ABL:精液中毒 * 3000) - 3000
		SOURCE:5 = SOURCE:5 * (ABL:精液中毒 * 300) / 100
		IF ABL:精液中毒 <= 10
			SOURCE:13 = SOURCE:13 * (200 - ABL:精液中毒 * 10) / 100
		ELSE
			TIMES SOURCE:13, 1.00
		ENDIF
	ENDIF

	;射精先タイプの設定
	TFLAG:703 = 4
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;;大量射精
;IF E == 2
;	TIMES SOURCE:7 , 2.00
;	TIMES SOURCE:5 , 1.50
;
;	EXP:PLAYER:射精経験 += 2
;	EXP:精液経験 += 9
;	PRINTL 大量射精
;	PRINTL 精液経験＋９
;	;精液汚れ
;	STAIN:PLAYER:ペニス |= 4
;
;	BASE:PLAYER:射精 -= EJAC*2
;	SIF BASE:PLAYER:射精 >= EJAC
;		BASE:PLAYER:射精 = EJAC-1
;
;	;口で射精させたフラグ
;	TFLAG:0 = 2
;;通常の射精
;ELSEIF E == 1
;	EXP:PLAYER:射精経験 += 1
;	EXP:精液経験 += 3
;	PRINTL 射精
;	PRINTL 精液経験＋３
;	;精液汚れ
;	STAIN:PLAYER:ペニス |= 4
;
;	BASE:PLAYER:射精 -= EJAC
;	SIF BASE:PLAYER:射精 >= EJAC
;		BASE:PLAYER:射精 = EJAC-1
;
;	;口で射精させたフラグ
;	TFLAG:0 = 1
;ENDIF

PRINTFORML %EXPNAME:22%＋１
EXP:フェラ経験 += 1

;奴隷の口⇔調教者のＰの汚れが移動
STAIN:口 |= STAIN:PLAYER:ペニス
STAIN:PLAYER:ペニス |= STAIN:口

;奉仕精神ＬＶ２以上、技巧ＬＶ２以上なら汚れをなめ取る
IF ABL:奉仕精神 >= 2 && ABL:技巧 >= 2
	STAIN:PLAYER:ペニス = 2
	SIF E >= 1
		TFLAG:8 = 1
ENDIF

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +7
	EXP:レズ経験 += 7
	EXP:PLAYER:レズ経験 += 7
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +7
	EXP:ＢＬ経験 += 7
	EXP:PLAYER:ＢＬ経験 += 7
ENDIF

SIF ASSIPLAY == 0 && EXP:フェラ経験 >= EXPLV:3
	TFLAG:30 += 1

;奴隷のＢ⇔調教者のＰの汚れが移動
STAIN:胸 |= STAIN:PLAYER:ペニス
STAIN:PLAYER:ペニス |= STAIN:胸

TFLAG:100 = 1

;既成事実２に相当
TFLAG:200 = 2

;調教者がふたなり
IF TALENT:PLAYER:ふたなり
	SOURCE:13 /= 2
ENDIF

RETURN 1

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_32
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	A += ABL:欲望 * 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	A += ABL:奉仕精神 * 4
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	A += ABL:精液中毒 * 3
ENDIF

IF MARK:快楽依存
	A += MARK:快楽依存 * 1
ENDIF


;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 1
ENDIF

;恥じらい
IF TALENT:恥じらい
	A -= 3
ENDIF
;汚臭鈍感
IF TALENT:汚臭鈍感
	A += 1
ENDIF
;汚臭敏感
IF TALENT:汚臭敏感
	A -= 3
ENDIF
;献身的
IF TALENT:献身的
	A += 6
ENDIF
;快感の否定
IF TALENT:快感の否定
	A -= 3
ENDIF
;男嫌い
IF TALENT:男嫌い && TALENT:PLAYER:オトコ
	A -= 12
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	A += 5
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:ふたなり
	A += 8
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:ペニス & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:ペニス & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:ペニス & 8
	Y += 7
SIF STAIN:PLAYER:ペニス & 16
	Y += 15

SIF TALENT:汚臭鈍感
	Y /= 3
SIF TALENT:汚臭敏感
	Y *= 2
;汚れあり
IF Y
	A -= Y
	;汚臭鈍感
	IF TALENT:汚臭鈍感
	ELSEIF TALENT:汚臭敏感
	ELSE
	ENDIF
ENDIF
;好感度
IF CFLAG:2 < 1000
	B = 0
	IF CFLAG:2 < 500
		B = 50
	ELSEIF CFLAG:2 < 750
		B = 20
	ELSEIF CFLAG:2 < 900
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 1500
	B = 0
	IF CFLAG:2 < 2000
		B = 5
	ELSE
		B = CFLAG:2 / 200
	ENDIF
	A += B
ENDIF

;合意
IF TALENT:合意 == 0
	A -= 20
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
		S = 1
	ENDIF
	;ABL:奉仕精神
	IF ABL:奉仕精神
		SIF S
			PRINT  + 
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
		S = 1
	ENDIF
	;ABL:精液中毒
	IF ABL:精液中毒
		SIF S
			PRINT  + 
		PRINTS ABLNAME:11
		PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 3,')
		S = 1
	ENDIF

	IF MARK:快楽依存
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:快楽依存,'(,MARK:快楽依存 * 1,')
		S = 1
	ENDIF


	;PALAM:欲情
	IF PALAM:5 < PALAMLV:1
		L = 0
	ELSEIF PALAM:5 < PALAMLV:2
		L = 1
	ELSEIF PALAM:5 < PALAMLV:3
		L = 2
	ELSEIF PALAM:5 < PALAMLV:4
		L = 3
	ELSEIF PALAM:5 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 1,')
		S = 1
	ENDIF

	;恥じらい
	IF TALENT:恥じらい
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,3,')
		S = 1
	ENDIF
	;汚臭鈍感
	IF TALENT:汚臭鈍感
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:61
		PRINTV '(,1,')
		S = 1
	ENDIF
	;汚臭敏感
	IF TALENT:汚臭敏感
		PRINT  - 
		PRINTS TALENTNAME:62
		PRINTV '(,3,')
		S = 1
	ENDIF
	;献身的
	IF TALENT:献身的
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:63
		PRINTV '(,6,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:快感の否定
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,3,')
		S = 1
	ENDIF
	;男嫌い
	IF TALENT:男嫌い && TALENT:PLAYER:オトコ
		PRINT  - 
		PRINTS TALENTNAME:82
		PRINTV '(,12,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:恋慕 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 1
	ENDIF

	;調教者がふたなり
	IF TALENT:PLAYER:ふたなり
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:121
		PRINTV '(,8,')
		S = 1
	ENDIF

	Y = 0
	;愛液の汚れ
	SIF STAIN:PLAYER:ペニス & 1
		Y += 1
	;精液の汚れ
	SIF STAIN:PLAYER:ペニス & 4
		Y += 3
	;アナルの汚れ
	SIF STAIN:PLAYER:ペニス & 8
		Y += 7
	SIF STAIN:PLAYER:ペニス & 16
		Y += 15

	SIF TALENT:汚臭鈍感
		Y /= 3
	SIF TALENT:汚臭敏感
		Y *= 2
	;汚れあり
	IF Y
		PRINT  - 
		;汚臭鈍感
		IF TALENT:汚臭鈍感
			PRINT 汚れあり、
			PRINTS TALENTNAME:61
		ELSEIF TALENT:汚臭敏感
			PRINT 汚れあり、
			PRINTS TALENTNAME:62
		ELSE
			PRINT 汚れあり
		ENDIF
		PRINTV '(,Y,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 < 1000
		B = 0
		PRINT  - 
		IF CFLAG:2 < 500
			B = 50
		ELSEIF CFLAG:2 < 750
			B = 20
		ELSEIF CFLAG:2 < 900
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 >= 1500
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 2000
			B = 5
		ELSE
			B = CFLAG:2 / 200
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合意
	IF TALENT:合意 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,20,')
		S = 1
	ENDIF

	;合計を表示(95以上で実行)
	PRINT  = 
	PRINTV A

	V = 95
	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF
