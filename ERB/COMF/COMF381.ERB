	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:21 += 300 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:21 += 600 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:21 += 800 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:21 += 1000 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:21 += 1200 + (ABL:親密 * 160)
	ELSE
		SOURCE:21 += 1500 + (ABL:親密 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:20 = 600
	ELSEIF ABL:サドっ気 == 1
		SOURCE:20 = 900
	ELSEIF ABL:サドっ気 == 2
		SOURCE:20 = 1200
	ELSEIF ABL:サドっ気 == 3
		SOURCE:20 = 1600
	ELSEIF ABL:サドっ気 == 4
		SOURCE:20 = 2000
	ELSE
		SOURCE:20 = 1000 + (ABL:サドっ気 * 300)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
;合意がないと好感度が下がる
IF TALENT:152 == 0 && TFLAG:48 == 0
	A -= 9
	SOURCE:15 += 400
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2


;調教者が処女
IF TALENT:PLAYER:処女
	TIMES SOURCE:13 , 10.00
	TIMES SOURCE:14 , 3.00
	TALENT:PLAYER:処女 = 0
	
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:22 = 5000
	ELSEIF ABL:サドっ気 == 1
		SOURCE:22 = 4000
	ELSEIF ABL:サドっ気 == 2
		SOURCE:22 = 3000
	ELSEIF ABL:サドっ気 == 3
		SOURCE:22 = 2000
	ELSEIF ABL:サドっ気 == 4
		SOURCE:22 = 1500
	ELSE
		SOURCE:22 = 1100 - (ABL:サドっ気 * 40)
		SIF SOURCE:22 < 0
			SOURCE:22 = 0
	ENDIF
	
	;サド持ちは懐疑1/4
	SIF TALENT:83
		TIMES SOURCE:22 , 0.25

	;小悪魔持ちは懐疑1/2
	SIF TALENT:87
		TIMES SOURCE:22 , 0.50

	PRINTFORML ＜%CALLNAME:PLAYER%が処女＞
	EXP:異常経験 += 3
	PRINTFORML %EXPNAME:50%+3
	IF TALENT:85 && ASSIPLAY == 0
		IF ABL:親密 < 10
			PRINTFORML そして%ABLNAME:0%が10になった
			ABL:親密 = 10
		ENDIF
		TIMES SOURCE:11 , 10.00
		TIMES SOURCE:3 , 3.00
		TIMES SOURCE:22 , 0.50
		SIF TALENT:89
			TIMES SOURCE:22 , 0.20
		;CFLAG:22 += 3
	ELSE
		IF ABL:親密 < 1
			PRINTFORML そして%ABLNAME:0%が1になった
			ABL:親密 = 1
		ENDIF
	ENDIF
ENDIF

EXP:PLAYER:Ｖ経験 += 1

