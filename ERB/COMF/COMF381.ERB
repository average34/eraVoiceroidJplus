@COM_ABLE381
;セックスさせる（着衣で本番。体位は妄想次第）実行判定
;COM_ABLE共通判定
CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中は無理
SIF TFLAG:44 == 1
	RETURN 0
;デート中は無理
SIF TEQUIP:2
	RETURN 0
;オムツ着用中は無理
SIF TEQUIP:20
	RETURN 0
;調教者がオトコだとダメ
SIF TALENT:PLAYER:122
	RETURN 0
;小人体型持ちは、双方が調教者と調教対象ならOK
;それ以外の場合は主人が禁断の知識持ちじゃないとダメ
SIF TALENT:149 && (TALENT:MASTER:125 == 0 && TALENT:PLAYER:149 == 0)
	RETURN 0
;オトコ、半人半妖、ふたなりでないとダメ
SIF (TALENT:119 || TALENT:121 || TALENT:122) == 0
	RETURN 0
;潤滑10000以上かつ、欲情10000以上が必要
SIF PALAM:3 < 10000 || PALAM:5 < 10000
	RETURN 0
;欲望9以上が必要
SIF ABL:欲望 < 9
	RETURN 0
;親密13以上or好感度6000以上が必要
SIF ABL:親密 < 13 && CFLAG:2 < 6000
	RETURN 0
;合意が必要
SIF TALENT:152 == 0
	RETURN 0
;淫乱が必要
SIF TALENT:76 == 0
	RETURN 0
;ムードか酒の勢いが必要
SIF TFLAG:37 < 50 && TFLAG:47 < 30
	RETURN 0
RETURN 1

