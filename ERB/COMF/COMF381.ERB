;	ENDIF
;
;	;射精先タイプの設定
;	TFLAG:703 = 9
;	;射精先選択の可・不可
;	;TFLAG:704 = 0
;	;汎用的な処理はこの関数で行う
;	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
;	CALL SAMEN_SHOOT
;ENDIF


IF TALENT:119 || TALENT:121 || TALENT:122
	;奴隷のＰ⇔調教者のＶの汚れが移動
	STAIN:2 |= STAIN:PLAYER:3
	STAIN:PLAYER:3 |= STAIN:2
ENDIF


IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:レズ経験 += 1
	EXP:PLAYER:レズ経験 += 1
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:ＢＬ経験 += 1
	EXP:PLAYER:ＢＬ経験 += 1
ENDIF

TFLAG:100 = 0

RETURN 1

;着衣正常位させる
