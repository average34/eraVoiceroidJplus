@COM63
;貝あわせ

PRINTL 貝あわせ

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = 貝あわせ
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 30
LOSEBASE:1 += 90

SOURCE:9 = 500
SOURCE:12 = 250
SOURCE:13 = 400
SOURCE:14 = 300

;ABL:従順をみる
IF ABL:親密 == 0
	SOURCE:11 = 200
ELSEIF ABL:親密 == 1
	SOURCE:11 = 120
ELSEIF ABL:親密 == 2
	SOURCE:11 = 60
ELSEIF ABL:親密 == 3
	SOURCE:11 = 20
ELSEIF ABL:親密 == 4
	SOURCE:11 = 0
ELSE
	SOURCE:11 = 0
ENDIF

;ABL:Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	SOURCE:0 = 20
	SOURCE:4 = 0
	SOURCE:5 = 0
	SOURCE:13 = 20
	TIMES SOURCE:12 , 0.80
ELSEIF ABL:Ｃ感覚 == 1
	SOURCE:0 = 80
	SOURCE:4 = 10
	SOURCE:5 = 50
	SOURCE:13 = 20
	TIMES SOURCE:12 , 0.90
ELSEIF ABL:Ｃ感覚 == 2
	SOURCE:0 = 350
	SOURCE:4 = 50
	SOURCE:5 = 100
	SOURCE:13 = 20
	TIMES SOURCE:12 , 1.00
ELSEIF ABL:Ｃ感覚 == 3
	SOURCE:0 = 750
	SOURCE:4 = 100
	SOURCE:5 = 300
	SOURCE:13 = 20
	TIMES SOURCE:12 , 1.10
ELSEIF ABL:Ｃ感覚 == 4
	SOURCE:0 = 1200
	SOURCE:4 = 700
	SOURCE:5 = 600
	SOURCE:13 = 20
	TIMES SOURCE:12 , 1.20
ELSE
	SOURCE:0 = 625 + (ABL:Ｃ感覚 * 225)
	SOURCE:4 = (ABL:Ｃ感覚 * 650) - 1250
	SOURCE:5 = ABL:Ｃ感覚 * 200
	SOURCE:13 = 20
	SOURCE:12 = SOURCE:12 * (ABL:Ｃ感覚 * 5 + 105) / 100
ENDIF

;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:4  , 0.70
	TIMES SOURCE:5  , 0.75
	TIMES SOURCE:13 , 0.50
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:4  , 0.80
	TIMES SOURCE:5  , 0.90
	TIMES SOURCE:13 , 0.60
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:4  , 1.00
	TIMES SOURCE:5  , 1.00
	TIMES SOURCE:13 , 0.70
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:4  , 1.20
	TIMES SOURCE:5  , 1.25
	TIMES SOURCE:13 , 0.80
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:4  , 1.40
	TIMES SOURCE:5  , 1.50
	TIMES SOURCE:13 , 0.90
ELSE
	TIMES SOURCE:4  , 1.60
	TIMES SOURCE:5  , 1.75
	TIMES SOURCE:13 , 1.00
ENDIF

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:奉仕精神 == 1
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:奉仕精神 == 2
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:奉仕精神 == 3
	TIMES SOURCE:5 , 1.40
ELSEIF ABL:奉仕精神 == 4
	TIMES SOURCE:5 , 1.70
ELSE
	SOURCE:5 = SOURCE:5 * (ABL:奉仕精神 * 15 + 125) / 100
ENDIF


;調教者のABL:従順をみる
IF ABL:PLAYER:親密 == 0
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:PLAYER:親密 == 1
	TIMES SOURCE:4 , 0.90
	TIMES SOURCE:5 , 0.70
ELSEIF ABL:PLAYER:親密 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:PLAYER:親密 == 3
	TIMES SOURCE:4 , 1.10
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:PLAYER:親密 == 4
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.40
ELSE
	SOURCE:4 = SOURCE:4 * (ABL:PLAYER:親密 * 5 + 105) / 100
	SOURCE:5 = SOURCE:5 * (ABL:PLAYER:親密 * 15 + 95) / 100
ENDIF


;奴隷のＶ⇔調教者のＶの汚れが移動
STAIN:3 |= STAIN:PLAYER:3
STAIN:PLAYER:3 |= STAIN:3

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +8
	EXP:40 += 8
	EXP:PLAYER:40 += 8
ENDIF

SIF ASSIPLAY == 0 && ABL:Ｃ感覚 >= 3
		TFLAG:30 += 1

TFLAG:100 = 1


;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:21 += 300 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:21 += 600 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:21 += 800 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:21 += 1000 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:21 += 1200 + (ABL:親密 * 160)
	ELSE
		SOURCE:21 += 1500 + (ABL:親密 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:20 = 600
	ELSEIF ABL:サドっ気 == 1
		SOURCE:20 = 900
	ELSEIF ABL:サドっ気 == 2
		SOURCE:20 = 1200
	ELSEIF ABL:サドっ気 == 3
		SOURCE:20 = 1600
	ELSEIF ABL:サドっ気 == 4
		SOURCE:20 = 2000
	ELSE
		SOURCE:20 = 1000 + (ABL:サドっ気 * 300)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
;合意がないと好感度が下がる
IF TALENT:152 == 0 && TFLAG:48 == 0
	A -= 8
	SOURCE:15 += 300
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

RETURN 1

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_63
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	A += ABL:欲望 * 2
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	A += ABL:奉仕精神 * 4
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	A += ABL:精液中毒 * 1
ENDIF

IF MARK:1
	A += MARK:1 * 2
ENDIF


;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 3
ENDIF

;恥じらい
IF TALENT:35
	A -= 1
ENDIF
;快感の否定
IF TALENT:71
	A -= 3
ENDIF
;男嫌い
IF TALENT:82 && TALENT:PLAYER:122
	A -= 7
ENDIF
;恋慕
IF TALENT:85 && ASSIPLAY == 0
	A += 3
	S = 3
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:121
	A += 8
ENDIF

;媚薬
IF TEQUIP:21
	A += 6
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:3 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:3 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:3 & 8
	Y += 7
SIF STAIN:PLAYER:3 & 16
	Y += 15

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2

;汚れの影響が少なめ
Y /= 3

;汚れあり
IF Y
	A -= Y
	;汚臭鈍感
	IF TALENT:61
	ELSEIF TALENT:62
	ELSE
	ENDIF
ENDIF
;好感度
IF CFLAG:2 < 1000
	B = 0
	IF CFLAG:2 < 500
		B = 50
	ELSEIF CFLAG:2 < 750
		B = 20
	ELSEIF CFLAG:2 < 900
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 1500
	B = 0
	IF CFLAG:2 < 2000
		B = 5
	ELSE
		B = CFLAG:2 / 200
	ENDIF
	A += B
ENDIF

;合意
IF TALENT:152 == 0
	A -= 5
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 2,')
		S = 1
	ENDIF
	;ABL:奉仕精神
	IF ABL:奉仕精神
		SIF S
			PRINT  + 
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
		S = 1
	ENDIF
	;ABL:精液中毒
	IF ABL:精液中毒
		SIF S
			PRINT  + 
		PRINTS ABLNAME:11
		PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 1,')
		S = 1
	ENDIF

	IF MARK:1
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:1,'(,MARK:1 * 2,')
		S = 1
	ENDIF


	;PALAM:欲情
	IF PALAM:5 < PALAMLV:1
		L = 0
	ELSEIF PALAM:5 < PALAMLV:2
		L = 1
	ELSEIF PALAM:5 < PALAMLV:3
		L = 2
	ELSEIF PALAM:5 < PALAMLV:4
		L = 3
	ELSEIF PALAM:5 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 3,')
		S = 1
	ENDIF

	;恥じらい
	IF TALENT:35
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,1,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:71
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,3,')
		S = 1
	ENDIF
	;男嫌い
	IF TALENT:82 && TALENT:PLAYER:122
		PRINT  - 
		PRINTS TALENTNAME:82
		PRINTV '(,7,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:85 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,3,')
		S = 3
	ENDIF

	;調教者がふたなり
	IF TALENT:PLAYER:121
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:121
		PRINTV '(,8,')
		S = 1
	ENDIF

	;媚薬
	IF TEQUIP:21
		SIF S
			PRINT  + 
		PRINTS ITEMNAME:21
		PRINTV '(,6,')
		S = 1
	ENDIF

	Y = 0
	;愛液の汚れ
	SIF STAIN:PLAYER:3 & 1
		Y += 1
	;精液の汚れ
	SIF STAIN:PLAYER:3 & 4
		Y += 3
	;アナルの汚れ
	SIF STAIN:PLAYER:3 & 8
		Y += 7
	SIF STAIN:PLAYER:3 & 16
		Y += 15

	SIF TALENT:61
		Y /= 3
	SIF TALENT:62
		Y *= 2

	;汚れの影響が少なめ
	Y /= 3

	;汚れあり
	IF Y
		PRINT  - 
		;汚臭鈍感
		IF TALENT:61
			PRINT 汚れあり、
			PRINTS TALENTNAME:61
		ELSEIF TALENT:62
			PRINT 汚れあり、
			PRINTS TALENTNAME:62
		ELSE
			PRINT 汚れあり
		ENDIF
		PRINTV '(,Y,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 < 1000
		B = 0
		PRINT  - 
		IF CFLAG:2 < 500
			B = 50
		ELSEIF CFLAG:2 < 750
			B = 20
		ELSEIF CFLAG:2 < 900
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 >= 1500
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 2000
			B = 5
		ELSE
			B = CFLAG:2 / 200
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合意
	IF TALENT:152 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,5,')
		S = 1
	ENDIF

	;合計を表示(70以上で実行)
	PRINT  = 
	PRINTV A

	V = 70
	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF

