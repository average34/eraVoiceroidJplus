@COM_ABLE155
;巨峰責め
RETURN 0

CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中じゃないと無理
SIF TFLAG:44 == 0
	RETURN 0
;調教者が秋姉妹でないとダメ
SIF 1
	RETURN 0
;触手調教中はダメ
SIF TEQUIP:90
	RETURN 0
;解除はいつでも可能
SIF TEQUIP:70
	RETURN 1
;体力が1000未満では装着不可能
SIF BASE:0 < 1000 && TEQUIP:70 == 0
	RETURN 0
;気力が０の時はダメ
SIF BASE:1 == 0
	RETURN 0
;Ａ拡張経験少ないとダメ
SIF EXP:53 < 5
	RETURN 0
;アナルバイブ使用中は不可
SIF TEQUIP:13
	RETURN 0
;アナルビーズ使用中は不可
SIF TEQUIP:19
	RETURN 0
;普通の浣腸使用中は不可
SIF TEQUIP:46
	RETURN 0
;空気浣腸使用中は不可
SIF TEQUIP:47
	RETURN 0
;バルーン使用中は不可
SIF TEQUIP:48
	RETURN 0
;電極使用中は不可
SIF TEQUIP:49
	RETURN 0
;氷柱挿入中は不可
SIF TEQUIP:79
	RETURN 0
;調教対象が小人体型の場合は主人が禁断の知識持ちじゃないとダメ
SIF TALENT:149 && TALENT:MASTER:125 == 0
	RETURN 0
RETURN 1

@COM155
;巨峰責め
;Ａ拡張経験がついた強化アナルビーズみたいなもの。

PRINTL 巨峰責め
STR:0 = 巨峰責め

LOSEBASE:0 += 300
LOSEBASE:1 += 400

SOURCE:14 = 2000

;ABL:Ａ感覚をみる
IF ABL:5 == 0
	SOURCE:2 = 100
	SOURCE:13 = 300
ELSEIF ABL:5 == 1
	SOURCE:2 = 300
	SOURCE:13 = 800
ELSEIF ABL:5 == 2
	SOURCE:2 = 600
	SOURCE:13 = 1400
ELSEIF ABL:5 == 3
	SOURCE:2 = 1000
	SOURCE:13 = 1800
ELSEIF ABL:5 == 4
	SOURCE:2 = 1500
	SOURCE:13 = 2100
ELSE
	SOURCE:2 = 2100
	SOURCE:13 = 2400
ENDIF

;EXP:Ａ経験をみる
IF EXP:1 < EXPLV:1
	TIMES SOURCE:2 , 0.50
	SOURCE:6 = 2000
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:2 , 1.00
	SOURCE:6 = 300
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 1.10
	SOURCE:6 = 50
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:2 , 1.20
	SOURCE:6 = 10
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:2 , 1.40
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:2 , 1.60
	SOURCE:6 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:6 += 1200
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:6 += 700
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:6 += 400
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:6 += 150
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:2 , 1.80
	SOURCE:6 += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:2 , 0.80
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:2 , 0.90
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:2 , 1.10
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:2 , 1.20
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES SOURCE:2 , 0.80
ELSEIF ABL:0 == 1
	TIMES SOURCE:2 , 0.90
ELSEIF ABL:0 == 2
	TIMES SOURCE:2 , 1.00
ELSEIF ABL:0 == 3
	TIMES SOURCE:2 , 1.10
ELSEIF ABL:0 == 4
	TIMES SOURCE:2 , 1.20
ELSE
	TIMES SOURCE:2 , 1.30
ENDIF

;小柄体型
IF TALENT:100
	TIMES SOURCE:6 , 2.00
SIF TALENT:132 || TALENT:131
	TIMES SOURCE:2 , 2.00
ENDIF

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:105
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:106
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

IF EXP:0 == 0 && TALENT:30
	SOURCE:13 /= 3
ENDIF

SIF TEQUIP:71 == 1
	TIMES SOURCE:2 , 1.80

IF TEQUIP:70 == 0
	EXP:1 += 2
	EXP:53 += 3
	PRINTL Ａ経験＋２
	PRINTL Ａ拡張経験＋３
ELSEIF TEQUIP:70 == 1
	EXP:1 += 3
	EXP:53 += 4
	PRINTL Ａ経験＋３
	PRINTL Ａ拡張経験＋４
ENDIF

CALL TRAIN_MESSAGE_B

;巨峰の着脱
IF TEQUIP:70
	TEQUIP:70 = 0
	TFLAG:74 += 5
ELSE
	TEQUIP:70 = 1
	TFLAG:74 += 10
ENDIF

RETURN 1

@TRAIN_MESSAGE_COM155
;巨峰の着脱
IF TEQUIP:70
	PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%の菊穴から生えている蔓をつまむと勢い良く引き抜いた…
	IF ABL:5 >= 5 && TALENT:76
		PRINTFORML 一時は幸せそうな顔をしていた%CALLNAME:TARGET%だったが、次第に直腸の喪失感が増してきたのか
		PRINTFORM 涙と涎をこぼし、腰を振って自らの手で肛門を拡げながら哀願するような目で%CALLNAME:PLAYER%を見上げて
	ELSEIF ABL:5 >= 3 && EXP:1 < 5 && TALENT:76
		PRINTFORM 葡萄の実が肛門から排泄されるとその度に%CALLNAME:TARGET%はぶるっと腰を震わせ
	ELSEIF ABL:5 >= 1 && EXP:1 < 3 && TALENT:76
		PRINTFORM 大きく拡がった%CALLNAME:TARGET%の肛門から腸内に溜まっていたであろう粘液が滴り出し
	ELSEIF ABL:5 == 0 && TALENT:76
		PRINTFORM %CALLNAME:TARGET%は痛そうに顔を歪ませながらも陰唇からは愛液を滲ませ
	ELSEIF ABL:5 >= 4
		PRINTFORM %CALLNAME:TARGET%は恍惚感を浮かべた目付きで%CALLNAME:PLAYER%の方を振り返っ
	ELSEIF ABL:5 >= 2 && EXP:1 < 4
		PRINTFORM %CALLNAME:TARGET%は荒い息をつきながら涙を流し
	ELSEIF ABL:5 <= 1
		PRINTFORM %CALLNAME:TARGET%は挿入された時以上の大きな叫び声を上げ
	ENDIF
	PRINTW ている…
ELSE
	PRINTFORM %CALLNAME:PLAYER%は%CALLNAME:TARGET%の
	SIF PALAM:3 >= PALAMLV:5
		PRINT たっぷりと
	SIF PALAM:3 >= PALAMLV:3
		PRINT 粘液をたたえた
	SIF PALAM:3 < PALAMLV:3
		PRINT 硬く閉ざされた
	IF TALENT:100
		PRINT 可愛らしい菊門を
	ELSE
		PRINT アヌスを
	ENDIF
	PRINTL 無理やり拡げ、大粒の葡萄の実ををひとつづつ捻じ込んでいった…
	PRINTFORM %CALLNAME:PLAYER%
	IF EXP:1 >= 201 && TALENT:85
		PRINT は１粒挿入されただけでガクガクと腰を震わせてイきそうな気配を漂わせて
	ELSEIF EXP:1 >= 101 && EXP:1 <= 200 && TALENT:85
		PRINT の肛門はまるで食べ盛りの子供みたく、貪欲に葡萄を飲み込んで
	ELSEIF EXP:1 >= 51 && EXP:1 <= 100 && TALENT:85
		PRINT は葡萄の実が腸内を転がっていく感触を楽しむため腰を高々と上げて
	ELSEIF EXP:1 >= 1 && EXP:1 <= 50 && TALENT:85
		PRINT は苦痛に顔をしかめていたが実を飲み込むごとに快楽を感じ始めたのか徐々に頬を緩ませて
	ELSEIF EXP:1 >= 201
		PRINT は直腸に巨大な質量をもつ何かが転がり込むたびに恍惚とした表情を浮かべて
	ELSEIF EXP:1 >= 101 && EXP:1 <= 200
		PRINT は実が腸壁に擦れる感触を楽しんでいるかのように腰を振って
	ELSEIF EXP:1 >= 1 && EXP:1 <= 100
		PRINT は苦痛に顔を歪ませながらも肛門から頭頂まで駆け抜けるような快楽に一瞬弛んだ表情を見せて
	ENDIF
	PRINTW いる…
ENDIF

@EQUIP_COM155
;巨峰

PRINTL ＜巨峰挿入中＞

LOSEBASE:0 += 70
LOSEBASE:1 += 120

;ABL:Ａ感覚をみる
IF ABL:5 == 0
	A = 40
	B = 100
ELSEIF ABL:5 == 1
	A = 80
	B = 300
ELSEIF ABL:5 == 2
	A = 100
	B = 500
ELSEIF ABL:5 == 3
	A = 150
	B = 700
ELSEIF ABL:5 == 4
	A = 200
	B = 900
ELSE
	A = 300
	B = 1200
ENDIF

;EXP:Ａ経験をみる
IF EXP:1 < EXPLV:1
	TIMES A , 0.50
	C = 3000
ELSEIF EXP:1 < EXPLV:2
	TIMES A , 1.00
	C = 1000
ELSEIF EXP:1 < EXPLV:3
	TIMES A , 1.10
	C = 500
ELSEIF EXP:1 < EXPLV:4
	TIMES A , 1.20
	C = 100
ELSEIF EXP:1 < EXPLV:5
	TIMES A , 1.40
	C = 50
ELSE
	TIMES A , 1.60
	C = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES A , 0.40
	C += 900
ELSEIF PALAM:3 < PALAMLV:2
	TIMES A , 0.80
	C += 600
ELSEIF PALAM:3 < PALAMLV:3
	TIMES A , 1.00
	C += 300
ELSEIF PALAM:3 < PALAMLV:4
	TIMES A , 1.40
	C += 150
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES A , 1.80
	C += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:5 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:5 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES A , 1.10
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES A , 1.20
ENDIF

;ABL:従順をみる
IF ABL:0 == 0
	TIMES A , 0.80
ELSEIF ABL:0 == 1
	TIMES A , 0.90
ELSEIF ABL:0 == 2
	TIMES A , 1.00
ELSEIF ABL:0 == 3
	TIMES A , 1.10
ELSEIF ABL:0 == 4
	TIMES A , 1.20
ELSE
	TIMES A , 1.30
ENDIF

;小柄体型
SIF TALENT:100
	TIMES C , 1.50

SOURCE:2 += A
SOURCE:13 += B
SOURCE:6 += C

;処女で貞操観念
IF EXP:0 == 0 && TALENT:30
	SOURCE:13 /= 3
ENDIF

EXP:1 += 2
EXP:53 += 2
PRINTL Ａ経験＋２
PRINTL Ａ拡張経験＋２

TFLAG:74 += 1

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:40 += 1
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:41 += 1
ENDIF

RETURN 1
