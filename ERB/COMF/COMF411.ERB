@COM_ABLE411
;口上独自純愛コマンド実行判定
;COM_ABLE共通判定
CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中だと無理
SIF TFLAG:44 == 1
	RETURN 0
;CFLAGがたてられてないと無理
SIF !GETBIT(CFLAG:TARGET:独自コマンド使用判定, 6)
	RETURN 0
RESULT = 0
TRYCALLFORM COM_ABLE411_{NO:TARGET}
SIF RESULT == 0
	RETURN 0
RETURN 1

@COM411
PRINTSL CSTR:86
STR:0 = %CSTR:86%
;実行判定を使用する場合はこの関数で、真で戻ってくれば判定失敗
TRYCCALLFORM COM_ORDER411_{NO:TARGET}
	IF RESULT == 1
		;口上呼び出し
		TFLAG:13 = 99
		CALL SELF_KOJO
		RETURN 0
	ENDIF
CATCH
ENDCATCH
;地の文を呼ぶ
CALL TRAIN_MESSAGE_B
;本体呼び出し、ここでソースなどの処理を
TRYCALLFORM COM411_{NO:TARGET}
RETURN 1

;コマンド前の地の文
@TRAIN_MESSAGE_COM411
TRYCALLFORM TRAIN_MESSAGE_B411_{NO:TARGET}

;絶頂時などの地の文
@TRAIN_MESSAGE_A411
TRYCALLFORM TRAIN_MESSAGE_A411_{NO:TARGET}

;コマンド本体後に出てくる地の文
@TEXT411
TRYCALLFORM TEXT411_{NO:TARGET}
