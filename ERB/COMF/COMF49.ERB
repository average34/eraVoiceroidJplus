@COM49
;アナル電極

PRINTL アナル電極
STR:0 = アナル電極


LOSEBASE:体力 += 100
LOSEBASE:気力 += 150

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:快Ａ = 200
	SOURCE:屈従 = 1000
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:快Ａ = 500
	SOURCE:屈従 = 2000
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:快Ａ = 900
	SOURCE:屈従 = 3000
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:快Ａ = 1800
	SOURCE:屈従 = 5000
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:快Ａ = 2400
	SOURCE:屈従 = 8000
ELSE
	SOURCE:快Ａ = 300 + (ABL:Ａ感覚 * 700)
	SOURCE:屈従 = 2000 + (ABL:Ａ感覚 * 2000)
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:快Ａ , 0.50
	SOURCE:痛み = 2000
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:快Ａ , 1.00
	SOURCE:痛み = 300
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:快Ａ , 1.10
	SOURCE:痛み = 50
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:快Ａ , 1.20
	SOURCE:痛み = 10
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:快Ａ , 1.40
	SOURCE:痛み = 0
ELSE
	TIMES SOURCE:快Ａ , 1.60
	SOURCE:痛み = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.40
	SOURCE:痛み += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.80
	SOURCE:痛み += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
	SOURCE:痛み += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.40
	SOURCE:痛み += 120
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:快Ａ , 1.80
	SOURCE:痛み += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.10
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:快Ａ , 1.20
ENDIF

;ABL:従順をみる
IF ABL:親密 == 0
	TIMES SOURCE:快Ａ , 0.80
ELSEIF ABL:親密 == 1
	TIMES SOURCE:快Ａ , 0.90
ELSEIF ABL:親密 == 2
	TIMES SOURCE:快Ａ , 1.00
ELSEIF ABL:親密 == 3
	TIMES SOURCE:快Ａ , 1.10
ELSEIF ABL:親密 == 4
	TIMES SOURCE:快Ａ , 1.20
ELSE
	SOURCE:快Ａ = SOURCE:快Ａ * (ABL:親密 * 5 + 105) / 100
ENDIF

;ABL:サドっ気をみる
IF ABL:PLAYER:サドっ気 == 0
	TIMES SOURCE:痛み , 1.00
ELSEIF ABL:PLAYER:サドっ気 == 1
	TIMES SOURCE:痛み , 1.05
ELSEIF ABL:PLAYER:サドっ気 == 2
	TIMES SOURCE:痛み , 1.10
ELSEIF ABL:PLAYER:サドっ気 == 3
	TIMES SOURCE:痛み , 1.20
ELSEIF ABL:PLAYER:サドっ気 == 4
	TIMES SOURCE:痛み , 1.30
ELSE
	SOURCE:痛み = SOURCE:痛み * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
ENDIF

;小柄体型
SIF TALENT:小柄体型
	TIMES SOURCE:痛み , 2.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES SOURCE:痛み , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES SOURCE:痛み , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

IF EXP:Ｖ経験 == 0 && TALENT:貞操観念
	SOURCE:屈従 /= 3
ENDIF

EXP:Ａ経験 += 5
PRINTL Ａ経験＋５

CALL TRAIN_MESSAGE_B

;電極の着脱
IF TEQUIP:49
	TEQUIP:49 = 0
ELSE
	TEQUIP:49 = 1
ENDIF

RETURN 1

@TRAIN_MESSAGE_COM49
;電極の着脱
IF TEQUIP:49
	PRINTFORML %CALLNAME:TARGET%のアヌスから電極を抜いてやった…
ELSE
	PRINTFORML %CALLNAME:TARGET%の菊穴に電極を挿入してスイッチを入れた…
ENDIF

@EQUIP_COM49
;アナル電極

PRINTL ＜アナル電極挿入中＞

LOSEBASE:体力 += 80
LOSEBASE:気力 += 120

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	A = 250
	B = 1000
ELSEIF ABL:Ａ感覚 == 1
	A = 600
	B = 2000
ELSEIF ABL:Ａ感覚 == 2
	A = 1200
	B = 3000
ELSEIF ABL:Ａ感覚 == 3
	A = 1900
	B = 5000
ELSEIF ABL:Ａ感覚 == 4
	A = 2500
	B = 8000
ELSE
	A = 400 + (ABL:Ａ感覚 * 700)
	B = 2000 + (ABL:Ａ感覚 * 2000)
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES A , 0.50
	C = 2000
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES A , 1.00
	C = 300
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES A , 1.10
	C = 50
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES A , 1.20
	C = 10
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES A , 1.40
	C = 0
ELSE
	TIMES A , 1.60
	C = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES A , 0.40
	C += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES A , 0.80
	C += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES A , 1.00
	C += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES A , 1.40
	C += 120
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES A , 1.80
	C += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES A , 1.10
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES A , 1.20
ENDIF

;ABL:従順をみる
IF ABL:親密 == 0
	TIMES A , 0.80
ELSEIF ABL:親密 == 1
	TIMES A , 0.90
ELSEIF ABL:親密 == 2
	TIMES A , 1.00
ELSEIF ABL:親密 == 3
	TIMES A , 1.10
ELSEIF ABL:親密 == 4
	TIMES A , 1.20
ELSE
	A = A * (ABL:親密 * 5 + 105) / 100
ENDIF

;小柄体型
SIF TALENT:小柄体型
	TIMES C , 2.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES B , 1.50
	TIMES C , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES B , 0.60
	TIMES C , 0.60
ENDIF

;処女で貞操観念
IF EXP:Ｖ経験 == 0 && TALENT:貞操観念
	B /= 3
ENDIF

SOURCE:快Ａ += A
SOURCE:屈従 += B
SOURCE:痛み += C

EXP:Ａ経験 += 5
PRINTL Ａ経験＋５

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:レズ経験 += 1
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:ＢＬ経験 += 1
ENDIF

SIF TEQUIP:90
	T += 1

RETURN 1
