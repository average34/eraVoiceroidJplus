@COM49
;アナル電極

PRINTL アナル電極
STR:0 = アナル電極


LOSEBASE:体力 += 100
LOSEBASE:気力 += 150

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:2 = 200
	SOURCE:13 = 1000
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:2 = 500
	SOURCE:13 = 2000
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:2 = 900
	SOURCE:13 = 3000
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:2 = 1800
	SOURCE:13 = 5000
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:2 = 2400
	SOURCE:13 = 8000
ELSE
	SOURCE:2 = 300 + (ABL:Ａ感覚 * 700)
	SOURCE:13 = 2000 + (ABL:Ａ感覚 * 2000)
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:2 , 0.50
	SOURCE:6 = 2000
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:2 , 1.00
	SOURCE:6 = 300
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:2 , 1.10
	SOURCE:6 = 50
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:2 , 1.20
	SOURCE:6 = 10
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:2 , 1.40
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:2 , 1.60
	SOURCE:6 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:6 += 800
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:6 += 500
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:6 += 300
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:6 += 120
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:2 , 1.80
	SOURCE:6 += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:2 , 0.80
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:2 , 0.90
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:2 , 1.10
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:2 , 1.20
ENDIF

;ABL:従順をみる
IF ABL:親密 == 0
	TIMES SOURCE:2 , 0.80
ELSEIF ABL:親密 == 1
	TIMES SOURCE:2 , 0.90
ELSEIF ABL:親密 == 2
	TIMES SOURCE:2 , 1.00
ELSEIF ABL:親密 == 3
	TIMES SOURCE:2 , 1.10
ELSEIF ABL:親密 == 4
	TIMES SOURCE:2 , 1.20
ELSE
	SOURCE:2 = SOURCE:2 * (ABL:親密 * 5 + 105) / 100
ENDIF

;ABL:サドっ気をみる
IF ABL:PLAYER:サドっ気 == 0
	TIMES SOURCE:6 , 1.00
ELSEIF ABL:PLAYER:サドっ気 == 1
	TIMES SOURCE:6 , 1.05
ELSEIF ABL:PLAYER:サドっ気 == 2
	TIMES SOURCE:6 , 1.10
ELSEIF ABL:PLAYER:サドっ気 == 3
	TIMES SOURCE:6 , 1.20
ELSEIF ABL:PLAYER:サドっ気 == 4
	TIMES SOURCE:6 , 1.30
ELSE
	SOURCE:6 = SOURCE:6 * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
ENDIF

;小柄体型
SIF TALENT:100
	TIMES SOURCE:6 , 2.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:105
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:106
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

IF EXP:Ｖ経験 == 0 && TALENT:30
	SOURCE:13 /= 3
ENDIF

EXP:Ａ経験 += 5
PRINTL Ａ経験＋５

CALL TRAIN_MESSAGE_B

;電極の着脱
IF TEQUIP:49
	TEQUIP:49 = 0
ELSE
	TEQUIP:49 = 1
ENDIF

RETURN 1

@TRAIN_MESSAGE_COM49
;電極の着脱
IF TEQUIP:49
	PRINTFORML %CALLNAME:TARGET%のアヌスから電極を抜いてやった…
ELSE
	PRINTFORML %CALLNAME:TARGET%の菊穴に電極を挿入してスイッチを入れた…
ENDIF

@EQUIP_COM49
;アナル電極

PRINTL ＜アナル電極挿入中＞

LOSEBASE:体力 += 80
LOSEBASE:気力 += 120

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	A = 250
	B = 1000
ELSEIF ABL:Ａ感覚 == 1
	A = 600
	B = 2000
ELSEIF ABL:Ａ感覚 == 2
	A = 1200
	B = 3000
ELSEIF ABL:Ａ感覚 == 3
	A = 1900
	B = 5000
ELSEIF ABL:Ａ感覚 == 4
	A = 2500
	B = 8000
ELSE
	A = 400 + (ABL:Ａ感覚 * 700)
	B = 2000 + (ABL:Ａ感覚 * 2000)
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES A , 0.50
	C = 2000
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES A , 1.00
	C = 300
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES A , 1.10
	C = 50
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES A , 1.20
	C = 10
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES A , 1.40
	C = 0
ELSE
	TIMES A , 1.60
	C = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES A , 0.40
	C += 800
ELSEIF PALAM:3 < PALAMLV:2
	TIMES A , 0.80
	C += 500
ELSEIF PALAM:3 < PALAMLV:3
	TIMES A , 1.00
	C += 300
ELSEIF PALAM:3 < PALAMLV:4
	TIMES A , 1.40
	C += 120
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES A , 1.80
	C += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:5 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:5 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES A , 1.10
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES A , 1.20
ENDIF

;ABL:従順をみる
IF ABL:親密 == 0
	TIMES A , 0.80
ELSEIF ABL:親密 == 1
	TIMES A , 0.90
ELSEIF ABL:親密 == 2
	TIMES A , 1.00
ELSEIF ABL:親密 == 3
	TIMES A , 1.10
ELSEIF ABL:親密 == 4
	TIMES A , 1.20
ELSE
	A = A * (ABL:親密 * 5 + 105) / 100
ENDIF

;小柄体型
SIF TALENT:100
	TIMES C , 2.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:105
	TIMES B , 1.50
	TIMES C , 1.50
ELSEIF TALENT:106
	TIMES B , 0.60
	TIMES C , 0.60
ENDIF

;処女で貞操観念
IF EXP:Ｖ経験 == 0 && TALENT:30
	B /= 3
ENDIF

SOURCE:2 += A
SOURCE:13 += B
SOURCE:6 += C

EXP:Ａ経験 += 5
PRINTL Ａ経験＋５

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:レズ経験 += 1
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:ＢＬ経験 += 1
ENDIF

SIF TEQUIP:90
	T += 1

RETURN 1
