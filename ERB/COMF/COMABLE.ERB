;全コマンドでTFLAG:DOTRAINとTFLAG:CALLTRAINのどちらかが立てられないとCOM_ABLEを抜けられないようになっています
;DOTRAIN及びCALLTRAINをするときは、命令直前にそれぞれのフラグを立ててください
;-------------------------------------------------
;共通実行判定
;-------------------------------------------------
@COM_ABLE_COMMON, ARG
;COM_ABLE強制終了フラグ
SIF TFLAG:DOTRAIN == 0 && TFLAG:CALLTRAIN == 0
	RETURN 0

;炬燵判定
;300;戦闘訓練 320;宴会に参加 321;遊びに行く 323;デート 305;家事 315;お散歩する
;309;素材探し 310;調合 322;ストリートライブ 367;着衣ローター
;363,着衣セックス 365,着衣後背位 366,着衣騎乗位 371,オムツを履かせる
IF ARG == 300 || ARG == 320 || ARG == 321 || ARG == 323 || ARG == 305 || ARG == 315 || ARG == 309 || ARG == 310 || ARG == 322 || ARG == 363 || ARG == 365 || ARG == 366 || ARG == 367 || ARG == 371
	;炬燵にあたっていると出られないor炬燵にあたってるとできない
	SIF TEQUIP:7
		RETURN 0
ENDIF

;Ａ系コマンド拡張判定
;9,アナル舐め 19,アナルビーズ 26,正常位アナル 27,後背位アナル 28,対面座位アナル 29,背面座位アナル 36,騎乗位アナル 46,浣腸器＋プラグ
IF ARG == 9 || ARG == 19 || ARG == 26 || ARG == 27 || ARG == 28 || ARG == 29 || ARG == 36 || ARG == 46
	;コンフィグでＡ系コマンド拡張がONになってないと無理
	SIF (FLAG:61 & 1p5) == 0
		RETURN 0
ENDIF

;REVMODE関係
IF TFLAG:34
	IF ARG >= 500 && ARG <= 510
		LOCAL = REVMODE_CONVERT_PASSIVECOM(PREVCOM, TFLAG:36)
		SIF REVMODE_ACTIVE_COMABLE(LOCAL, ARG-500)
			RETURN 1
		RETURN 0
	ELSE
		RETURN 0
	ENDIF
;REVMODE（非ウフフ時）
ELSEIF TFLAG:53
	IF ARG >= 500 && ARG <= 510
		LOCAL = PASSIVEMODE_CONVERT_PASSIVECOM(PREVCOM, TFLAG:53)
		SIF PASSIVEMODE_ACTIVE_COMABLE(LOCAL, ARG-500)
			RETURN 1
		RETURN 0
	ELSE
		RETURN 0
	ENDIF
ELSE
	SIF ARG >= 500 && ARG <= 510
		RETURN 0
ENDIF

;ウフフコマンド拡張判定
IF ARG < 300
	IF (ARG >= 0 && ARG <= 8) || ARG == 10 || ARG == 11 || ARG == 13 || ARG == 18 || (ARG >= 20 && ARG <= 25)
		RETURN 1
	ELSEIF (ARG >= 30 && ARG <= 35) || ARG == 37 || (ARG >= 40 && ARG <= 45) || (ARG >= 50 && ARG <= 59)
		RETURN 1
	ELSEIF ARG == 61 || ARG == 63 || (ARG >= 65 && ARG <= 68) || ARG == 75 || ARG == 80 || (ARG >= 100 && ARG <= 109) || ARG == 190
		RETURN 1
	ELSEIF (ARG >= 280 && ARG <= 284)
		RETURN 1
	ELSEIF (FLAG:61 & 1p3) == 0 
		RETURN 0
	ENDIF
ENDIF

;非ウフフコマンド拡張判定
IF ARG >= 300
	IF (ARG >= 300 && ARG <= 305) || (ARG >= 320 && ARG <= 323) || (ARG >= 340 && ARG <= 342) || (ARG >= 360 && ARG <= 363) || ARG == 390 || ARG == 399
		RETURN 1
	ELSEIF (ARG >= 410 && ARG <= 414)
		RETURN 1
	ELSEIF (FLAG:61 & 1p4) == 0 
		RETURN 0
	ENDIF
ENDIF

RETURN 1

