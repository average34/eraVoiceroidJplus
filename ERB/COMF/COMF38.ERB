@COM_ABLE38
;尻素股実行判定
;COM_ABLE共通判定
CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中じゃないと無理
SIF TFLAG:44 == 0
	RETURN 0
;調教者がオトコか半人半妖かふたなりじゃないとダメ
SIF TALENT:PLAYER:119 == 0 && TALENT:PLAYER:121 == 0 && TALENT:PLAYER:122 == 0
	RETURN 0
;潤滑が高くないとダメ
SIF PALAM:3 < 2000
	RETURN 0
;アナルバイブ挿入中はダメ
SIF TEQUIP:13
	RETURN 0
;アナルビーズ使用中は不可
SIF TEQUIP:19
	RETURN 0
;浣腸＋アナルプラグ使用中はダメ
SIF TEQUIP:46
	RETURN 0
;空気浣腸使用中は不可
SIF TEQUIP:47
	RETURN 0
;バルーン使用中は不可
SIF TEQUIP:48
	RETURN 0
;電極使用中は不可
SIF TEQUIP:49
	RETURN 0
;クスコ挿入中はダメ
SIF TEQUIP:61
	RETURN 0
;巨峰挿入中は不可
SIF TEQUIP:70
	RETURN 0
;縄使用中は不可
SIF TEQUIP:44
	RETURN 0
;触手調教中はダメ
SIF TEQUIP:90
	RETURN 0
;お風呂場プレイ中はプレイマットが無いとダメ
IF TEQUIP:58
SIF ITEM:13 == 0 && NOITEM == 0
	RETURN 0
ENDIF
RETURN 1

@COM38
;尻素股

PRINTL 尻素股

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = 尻素股
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
SIF TALENT:PLAYER:119 || TALENT:PLAYER:122  || TALENT:PLAYER:121
	BASE:PLAYER:2 += CALC_EJAC(SELECTCOM, PLAYER)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
;尻素股
LOSEBASE:0 += 40
LOSEBASE:1 += 130

SOURCE:13 = 1200
SOURCE:14 = 400

;上のほうで計算した汚れデータ
SOURCE:8 = Y*10 + 60

;ABL:奉仕精神をみる
IF ABL:6 == 0
	SOURCE:4 = 200
	SOURCE:5 = 100
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:6 == 1
	SOURCE:4 = 250
	SOURCE:5 = 180
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:6 == 2
	SOURCE:4 = 300
	SOURCE:5 = 250
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:6 == 3
	SOURCE:4 = 350
	SOURCE:5 = 350
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:6 == 4
	SOURCE:4 = 400
	SOURCE:5 = 500
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:4 = 325 + (ABL:6 * 25)
	SOURCE:5 = 50 + (ABL:6 * 150)
	TIMES SOURCE:8 , 0.10
ENDIF
	
;ABL:技巧をみる
IF ABL:2 == 0
	TIMES SOURCE:4 , 0.70
	TIMES SOURCE:5 , 0.70
ELSEIF ABL:2 == 1
	TIMES SOURCE:4 , 0.90
	TIMES SOURCE:5 , 0.90
ELSEIF ABL:2 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:2 == 3
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:2 == 4
	TIMES SOURCE:4 , 1.40
	TIMES SOURCE:5 , 1.40
ELSE
	SOURCE:4 = SOURCE:4 * (ABL:2 * 10 + 110) / 100
	SOURCE:5 = SOURCE:5 * (ABL:2 * 10 + 110) / 100
ENDIF

;ABL:A感覚をみる
IF ABL:5 == 0
	SOURCE:2 = 0
ELSEIF ABL:5 == 1
	SOURCE:2 = 10
ELSEIF ABL:5 == 2
	SOURCE:2 = 50
ELSEIF ABL:5 == 3
	SOURCE:2 = 200
ELSEIF ABL:5 == 4
	SOURCE:2 = 400
ELSE
	SOURCE:2 = (ABL:5 * 600) - 1500
ENDIF

;PALAM:潤滑
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:0 , 0.30
	TIMES SOURCE:4 , 0.60
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:0 , 0.60
	TIMES SOURCE:4 , 0.80
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:4 , 1.00
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:4 , 1.20
ELSEIF PALAM:3 < PALAMLV:5
	TIMES SOURCE:0 , 2.00
	TIMES SOURCE:4 , 1.40
ELSE
	TIMES SOURCE:0 , 2.50
	TIMES SOURCE:4 , 1.60
ENDIF

;主導権判定
;パートナーに主導権
IF TFLAG:45 == 1
	;ABL:サドっ気をみる
	IF ABL:15 == 0
		SOURCE:20 = 800
	ELSEIF ABL:15 == 1
		SOURCE:20 = 1100
	ELSEIF ABL:15 == 2
		SOURCE:20 = 1400
	ELSEIF ABL:15 == 3
		SOURCE:20 = 1800
	ELSEIF ABL:15 == 4
		SOURCE:20 = 2300
	ELSE
		SOURCE:20 = 1000 + (ABL:15 * 350)
	ENDIF
;それ以外
ELSE
	;ABL:従順をみる
	IF ABL:0 <= 1
		SOURCE:21 += 300 + (ABL:0 * 50)
	ELSEIF ABL:0 <= 3
		SOURCE:21 += 600 + (ABL:0 * 100)
	ELSEIF ABL:0 <= 5
		SOURCE:21 += 800 + (ABL:0 * 120)
	ELSEIF ABL:0 <= 8
		SOURCE:21 += 1000 + (ABL:0 * 140)
	ELSEIF ABL:0 <= 11
		SOURCE:21 += 1200 + (ABL:0 * 160)
	ELSE
		SOURCE:21 += 1500 + (ABL:0 * 180)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
SIF TALENT:85 || TALENT:153
	A += 2
SIF TALENT:88
	A += 3
;合意がないと好感度が下がる
IF TALENT:152 == 0 && TFLAG:48 == 0
	A -= 8
	SOURCE:15 += 300
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK
;;オーバーフロー対策に、値が大きすぎる場合と、負の場合は調整を加える
;IF BASE:PLAYER:2 > 100000000 || BASE:PLAYER:2 < 0
;	BASE:PLAYER:2 = 100000000
;ENDIF
;S = BASE:PLAYER:2
;EJAC = MAXBASE:PLAYER:2
;
;IF  S > EJAC * 2
;	E = 2
;ELSEIF S > EJAC
;	E = 1
;ELSE
;	E = 0
;ENDIF

IF E
;射精している
	TIMES SOURCE:4 , 2.00

	;ABL:精液中毒をみる
	IF ABL:11 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 1.50
		TIMES SOURCE:13, 1.40
	ELSEIF ABL:11 == 1
		SOURCE:7 = 200
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 1.00
	ELSEIF ABL:11 == 2
		SOURCE:7 = 400
		TIMES SOURCE:5 , 2.50
		TIMES SOURCE:13, 0.80
	ELSEIF ABL:11 == 3
		SOURCE:7 = 700
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 0.50
	ELSEIF ABL:11 == 4
		SOURCE:7 = 1000
		TIMES SOURCE:5 , 4.00
		TIMES SOURCE:13, 0.20
	ELSE
		SOURCE:7 = 250 + (ABL:11 * 250)
		SOURCE:5 = SOURCE:5 * (ABL:11 * 50 + 250) / 100
		TIMES SOURCE:13, 0.00
	ENDIF

	;射精先タイプの設定
	TFLAG:703 = 5
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;;大量射精
;IF E == 2
;	TIMES SOURCE:7 , 1.50
;	TIMES SOURCE:5 , 1.20
;
;	EXP:PLAYER:3 += 2
;	EXP:20 += 2
;	PRINTL 大量射精
;	PRINTL 精液経験＋２
;	;精液汚れ
;	STAIN:PLAYER:2 |= 4
;
;	BASE:PLAYER:2 -= EJAC*2
;	SIF BASE:PLAYER:2 >= EJAC
;		BASE:PLAYER:2 = EJAC-1
;
;	;素股で射精させたフラグ
;	TFLAG:9 = 2
;;通常の射精
;ELSEIF E == 1
;	EXP:PLAYER:3 += 1
;	EXP:20 += 1
;	PRINTL 射精
;	PRINTL 精液経験＋１
;	;精液汚れ
;	STAIN:PLAYER:2 |= 4
;
;	BASE:PLAYER:2 -= EJAC
;	SIF BASE:PLAYER:2 >= EJAC
;		BASE:PLAYER:2 = EJAC-1
;
;	;素股で射精させたフラグ
;	TFLAG:9 = 1
;ENDIF

;奴隷のA⇔調教者のＰの汚れが移動
STAIN:4 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:4

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +7
	EXP:40 += 7
	EXP:PLAYER:40 += 7
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +7
	EXP:41 += 7
	EXP:PLAYER:41 += 7
ENDIF

SIF ASSIPLAY == 0
	TFLAG:30 += 1

TFLAG:100 = 1

;既成事実2に相当
TFLAG:200 = 2

;調教者がふたなり
IF TALENT:PLAYER:121
	SOURCE:13 /= 2
ENDIF

RETURN 1

;--------------------------------------------------
;調教時メッセージ
;--------------------------------------------------
;尻素股
@TRAIN_MESSAGE_COM38
;パートナーに主導権
IF TFLAG:45
	PRINTFORML %CALLNAME:TARGET%は%CALLNAME:PLAYER%に跨り、ペニスを尻の割れ目ではさみ、
	;恋慕＋淫乱＋欲情Lv5以上
	IF TALENT:85 && TALENT:76 && PALAM:5 > PALAMLV:6
		PRINTFORM 快楽に溺れきった表情で%CALLNAME:PLAYER%を見つめながら
	ELSEIF TALENT:85 && PALAM:5 > PALAMLV:6
		PRINT 今にも蕩けそうな表情を浮かべながら
	ELSEIF TALENT:76 && PALAM:5 > PALAMLV:6
		PRINTFORM 懇願するような眼差しで
	ELSEIF PALAM:5 > PALAMLV:5
		PRINT 口の端から涎を溢しながら
	ELSEIF TALENT:85 && TALENT:76
		PRINTFORM 陶酔しきった瞳で%CALLNAME:PLAYER%を見つめながら
	ELSEIF TALENT:85
		PRINT 幸せそうな表情を浮かべながら
	ELSEIF TALENT:76
		PRINT 淫猥な目つきで挑発しながら
	ELSEIF TALENT:36
		PRINT 大胆に腰を動かしながら
	ELSEIF TALENT:22
		PRINT うっすらと上気した顔で
	ELSE
		PRINT 恥ずかしそうに
	ENDIF
	PRINTFORM ペニスの裏側に手を添えて、
	PRINT 自らのお尻の
	;小柄体型
	IF TALENT:100
		PRINT 小さなワレメ
	ELSE
		PRINT ワレメ
	ENDIF
	PRINTL に擦りつけている…
;プレイヤーに主導権
ELSE
	PRINTFORM %CALLNAME:PLAYER%は自分のペニスを
	;小柄体型&ひんぬー
	SIF TALENT:100 && TALENT:109
		PRINT 肉付きの薄い
	PRINTFORML %CALLNAME:TARGET%のお尻に挟んで激しく擦った
	IF TALENT:85 == 0 && TALENT:88 == 0 && TALENT:89 == 0 && MARK:3 > 1
		PRINTFORML %CALLNAME:TARGET%はあまりの屈辱に、目に涙を浮かべていた…
	ELSEIF ASSIPLAY == 0 && TALENT:MASTER:219 && TALENT:220 && PALAM:5 > PALAMLV:5
		PRINTFORML %CALLNAME:TARGET%は巧みに腰を操り、%CALLNAME:PLAYER%の射精感を引き出した…
	ELSEIF ASSIPLAY == 0 && TALENT:85 && PALAM:5 > PALAMLV:5
		PRINTFORML %CALLNAME:TARGET%は、愛する人にお尻を使われて気持ちが高ぶっているようだ…
	ELSEIF PALAM:8 > PALAMLV:5
		PRINTFORML %CALLNAME:TARGET%は、さすがにお尻を使われることは恥ずかしいと思っているようだ…
	ELSE
		PRINTFORML %CALLNAME:TARGET%は、%CALLNAME:PLAYER%のなすがままにされていた…
	ENDIF
ENDIF

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_38
A = 0
S = 0
V = 75

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	A += ABL:1 * 2
ENDIF
;ABL:奉仕精神
IF ABL:6
	A += ABL:6 * 4
ENDIF
;ABL:精液中毒
IF ABL:11
	A += ABL:11 * 1
ENDIF

IF MARK:1
	A += MARK:1 * 2
ENDIF


;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 3
ENDIF

;恥じらい
IF TALENT:35
	A -= 1
ENDIF
;快感の否定
IF TALENT:71
	A -= 3
ENDIF
;男嫌い
IF TALENT:82 && TALENT:PLAYER:122
	A -= 7
ENDIF
;恋慕
IF TALENT:85 && ASSIPLAY == 0
	A += 3
	S = 3
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:121
	A += 8
ENDIF

;媚薬
IF TEQUIP:21
	A += 6
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2

;素股では汚れの影響が少なめ
Y /= 3

;汚れあり
IF Y
	A -= Y
	;汚臭鈍感
	IF TALENT:61
	ELSEIF TALENT:62
	ELSE
	ENDIF
ENDIF
;好感度
IF CFLAG:2 < 1000
	B = 0
	IF CFLAG:2 < 500
		B = 50
	ELSEIF CFLAG:2 < 750
		B = 20
	ELSEIF CFLAG:2 < 900
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 1500
	B = 0
	IF CFLAG:2 < 2000
		B = 5
	ELSE
		B = CFLAG:2 / 200
	ENDIF
	A += B
ENDIF

;合意
IF TALENT:152 == 0
	A -= 5
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:1
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:1,'(,ABL:1 * 2,')
		S = 1
	ENDIF
	;ABL:奉仕精神
	IF ABL:6
		SIF S
			PRINT  + 
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:6,'(,ABL:6 * 4,')
		S = 1
	ENDIF
	;ABL:精液中毒
	IF ABL:11
		SIF S
			PRINT  + 
		PRINTS ABLNAME:11
		PRINTV 'LV,ABL:11,'(,ABL:11 * 1,')
		S = 1
	ENDIF

	IF MARK:1
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:1,'(,MARK:1 * 2,')
		S = 1
	ENDIF


	;PALAM:欲情
	IF PALAM:5 < PALAMLV:1
		L = 0
	ELSEIF PALAM:5 < PALAMLV:2
		L = 1
	ELSEIF PALAM:5 < PALAMLV:3
		L = 2
	ELSEIF PALAM:5 < PALAMLV:4
		L = 3
	ELSEIF PALAM:5 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 3,')
		S = 1
	ENDIF

	;恥じらい'
	IF TALENT:35
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,1,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:71
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,3,')
		S = 1
	ENDIF
	;男嫌い
	IF TALENT:82 && TALENT:PLAYER:122
		PRINT  - 
		PRINTS TALENTNAME:82
		PRINTV '(,7,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:85 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,3,')
		S = 3
	ENDIF

	;調教者がふたなり
	IF TALENT:PLAYER:121
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:121
		PRINTV '(,8,')
		S = 1
	ENDIF

	;媚薬
	IF TEQUIP:21
		SIF S
			PRINT  + 
		PRINTS ITEMNAME:21
		PRINTV '(,6,')
		S = 1
	ENDIF

	Y = 0
	;愛液の汚れ
	SIF STAIN:PLAYER:2 & 1
		Y += 1
	;精液の汚れ
	SIF STAIN:PLAYER:2 & 4
		Y += 3
	;アナルの汚れ
	SIF STAIN:PLAYER:2 & 8
		Y += 7
	SIF STAIN:PLAYER:2 & 16
		Y += 15

	SIF TALENT:61
		Y /= 3
	SIF TALENT:62
		Y *= 2

	;素股では汚れの影響が少なめ
	Y /= 3

	;汚れあり
	IF Y
		PRINT  - 
		;汚臭鈍感
		IF TALENT:61
			PRINT 汚れあり、
			PRINTS TALENTNAME:61
		ELSEIF TALENT:62
			PRINT 汚れあり、
			PRINTS TALENTNAME:62
		ELSE
			PRINT 汚れあり
		ENDIF
		PRINTV '(,Y,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 < 1000
		B = 0
		PRINT  - 
		IF CFLAG:2 < 500
			B = 50
		ELSEIF CFLAG:2 < 750
			B = 20
		ELSEIF CFLAG:2 < 900
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 >= 1500
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 2000
			B = 5
		ELSE
			B = CFLAG:2 / 200
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合意
	IF TALENT:152 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,5,')
		S = 1
	ENDIF

	;合計を表示(75以上で実行)
	PRINT  = 
	PRINTV A

	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF
