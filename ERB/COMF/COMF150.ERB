@COM_ABLE150
;吸血
CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中じゃないと無理
SIF TFLAG:44 == 0
	RETURN 0
;今回の調教で既にしているとダメ
SIF TFLAG:65 & 3
	RETURN 0
;調教者が吸血鬼でないとダメ
SIF TALENT:PLAYER:114 == 0
	RETURN 0
;異常経験フィルタ
SIF (FLAG:61 & 1) == 0
	RETURN 0
RETURN 1

@COM150
;吸血する
PRINTL 吸血

;ベースは針。成功判定は主に足コキから。
;TFLAG:65とEXP:56、EXP:57を使用します

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = 吸血
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------

LOSEBASE:0 += 50
LOSEBASE:1 += 150

SOURCE:13 = 1000
SOURCE:14 = 4000
SOURCE:11 = 1000
SOURCE:16 = 1000
SOURCE:23 = 1000

;PALAM:苦痛をみる
IF PALAM:9 < PALAMLV:1
	SOURCE:6 = 2000
ELSEIF PALAM:9 < PALAMLV:2
	SOURCE:6 = 2300
ELSEIF PALAM:9 < PALAMLV:3
	SOURCE:6 = 2600
ELSEIF PALAM:9 < PALAMLV:4
	SOURCE:6 = 2850
ELSEIF PALAM:9 >= PALAMLV:4
	SOURCE:6 = 3000
ENDIF

;被吸血経験を見る
IF EXP:57 < EXPLV:1
	TIMES SOURCE:13 , 0.10
	TIMES SOURCE:14 , 2.50
	TIMES SOURCE:11 , 0.00
	TIMES SOURCE:16 , 0.00
	TIMES SOURCE:23 , 2.00
ELSEIF EXP:57 < EXPLV:2
	TIMES SOURCE:13 , 1.00
	TIMES SOURCE:14 , 1.00
	TIMES SOURCE:11 , 1.00
	TIMES SOURCE:16 , 0.50
	TIMES SOURCE:23 , 0.80
ELSEIF EXP:57 < EXPLV:3
	TIMES SOURCE:13 , 2.00
	TIMES SOURCE:14 , 0.25
	TIMES SOURCE:11 , 10.0
	TIMES SOURCE:16 , 5.00
	TIMES SOURCE:23 , 0.15
ELSE
	TIMES SOURCE:13 , 5.00
	TIMES SOURCE:14 , 0.00
	TIMES SOURCE:11 , 20.0
	TIMES SOURCE:16 , 10.0
	TIMES SOURCE:23 , 0.00
ENDIF

;調教者の技巧LVをみる
IF ABL:PLAYER:2 < 1
	TIMES SOURCE:6 , 2.00
ELSEIF ABL:PLAYER:2 < 2
	TIMES SOURCE:6 , 1.75
ELSEIF ABL:PLAYER:2 < 3
	TIMES SOURCE:6 , 1.50
ELSEIF ABL:PLAYER:2 < 4
	TIMES SOURCE:6 , 1.25
ELSEIF ABL:PLAYER:2 < 5
	TIMES SOURCE:6 , 1.00
ELSE
	;下がり過ぎないようにする
	IF ABL:PLAYER:2 <= 10
		SOURCE:6 = SOURCE:6 * (100 - ABL:PLAYER:2 * 5) / 100
	ELSE
		TIMES SOURCE:6 , 0.50
	ENDIF
ENDIF

;調教者の吸血経験を見る
IF EXP:PLAYER:56 < EXPLV:1
	TIMES SOURCE:6 , 1.20
ELSEIF EXP:PLAYER:56 < EXPLV:2
	TIMES SOURCE:6 , 1.00
ELSEIF EXP:PLAYER:56 < EXPLV:3
	TIMES SOURCE:6 , 0.80
ELSE
	TIMES SOURCE:6 , 0.50
ENDIF

;狂気(むしろ妹様かどうか、か)
IF TALENT:PLAYER:123
	TIMES SOURCE:6 , 5.00
	LOSEBASE:0 += 450
	LOSEBASE:1 += 350
ENDIF

;主人が吸うときに好感度がバカみたいに高いと情愛追加
;恭順追加に補正掛ける方が良いのかな
IF CFLAG:2 > 10000 && ASSIPLAY == 0
	SOURCE:3 += 1000 + (CFLAG:2 - 10000) / 50
	;相愛
	SIF TALENT:89
		TIMES SOURCE:3 , 3.00
ENDIF

;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:0 <= 1
		SOURCE:21 += 300 + (ABL:0 * 50)
	ELSEIF ABL:0 <= 3
		SOURCE:21 += 600 + (ABL:0 * 100)
	ELSEIF ABL:0 <= 5
		SOURCE:21 += 800 + (ABL:0 * 120)
	ELSEIF ABL:0 <= 8
		SOURCE:21 += 1000 + (ABL:0 * 140)
	ELSEIF ABL:0 <= 11
		SOURCE:21 += 1200 + (ABL:0 * 160)
	ELSE
		SOURCE:21 += 1500 + (ABL:0 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:15 == 0
		SOURCE:20 = 600
	ELSEIF ABL:15 == 1
		SOURCE:20 = 900
	ELSEIF ABL:15 == 2
		SOURCE:20 = 1200
	ELSEIF ABL:15 == 3
		SOURCE:20 = 1600
	ELSEIF ABL:15 == 4
		SOURCE:20 = 2000
	ELSE
		SOURCE:20 = 1000 + (ABL:15 * 300)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
SIF TALENT:85 || TALENT:153
	A += 5
SIF TALENT:88
	A += 5
;合意がないと好感度が下がる
IF TALENT:152 == 0 && TFLAG:48 == 0
	A -= 9
	SOURCE:15 += 400
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +2
	EXP:40 += 2
	EXP:PLAYER:40 += 2
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +2
	EXP:41 += 2
	EXP:PLAYER:41 += 2
ENDIF


EXP:PLAYER:56 += 1

EXP:57 += 1
PRINTFORML %EXPNAME:57%+1

EXP:50 += 5
PRINTFORML %EXPNAME:50%+5

IF ASSIPLAY
	TFLAG:65 |= 2
ELSE
	TFLAG:65 |= 1
ENDIF

RETURN 1

;吸血
@TRAIN_MESSAGE_COM150
PRINTFORMW %CALLNAME:PLAYER%は%CALLNAME:TARGET%の首筋に鋭く尖った犬歯を突き立てた…
PRINTFORM 傷口から溢れた血を%CALLNAME:PLAYER%が舐め取るのを、%CALLNAME:TARGET%は
IF (EXP:57 >= EXPLV:2) || (ABL:8 >= 5)
	PRINTFORM 恍惚とした表情で
ELSE
	PRINTFORM 苦痛に表情を歪めながら
ENDIF
PRINTFORMW 見つめている

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_150
A = 0
S = 0

V = 130
;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	A += ABL:1 * 1
ENDIF
;ABL:マゾっ気
IF ABL:8
	A += ABL:8 * 4
ENDIF

;既成事実
IF MARK:2
	A += MARK:1 * 1
ENDIF

;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 1
ENDIF

;;保守的
;IF TALENT:24
;	A -= 3
;ENDIF
;恥じらい
IF TALENT:35
	A -= 1
ENDIF

;快感に素直
IF TALENT:70
	A += 3
ENDIF
;快感の否定
IF TALENT:71
	A -= 1
ENDIF



;一線越えない
IF TALENT:151
	A -= 20
ENDIF



;調教者の小悪魔
IF TALENT:PLAYER:87
	A += 3
ENDIF

;恋慕
IF TALENT:85 && ASSIPLAY == 0
	A += 5
ENDIF

;ムードボーナス
B = 0
IF TFLAG:37 < 10
	B -= 10
ELSEIF TFLAG:37 < 30
	B -= 1
ELSEIF TFLAG:37 < 50
	B += 1
ELSEIF TFLAG:37 < 100
	B += TFLAG:37 / 10
ELSEIF TFLAG:37 < 300
	B += TFLAG:37 / 5
ELSE
	B += TFLAG:37 / 4
ENDIF
A += B
IF B < 0
	B = B * -1
ELSE
ENDIF


;好感度
IF CFLAG:2 < 3000
	B = 0
	IF CFLAG:2 < 1000
		B = 50
	ELSEIF CFLAG:2 < 1700
		B = 20
	ELSEIF CFLAG:2 < 2300
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 3500
	B = 0
	IF CFLAG:2 < 4500
		B = 5
	ELSE
		B = CFLAG:2 / 250
	ENDIF
	A += B
ENDIF

;被吸血経験
IF EXP:57 == 0
	A -= 15
ENDIF

;合意
IF TALENT:152 == 0
	A -= 20
ENDIF


IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:1
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:1,'(,ABL:1 * 1,')
		S = 1
	ENDIF
	;ABL:マゾっ気
	IF ABL:8
		SIF S
			PRINT  + 
		PRINTS ABLNAME:8
		PRINTV 'LV,ABL:8,'(,ABL:8 * 4,')
		S = 1
	ENDIF

	;既成事実
	IF MARK:2
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:1,'(,MARK:1 * 1,')
		S = 1
	ENDIF

	;PALAM:欲情
	IF PALAM:5 < PALAMLV:1
		L = 0
	ELSEIF PALAM:5 < PALAMLV:2
		L = 1
	ELSEIF PALAM:5 < PALAMLV:3
		L = 2
	ELSEIF PALAM:5 < PALAMLV:4
		L = 3
	ELSEIF PALAM:5 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 1,')
		S = 1
	ENDIF

	;;保守的
	;IF TALENT:24
	;	SIF S
	;		PRINT  - 
	;	PRINTS TALENTNAME:24
	;	PRINTV '(,3,')
	;	S = 1
	;ENDIF
	;恥じらい
	IF TALENT:35
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,1,')
		S = 1
	ENDIF

	;快感に素直
	IF TALENT:70
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:70
		PRINTV '(,3,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:71
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,1,')
		S = 1
	ENDIF



	;一線越えない
	IF TALENT:151
		PRINT  - 
		PRINTS TALENTNAME:27
		PRINTV '(,20,')
		S = 1
	ENDIF



	;調教者の小悪魔
	IF TALENT:PLAYER:87
		SIF S
			PRINT  + 
		PRINTFORM %NAME:PLAYER%が
		PRINTS TALENTNAME:87
		PRINTV '(,3,')
		S = 1
	ENDIF

	;恋慕
	IF TALENT:85 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 5
	ENDIF

	;ムードボーナス
	B = 0
	IF TFLAG:37 < 10
		B -= 10
	ELSEIF TFLAG:37 < 30
		B -= 1
	ELSEIF TFLAG:37 < 50
		B += 1
	ELSEIF TFLAG:37 < 100
		B += TFLAG:37 / 10
	ELSEIF TFLAG:37 < 300
		B += TFLAG:37 / 5
	ELSE
		B += TFLAG:37 / 4
	ENDIF
	IF B < 0
		B = B * -1
		PRINT  - 
		PRINT ムード悪い
		PRINTV '(,B,')
		S = 1
	ELSE
		PRINT  + 
		PRINT ムード良い
		PRINTV '(,B,')
		S = 1
	ENDIF


	;好感度
	IF CFLAG:2 < 3000
		B = 0
		PRINT  - 
		IF CFLAG:2 < 1000
			B = 50
		ELSEIF CFLAG:2 < 1700
			B = 20
		ELSEIF CFLAG:2 < 2300
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 >= 3500
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 4500
			B = 5
		ELSE
			B = CFLAG:2 / 250
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;被吸血経験
	IF EXP:57 == 0
		PRINT  - 
		PRINTFORM %EXPNAME:57%無し
		PRINTV '(,15,')
		S = 1
	ENDIF

	;合意
	IF TALENT:152 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,20,')
		S = 1
	ENDIF

	;合計を表示(130以上で実行)
	PRINT  = 
	PRINTV A

	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF

