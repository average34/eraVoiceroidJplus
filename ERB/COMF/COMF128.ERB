@COM_ABLE128
;膣酒呑み実行判定
;COM_ABLE共通判定
CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中じゃないと無理
SIF TFLAG:44 == 0
	RETURN 0
;オトコだとダメ
SIF TALENT:オトコ
	RETURN 0
;処女だとダメ
SIF TALENT:処女
	RETURN 0
;従順3以下・レズっ気3以下のASSIの場合はできない（酒豪ならＯＫ）
IF ASSIPLAY
	SIF (ABL:ASSI:親密 <= 3 || ABL:ASSI:レズっ気 <= 3) && TALENT:ASSI:酒豪 == 0
		RETURN 0
ENDIF
;バイブ装着中はダメ
SIF TEQUIP:11
	RETURN 0
;触手調教中はダメ
SIF TEQUIP:90
	RETURN 0
RETURN 1

@COM128
;膣酒呑み

PRINTL 膣酒呑み
STR:0 = 膣酒呑み
CALL TRAIN_MESSAGE_B

;V経験を伴うコマンドのフラグ(処女膜再生対応)
TFLAG:19 = 1

;痛み
SOURCE:痛み += 200
;液体
SOURCE:液体追加 = 4000
;欲情
SOURCE:欲情追加 = 1000
;露出
SOURCE:露出 = 500
;屈服
SOURCE:屈従 = 200
;逸脱
SOURCE:逸脱 = 1000

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:快Ｖ = 15
	SOURCE:屈従 += 50
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:快Ｖ = 50
	SOURCE:屈従 += 70
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:快Ｖ = 80
	SOURCE:屈従 += 90
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:快Ｖ = 100
	SOURCE:屈従 += 120
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:快Ｖ = 200
	SOURCE:屈従 += 400
ELSE
	SOURCE:快Ｖ = 0 + (ABL:Ｖ感覚 * 50)
	SOURCE:屈従 += 0 + (ABL:Ｖ感覚 * 100)
ENDIF

;EXP:Ｖ経験をみる
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:快Ｖ , 0.20
	TIMES SOURCE:屈従, 0.20
	SOURCE:痛み = 400
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:快Ｖ , 0.50
	TIMES SOURCE:屈従, 0.50
	SOURCE:痛み = 350
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:屈従, 0.80
	SOURCE:痛み = 350
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:快Ｖ , 1.20
	TIMES SOURCE:屈従, 1.00
	SOURCE:痛み = 300
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:快Ｖ , 1.60
	TIMES SOURCE:屈従, 1.20
	SOURCE:痛み = 200
ELSE
	TIMES SOURCE:快Ａ , 1.80
	TIMES SOURCE:屈従, 1.50
	SOURCE:痛み = 100
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ｖ , 0.10
	SOURCE:痛み += 1500
	TIMES SOURCE:痛み , 3.00
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ｖ , 0.20
	SOURCE:痛み += 200
	TIMES SOURCE:痛み , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ｖ , 0.60
	TIMES SOURCE:痛み , 0.80
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:痛み , 0.50
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:快Ｖ , 2.00
	TIMES SOURCE:痛み , 0.10
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:快Ｖ , 0.50
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:快Ｖ , 0.80
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:快Ｖ , 1.20
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.50
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.80
ENDIF


;Ｖ敏感、鈍感をみる
;快Ｖ自体のチェックは後でまとめてやる
IF TALENT:Ｖ鈍感
	TIMES SOURCE:痛み , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ｖ敏感
	TIMES SOURCE:痛み , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

;処女で貞操観念
;IF EXP:Ｖ経験 == 0 && TALENT:貞操観念 && TALENT:恋慕 == 0
;	TIMES SOURCE:屈従 , 2.00
;ENDIF

EXP:Ｖ経験 += 1
PRINTL Ｖ経験＋１

;異常経験
EXP:異常経験 += 1
PRINTFORML %EXPNAME:50%＋１

IF TEQUIP:90
	;奴隷のＶに触手の汚れがつく
	STAIN:ヴァギナ |= 2
	STAIN:ヴァギナ |= 4
ELSE
	;奴隷のＶ⇔調教者の指の汚れが移動
	STAIN:ヴァギナ |= STAIN:PLAYER:手
	STAIN:PLAYER:手 |= STAIN:ヴァギナ
ENDIF

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:レズ経験 += 1
	EXP:PLAYER:レズ経験 += 1
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:ＢＬ経験 += 1
	EXP:PLAYER:ＢＬ経験 += 1
ENDIF


;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:受動 += 300 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:受動 += 600 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:受動 += 800 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:受動 += 1000 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:受動 += 1200 + (ABL:親密 * 160)
	ELSE
		SOURCE:受動 += 1500 + (ABL:親密 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:征服 = 600
	ELSEIF ABL:サドっ気 == 1
		SOURCE:征服 = 900
	ELSEIF ABL:サドっ気 == 2
		SOURCE:征服 = 1200
	ELSEIF ABL:サドっ気 == 3
		SOURCE:征服 = 1600
	ELSEIF ABL:サドっ気 == 4
		SOURCE:征服 = 2000
	ELSE
		SOURCE:征服 = 1000 + (ABL:サドっ気 * 300)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
;合意がないと好感度が下がる
IF TALENT:合意 == 0 && TFLAG:48 == 0
	A -= 15
	SOURCE:反感追加 += 400
ENDIF
;酒豪もちの場合変化
IF TALENT:TARGET:酒豪
	A += RAND:12 + 8
ELSE
	A = RAND:7 - 7
ENDIF
	TFLAG:30 += A
TFLAG:30 += A
TFLAG:37 += A / 2

;ほろ酔い値＋
;酒豪もちの場合効果上昇
IF TALENT:TARGET:酒豪
	TFLAG:47 += 20
ELSE
	TFLAG:47 += 15
ENDIF


RETURN 1

;膣酒呑み
@TRAIN_MESSAGE_COM128
PRINTFORM %CALLNAME:TARGET%の
IF TEQUIP:61
	PRINT クスコで拡げられた膣内に
ELSEIF STAIN:ヴァギナ & 4 && TFLAG:2 > 0
	PRINT 精液で満たされた膣内に
ELSEIF ABL:Ｖ感覚 >= 4
	PRINT 指で押し広げた膣内に
ELSE
	PRINT ヒクつく膣内に
ENDIF
PRINTL 酒瓶を突き立て、中身を流し込んだ……
PRINTL 
PRINTFORM %CALLNAME:TARGET%は
IF TFLAG:47 >= 21
	PRINTFORM 既に泥酔していた事もあり、呂律の回らない口で悦びの声を上げながら
ELSEIF ABL:Ｖ感覚 >= 5
	PRINTFORM 膣内を満たすアルコールの感覚に酔い痴れる様に腰を震わせ、蕩けきった表情で
ELSE
	PRINTFORM 膣内を焼かれる様な感覚に悲鳴を上げ、腰をしきりに跳ねさせながら
ENDIF
PRINTW ボコボコと音を立て、酒を飲み干していった……

