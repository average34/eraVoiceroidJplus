@COM90
;アナル愛撫させる

PRINTL アナル愛撫させる

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = アナル愛撫させる
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック（ふたなりの場合）
;-------------------------------------------------
SIF TALENT:PLAYER:半人半妖 || TALENT:PLAYER:オトコ  || TALENT:PLAYER:ふたなり
	BASE:PLAYER:射精 += CALC_EJAC(SELECTCOM, PLAYER)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 10
LOSEBASE:気力 += 50

SOURCE:13 = 300
SOURCE:14 = 500

;上のほうで計算した汚れデータ
SOURCE:8 = Y*80 + 50

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:4 = 200
	SOURCE:5 = 100
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:奉仕精神 == 1
	SOURCE:4 = 400
	SOURCE:5 = 200
	TIMES SOURCE:8 , 1.25
ELSEIF ABL:奉仕精神 == 2
	SOURCE:4 = 800
	SOURCE:5 = 400
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:奉仕精神 == 3
	SOURCE:4 = 1200
	SOURCE:5 = 800
	TIMES SOURCE:8 , 0.50
ELSEIF ABL:奉仕精神 == 4
	SOURCE:4 = 1600
	SOURCE:5 = 1200
	TIMES SOURCE:8 , 0.25
ELSE
	SOURCE:4 = (ABL:奉仕精神 * 400)
	SOURCE:5 = (ABL:奉仕精神 * 300)
	TIMES SOURCE:8 , 0.10
ENDIF


;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:4 , 0.10
	TIMES SOURCE:5 , 0.10
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:4 , 0.25
	TIMES SOURCE:5 , 0.25
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:4 , 2.00
	TIMES SOURCE:5 , 2.00
ELSE
	SOURCE:4 = SOURCE:4 * (ABL:技巧 * 75 + 25) / 100
	SOURCE:5 = SOURCE:5 * (ABL:技巧 * 75 + 25) / 100
ENDIF

;ABL:サドっ気をみる
IF ABL:サドっ気 == 0
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:サドっ気 == 1
	TIMES SOURCE:4 , 1.05
	TIMES SOURCE:5 , 1.05
ELSEIF ABL:サドっ気 == 2
	TIMES SOURCE:4 , 1.10
	TIMES SOURCE:5 , 1.10
ELSEIF ABL:サドっ気 == 3
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:サドっ気 == 4
	TIMES SOURCE:4 , 1.30
	TIMES SOURCE:5 , 1.30
ELSE
	SOURCE:4 = SOURCE:4 * (ABL:サドっ気 * 5 + 125) / 100
	SOURCE:5 = SOURCE:5 * (ABL:サドっ気 * 5 + 125) / 100
ENDIF

;ABL:Ａ経験をみる
IF EXP:PLAYER:Ａ経験 < EXPLV:3
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF EXP:PLAYER:Ａ経験 < EXPLV:4
	TIMES SOURCE:4 , 0.75
	TIMES SOURCE:5 , 0.75
ELSEIF EXP:PLAYER:Ａ経験 < EXPLV:5
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSE
	SOURCE:4 = SOURCE:4 * EXP:PLAYER:Ａ経験 / 200
	SOURCE:5 = SOURCE:5 * EXP:PLAYER:Ａ経験 / 200
ENDIF

;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:21 += 300 + (ABL:親密 * 80)
	ELSEIF ABL:親密 <= 3
		SOURCE:21 += 600 + (ABL:親密 * 130)
	ELSEIF ABL:親密 <= 5
		SOURCE:21 += 800 + (ABL:親密 * 150)
	ELSEIF ABL:親密 <= 8
		SOURCE:21 += 1000 + (ABL:親密 * 170)
	ELSEIF ABL:親密 <= 11
		SOURCE:21 += 1200 + (ABL:親密 * 190)
	ELSE
		SOURCE:21 += 1500 + (ABL:親密 * 210)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:20 = 600
	ELSEIF ABL:サドっ気 == 1
		SOURCE:20 = 900
	ELSEIF ABL:サドっ気 == 2
		SOURCE:20 = 1200
	ELSEIF ABL:サドっ気 == 3
		SOURCE:20 = 1600
	ELSEIF ABL:サドっ気 == 4
		SOURCE:20 = 2000
	ELSE
		SOURCE:20 = 1000 + (ABL:サドっ気 * 300)
	ENDIF
ENDIF

EXP:PLAYER:Ａ経験 += 1
PRINTFORML Ａ経験＋１(%CALLNAME:PLAYER%)　(現在{EXP:PLAYER:Ａ経験})

;好感度ランダム変化
A = RAND:10 + 5
SIF TALENT:恋慕 || TALENT:恋人
	A += 5
SIF TALENT:親愛
	A += 5
;合意がないと好感度が下がる
IF TALENT:合意 == 0 && TFLAG:48 == 0
	A -= 10
	SOURCE:15 += 800
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2
;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK
;;オーバーフロー対策に、値が大きすぎる場合と、負の場合は調整を加える
;IF BASE:PLAYER:射精 > 100000000 || BASE:PLAYER:射精 < 0
;	BASE:PLAYER:射精 = 100000000
;ENDIF
;S = BASE:PLAYER:射精
;EJAC = MAXBASE:PLAYER:射精
;
;IF  S > EJAC * 2
;	E = 2
;ELSEIF S > EJAC
;	E = 1
;ELSE
;	E = 0
;ENDIF

IF E
;射精している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:精液中毒 == 1
		SOURCE:7 = 200
		TIMES SOURCE:5 , 2.50
		TIMES SOURCE:13, 1.60
	ELSEIF ABL:精液中毒 == 2
		SOURCE:7 = 500
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 1.00
	ELSEIF ABL:精液中毒 == 3
		SOURCE:7 = 1200
		TIMES SOURCE:5 , 4.50
		TIMES SOURCE:13, 0.70
	ELSEIF ABL:精液中毒 == 4
		SOURCE:7 = 2500
		TIMES SOURCE:5 , 6.00
		TIMES SOURCE:13, 0.40
	ELSE
		SOURCE:7 = (ABL:精液中毒 * 1200) - 1000
		SOURCE:5 = SOURCE:5 * (ABL:精液中毒 + 3)
		TIMES SOURCE:13, 0.10
	ENDIF

	;射精先タイプの設定
	TFLAG:703 = 3
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;;大量射精
;IF E == 2
;	TIMES SOURCE:7 , 2.00
;	TIMES SOURCE:5 , 1.50
;
;	EXP:PLAYER:射精経験 += 2
;	EXP:精液経験 += 4
;	PRINTL 大量射精
;	PRINTL 精液経験＋４
;	;精液汚れ
;	STAIN:PLAYER:ペニス |= 4
;
;	BASE:PLAYER:射精 -= EJAC*2
;	SIF BASE:PLAYER:射精 >= EJAC
;		BASE:PLAYER:射精 = EJAC-1
;
;	;クンニで射精させたフラグ
;	TFLAG:5 = 2
;;通常の射精
;ELSEIF E == 1
;	EXP:PLAYER:射精経験 += 1
;	EXP:精液経験 += 1
;	PRINTL 射精
;	PRINTL 精液経験＋１
;	;精液汚れ
;	STAIN:PLAYER:ペニス |= 4
;
;	BASE:PLAYER:射精 -= EJAC
;	SIF BASE:PLAYER:射精 >= EJAC
;		BASE:PLAYER:射精 = EJAC-1
;
;	;クンニで射精させたフラグ
;	TFLAG:5 = 1
;ENDIF

;奴隷の口⇔調教者のＡの汚れが移動
STAIN:口 |= STAIN:PLAYER:アナル
STAIN:PLAYER:アナル |= STAIN:口

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +6
	EXP:レズ経験 += 6
ENDIF

TFLAG:100 = 1

;既成事実２に相当
TFLAG:200 = 2

RETURN 1

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_90
A = 0
S = 0
V = 12

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	A += ABL:欲望 * 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	A += ABL:奉仕精神 * 3
ENDIF

IF MARK:快楽依存
	A += MARK:快楽依存 * 2
ENDIF

;恥じらい
IF TALENT:恥じらい
	A -= 1
ENDIF
;汚臭鈍感
IF TALENT:汚臭鈍感
	A += 2
ENDIF
;汚臭敏感
IF TALENT:汚臭敏感
	A -= 2
ENDIF
;献身的
IF TALENT:献身的
	A += 4
ENDIF
;汚れ無視
IF TALENT:汚れ無視
	A += 3
ENDIF
;快感の否定
IF TALENT:快感の否定
	A -= 1
ENDIF
;倒錯的
IF TALENT:倒錯的
	A += 3
ENDIF
;サド
IF TALENT:PLAYER:サド
	A += 3
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	A += 5
	S = 5
ENDIF


Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:アナル & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:アナル & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:アナル & 8
	Y += 7
SIF STAIN:PLAYER:アナル & 16
	Y += 15

SIF TALENT:汚臭鈍感
	Y /= 3
SIF TALENT:汚臭敏感
	Y *= 2

;アナル愛撫させるでは汚れがそれほど影響しない
Y /= 2

;汚れあり
IF Y
	A -= Y
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
		S = 1
	ENDIF
	;ABL:奉仕精神
	IF ABL:奉仕精神
		SIF S
			PRINT  + 
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 3,')
		S = 1
	ENDIF

	IF MARK:快楽依存
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:快楽依存,'(,MARK:快楽依存 * 2,')
		S = 1
	ENDIF

	;恥じらい'
	IF TALENT:恥じらい
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,1,')
		S = 1
	ENDIF
	;汚臭鈍感
	IF TALENT:汚臭鈍感
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:61
		PRINTV '(,2,')
		S = 1
	ENDIF
	;汚臭敏感
	IF TALENT:汚臭敏感
		PRINT  - 
		PRINTS TALENTNAME:62
		PRINTV '(,2,')
		S = 1
	ENDIF
	;献身的
	IF TALENT:献身的
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:63
		PRINTV '(,4,')
		S = 1
	ENDIF
	;汚れ無視
	IF TALENT:汚れ無視
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:64
		PRINTV '(,3,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:快感の否定
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,1,')
		S = 1
	ENDIF
	;倒錯的
	IF TALENT:倒錯的
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:80
		PRINTV '(,3,')
		S = 1
	ENDIF
	;サド
	IF TALENT:PLAYER:サド
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:83
		PRINTV '(,3,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:恋慕 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 5
	ENDIF


	Y = 0
	;愛液の汚れ
	SIF STAIN:PLAYER:アナル & 1
		Y += 1
	;精液の汚れ
	SIF STAIN:PLAYER:アナル & 4
		Y += 3
	;アナルの汚れ
	SIF STAIN:PLAYER:アナル & 8
		Y += 7
	SIF STAIN:PLAYER:アナル & 16
		Y += 15

	SIF TALENT:汚臭鈍感
		Y /= 3
	SIF TALENT:汚臭敏感
		Y *= 2

	;アナル愛撫させるでは汚れがそれほど影響しない
	Y /= 2
	;汚れあり
	IF Y
		PRINT  - 
		;汚臭鈍感
		IF TALENT:汚臭鈍感
			PRINT 汚れあり、
			PRINTS TALENTNAME:61
		ELSEIF TALENT:汚臭敏感
			PRINT 汚れあり、
			PRINTS TALENTNAME:62
		ELSE
			PRINT 汚れあり
		ENDIF
		PRINTV '(,Y,')
		S = 1
	ENDIF


	;合計を表示(12以上で実行)
	PRINT  = 
	PRINTV A

	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF

;アナル愛撫させる
@TRAIN_MESSAGE_COM90
PRINTFORM %CALLNAME:TARGET%は
SIF TALENT:恋慕
	PRINT 愛しむような表情で、
SIF TALENT:淫乱
	PRINT 自分から
IF EXP:PLAYER:Ａ経験 > EXPLV:4 && RAND:4
	PRINTFORMW %CALLNAME:PLAYER%のアヌスに指を根元まで突き刺し、精嚢を直腸側から圧迫している…
	TFLAG:17 = 5
ELSEIF EXP:PLAYER:Ａ経験 > EXPLV:4 && RAND:3
	PRINTFORMW %CALLNAME:PLAYER%のアヌスに指を深く差し込み、関節を器用に折り曲げて指の腹で前立腺を刺激している…
	TFLAG:17 = 4
ELSEIF EXP:PLAYER:Ａ経験 > EXPLV:3 && RAND:2
	PRINTFORMW %CALLNAME:PLAYER%のアヌスに指を出し入れしている…
	TFLAG:17 = 3
ELSEIF EXP:PLAYER:Ａ経験 > EXPLV:2 && RAND:2
	PRINTFORMW %CALLNAME:PLAYER%のアヌスに指の先端を差し込み、縁をなぞりながら菊門を拡げようとしている…
	TFLAG:17 = 2
ELSE
	PRINTFORMW %CALLNAME:PLAYER%の窄まりに指を押し当て、円を描く様に皺襞をなぞっている…
	TFLAG:17 = 1
ENDIF

@COM_ABLE90
;アナル愛撫させる実行判定
;COM_ABLE共通判定
CALL COM_ABLE_COMMON, SELECTCOM
SIF !RESULT
	RETURN 0
;ウフフ中じゃないと無理
SIF TFLAG:44 == 0
	RETURN 0
;調教者がオトコか半人半妖かふたなりじゃないとダメ
SIF TALENT:PLAYER:半人半妖 == 0 && TALENT:PLAYER:ふたなり == 0 && TALENT:PLAYER:オトコ == 0
	RETURN 0
;縄使用中は不可
SIF TEQUIP:44
	RETURN 0
;触手調教中はダメ
SIF TEQUIP:90
	RETURN 0
RETURN 1
