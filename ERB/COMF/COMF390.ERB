@CASINO_GAME
$CASINO_GAME
T = 0
Z = 0
TFLAG:80 = 0
	DRAWLINE
	PRINTL 
	PRINTL チップは1枚$100です。チップを購入しますか？
	PRINTFORML 現在の所持金：${MONEY}
	$INPUT_LOOP_CASINO
	PRINTL 
	PRINTL [0] 購入する
	PRINTL [1] やっぱりやめる
	PRINTL 
	INPUT
	$INPUT_TIP
	IF RESULT == 0 || Z == 1
		PRINTL 
		PRINTL チップを購入します。何枚購入しますか？
		INPUT
		IF RESULT > MONEY/100
			PRINTL 所持金が足りません。もう一度枚数を入力してください
			Z = 1
			GOTO INPUT_TIP
		ELSEIF RESULT == 0
			PRINTW 購入をやめました
			RETURN 1
		ELSEIF RESULT < 0
			PRINTL 正しい枚数を入力してください
			GOTO INPUT_TIP
		ELSE
			T += RESULT
			PRINTFORMW チップを{T}枚購入しました
			MONEY -= T * 100
			E = T
		ENDIF
	ELSEIF RESULT < 0 || RESULT > 1
		GOTO INPUT_LOOP_CASINO
	ELSEIF RESULT == 1
		PRINTW 購入をやめました
		RETURN 1
	ENDIF
	$INPUT_LOOP_CASINO_GAME
	PRINTL 
	PRINTL どのゲームをしますか？
	PRINTFORML 現在のチップ：{T}枚
	PRINTL 
	PRINTL [0] カードめくり
	PRINTL [1] レッドドッグ
	PRINTL [2] ブラックジャック
	PRINTL [3] ポーカー
	PRINTL 
	INPUT
	IF RESULT < 0 || RESULT > 3
		Z = 2
		PRINTFORML ゲームを選択してください
		GOTO INPUT_LOOP_CASINO_GAME
	;カードめくり
	ELSEIF RESULT == 0
		CALL TURN_UP_A_CARD
	;レッドドッグ
	ELSEIF RESULT == 1
		CALL RED_DOG
	;ブラックジャック
	ELSEIF RESULT == 2
		CALL BLACK_JACK
	;ポーカー
	ELSEIF RESULT == 3
		CALL POKER
	ENDIF

RETURN 1

