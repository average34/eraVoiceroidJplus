@COM_ABLE195
;騎乗位する実行判定
;COM_ABLE共通判定
CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中じゃないと無理
SIF TFLAG:44 == 0
	RETURN 0
;調教者がオトコだとダメ
SIF TALENT:PLAYER:オトコ
	RETURN 0
;小人体型持ちは、双方が調教者と調教対象ならOK
;それ以外の場合は主人が禁断の知識持ちじゃないとダメ
SIF TALENT:小人体型 && (TALENT:MASTER:禁断の知識 == 0 && TALENT:PLAYER:小人体型 == 0)
	RETURN 0
;オトコ、半人半妖、ふたなりでない場合、ペニスバンドがないとダメ
IF (TALENT:TARGET:半人半妖 || TALENT:TARGET:ふたなり || TALENT:TARGET:オトコ) == 0
	SIF ITEM:ペニスバンド == 0 && NOITEM == 0
		RETURN 0
ENDIF
;オナホール使用中はダメ
SIF TEQUIP:17
	RETURN 0
;触手調教中はダメ
SIF TEQUIP:90
	RETURN 0
;お風呂場プレイ中はプレイマットが無いとダメ
IF TEQUIP:58
SIF ITEM:プレイマット == 0 && NOITEM == 0
	RETURN 0
ENDIF
;新妻プレイ中はダメ
SIF TEQUIP:59
	RETURN 0
;時止め中は無理
SIF TEQUIP:81
	RETURN 0
RETURN 1

@COM195
;騎乗位する

PRINTL 騎乗位する

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = 騎乗位する
CALL TRAIN_MESSAGE_B

;同様に前回の調教が正常位させるか後背位させるの場合
IF TFLAG:60 && (PREVCOM == 193 || PREVCOM == 194) && ((ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0))
	SIF TFLAG:31
		TFLAG:31 = 2
ENDIF

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
;SIF TALENT:PLAYER:半人半妖 || TALENT:PLAYER:ふたなり || TALENT:PLAYER:オトコ
;	BASE:PLAYER:射精 += B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 100
LOSEBASE:気力 += 300

SOURCE:情愛 = 900
SOURCE:達成感 = 1000
SOURCE:接触 = 500
SOURCE:欲情追加 = 800
SOURCE:屈従 = 500
SOURCE:逸脱 = 500

;ABL:Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	SOURCE:快Ｃ = 1000
	SOURCE:中毒充足 = 1000
ELSEIF ABL:Ｃ感覚 == 1
	SOURCE:快Ｃ = 1400
	SOURCE:中毒充足 = 1300
ELSEIF ABL:Ｃ感覚 == 2
	SOURCE:快Ｃ = 1900
	SOURCE:中毒充足 = 1700
ELSEIF ABL:Ｃ感覚 == 3
	SOURCE:快Ｃ = 2500
	SOURCE:中毒充足 = 2000
ELSEIF ABL:Ｃ感覚 == 4
	SOURCE:快Ｃ = 3000
	SOURCE:中毒充足 = 2500
ELSE
	SOURCE:快Ｃ = 2550 + (ABL:Ｃ感覚 * 150)
	SOURCE:中毒充足 = 3100
ENDIF


;ABL:技巧をみる
IF ABL:PLAYER:技巧 == 0
	TIMES SOURCE:快Ｃ , 0.50
	TIMES SOURCE:中毒充足 , 0.50
ELSEIF ABL:PLAYER:技巧 == 1
	TIMES SOURCE:快Ｃ , 0.80
	TIMES SOURCE:中毒充足 , 0.80
ELSEIF ABL:PLAYER:技巧 == 2
	TIMES SOURCE:快Ｃ , 1.00
	TIMES SOURCE:中毒充足 , 1.00
ELSEIF ABL:PLAYER:技巧 == 3
	TIMES SOURCE:快Ｃ , 1.20
	TIMES SOURCE:中毒充足 , 1.50
ELSEIF ABL:PLAYER:技巧 == 4
	TIMES SOURCE:快Ｃ , 1.50
	TIMES SOURCE:中毒充足 , 2.50
ELSE
	SOURCE:快Ｃ = SOURCE:快Ｃ * (ABL:PLAYER:技巧 * 75 + 25) / 200
	SOURCE:中毒充足 = SOURCE:中毒充足 * (ABL:PLAYER:技巧 * 75 + 25) / 100
ENDIF

;ABL:奉仕精神をみる
IF ABL:PLAYER:奉仕精神 == 0
	TIMES SOURCE:情愛 , 0.60
;	TIMES SOURCE:中毒充足 , 0.60
ELSEIF ABL:PLAYER:奉仕精神 == 1
	TIMES SOURCE:情愛 , 0.80
;	TIMES SOURCE:中毒充足 , 0.80
ELSEIF ABL:PLAYER:奉仕精神 == 2
	TIMES SOURCE:情愛 , 1.00
;	TIMES SOURCE:中毒充足 , 1.00
ELSEIF ABL:PLAYER:奉仕精神 == 3
	TIMES SOURCE:情愛 , 1.50
;	TIMES SOURCE:中毒充足 , 1.50
ELSEIF ABL:PLAYER:奉仕精神 == 4
	TIMES SOURCE:情愛 , 2.50
;	TIMES SOURCE:中毒充足 , 2.50
ELSE
	SOURCE:情愛 = SOURCE:情愛 * (ABL:PLAYER:技巧 * 75 + 25) / 100
;	SOURCE:中毒充足 = SOURCE:中毒充足 * (ABL:PLAYER:技巧 * 75 + 25) / 100
ENDIF

;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:受動 += 300 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:受動 += 600 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:受動 += 800 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:受動 += 1000 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:受動 += 1200 + (ABL:親密 * 160)
	ELSE
		SOURCE:受動 += 1500 + (ABL:親密 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:征服 = 600
	ELSEIF ABL:サドっ気 == 1
		SOURCE:征服 = 900
	ELSEIF ABL:サドっ気 == 2
		SOURCE:征服 = 1200
	ELSEIF ABL:サドっ気 == 3
		SOURCE:征服 = 1600
	ELSEIF ABL:サドっ気 == 4
		SOURCE:征服 = 2000
	ELSE
		SOURCE:征服 = 1000 + (ABL:サドっ気 * 300)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
;合意がないと好感度が下がる
IF TALENT:合意 == 0 && TFLAG:48 == 0
	A -= 9
	SOURCE:反感追加 += 400
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;調教者が処女
IF TALENT:PLAYER:処女
	TIMES SOURCE:屈従 , 10.00
	TIMES SOURCE:逸脱 , 3.00
	TALENT:PLAYER:処女 = 0
	
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:懐疑 = 5000
	ELSEIF ABL:サドっ気 == 1
		SOURCE:懐疑 = 4000
	ELSEIF ABL:サドっ気 == 2
		SOURCE:懐疑 = 3000
	ELSEIF ABL:サドっ気 == 3
		SOURCE:懐疑 = 2000
	ELSEIF ABL:サドっ気 == 4
		SOURCE:懐疑 = 1500
	ELSE
		SOURCE:懐疑 = 1100 - (ABL:サドっ気 * 40)
		SIF SOURCE:懐疑 < 0
			SOURCE:懐疑 = 0
	ENDIF
	
	;サド持ちは懐疑1/4
	SIF TALENT:サド
		TIMES SOURCE:懐疑 , 0.25

	;小悪魔持ちは懐疑1/2
	SIF TALENT:小悪魔
		TIMES SOURCE:懐疑 , 0.50

	PRINTFORML ＜%CALLNAME:PLAYER%が処女＞
	EXP:異常経験 += 3
	PRINTFORML %EXPNAME:50%+3
	IF TALENT:恋慕 && ASSIPLAY == 0
		IF ABL:親密 < 10
			PRINTFORML そして%ABLNAME:0%が10になった
			ABL:親密 = 10
		ENDIF
		TIMES SOURCE:欲情追加 , 10.00
		TIMES SOURCE:情愛 , 3.00
		TIMES SOURCE:懐疑 , 0.50
		SIF TALENT:相愛
			TIMES SOURCE:懐疑 , 0.20
	ELSE
		IF ABL:親密 < 1
			PRINTFORML そして%ABLNAME:0%が1になった
			ABL:親密 = 1
		ENDIF
	ENDIF
ENDIF
EXP:PLAYER:Ｖ経験 += 1

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
;CALL SAMEN_CHECK

;射精時の処理
;IF E >= 1
;	;コマンドごとに固有の処理はこの下に書く
;	TIMES SOURCE:性行動 , 3.00
;
;	;ABL:精液中毒をみる
;	IF ABL:精液中毒 == 0
;		SOURCE:中毒充足 = 0
;		TIMES SOURCE:快Ｃ , 2.00
;		TIMES SOURCE:屈従, 2.00
;	ELSEIF ABL:精液中毒 == 1
;		SOURCE:中毒充足 = 50
;		TIMES SOURCE:快Ｃ , 2.50
;		TIMES SOURCE:屈従, 1.60
;	ELSEIF ABL:精液中毒 == 2
;		SOURCE:中毒充足 = 150
;		TIMES SOURCE:快Ｃ , 3.00
;		TIMES SOURCE:屈従, 1.00
;	ELSEIF ABL:精液中毒 == 3
;		SOURCE:中毒充足 = 300
;		TIMES SOURCE:快Ｃ , 4.50
;		TIMES SOURCE:屈従, 0.70
;	ELSEIF ABL:精液中毒 == 4
;		SOURCE:中毒充足 = 600
;		TIMES SOURCE:快Ｃ , 6.00
;		TIMES SOURCE:屈従, 0.40
;	ELSE
;		SOURCE:中毒充足 = 1200
;		TIMES SOURCE:快Ｃ , 8.00
;		TIMES SOURCE:屈従, 0.10
;	ENDIF
;
;	;射精先タイプの設定
;	TFLAG:703 = 9
;	;射精先選択の可・不可
;	;TFLAG:704 = 0
;	;汎用的な処理はこの関数で行う
;	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
;	CALL SAMEN_SHOOT
;ENDIF


IF TALENT:半人半妖 || TALENT:ふたなり || TALENT:オトコ
	;奴隷のＰ⇔調教者のＶの汚れが移動
	STAIN:ペニス |= STAIN:PLAYER:ヴァギナ
	STAIN:PLAYER:ヴァギナ |= STAIN:ペニス
ENDIF

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +4
	EXP:レズ経験 += 4
	EXP:PLAYER:レズ経験 += 4
ENDIF

TFLAG:100 = 0

;既成事実１に相当
TFLAG:200 = 1

RETURN 1



;騎乗位する
@TRAIN_MESSAGE_COM195
;前回と今回の調教者が同じで、前回の調教が騎乗位するであり、抜かないフラグがある場合
IF TFLAG:60 && PREVCOM == 195 && ((ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0))
	PRINTFORMW %CALLNAME:PLAYER%は%CALLNAME:TARGET%のペニス\@ TALENT:半人半妖 || TALENT:ふたなり || TALENT:オトコ ? # バンド \@をきつく締めつけたまま、%CALLNAME:TARGET%のために腰を振りたてた…
;同様に前回の調教が正常位させるか後背位させるの場合
ELSEIF TFLAG:60 && (PREVCOM == 193 || PREVCOM == 194) && ((ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0))
	PRINTFORMW %CALLNAME:TARGET%は、膣内から抜き去ることなく%CALLNAME:PLAYER%を抱き上げ、下から腰を突き上げた…
ELSE
	PRINTFORM %CALLNAME:PLAYER%は%CALLNAME:TARGET%を仰向けに寝かせると、
	SIF EXP:PLAYER:Ｖ経験 == 0
		PRINT 男を受け入れた事のない
	PRINTFORMW 自らのヴァギナで%CALLNAME:TARGET%のペニス\@ TALENT:半人半妖 || TALENT:ふたなり || TALENT:オトコ ? # バンド \@を咥えこんだ…
ENDIF

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_195
A = 0
S = 0
V = 95
;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER


;ABL:欲望
IF ABL:欲望
	A += ABL:欲望 * 2
ENDIF

IF MARK:快楽依存
	A += MARK:快楽依存 * 2
ENDIF

;PALAM:欲情
IF PALAM:欲情 < PALAMLV:1
	L = 0
ELSEIF PALAM:欲情 < PALAMLV:2
	L = 1
ELSEIF PALAM:欲情 < PALAMLV:3
	L = 2
ELSEIF PALAM:欲情 < PALAMLV:4
	L = 3
ELSEIF PALAM:欲情 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 2
ENDIF

;快感に素直
IF TALENT:快感に素直
	A += 2
ENDIF
;快感の否定
IF TALENT:快感の否定
	A -= 2
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	A += 5
ENDIF

;ムードボーナス
B = 0
IF TFLAG:37 < 10
	B -= 10
ELSEIF TFLAG:37 < 30
	B -= 1
ELSEIF TFLAG:37 < 50
	B += 1
ELSEIF TFLAG:37 < 100
	B += TFLAG:37 / 10
ELSEIF TFLAG:37 < 300
	B += TFLAG:37 / 5
ELSE
	B += TFLAG:37 / 4
ENDIF
A += B
IF B < 0
	B = B * -1
ELSE
ENDIF

;好感度
IF CFLAG:2 < 1000
	B = 0
	IF CFLAG:2 < 500
		B = 50
	ELSEIF CFLAG:2 < 750
		B = 20
	ELSEIF CFLAG:2 < 900
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 1500
	B = 0
	IF CFLAG:2 < 2000
		B = 5
	ELSE
		B = CFLAG:2 / 200
	ENDIF
	A += B
ENDIF

;合意
IF TALENT:合意 == 0
	A -= 20
ENDIF

;調教者が処女
IF EXP:PLAYER:Ｖ経験 == 0
	A -= 15
ENDIF

;調教者が貞操観念
IF TALENT:PLAYER:貞操観念
	A -= 10
ENDIF

;調教者が貞操無頓着
IF TALENT:PLAYER:貞操無頓着
	A += 5
ENDIF

;調教者のABL:技巧
IF ABL:PLAYER:技巧
	A += ABL:PLAYER:技巧 * 4
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 2,')
		S = 1
	ENDIF

	IF MARK:快楽依存
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:快楽依存,'(,MARK:快楽依存 * 2,')
		S = 1
	ENDIF

	;PALAM:欲情
	IF PALAM:欲情 < PALAMLV:1
		L = 0
	ELSEIF PALAM:欲情 < PALAMLV:2
		L = 1
	ELSEIF PALAM:欲情 < PALAMLV:3
		L = 2
	ELSEIF PALAM:欲情 < PALAMLV:4
		L = 3
	ELSEIF PALAM:欲情 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 2,')
		S = 1
	ENDIF

	;快感に素直
	IF TALENT:快感に素直
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:70
		PRINTV '(,2,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:快感の否定
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,2,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:恋慕 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 1
	ENDIF

	;ムードボーナス
	B = 0
	IF TFLAG:37 < 10
		B -= 10
	ELSEIF TFLAG:37 < 30
		B -= 1
	ELSEIF TFLAG:37 < 50
		B += 1
	ELSEIF TFLAG:37 < 100
		B += TFLAG:37 / 10
	ELSEIF TFLAG:37 < 300
		B += TFLAG:37 / 5
	ELSE
		B += TFLAG:37 / 4
	ENDIF
	IF B < 0
		B = B * -1
		PRINT  - 
		PRINT ムード悪い
		PRINTV '(,B,')
		S = 1
	ELSE
		PRINT  + 
		PRINT ムード良い
		PRINTV '(,B,')
		S = 1
	ENDIF

	;好感度
	IF CFLAG:2 < 1000
		B = 0
		PRINT  - 
		IF CFLAG:2 < 500
			B = 50
		ELSEIF CFLAG:2 < 750
			B = 20
		ELSEIF CFLAG:2 < 900
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 >= 1500
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 2000
			B = 5
		ELSE
			B = CFLAG:2 / 200
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合意
	IF TALENT:合意 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,20,')
		S = 1
	ENDIF

	;調教者が処女
	IF EXP:PLAYER:Ｖ経験 == 0
		PRINT  - 
		PRINTFORM %NAME:PLAYER%が処女
		PRINTV '(,15,')
		S = 1
	ENDIF

	;調教者が貞操観念
	IF TALENT:PLAYER:貞操観念
		PRINT  - 
		PRINTFORM %NAME:PLAYER%が貞操観念
		PRINTV '(,10,')
		S = 1
	ENDIF

	;調教者が貞操無頓着
	IF TALENT:PLAYER:貞操無頓着
		PRINT  + 
		PRINTFORM %NAME:PLAYER%が貞操無頓着
		PRINTV '(,5,')
		S = 1
	ENDIF

	;調教者のABL:技巧
	IF ABL:PLAYER:技巧
		SIF S
			PRINT  + 
		PRINTFORM %NAME:PLAYER%の
		PRINTS ABLNAME:2
		PRINTV 'LV,ABL:PLAYER:技巧,'(,ABL:PLAYER:技巧 * 4,')
		S = 1
	ENDIF

	;合計を表示(95以上で実行)
	PRINT  = 
	PRINTV A

	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF

