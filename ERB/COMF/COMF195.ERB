;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 100
LOSEBASE:1 += 300

SOURCE:3 = 900
SOURCE:5 = 1000
SOURCE:9 = 500
SOURCE:11 = 800
SOURCE:13 = 500
SOURCE:14 = 500

;ABL:Ｃ感覚をみる
IF ABL:3 == 0
	SOURCE:0 = 1000
	SOURCE:7 = 1000
ELSEIF ABL:3 == 1
	SOURCE:0 = 1400
	SOURCE:7 = 1300
ELSEIF ABL:3 == 2
	SOURCE:0 = 1900
	SOURCE:7 = 1700
ELSEIF ABL:3 == 3
	SOURCE:0 = 2500
	SOURCE:7 = 2000
ELSEIF ABL:3 == 4
	SOURCE:0 = 3000
	SOURCE:7 = 2500
ELSE
	SOURCE:0 = 2550 + (ABL:3 * 150)
	SOURCE:7 = 3100
ENDIF


;ABL:技巧をみる
IF ABL:PLAYER:2 == 0
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:7 , 0.50
ELSEIF ABL:PLAYER:2 == 1
	TIMES SOURCE:0 , 0.80
	TIMES SOURCE:7 , 0.80
ELSEIF ABL:PLAYER:2 == 2
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:7 , 1.00
ELSEIF ABL:PLAYER:2 == 3
	TIMES SOURCE:0 , 1.20
	TIMES SOURCE:7 , 1.50
ELSEIF ABL:PLAYER:2 == 4
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:7 , 2.50
ELSE
	SOURCE:0 = SOURCE:0 * (ABL:PLAYER:2 * 75 + 25) / 200
	SOURCE:7 = SOURCE:7 * (ABL:PLAYER:2 * 75 + 25) / 100
ENDIF

;ABL:奉仕精神をみる
IF ABL:PLAYER:6 == 0
	TIMES SOURCE:3 , 0.60
;	TIMES SOURCE:7 , 0.60
ELSEIF ABL:PLAYER:6 == 1
	TIMES SOURCE:3 , 0.80
;	TIMES SOURCE:7 , 0.80
ELSEIF ABL:PLAYER:6 == 2
	TIMES SOURCE:3 , 1.00
;	TIMES SOURCE:7 , 1.00
ELSEIF ABL:PLAYER:6 == 3
	TIMES SOURCE:3 , 1.50
;	TIMES SOURCE:7 , 1.50
ELSEIF ABL:PLAYER:6 == 4
	TIMES SOURCE:3 , 2.50
;	TIMES SOURCE:7 , 2.50
ELSE
	SOURCE:3 = SOURCE:3 * (ABL:PLAYER:2 * 75 + 25) / 100
;	SOURCE:7 = SOURCE:7 * (ABL:PLAYER:2 * 75 + 25) / 100
ENDIF

;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:0 <= 1
		SOURCE:21 += 300 + (ABL:0 * 50)
	ELSEIF ABL:0 <= 3
		SOURCE:21 += 600 + (ABL:0 * 100)
	ELSEIF ABL:0 <= 5
		SOURCE:21 += 800 + (ABL:0 * 120)
	ELSEIF ABL:0 <= 8
		SOURCE:21 += 1000 + (ABL:0 * 140)
	ELSEIF ABL:0 <= 11
		SOURCE:21 += 1200 + (ABL:0 * 160)
	ELSE
		SOURCE:21 += 1500 + (ABL:0 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:15 == 0
		SOURCE:20 = 600
	ELSEIF ABL:15 == 1
		SOURCE:20 = 900
	ELSEIF ABL:15 == 2
		SOURCE:20 = 1200
	ELSEIF ABL:15 == 3
		SOURCE:20 = 1600
	ELSEIF ABL:15 == 4
		SOURCE:20 = 2000
	ELSE
		SOURCE:20 = 1000 + (ABL:15 * 300)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
;合意がないと好感度が下がる
IF TALENT:152 == 0 && TFLAG:48 == 0
	A -= 9
	SOURCE:15 += 400
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;調教者が処女
IF TALENT:PLAYER:0
	TIMES SOURCE:13 , 10.00
	TIMES SOURCE:14 , 3.00
	TALENT:PLAYER:0 = 0
	
	;ABL:サドっ気をみる
	IF ABL:15 == 0
		SOURCE:22 = 5000
	ELSEIF ABL:15 == 1
		SOURCE:22 = 4000
	ELSEIF ABL:15 == 2
		SOURCE:22 = 3000
	ELSEIF ABL:15 == 3
		SOURCE:22 = 2000
	ELSEIF ABL:15 == 4
		SOURCE:22 = 1500
	ELSE
		SOURCE:22 = 1100 - (ABL:15 * 40)
		SIF SOURCE:22 < 0
			SOURCE:22 = 0
	ENDIF
	
	;サド持ちは懐疑1/4
	SIF TALENT:83
		TIMES SOURCE:22 , 0.25

	;小悪魔持ちは懐疑1/2
	SIF TALENT:87
		TIMES SOURCE:22 , 0.50

	PRINTFORML ＜%CALLNAME:PLAYER%が処女＞
	EXP:50 += 3
	PRINTFORML %EXPNAME:50%+3
	IF TALENT:85 && ASSIPLAY == 0
		IF ABL:0 < 10
			PRINTFORML そして%ABLNAME:0%が10になった
			ABL:0 = 10
		ENDIF
		TIMES SOURCE:11 , 10.00
		TIMES SOURCE:3 , 3.00
		TIMES SOURCE:22 , 0.50
		SIF TALENT:89
			TIMES SOURCE:22 , 0.20
	ELSE
		IF ABL:0 < 1
			PRINTFORML そして%ABLNAME:0%が1になった
			ABL:0 = 1
		ENDIF
	ENDIF
ENDIF
EXP:PLAYER:0 += 1

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
;CALL SAMEN_CHECK

;射精時の処理
;IF E >= 1
;	;コマンドごとに固有の処理はこの下に書く
;	TIMES SOURCE:4 , 3.00
;
;	;ABL:精液中毒をみる
;	IF ABL:11 == 0
;		SOURCE:7 = 0
;		TIMES SOURCE:0 , 2.00
;		TIMES SOURCE:13, 2.00
;	ELSEIF ABL:11 == 1
;		SOURCE:7 = 50
;		TIMES SOURCE:0 , 2.50
;		TIMES SOURCE:13, 1.60
;	ELSEIF ABL:11 == 2
;		SOURCE:7 = 150
;		TIMES SOURCE:0 , 3.00
;		TIMES SOURCE:13, 1.00
;	ELSEIF ABL:11 == 3
;		SOURCE:7 = 300
;		TIMES SOURCE:0 , 4.50
;		TIMES SOURCE:13, 0.70
;	ELSEIF ABL:11 == 4
;		SOURCE:7 = 600
;		TIMES SOURCE:0 , 6.00
;		TIMES SOURCE:13, 0.40
;	ELSE
;		SOURCE:7 = 1200
;		TIMES SOURCE:0 , 8.00
;		TIMES SOURCE:13, 0.10
;	ENDIF
;
;	;射精先タイプの設定
;	TFLAG:703 = 9
;	;射精先選択の可・不可
;	;TFLAG:704 = 0
;	;汎用的な処理はこの関数で行う
;	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
;	CALL SAMEN_SHOOT
;ENDIF


IF TALENT:119 || TALENT:121 || TALENT:122
	;奴隷のＰ⇔調教者のＶの汚れが移動
	STAIN:2 |= STAIN:PLAYER:3
	STAIN:PLAYER:3 |= STAIN:2
ENDIF

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +4
	EXP:40 += 4
	EXP:PLAYER:40 += 4
ENDIF

TFLAG:100 = 0

;既成事実１に相当
TFLAG:200 = 1

RETURN 1



;騎乗位する
@TRAIN_MESSAGE_COM195
;前回と今回の調教者が同じで、前回の調教が騎乗位するであり、抜かないフラグがある場合
IF TFLAG:60 && PREVCOM == 195 && ((ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0))
	PRINTFORMW %CALLNAME:PLAYER%は%CALLNAME:TARGET%のペニス\@ TALENT:119 || TALENT:121 || TALENT:122 ? # バンド \@をきつく締めつけたまま、%CALLNAME:TARGET%のために腰を振りたてた…
;同様に前回の調教が正常位させるか後背位させるの場合
ELSEIF TFLAG:60 && (PREVCOM == 193 || PREVCOM == 194) && ((ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0))
	PRINTFORMW %CALLNAME:TARGET%は、膣内から抜き去ることなく%CALLNAME:PLAYER%を抱き上げ、下から腰を突き上げた…
ELSE
	PRINTFORM %CALLNAME:PLAYER%は%CALLNAME:TARGET%を仰向けに寝かせると、
	SIF EXP:PLAYER:0 == 0
		PRINT 男を受け入れた事のない
	PRINTFORMW 自らのヴァギナで%CALLNAME:TARGET%のペニス\@ TALENT:119 || TALENT:121 || TALENT:122 ? # バンド \@を咥えこんだ…
ENDIF

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_195
A = 0
S = 0
V = 95
;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER


;ABL:欲望
IF ABL:1
	A += ABL:1 * 2
ENDIF

IF MARK:1
	A += MARK:1 * 2
ENDIF

;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 2
ENDIF

;快感に素直
IF TALENT:70
	A += 2
ENDIF
;快感の否定
IF TALENT:71
	A -= 2
ENDIF
;恋慕
IF TALENT:85 && ASSIPLAY == 0
	A += 5
ENDIF

;ムードボーナス
B = 0
IF TFLAG:37 < 10
	B -= 10
ELSEIF TFLAG:37 < 30
	B -= 1
ELSEIF TFLAG:37 < 50
	B += 1
ELSEIF TFLAG:37 < 100
	B += TFLAG:37 / 10
ELSEIF TFLAG:37 < 300
	B += TFLAG:37 / 5
ELSE
	B += TFLAG:37 / 4
ENDIF
A += B
IF B < 0
	B = B * -1
ELSE
ENDIF

;好感度
IF CFLAG:2 < 1000
	B = 0
	IF CFLAG:2 < 500
		B = 50
	ELSEIF CFLAG:2 < 750
		B = 20
	ELSEIF CFLAG:2 < 900
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 1500
	B = 0
	IF CFLAG:2 < 2000
		B = 5
	ELSE
		B = CFLAG:2 / 200
	ENDIF
	A += B
ENDIF

;合意
IF TALENT:152 == 0
	A -= 20
ENDIF

;調教者が処女
IF EXP:PLAYER:0 == 0
	A -= 15
ENDIF

;調教者が貞操観念
IF TALENT:PLAYER:30
	A -= 10
ENDIF

;調教者が貞操無頓着
IF TALENT:PLAYER:31
	A += 5
ENDIF

;調教者のABL:技巧
IF ABL:PLAYER:2
	A += ABL:PLAYER:2 * 4
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:1
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:1,'(,ABL:1 * 2,')
		S = 1
	ENDIF

	IF MARK:1
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:1,'(,MARK:1 * 2,')
		S = 1
	ENDIF

	;PALAM:欲情
	IF PALAM:5 < PALAMLV:1
		L = 0
	ELSEIF PALAM:5 < PALAMLV:2
		L = 1
	ELSEIF PALAM:5 < PALAMLV:3
		L = 2
	ELSEIF PALAM:5 < PALAMLV:4
		L = 3
	ELSEIF PALAM:5 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 2,')
		S = 1
	ENDIF

	;快感に素直
	IF TALENT:70
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:70
		PRINTV '(,2,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:71
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,2,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:85 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 1
	ENDIF

	;ムードボーナス
	B = 0
	IF TFLAG:37 < 10
		B -= 10
	ELSEIF TFLAG:37 < 30
		B -= 1
	ELSEIF TFLAG:37 < 50
		B += 1
	ELSEIF TFLAG:37 < 100
		B += TFLAG:37 / 10
	ELSEIF TFLAG:37 < 300
		B += TFLAG:37 / 5
	ELSE
		B += TFLAG:37 / 4
	ENDIF
	IF B < 0
		B = B * -1
		PRINT  - 
		PRINT ムード悪い
		PRINTV '(,B,')
		S = 1
	ELSE
		PRINT  + 
		PRINT ムード良い
		PRINTV '(,B,')
		S = 1
	ENDIF

	;好感度
	IF CFLAG:2 < 1000
		B = 0
		PRINT  - 
		IF CFLAG:2 < 500
			B = 50
		ELSEIF CFLAG:2 < 750
			B = 20
		ELSEIF CFLAG:2 < 900
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 >= 1500
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 2000
			B = 5
		ELSE
			B = CFLAG:2 / 200
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合意
	IF TALENT:152 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,20,')
		S = 1
	ENDIF

	;調教者が処女
	IF EXP:PLAYER:0 == 0
		PRINT  - 
		PRINTFORM %NAME:PLAYER%が処女
		PRINTV '(,15,')
		S = 1
	ENDIF

	;調教者が貞操観念
	IF TALENT:PLAYER:30
		PRINT  - 
		PRINTFORM %NAME:PLAYER%が貞操観念
		PRINTV '(,10,')
		S = 1
	ENDIF

	;調教者が貞操無頓着
	IF TALENT:PLAYER:31
		PRINT  + 
		PRINTFORM %NAME:PLAYER%が貞操無頓着
		PRINTV '(,5,')
		S = 1
	ENDIF

	;調教者のABL:技巧
	IF ABL:PLAYER:2
		SIF S
			PRINT  + 
		PRINTFORM %NAME:PLAYER%の
		PRINTS ABLNAME:2
		PRINTV 'LV,ABL:PLAYER:2,'(,ABL:PLAYER:2 * 4,')
		S = 1
	ENDIF

	;合計を表示(95以上で実行)
	PRINT  = 
	PRINTV A

	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF

