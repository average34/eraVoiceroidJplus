@COM_ABLE37
;足扱き実行判定
;COM_ABLE共通判定
CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中じゃないと無理
SIF TFLAG:44 == 0
	RETURN 0
;調教者がオトコか半人半妖かふたなりじゃないとダメ
SIF TALENT:PLAYER:半人半妖 == 0 && TALENT:PLAYER:ふたなり == 0 && TALENT:PLAYER:オトコ == 0
	RETURN 0
;乗馬中はダメ
SIF TEQUIP:41
	RETURN 0
;縄使用中は不可
SIF TEQUIP:44
	RETURN 0
;女体盛り中はダメ
SIF TEQUIP:69
	RETURN 0
;○○呂入浴中はダメ
SIF TEQUIP:78 || TEQUIP:92
	RETURN 0
;時止め中は無理
SIF TEQUIP:81
	RETURN 0
;触手調教中はダメ
SIF TEQUIP:90
	RETURN 0
RETURN 1

@COM37
;足扱き
PRINTL 足扱き

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = 足扱き
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
SIF TALENT:PLAYER:半人半妖 || TALENT:PLAYER:オトコ  || TALENT:PLAYER:ふたなり
	BASE:PLAYER:射精 += CALC_EJAC(SELECTCOM, PLAYER)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 10
LOSEBASE:気力 += 150

SOURCE:屈従 = 550
SOURCE:逸脱 = 400

;上のほうで計算した汚れデータ
SOURCE:不潔 = Y*10 + 50

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:性行動 = 300
	SOURCE:達成感 = 100
	TIMES SOURCE:不潔 , 4.00
ELSEIF ABL:奉仕精神 == 1
	SOURCE:性行動 = 350
	SOURCE:達成感 = 150
	TIMES SOURCE:不潔 , 2.50
ELSEIF ABL:奉仕精神 == 2
	SOURCE:性行動 = 400
	SOURCE:達成感 = 350
	TIMES SOURCE:不潔 , 1.50
ELSEIF ABL:奉仕精神 == 3
	SOURCE:性行動 = 450
	SOURCE:達成感 = 750
	TIMES SOURCE:不潔 , 1.00
ELSEIF ABL:奉仕精神 == 4
	SOURCE:性行動 = 500
	SOURCE:達成感 = 1100
	TIMES SOURCE:不潔 , 0.50
ELSE
	SOURCE:性行動 = 580
	SOURCE:達成感 = 1500
	TIMES SOURCE:不潔 , 0.10
ENDIF


;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:性行動 , 0.50
	TIMES SOURCE:達成感 , 0.50
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:性行動 , 0.80
	TIMES SOURCE:達成感 , 0.80
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:性行動 , 1.00
	TIMES SOURCE:達成感 , 1.00
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:性行動 , 1.20
	TIMES SOURCE:達成感 , 1.20
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:性行動 , 1.50
	TIMES SOURCE:達成感 , 1.50
ELSE
	TIMES SOURCE:性行動 , 2.00
	TIMES SOURCE:達成感 , 2.00
ENDIF

;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:受動 += 300 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:受動 += 600 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:受動 += 800 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:受動 += 1000 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:受動 += 1200 + (ABL:親密 * 160)
	ELSE
		SOURCE:受動 += 1500 + (ABL:親密 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:征服 = 800
	ELSEIF ABL:サドっ気 == 1
		SOURCE:征服 = 1200
	ELSEIF ABL:サドっ気 == 2
		SOURCE:征服 = 1800
	ELSEIF ABL:サドっ気 == 3
		SOURCE:征服 = 2300
	ELSEIF ABL:サドっ気 == 4
		SOURCE:征服 = 2800
	ELSE
		SOURCE:征服 = 1500 + (ABL:サドっ気 * 400)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
SIF TALENT:恋慕 || TALENT:恋人
	A += 2
SIF TALENT:親愛
	A += 3
;合意がないと好感度が下がる
IF TALENT:合意 == 0 && TFLAG:48 == 0
	A -= 8
	SOURCE:反感追加 += 300
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK
;;オーバーフロー対策に、値が大きすぎる場合と、負の場合は調整を加える
;IF BASE:PLAYER:射精 > 100000000 || BASE:PLAYER:射精 < 0
;	BASE:PLAYER:射精 = 100000000
;ENDIF
;S = BASE:PLAYER:射精
;EJAC = MAXBASE:PLAYER:射精
;
;IF  S > EJAC * 2
;	E = 2
;ELSEIF S > EJAC
;	E = 1
;ELSE
;	E = 0
;ENDIF

;射精時の処理
IF E >= 1
	;コマンドごとに固有の処理はこの下に書く
	TIMES SOURCE:性行動 , 3.00

	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		SOURCE:中毒充足 = 0
		TIMES SOURCE:達成感 , 2.00
		TIMES SOURCE:屈従, 2.00
	ELSEIF ABL:精液中毒 == 1
		SOURCE:中毒充足 = 200
		TIMES SOURCE:達成感 , 2.50
		TIMES SOURCE:屈従, 1.60
	ELSEIF ABL:精液中毒 == 2
		SOURCE:中毒充足 = 600
		TIMES SOURCE:達成感 , 3.00
		TIMES SOURCE:屈従, 1.00
	ELSEIF ABL:精液中毒 == 3
		SOURCE:中毒充足 = 1500
		TIMES SOURCE:達成感 , 4.00
		TIMES SOURCE:屈従, 0.70
	ELSEIF ABL:精液中毒 == 4
		SOURCE:中毒充足 = 3000
		TIMES SOURCE:達成感 , 5.00
		TIMES SOURCE:屈従, 0.40
	ELSE
		SOURCE:中毒充足 = 6000
		TIMES SOURCE:達成感 , 6.00
		TIMES SOURCE:屈従, 0.10
	ENDIF

	;射精先タイプの設定
	TFLAG:703 = 10
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;;大量射精
;IF E == 2
;	TIMES SOURCE:中毒充足 , 1.50
;	TIMES SOURCE:達成感 , 1.20
;
;	EXP:PLAYER:射精経験 += 2
;	EXP:精液経験 += 2
;	PRINTL 大量射精
;	PRINTL 精液経験＋２
;
;	BASE:PLAYER:射精 -= EJAC*2
;	SIF BASE:PLAYER:射精 >= EJAC
;		BASE:PLAYER:射精 = EJAC-1
;
;	;素股で射精させたフラグと同じ
;	TFLAG:9 = 2
;;通常の射精
;ELSEIF E == 1
;	EXP:PLAYER:射精経験 += 1
;	EXP:精液経験 += 1
;	PRINTL 射精
;	PRINTL 精液経験＋１
;
;	BASE:PLAYER:射精 -= EJAC
;	SIF BASE:PLAYER:射精 >= EJAC
;		BASE:PLAYER:射精 = EJAC-1
;
;	;素股で射精させたフラグと同じ
;	TFLAG:9 = 1
;ENDIF

;奴隷の指⇔調教者のＰの汚れが移動
;IF TALENT:PLAYER:半人半妖 || TALENT:PLAYER:オトコ  || TALENT:PLAYER:ふたなり
;	STAIN:手 |= STAIN:PLAYER:ペニス
;	STAIN:PLAYER:ペニス |= STAIN:手
;奴隷の指⇔調教者のＶの汚れが移動
;ELSE
;	STAIN:ヴァギナ |= STAIN:PLAYER:ペニス
;	STAIN:PLAYER:ペニス |= STAIN:ヴァギナ
;ENDIF

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTFORML %EXPNAME:40%+4
	EXP:レズ経験 += 4
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTFORML %EXPNAME:41%+4
	EXP:ＢＬ経験 += 4
ENDIF

TFLAG:100 = 1

;既成事実１に相当
TFLAG:200 = 1

;調教者がふたなり
IF TALENT:PLAYER:ふたなり
	SOURCE:屈従 /= 2
ENDIF

RETURN 1

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_37
A = 0
S = 0
V = 70

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	S = ABL:欲望
	A += S
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	S = ABL:奉仕精神
	A += S * 4
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	S = ABL:精液中毒
	A += S * 3
ENDIF


;ABL:サドっ気
IF ABL:サドっ気
	S = ABL:サドっ気
	A += S * 4
ENDIF

;快楽刻印
IF MARK:快楽依存
	S = MARK:快楽依存
	A += S
ENDIF

;PALAM:欲情
IF PALAM:欲情 < PALAMLV:1
	L = 0
ELSEIF PALAM:欲情 < PALAMLV:2
	L = 1
ELSEIF PALAM:欲情 < PALAMLV:3
	L = 2
ELSEIF PALAM:欲情 < PALAMLV:4
	L = 3
ELSEIF PALAM:欲情 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	S = L
	A += S
ENDIF

;恥じらい
IF TALENT:恥じらい
	A -= S
ENDIF
;汚臭鈍感
IF TALENT:汚臭鈍感
	A += S
ENDIF
;汚臭敏感
IF TALENT:汚臭敏感
	A -= S
ENDIF
;献身的
IF TALENT:献身的
	S = 6
	A += S
ENDIF
;快感の否定
IF TALENT:快感の否定
	S = 3
	A -= S
ENDIF
;男嫌い
IF TALENT:男嫌い && TALENT:PLAYER:オトコ
	S = 8
	A -= S
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	S = 5
	A += S
ENDIF
;倒錯的
IF TALENT:倒錯的
	S = 4
	A += S
ENDIF
;サド
IF TALENT:サド
	S = 7
	A += S
ENDIF
;小悪魔
IF TALENT:小悪魔
	S = 4
	A += S
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:ふたなり
	S = 8
	A += S
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:ペニス & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:ペニス & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:ペニス & 8
	Y += 7
SIF STAIN:PLAYER:ペニス & 16
	Y += 15

SIF TALENT:汚臭鈍感
	Y /= 3
SIF TALENT:汚臭敏感
	Y *= 2

;足扱きでは汚れがそれほど影響しない
Y /= 3

;汚れあり
IF Y
	A -= Y
	;汚臭鈍感
	IF TALENT:汚臭鈍感
	ELSEIF TALENT:汚臭敏感
	ENDIF
ENDIF

;好感度
IF CFLAG:2 < 1000
	B = 0
	IF CFLAG:2 < 500
		B = 50
	ELSEIF CFLAG:2 < 750
		B = 20
	ELSEIF CFLAG:2 < 900
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF

;好感度
IF CFLAG:2 >= 1500
	B = 0
	IF CFLAG:2 < 2000
		B = 5
	ELSE
		B = CFLAG:2 / 200
	ENDIF
	A += B
ENDIF

;合意
IF TALENT:合意 == 0
	A -= 5
ENDIF
IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		S = ABL:欲望
		PRINTFORM %ABLNAME:1%LV{S}({S})
	ENDIF
	;ABL:奉仕精神
	IF ABL:奉仕精神
		SIF S
			PRINT  + 
		S = ABL:奉仕精神
		PRINTFORM %ABLNAME:6%LV{S}({S * 4})
	ENDIF
	;ABL:精液中毒
	IF ABL:精液中毒
		SIF S
			PRINT  + 
		S = ABL:精液中毒
		PRINTFORM %ABLNAME:11%LV{S}({S * 3})
	ENDIF


	;ABL:サドっ気
	IF ABL:サドっ気
		SIF S
			PRINT  + 
		S = ABL:サドっ気
		PRINTFORM %ABLNAME:15%LV{S}({S * 3})
	ENDIF

	;快楽刻印
	IF MARK:快楽依存
		SIF S
			PRINT  + 
		S = MARK:快楽依存
		PRINTFORM %MARKNAME:1%LV{S}({S})
	ENDIF

	;PALAM:欲情
	IF PALAM:欲情 < PALAMLV:1
		L = 0
	ELSEIF PALAM:欲情 < PALAMLV:2
		L = 1
	ELSEIF PALAM:欲情 < PALAMLV:3
		L = 2
	ELSEIF PALAM:欲情 < PALAMLV:4
		L = 3
	ELSEIF PALAM:欲情 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		S = L
		PRINTFORM %PALAMNAME:5%LV{L}({S})
	ENDIF

	;恥じらい
	IF TALENT:恥じらい
		PRINT  - 
		S = 1
		PRINTFORM %TALENTNAME:35%({S})
	ENDIF
	;汚臭鈍感
	IF TALENT:汚臭鈍感
		SIF S
			PRINT  + 
		S = 1
		PRINTFORM %TALENTNAME:61%({S})
	ENDIF
	;汚臭敏感
	IF TALENT:汚臭敏感
		PRINT  - 
		S = 1
		PRINTFORM %TALENTNAME:62%({S})
	ENDIF
	;献身的
	IF TALENT:献身的
		SIF S
			PRINT  + 
		S = 6
		PRINTFORM %TALENTNAME:63%({S})
	ENDIF
	;快感の否定
	IF TALENT:快感の否定
		PRINT  - 
		S = 3
		PRINTFORM %TALENTNAME:71%({S})
	ENDIF
	;男嫌い
	IF TALENT:男嫌い && TALENT:PLAYER:オトコ
		PRINT  - 
		S = 8
		PRINTFORM %TALENTNAME:82%({S})
	ENDIF
	;恋慕
	IF TALENT:恋慕 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		S = 5
		PRINTFORM %TALENTNAME:85%({S})
	ENDIF
	;倒錯的
	IF TALENT:倒錯的
		SIF S
			PRINT  + 
		S = 4
		PRINTFORM %TALENTNAME:80%({S})
	ENDIF
	;サド
	IF TALENT:サド
		SIF S
			PRINT  + 
		S = 7
		PRINTFORM %TALENTNAME:83%({S})
	ENDIF
	;小悪魔
	IF TALENT:小悪魔
		SIF S
			PRINT  + 
		S = 4
		PRINTFORM %TALENTNAME:87%({S})
	ENDIF

	;調教者がふたなり
	IF TALENT:PLAYER:ふたなり
		SIF S
			PRINT  + 
		S = 8
		PRINTFORM %TALENTNAME:121%({S})
	ENDIF

	Y = 0
	;愛液の汚れ
	SIF STAIN:PLAYER:ペニス & 1
		Y += 1
	;精液の汚れ
	SIF STAIN:PLAYER:ペニス & 4
		Y += 3
	;アナルの汚れ
	SIF STAIN:PLAYER:ペニス & 8
		Y += 7
	SIF STAIN:PLAYER:ペニス & 16
		Y += 15

	SIF TALENT:汚臭鈍感
		Y /= 3
	SIF TALENT:汚臭敏感
		Y *= 2

	;足扱きでは汚れがそれほど影響しない
	Y /= 3

	;汚れあり
	IF Y
		PRINT  - 
		PRINT 汚れあり
		;汚臭鈍感
		IF TALENT:汚臭鈍感
			PRINTFORM 、%TALENTNAME:61%
		ELSEIF TALENT:汚臭敏感
			PRINTFORM 、%TALENTNAME:62%
		ENDIF
		PRINTFORM ({Y})
		S = 1
	ENDIF

	;好感度
	IF CFLAG:2 < 1000
		B = 0
		PRINT  - 
		IF CFLAG:2 < 500
			B = 50
		ELSEIF CFLAG:2 < 750
			B = 20
		ELSEIF CFLAG:2 < 900
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF

	;好感度
	IF CFLAG:2 >= 1500
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 2000
			B = 5
		ELSE
			B = CFLAG:2 / 200
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合意
	IF TALENT:合意 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,5,')
		S = 1
	ENDIF

	;合計を表示(70以上で実行)
	PRINT  = 
	PRINTV A


	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF
