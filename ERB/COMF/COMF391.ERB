@GET_CLOTH_SHORT_NAME(ARG:0)
#FUNCTIONS
	SELECTCASE ARG:0
		CASE 0, 1 TO 3, 8 TO 10, 12 TO 16, 18 TO 21, 23, 24
			RETURNF "服"
		CASE 4 TO 6
			RETURNF "水着"
		CASE 7
			RETURNF "白衣"
		CASE 11
			RETURNF "浴衣"
		CASE 17
			RETURNF "リボン"
		CASE 22
			RETURNF "シャツ"
		CASE 255
			RETURNF "布切れ"
	ENDSELECT

