	EXP:PLAYER:レズ経験 += 7
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +7
	EXP:ＢＬ経験 += 7
	EXP:PLAYER:ＢＬ経験 += 7
ENDIF

SIF ASSIPLAY == 0 && EXP:フェラ経験 >= EXPLV:3
	TFLAG:30 += 1

