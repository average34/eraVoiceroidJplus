@COM31
;フェラチオ

;前回と今回の調教者が同じ
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;縄使用中でなく、前回の調教がクンニかフェラするかシックスナインだとシックスナインに
	IF (PREVCOM == 1 || PREVCOM == 4 || PREVCOM == 69) && TEQUIP:44 == 0
		CALL COMABLE_69
		SIF RESULT == 1
			JUMP COM69
	ENDIF
ENDIF

;前回の調教が3Pのとき3Pへ
TFLAG:42 = 0
IF PREVCOM == 64
	CALL COMABLE_3P
	IF RESULT == 1
		TFLAG:42 = 1
		JUMP COM64
	ENDIF
;調教者が前回:助手で今回:主人か、前回:主人で今回:助手のとき
ELSEIF (ASSIPLAY && TFLAG:50 == 0) || (ASSIPLAY == 0 && TFLAG:50)
	;前回の調教が正常位か後背位かアナルセックスのときは3Pへ
	IF PREVCOM == 20 || PREVCOM == 21 || PREVCOM == 22
		CALL COMABLE_3P
		SIF RESULT == 1
			JUMP COM64
	ENDIF
ENDIF

;対象が恋慕さとりだと変化
IF NAME_CSV(NO:TARGET, 0) == "さとり" && TALENT:恋慕
	PRINTL 読心フェラ
;風呂プレイ中だと変化
ELSEIF TEQUIP:58
	PRINTL 潜望鏡
ELSE
	PRINTL フェラチオ
ENDIF

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = フェラチオ
