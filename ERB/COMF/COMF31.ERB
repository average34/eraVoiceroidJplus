@COM31
;フェラチオ

;前回と今回の調教者が同じ
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;縄使用中でなく、前回の調教がクンニかフェラするかシックスナインだとシックスナインに
	IF (PREVCOM == 1 || PREVCOM == 4 || PREVCOM == 69) && TEQUIP:44 == 0
		CALL COMABLE_69
		SIF RESULT == 1
			JUMP COM69
	ENDIF
ENDIF

;前回の調教が3Pのとき3Pへ
TFLAG:42 = 0
IF PREVCOM == 64
	CALL COMABLE_3P
	IF RESULT == 1
		TFLAG:42 = 1
		JUMP COM64
	ENDIF
;調教者が前回:助手で今回:主人か、前回:主人で今回:助手のとき
ELSEIF (ASSIPLAY && TFLAG:50 == 0) || (ASSIPLAY == 0 && TFLAG:50)
	;前回の調教が正常位か後背位かアナルセックスのときは3Pへ
	IF PREVCOM == 20 || PREVCOM == 21 || PREVCOM == 22
		CALL COMABLE_3P
		SIF RESULT == 1
			JUMP COM64
	ENDIF
ENDIF

;対象が恋慕さとりだと変化
IF NAME_CSV(NO:TARGET, 0) == "さとり" && TALENT:85
	PRINTL 読心フェラ
;風呂プレイ中だと変化
ELSEIF TEQUIP:58
	PRINTL 潜望鏡
ELSE
	PRINTL フェラチオ
ENDIF

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = フェラチオ
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
SIF TALENT:PLAYER:119 || TALENT:PLAYER:122  || TALENT:PLAYER:121
	BASE:PLAYER:2 += CALC_EJAC(SELECTCOM, PLAYER)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 10
LOSEBASE:1 += 150

SOURCE:13 = 1500
SOURCE:14 = 500

;上のほうで計算した汚れデータ
SOURCE:8 = Y*40 + 100

;Ｍ感覚を設定している場合快Ｍソース等取得
SIF FLAG:62 & 4194304
	CALL SOURCE_M_COM31

;ABL:奉仕精神をみる
IF ABL:6 == 0
	SOURCE:4 = 420
	SOURCE:5 = 150
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:6 == 1
	SOURCE:4 = 500
	SOURCE:5 = 300
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:6 == 2
	SOURCE:4 = 580
	SOURCE:5 = 600
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:6 == 3
	SOURCE:4 = 660
	SOURCE:5 = 900
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:6 == 4
	SOURCE:4 = 740
	SOURCE:5 = 1500
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:4 = 620 + (ABL:6 * 40)
	SOURCE:5 = 450 + (ABL:6 * 350)
	TIMES SOURCE:8 , 0.10
ENDIF

;ABL:技巧をみる
IF ABL:2 == 0
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:2 == 1
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:2 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:2 == 3
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:2 == 4
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSE
	SOURCE:4 = SOURCE:4 * (ABL:2 * 25 + 75) / 100
	SOURCE:5 = SOURCE:5 * (ABL:2 * 25 + 75) / 100
ENDIF

;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:0 <= 1
		SOURCE:21 += 300 + (ABL:0 * 50)
	ELSEIF ABL:0 <= 3
		SOURCE:21 += 600 + (ABL:0 * 100)
	ELSEIF ABL:0 <= 5
		SOURCE:21 += 800 + (ABL:0 * 120)
	ELSEIF ABL:0 <= 8
		SOURCE:21 += 1000 + (ABL:0 * 140)
	ELSEIF ABL:0 <= 11
		SOURCE:21 += 1200 + (ABL:0 * 160)
	ELSE
		SOURCE:21 += 1500 + (ABL:0 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:15 == 0
		SOURCE:20 = 800
	ELSEIF ABL:15 == 1
		SOURCE:20 = 1100
	ELSEIF ABL:15 == 2
		SOURCE:20 = 1400
	ELSEIF ABL:15 == 3
		SOURCE:20 = 1800
	ELSEIF ABL:15 == 4
		SOURCE:20 = 2300
	ELSE
		SOURCE:20 = 1000 + (ABL:15 * 350)
	ENDIF
ENDIF

;-------------------------------------------------
;読心フェラの場合効果アップ（2倍）
;-------------------------------------------------

;対象が恋慕さとりだと変化
IF NAME_CSV(NO:TARGET, 0) == "さとり" && TALENT:85
	TIMES SOURCE:4 , 2.00
	TIMES SOURCE:5 , 2.00
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
SIF TALENT:85 || TALENT:153
	A += 2
SIF TALENT:88
	A += 3
;合意がないと好感度が下がる
IF TALENT:152 == 0 && TFLAG:48 == 0
	A -= 8
	SOURCE:15 += 300
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK
;;オーバーフロー対策に、値が大きすぎる場合と、負の場合は調整を加える
;IF BASE:PLAYER:2 > 100000000 || BASE:PLAYER:2 < 0
;	BASE:PLAYER:2 = 100000000
;ENDIF
;S = BASE:PLAYER:2
;EJAC = MAXBASE:PLAYER:2
;
;IF  S > EJAC * 2
;	E = 2
;ELSEIF S > EJAC
;	E = 1
;ELSE
;	E = 0
;ENDIF

IF E
;射精している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:11 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 4.00
	ELSEIF ABL:11 == 1
		SOURCE:7 = 500
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 3.00
	ELSEIF ABL:11 == 2
		SOURCE:7 = 1200
		TIMES SOURCE:5 , 4.00
		TIMES SOURCE:13, 2.50
	ELSEIF ABL:11 == 3
		SOURCE:7 = 3000
		TIMES SOURCE:5 , 6.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:11 == 4
		SOURCE:7 = 6000
		TIMES SOURCE:5 , 9.00
		TIMES SOURCE:13, 1.50
	ELSE
		SOURCE:7 = (ABL:11 * 3000) - 3000
		SOURCE:5 = SOURCE:5 * (ABL:11 * 300) / 100
		IF ABL:11 <= 10
			SOURCE:13 = SOURCE:13 * (150 - ABL:11 * 10) / 100
		ELSE
			TIMES SOURCE:13, 0.50
		ENDIF
	ENDIF

	;射精先タイプの設定
	TFLAG:703 = 4
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;;大量射精
;IF E == 2
;	TIMES SOURCE:7 , 2.00
;	TIMES SOURCE:5 , 1.50
;
;	EXP:PLAYER:3 += 2
;	EXP:20 += 9
;	PRINTL 大量射精
;	PRINTL 精液経験＋９
;	;精液汚れ
;	STAIN:PLAYER:2 |= 4
;
;	BASE:PLAYER:2 -= EJAC*2
;	SIF BASE:PLAYER:2 >= EJAC
;		BASE:PLAYER:2 = EJAC-1
;
;	;口で射精させたフラグ
;	TFLAG:0 = 2
;;通常の射精
;ELSEIF E == 1
;	EXP:PLAYER:3 += 1
;	EXP:20 += 3
;	PRINTL 射精
;	PRINTL 精液経験＋３
;	;精液汚れ
;	STAIN:PLAYER:2 |= 4
;
;	BASE:PLAYER:2 -= EJAC
;	SIF BASE:PLAYER:2 >= EJAC
;		BASE:PLAYER:2 = EJAC-1
;
;	;口で射精させたフラグ
;	TFLAG:0 = 1
;ENDIF

PRINTFORML %EXPNAME:22%+1
EXP:22 += 1

;奴隷の口⇔調教者のＰの汚れが移動
STAIN:0 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:0

;奉仕精神ＬＶ２以上、技巧ＬＶ２以上なら汚れをなめ取る
IF ABL:6 >= 2 && ABL:2 >= 2
	STAIN:PLAYER:2 = 2
	SIF E >= 1
		TFLAG:8 = 1
ENDIF

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +7
	EXP:40 += 7
	EXP:PLAYER:40 += 7
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +7
	EXP:41 += 7
	EXP:PLAYER:41 += 7
ENDIF

SIF ASSIPLAY == 0 && EXP:22 >= EXPLV:3
	TFLAG:30 += 1

;調教者がふたなり
IF TALENT:PLAYER:121
	SOURCE:13 /= 2
ENDIF

TFLAG:100 = 1

;既成事実２に相当
TFLAG:200 = 2

RETURN 1

;Ｍ感覚を設定している場合快Ｍソース取得
@SOURCE_M_COM31
VARSET LOCAL, 0

;潤滑追加
LOCAL:10 = 250
;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES LOCAL:10 , 0.10
ELSEIF PALAM:5 < PALAMLV:2
	TIMES LOCAL:10 , 0.40
ELSEIF PALAM:5 < PALAMLV:3
	TIMES LOCAL:10 , 0.75
ELSEIF PALAM:5 < PALAMLV:4
	TIMES LOCAL:10 , 1.00
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES LOCAL:10 , 1.20
ENDIF

;ABL:Ｍ感覚をみる
IF ABL:17 == 1
	LOCAL:24 = 20
ELSEIF ABL:17 == 2
	LOCAL:24 = 50
ELSEIF ABL:17 == 3
	LOCAL:24 = 100
ELSEIF ABL:17 == 4
	LOCAL:24 = 200
ELSEIF ABL:17 >= 5
	LOCAL:24 = 200 + (200 * (ABL:17 - 4))
ENDIF

;技巧を見る
;ABL:技巧をみる
IF ABL:2 == 0
	TIMES LOCAL:24 , 0.50
ELSEIF ABL:2 == 1
	TIMES LOCAL:24 , 0.80
ELSEIF ABL:2 == 2
	TIMES LOCAL:24 , 1.00
ELSEIF ABL:2 == 3
	TIMES LOCAL:24 , 1.20
ELSEIF ABL:2 == 4
	TIMES LOCAL:24 , 1.50
ELSE
	LOCAL:24 = LOCAL:24 * (ABL:2 * 10 + 110) / 100
ENDIF

;TALENT:恋慕をみる
SIF TALENT:85
	TIMES LOCAL:24 , 1.50
;TALENT:恋人をみる
SIF TALENT:153
	TIMES LOCAL:24 , 1.50
;TALENT:親愛をみる
SIF TALENT:88
	TIMES LOCAL:24 , 2.00
;TALENT:相愛をみる
SIF TALENT:89
	TIMES LOCAL:24 , 2.00

;ABL:精液中毒をみる
IF ABL:11 == 0
	TIMES LOCAL:10 , 0.10
	TIMES LOCAL:24 , 0.80
ELSEIF ABL:11 == 1
	TIMES LOCAL:10 , 0.20
	TIMES LOCAL:24 , 1.00
ELSEIF ABL:11 == 2
	TIMES LOCAL:10 , 0.50
	TIMES LOCAL:24 , 1.20
ELSEIF ABL:11 == 3
	TIMES LOCAL:10 , 1.00
	TIMES LOCAL:24 , 1.40
ELSEIF ABL:11 == 4
	TIMES LOCAL:10 , 1.20
	TIMES LOCAL:24 , 1.60
ELSE
	TIMES LOCAL:10 , 1.50
	LOCAL:24 = LOCAL:24 * (80 + (ABL:11 * 20)) / 100
ENDIF

;ABL:奉仕精神をみる
IF ABL:6 == 0
	TIMES LOCAL:10 , 0.10
	TIMES LOCAL:24 , 0.00
ELSEIF ABL:6 == 1
	TIMES LOCAL:10 , 0.20
	TIMES LOCAL:24 , 0.00
ELSEIF ABL:6 == 2
	TIMES LOCAL:10 , 0.50
	TIMES LOCAL:24 , 0.00
ELSEIF ABL:6 == 3
	TIMES LOCAL:10 , 1.00
	TIMES LOCAL:24 , 0.85
ELSEIF ABL:6 == 4
	TIMES LOCAL:10 , 1.20
	TIMES LOCAL:24 , 1.00
ELSE
	TIMES LOCAL:10 , 1.50
	LOCAL:24 = LOCAL:24 * (100 + (ABL:6 * 3)) / 100
ENDIF

SOURCE:10 += LOCAL:10
SOURCE:24 += LOCAL:24

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_31
A = 0
S = 0
V = 85
;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	A += ABL:1 * 1
ENDIF
;ABL:奉仕精神
IF ABL:6
	A += ABL:6 * 4
ENDIF
;ABL:精液中毒
IF ABL:11
	A += ABL:11 * 3
ENDIF

IF MARK:1
	A += MARK:1 * 1
ENDIF


;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 1
ENDIF

;恥じらい
IF TALENT:35
	A -= 1
ENDIF
;汚臭鈍感
IF TALENT:61
	A += 1
ENDIF
;汚臭敏感
IF TALENT:62
	A -= 3
ENDIF
;献身的
IF TALENT:63
	A += 6
ENDIF
;快感の否定
IF TALENT:71
	A -= 1
ENDIF
;男嫌い
IF TALENT:82 && TALENT:PLAYER:122
	A -= 12
ENDIF
;恋慕
IF TALENT:85 && ASSIPLAY == 0
	A += 5
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:121
	A += 8
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2



;汚れあり
IF Y
	A -= Y
	;汚臭鈍感
	IF TALENT:61
	ELSEIF TALENT:62
	ELSE
	ENDIF
ENDIF
;好感度
IF CFLAG:2 < 1000
	B = 0
	IF CFLAG:2 < 500
		B = 50
	ELSEIF CFLAG:2 < 750
		B = 20
	ELSEIF CFLAG:2 < 900
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 1500
	B = 0
	IF CFLAG:2 < 2000
		B = 5
	ELSE
		B = CFLAG:2 / 200
	ENDIF
	A += B
ENDIF

;合意
IF TALENT:152 == 0
	A -= 10
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:1
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:1,'(,ABL:1 * 1,')
		S = 1
	ENDIF
	;ABL:奉仕精神
	IF ABL:6
		SIF S
			PRINT  + 
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:6,'(,ABL:6 * 4,')
		S = 1
	ENDIF
	;ABL:精液中毒
	IF ABL:11
		SIF S
			PRINT  + 
		PRINTS ABLNAME:11
		PRINTV 'LV,ABL:11,'(,ABL:11 * 3,')
		S = 1
	ENDIF

	IF MARK:1
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:1,'(,MARK:1 * 1,')
		S = 1
	ENDIF


	;PALAM:欲情
	IF PALAM:5 < PALAMLV:1
		L = 0
	ELSEIF PALAM:5 < PALAMLV:2
		L = 1
	ELSEIF PALAM:5 < PALAMLV:3
		L = 2
	ELSEIF PALAM:5 < PALAMLV:4
		L = 3
	ELSEIF PALAM:5 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 1,')
		S = 1
	ENDIF

	;恥じらい
	IF TALENT:35
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,1,')
		S = 1
	ENDIF
	;汚臭鈍感
	IF TALENT:61
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:61
		PRINTV '(,1,')
		S = 1
	ENDIF
	;汚臭敏感
	IF TALENT:62
		PRINT  - 
		PRINTS TALENTNAME:62
		PRINTV '(,3,')
		S = 1
	ENDIF
	;献身的
	IF TALENT:63
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:63
		PRINTV '(,6,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:71
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,1,')
		S = 1
	ENDIF
	;男嫌い
	IF TALENT:82 && TALENT:PLAYER:122
		PRINT  - 
		PRINTS TALENTNAME:82
		PRINTV '(,12,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:85 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 1
	ENDIF

	;調教者がふたなり
	IF TALENT:PLAYER:121
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:121
		PRINTV '(,8,')
		S = 1
	ENDIF

	Y = 0
	;愛液の汚れ
	SIF STAIN:PLAYER:2 & 1
		Y += 1
	;精液の汚れ
	SIF STAIN:PLAYER:2 & 4
		Y += 3
	;アナルの汚れ
	SIF STAIN:PLAYER:2 & 8
		Y += 7
	SIF STAIN:PLAYER:2 & 16
		Y += 15

	SIF TALENT:61
		Y /= 3
	SIF TALENT:62
		Y *= 2



	;汚れあり
	IF Y
		PRINT  - 
		;汚臭鈍感
		IF TALENT:61
			PRINT 汚れあり、
			PRINTS TALENTNAME:61
		ELSEIF TALENT:62
			PRINT 汚れあり、
			PRINTS TALENTNAME:62
		ELSE
			PRINT 汚れあり
		ENDIF
		PRINTV '(,Y,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 < 1000
		B = 0
		PRINT  - 
		IF CFLAG:2 < 500
			B = 50
		ELSEIF CFLAG:2 < 750
			B = 20
		ELSEIF CFLAG:2 < 900
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 >= 1500
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 2000
			B = 5
		ELSE
			B = CFLAG:2 / 200
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合意
	IF TALENT:152 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,10,')
		S = 1
	ENDIF

	;合計を表示(85以上で実行)
	PRINT  = 
	PRINTV A

	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF
