@COM_ABLE373
;着衣フェラチオ実行判定
;COM_ABLE共通判定
CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中は無理
SIF TFLAG:44 == 1
	RETURN 0
;助手では無理
SIF ASSIPLAY
	RETURN 0
;マスターがふたなりかオトコじゃないと無理
SIF TALENT:MASTER:121 == 0 && TALENT:MASTER:122 == 0
	RETURN 0
;[猫舌]だと技巧8以上じゃないと無理
SIF ABL:2 < 8 && TALENT:54
	RETURN 0
;デート中は淫乱じゃないと無理
SIF TEQUIP:2 && TALENT:76 == 0
	RETURN 0
;欲望6以上かつ、奉仕精神6以上かつ、親密8以上or好感度800以上or恋人が必要
SIF ABL:1 < 6 || ABL:6 < 6 || (ABL:0 < 8 && CFLAG:2 < 800 && TALENT:153 == 0)
	RETURN 0
RETURN 1

@COM373
;フェラチオ
;対象が恋慕さとりか、猫舌だと変化
IF NAME_CSV(NO:TARGET, 0) == "さとり" && TALENT:85
	PRINTL 読心フェラ
ELSEIF TALENT:54
	PRINTL 猫舌フェラ
ELSE
	PRINTL フェラチオ
ENDIF


;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = フェラチオ
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
SIF TALENT:MASTER:122  || TALENT:MASTER:121
	BASE:PLAYER:2 += CALC_EJAC(SELECTCOM, MASTER)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 10
LOSEBASE:1 += 120

SOURCE:13 = 800
SOURCE:14 = 700

;上のほうで計算した汚れデータ
SOURCE:8 = Y*40 + 100

;Ｍ感覚を設定している場合快Ｍソース等取得
SIF FLAG:62 & 4194304
	CALL SOURCE_M_COM31

;ABL:奉仕精神をみる
IF ABL:6 == 0
	SOURCE:4 = 420
	SOURCE:5 = 150
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:6 == 1
	SOURCE:4 = 500
	SOURCE:5 = 300
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:6 == 2
	SOURCE:4 = 580
	SOURCE:5 = 600
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:6 == 3
	SOURCE:4 = 660
	SOURCE:5 = 900
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:6 == 4
	SOURCE:4 = 740
	SOURCE:5 = 1500
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:4 = 620 + (ABL:6 * 40)
	SOURCE:5 = 450 + (ABL:6 * 350)
	TIMES SOURCE:8 , 0.10
ENDIF


;ABL:技巧をみる
IF ABL:2 == 0
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:2 == 1
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:2 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:2 == 3
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:2 == 4
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSE
	SOURCE:4 = SOURCE:4 * (ABL:2 * 25 + 75) / 100
	SOURCE:5 = SOURCE:5 * (ABL:2 * 25 + 75) / 100
ENDIF

;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:0 <= 1
		SOURCE:21 += 300 + (ABL:0 * 50)
	ELSEIF ABL:0 <= 3
		SOURCE:21 += 600 + (ABL:0 * 100)
	ELSEIF ABL:0 <= 5
		SOURCE:21 += 800 + (ABL:0 * 120)
	ELSEIF ABL:0 <= 8
		SOURCE:21 += 1000 + (ABL:0 * 140)
	ELSEIF ABL:0 <= 11
		SOURCE:21 += 1200 + (ABL:0 * 160)
	ELSE
		SOURCE:21 += 1500 + (ABL:0 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:15 == 0
		SOURCE:20 = 800
	ELSEIF ABL:15 == 1
		SOURCE:20 = 1100
	ELSEIF ABL:15 == 2
		SOURCE:20 = 1400
	ELSEIF ABL:15 == 3
		SOURCE:20 = 1800
	ELSEIF ABL:15 == 4
		SOURCE:20 = 2300
	ELSE
		SOURCE:20 = 1000 + (ABL:15 * 350)
	ENDIF
ENDIF


;デート中は
IF TEQUIP:2 > 0
	;ポルノ映画以外は
	IF TEQUIP:2 != 22
		;逸脱追加
		SOURCE:14 += 3000
		;さらに露出追加
		SOURCE:12 += 4000
		;さらに異常経験追加
		EXP:50 += 2
		PRINTL 異常経験+2
	ELSE
	;ポルノ映画は
		;逸脱追加
		SOURCE:14 += 1500
		;さらに露出追加
		SOURCE:12 += 2000
		;さらに異常経験追加
		EXP:50 += 1
		PRINTL 異常経験+1
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
SIF TALENT:85 || TALENT:153
	A += 2
SIF TALENT:88
	A += 3
;合意がないと好感度が下がる
IF TALENT:152 == 0 && TFLAG:48 == 0
	A -= 8
	SOURCE:22 += 300
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;既成事実2に相当
TFLAG:200 = 2

;-------------------------------------------------
;読心フェラの場合効果アップ（2倍）
;-------------------------------------------------

;対象が恋慕さとりだと変化
IF NAME_CSV(NO:TARGET, 0) == "さとり" && TALENT:85
	TIMES SOURCE:4 , 2.00
	TIMES SOURCE:5 , 2.00
ENDIF


;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK

IF E
;射精している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:11 == 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 4.00
	ELSEIF ABL:11 == 1
		TIMES SOURCE:5 , 3.00
		SOURCE:7 = 500
		TIMES SOURCE:13, 3.00
	ELSEIF ABL:11 == 2
		TIMES SOURCE:5 , 4.00
		SOURCE:7 = 1200
		TIMES SOURCE:13, 2.50
	ELSEIF ABL:11 == 3
		TIMES SOURCE:5 , 6.00
		SOURCE:7 = 3000
		SOURCE:10 += 500 + (ABL:11 * 100)
		TIMES SOURCE:12, 1.50
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:11 == 4
		TIMES SOURCE:5 , 9.00
		SOURCE:7 = 6000
		SOURCE:10 += 700 + (ABL:11 * 200)
		TIMES SOURCE:12, 4.00
		TIMES SOURCE:13, 1.50
	ELSE
		SOURCE:7 = (ABL:11 * 3000) - 3000
		SOURCE:5 = SOURCE:5 * (ABL:11 * 300) / 100
		SOURCE:10 += 500 + (ABL:11 * 300)
		SOURCE:12 = SOURCE:12 * (ABL:11 * 200) / 130
		IF ABL:11 <= 10
			SOURCE:13 = SOURCE:13 * (150 - ABL:11 * 10) / 100
		ELSE
			TIMES SOURCE:13, 0.50
		ENDIF
	ENDIF

	;射精先タイプの設定
	TFLAG:703 = 4
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF


PRINTFORML %EXPNAME:22%+1
EXP:22 += 1

;奴隷の口⇔調教者のＰの汚れが移動
STAIN:0 |= STAIN:MASTER:2
STAIN:MASTER:2 |= STAIN:0

;精液中毒Lv2以上、技巧Lv10以上、ペニス以外の汚れがある時なら汚れをなめ取ってくれる
IF ABL:11 >= 2 && ABL:2 >= 10 && (STAIN:MASTER:2 & 29)
	STAIN:MASTER:2 = 2
;	TFLAG:8 = 1
ENDIF

;レズ経験
IF TALENT:122 == 0 && TALENT:MASTER:122 == 0
	PRINTFORML %EXPNAME:40%+7
	EXP:40 += 7
	EXP:MASTER:40 += 7
;BL経験
ELSEIF TALENT:122 && TALENT:MASTER:122
	PRINTFORML %EXPNAME:41%+7
	EXP:41 += 7
	EXP:MASTER:41 += 7
ENDIF

PRINTL 

;フェラ経験が20以上だと好感度ボーナス
SIF EXP:22 >= EXPLV:3
	TFLAG:30 += 1

;調教者がふたなり
IF TALENT:MASTER:121
	SOURCE:13 /= 2
ENDIF

TFLAG:100 = 1

RETURN 1

