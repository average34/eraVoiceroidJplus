@COM_ABLE365
;着衣後背位実行判定
;COM_ABLE共通判定
CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中は無理
SIF TFLAG:44 == 1
	RETURN 0
;デート中は無理
SIF TEQUIP:2
	RETURN 0
;オムツ着用中は無理
SIF TEQUIP:20
	RETURN 0
;オトコだとダメ
SIF TALENT:オトコ
	RETURN 0
;調教者がオトコ、半人半妖、ふたなり、もしくはペニスバンドがないとダメ
SIF TALENT:PLAYER:半人半妖 == 0 && TALENT:PLAYER:ふたなり == 0 && TALENT:PLAYER:オトコ == 0 && ITEM:PBAND == 0
	RETURN 0
;小人体型持ちは、双方が調教者と調教対象ならOK
;それ以外の場合は主人が禁断の知識持ちじゃないとダメ
SIF TALENT:小人体型 && (TALENT:MASTER:禁断の知識 == 0 && TALENT:PLAYER:小人体型 == 0)
	RETURN 0
;潤滑10000以上かつ、欲情10000以上が必要
SIF PALAM:3 < 10000 || PALAM:5 < 10000
	RETURN 0
;欲望9以上が必要
SIF ABL:欲望 < 9
	RETURN 0
;親密13以上or好感度6000以上が必要
SIF ABL:親密 < 13 && CFLAG:2 < 6000
	RETURN 0
;合意が必要
SIF TALENT:合意 == 0
	RETURN 0
;淫乱が必要
SIF TALENT:淫乱 == 0
	RETURN 0
;ムードか酒の勢いが必要
SIF TFLAG:37 < 50 && TFLAG:47 < 30
	RETURN 0
;着衣挿入コマンド拡張コンフィグがONじゃないと無理
SIF FLAG:61 & 1p6 == 0
	RETURN 0
RETURN 1

;着衣後背位
@TRAIN_MESSAGE_COM365
;前回と今回の調教者が同じで、前回の調教が同一コマンドであり、抜かないフラグがある場合
IF TFLAG:60 && PREVCOM == 365 && ((ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0))
	;調教者がオトコか半人半妖かふたなりである
