;	STAIN:PLAYER:ペニス |= 4
;
;	BASE:PLAYER:射精 -= EJAC
;	SIF BASE:PLAYER:射精 >= EJAC
;		BASE:PLAYER:射精 = EJAC-1
;
;	;セックスで射精フラグ
;	TFLAG:2 = 1
;ENDIF

;奴隷のＶ⇔調教者のＰの汚れが移動
STAIN:ヴァギナ |= STAIN:PLAYER:ペニス
STAIN:PLAYER:ペニス |= STAIN:ヴァギナ

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +4
	EXP:レズ経験 += 4
	EXP:PLAYER:レズ経験 += 4
ENDIF

;IF ASSIPLAY == 0
;	IF ABL:Ｖ感覚 >= 3
;		TFLAG:30 += 2
;	ELSE
;		TFLAG:30 += 1
;	ENDIF
;ENDIF

