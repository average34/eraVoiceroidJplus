@COM66
;Ｗフェラ
;二本舐めさせる

PRINTL Ｗフェラ

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = Ｗフェラ(二本舐め)
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
SIF TALENT:MASTER:121 || TALENT:MASTER:122
	BASE:MASTER:2 += CALC_EJAC(SELECTCOM, MASTER)

;-------------------------------------------------
;射精ゲージチェック２（助手）
;-------------------------------------------------
SIF TALENT:ASSI:121 || TALENT:ASSI:122
	BASE:ASSI:2 += CALC_EJAC(SELECTCOM, ASSI)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 80
LOSEBASE:1 += 250

SOURCE:13 = 6000
SOURCE:14 = 600

;上のほうで計算した汚れデータ
SOURCE:8 = Y*40 + 100

;Ｍ感覚を設定している場合快Ｍソース等取得
;フェラチオの快Ｍソース計算を使用
SIF FLAG:62 & 4194304
	CALL SOURCE_M_COM31

;ABL:奉仕精神をみる
IF ABL:6 == 0
	SOURCE:4 = 800
	SOURCE:5 = 600
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:6 == 1
	SOURCE:4 = 1200
	SOURCE:5 = 900
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:6 == 2
	SOURCE:4 = 1400
	SOURCE:5 = 1000
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:6 == 3
	SOURCE:4 = 1600
	SOURCE:5 = 1200
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:6 == 4
	SOURCE:4 = 1800
	SOURCE:5 = 1500
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:4 = 1500 + (ABL:6 * 100)
	SOURCE:5 = 450 + (ABL:6 * 350)
	TIMES SOURCE:8 , 0.10
ENDIF


;ABL:技巧をみる
IF ABL:2 == 0
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:2 == 1
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:2 == 2
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:2 == 3
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSEIF ABL:2 == 4
	TIMES SOURCE:4 , 2.00
	TIMES SOURCE:5 , 2.00
ELSE
	SOURCE:4 = SOURCE:4 * (ABL:2 * 10 + 170) / 100
	SOURCE:5 = SOURCE:5 * (ABL:2 * 10 + 170) / 100
ENDIF


;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:0 <= 1
		SOURCE:21 += 300 + (ABL:0 * 50)
	ELSEIF ABL:0 <= 3
		SOURCE:21 += 600 + (ABL:0 * 100)
	ELSEIF ABL:0 <= 5
		SOURCE:21 += 800 + (ABL:0 * 120)
	ELSEIF ABL:0 <= 8
		SOURCE:21 += 1000 + (ABL:0 * 140)
	ELSEIF ABL:0 <= 11
		SOURCE:21 += 1200 + (ABL:0 * 160)
	ELSE
		SOURCE:21 += 1500 + (ABL:0 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:15 == 0
		SOURCE:20 = 800
	ELSEIF ABL:15 == 1
		SOURCE:20 = 1100
	ELSEIF ABL:15 == 2
		SOURCE:20 = 1400
	ELSEIF ABL:15 == 3
		SOURCE:20 = 1800
	ELSEIF ABL:15 == 4
		SOURCE:20 = 2300
	ELSE
		SOURCE:20 = 1000 + (ABL:15 * 350)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
SIF TALENT:85 || TALENT:153
	A += 2
SIF TALENT:88
	A += 3
;合意がないと好感度が下がる
IF TALENT:152 == 0 && TFLAG:48 == 0
	A -= 9
	SOURCE:15 += 400
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;ASSIPLAYを保持
A:1 = ASSIPLAY

;-------------------------------------------------
;射精チェック(主人)
;-------------------------------------------------
ASSIPLAY = 0
PLAYER = MASTER
CALL SAMEN_CHECK
;S = BASE:MASTER:2
;EJAC = MAXBASE:MASTER:2
;
;IF  S > EJAC * 2
;	E = 2
;ELSEIF S > EJAC
;	E = 1
;ELSE
;	E = 0
;ENDIF

IF E
;射精している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:11 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 4.00
	ELSEIF ABL:11 == 1
		SOURCE:7 = 500
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 3.00
	ELSEIF ABL:11 == 2
		SOURCE:7 = 1200
		TIMES SOURCE:5 , 4.00
		TIMES SOURCE:13, 2.50
	ELSEIF ABL:11 == 3
		SOURCE:7 = 3000
		TIMES SOURCE:5 , 6.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:11 == 4
		SOURCE:7 = 6000
		TIMES SOURCE:5 , 9.00
		TIMES SOURCE:13, 1.50
	ELSE
		SOURCE:7 = (ABL:11 * 3000) - 3000
		SOURCE:5 = SOURCE:5 * (ABL:0 * 3)
		TIMES SOURCE:13, 1.00
	ENDIF
	
	;射精先タイプの設定
	TFLAG:703 = 12
	;フェラで射精(3P用の処理を使用)
	TFLAG:40 = 3
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;;大量射精
;IF E == 2
;	TIMES SOURCE:7 , 2.00
;	TIMES SOURCE:5 , 1.50
;
;	EXP:MASTER:3 += 2
;	EXP:20 += 9
;	PRINTL 大量射精
;	PRINTL 精液経験＋９
;	;精液汚れ
;	STAIN:MASTER:2 |= 4
;
;	BASE:MASTER:2 -= EJAC*2
;	SIF BASE:MASTER:2 >= EJAC
;		BASE:MASTER:2 = EJAC-1
;
;	;口で射精させたフラグ
;	TFLAG:0 = 2
;;通常の射精
;ELSEIF E == 1
;	EXP:MASTER:3 += 1
;	EXP:20 += 3
;	PRINTL 射精
;	PRINTL 精液経験＋３
;	;精液汚れ
;	STAIN:MASTER:2 |= 4
;
;	BASE:MASTER:2 -= EJAC
;	SIF BASE:MASTER:2 >= EJAC
;		BASE:MASTER:2 = EJAC-1
;
;	;口で射精させたフラグ
;	TFLAG:0 = 1
;ENDIF

PRINTFORML %EXPNAME:22%+2

EXP:22 += 2

;奴隷の口⇔調教者のＰの汚れが移動
IF TALENT:MASTER:121 || TALENT:MASTER:122
	STAIN:0 |= STAIN:MASTER:2
	STAIN:MASTER:2 |= STAIN:0
ENDIF

;奉仕精神ＬＶ２以上、技巧ＬＶ２以上なら汚れをなめ取る
IF ABL:6 >= 2 && ABL:2 >= 2
	STAIN:MASTER:2 = 2
	SIF E >= 1
		TFLAG:8 = 1
ENDIF

;マスターと奴隷のレズorBL
IF TALENT:122 == 0 && TALENT:MASTER:122 == 0
	PRINTFORML %EXPNAME:40%+7
	EXP:40 += 7
	EXP:MASTER:40 += 7
ELSEIF TALENT:122 == 1 && TALENT:MASTER:122 == 1
	PRINTFORML %EXPNAME:40%+7
	EXP:41 += 7
	EXP:MASTER:41 += 7
ENDIF

;調教者がふたなり
IF TALENT:MASTER:121
	SOURCE:13 /= 2
ENDIF
;-------------------------------------------------
;射精チェック２(助手)
;-------------------------------------------------
ASSIPLAY = 1
PLAYER = ASSI
CALL SAMEN_CHECK
;S = BASE:ASSI:2
;EJAC = MAXBASE:ASSI:2
;
;IF  S > EJAC * 2
;	E = 2
;ELSEIF S > EJAC
;	E = 1
;ELSE
;	E = 0
;ENDIF

IF E
	;射精先タイプの設定
	TFLAG:703 = 11
	;フェラで射精(3P用の処理を使用)
	TFLAG:41 = 3
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
;;大量射精
;IF E == 2
;	EXP:ASSI:3 += 2
;	EXP:20 += 3
;	PRINTL 大量射精（助手）
;	PRINTL 精液経験+3
;	;Ｐに精液汚れ
;	STAIN:ASSI:2 |= 4
;
;	BASE:ASSI:2 -= EJAC*2
;	SIF BASE:ASSI:2 >= EJAC
;		BASE:ASSI:2 = EJAC-1
;	;助手が射精フラグ
;	TFLAG:6 = 2
;;通常の射精
;ELSEIF E == 1
;	EXP:ASSI:3 += 1
;	EXP:20 += 1
;	PRINTL 射精（助手）
;	PRINTL 精液経験+1
;	;Ｐに精液汚れ
;
;	STAIN:ASSI:2 |= 4
;
;	BASE:ASSI:2 -= EJAC
;	SIF BASE:ASSI:2 >= EJAC
;		BASE:ASSI:2 = EJAC-1
;	;助手が射精フラグ
;	TFLAG:6 = 1
ENDIF

;奴隷の口⇔主人のＰの汚れが移動
IF TALENT:MASTER:121 || TALENT:MASTER:122
	STAIN:0 |= STAIN:MASTER:2
	STAIN:MASTER:2 |= STAIN:0
ENDIF

;奴隷の口⇔助手のＰの汚れが移動
IF TALENT:ASSI:121 || TALENT:ASSI:122
	STAIN:0 |= STAIN:ASSI:2
	STAIN:ASSI:2 |= STAIN:0
ENDIF

;奉仕精神ＬＶ２以上、技巧ＬＶ２以上なら汚れをなめ取る
IF ABL:6 >= 2 && ABL:2 >= 2
	STAIN:ASSI:2 = 2
	SIF E >= 1
		TFLAG:8 = 1
ENDIF

;助手と奴隷のレズorBL
IF TALENT:122 == 0 && TALENT:ASSI:122 == 0
	PRINTFORML %EXPNAME:40%(助手)+7
	PRINTFORML %EXPNAME:40%+7
	EXP:40 += 7
	EXP:ASSI:40 += 7
ELSEIF TALENT:122 == 1 && TALENT:ASSI:122 == 1
	PRINTFORML %EXPNAME:41%(助手)+7
	PRINTFORML %EXPNAME:41%+7
	EXP:41 += 7
	EXP:ASSI:41 += 7
ENDIF

;ASSIPLAYとPLAYERの戻し
ASSIPLAY = A:1
IF A:1
	PLAYER = ASSI
ELSE
	PLAYER = MASTER
ENDIF

TFLAG:100 = 1

;既成事実２に相当
TFLAG:200 = 2

RETURN 1

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_66
A = 0
S = 0
V = 110
;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:1
	A += ABL:1 * 1
ENDIF
;ABL:奉仕精神
IF ABL:6
	A += ABL:6 * 4
ENDIF
;ABL:精液中毒
IF ABL:11
	A += ABL:11 * 3
ENDIF

IF MARK:1
	A += MARK:1 * 1
ENDIF


;PALAM:欲情
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	A += L * 1
ENDIF

;恥じらい
IF TALENT:35
	A -= 1
ENDIF
;汚臭鈍感
IF TALENT:61
	A += 1
ENDIF
;汚臭敏感
IF TALENT:62
	A -= 3
ENDIF
;献身的
IF TALENT:63
	A += 6
ENDIF
;快感の否定
IF TALENT:71
	A -= 1
ENDIF
;恋慕
IF TALENT:85 && ASSIPLAY == 0
	A += 5
ENDIF
;調教者がふたなり
IF TALENT:PLAYER:121
	A += 8
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2

;汚れあり
SIF Y
	A -= Y

;好感度
IF CFLAG:2 < 1000
	B = 0
	IF CFLAG:2 < 500
		B = 50
	ELSEIF CFLAG:2 < 750
		B = 20
	ELSEIF CFLAG:2 < 900
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF
;好感度
IF CFLAG:2 >= 1500
	B = 0
	IF CFLAG:2 < 2000
		B = 5
	ELSE
		B = CFLAG:2 / 200
	ENDIF
	A += B
ENDIF

;合意
IF TALENT:152 == 0
	A -= 10
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;ABL:欲望
	IF ABL:1
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:1,'(,ABL:1 * 1,')
		S = 1
	ENDIF
	;ABL:奉仕精神
	IF ABL:6
		SIF S
			PRINT  + 
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:6,'(,ABL:6 * 4,')
		S = 1
	ENDIF
	;ABL:精液中毒
	IF ABL:11
		SIF S
			PRINT  + 
		PRINTS ABLNAME:11
		PRINTV 'LV,ABL:11,'(,ABL:11 * 3,')
		S = 1
	ENDIF

	IF MARK:1
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:1,'(,MARK:1 * 1,')
		S = 1
	ENDIF


	;PALAM:欲情
	IF PALAM:5 < PALAMLV:1
		L = 0
	ELSEIF PALAM:5 < PALAMLV:2
		L = 1
	ELSEIF PALAM:5 < PALAMLV:3
		L = 2
	ELSEIF PALAM:5 < PALAMLV:4
		L = 3
	ELSEIF PALAM:5 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 1,')
		S = 1
	ENDIF

	;恥じらい
	IF TALENT:35
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,1,')
		S = 1
	ENDIF
	;汚臭鈍感
	IF TALENT:61
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:61
		PRINTV '(,1,')
		S = 1
	ENDIF
	;汚臭敏感
	IF TALENT:62
		PRINT  - 
		PRINTS TALENTNAME:62
		PRINTV '(,3,')
		S = 1
	ENDIF
	;献身的
	IF TALENT:63
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:63
		PRINTV '(,6,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:71
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,1,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:85 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 1
	ENDIF
	;調教者がふたなり
	IF TALENT:PLAYER:121
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:121
		PRINTV '(,8,')
		S = 1
	ENDIF

	Y = 0
	;愛液の汚れ
	SIF STAIN:PLAYER:2 & 1
		Y += 1
	;精液の汚れ
	SIF STAIN:PLAYER:2 & 4
		Y += 3
	;アナルの汚れ
	SIF STAIN:PLAYER:2 & 8
		Y += 7
	SIF STAIN:PLAYER:2 & 16
		Y += 15

	SIF TALENT:61
		Y /= 3
	SIF TALENT:62
		Y *= 2



	;汚れあり
	IF Y
		PRINT  - 
		;汚臭鈍感
		IF TALENT:61
			PRINT 汚れあり、
			PRINTS TALENTNAME:61
		ELSEIF TALENT:62
			PRINT 汚れあり、
			PRINTS TALENTNAME:62
		ELSE
			PRINT 汚れあり
		ENDIF
		PRINTV '(,Y,')
		S = 1
	ENDIF

	;好感度
	IF CFLAG:2 < 1000
		B = 0
		PRINT  - 
		IF CFLAG:2 < 500
			B = 50
		ELSEIF CFLAG:2 < 750
			B = 20
		ELSEIF CFLAG:2 < 900
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF
	;好感度
	IF CFLAG:2 >= 1500
		B = 0
		SIF S
			PRINT  + 
		IF CFLAG:2 < 2000
			B = 5
		ELSE
			B = CFLAG:2 / 200
		ENDIF
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF

	;合意
	IF TALENT:152 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,10,')
		S = 1
	ENDIF


	;合計を表示(110以上で実行)
	PRINT  = 
	PRINTV A

	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF

