@TYOGO_MISS2
;調合失敗時に素材が惜しいとヒントを出す

;選択した素材数をKに記録
K = 0
REPEAT 100
	K += TFLAG:(900 + COUNT)
REND

VARSET LOCAL, 0
FOR LOCAL, 0, 300
	VARSET A, 0
	A = LOCAL + 2000
	;複数選択肢のある素材はTFLAGで判断するよう引数を1にする
	CALL TYOGO_USE_SOZAI, 1
	;各レシピごとに以下を算出
	;必要な素材が選択されているか
	;必要な素材数はいくつか
	LOCAL:2 = 0
	LOCAL:3 = 0
	FOR LOCAL:1, 1, 40
		;A:○が指定されていない場合（それ以上必要な素材がない場合）は処理終了しヒントが出せるか算出
		IF A:(LOCAL:1) == 0
			BREAK
		ENDIF
		;A:○で指定されている必要な素材が選択されている場合LOCAL:2に記録
		IF TFLAG:(A:(LOCAL:1)) == 1
			LOCAL:2 += 1
		ENDIF
		;A:○があれば+1していくことで必要な素材数をLOCAL:3に記録
		LOCAL:3 += 1
	NEXT
	
	;選択した素材数、必要な素材数と比較してヒントが出せるか算出
	;K = LOCAL:3でLOCAL:2 = LOCAL:3だとそのレシピが作れる事になる
	;素材が１つ余分な場合
	IF K - 1 == LOCAL:3 && LOCAL:2 == LOCAL:3
		LOCAL:4 = 1
		PRINTL 
		PRINTFORMW …余分なものを除けば何かできるかもしれない
	;素材を１つ変えれば出来る場合
	ELSEIF K == LOCAL:3 && LOCAL:2 == LOCAL:3 - 1
		LOCAL:4 = 1
		PRINTL 
		PRINTFORMW 素材を1つ変更すれば何か出来る気がする
	;素材を１つ加えれば出来る場合(ただし、2つ以上の調合に限る)
	ELSEIF K + 1 == LOCAL:3 && LOCAL:2 + 1 == LOCAL:3 && K >= 2
		LOCAL:4 = 1
		PRINTL 
		PRINTFORMW …何かが１つ足りない感じがする
	ENDIF
	;ヒントが出るなら処理終了
	SIF LOCAL:4
		BREAK
NEXT

