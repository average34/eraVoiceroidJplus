		;主導権が主人公側
		;ABL:従順をみる
		IF ABL:親密 == 0
			D = 400
		ELSEIF ABL:親密 == 1
			D = 600
		ELSEIF ABL:親密 == 2
			D = 800
		ELSEIF ABL:親密 == 3
			D = 1000
		ELSEIF ABL:親密 == 4
			D = 1200
		ELSE
			D = 1000 + (ABL:親密 * 100)
		ENDIF
	ELSE
		;主導権が相手側
		;ABL:サドっ気をみる
		IF ABL:サドっ気 == 0
			C = 500
		ELSEIF ABL:サドっ気 == 1
			C = 700
		ELSEIF ABL:サドっ気 == 2
			C = 900
		ELSEIF ABL:サドっ気 == 3
			C = 1100
		ELSEIF ABL:サドっ気 == 4
			C = 1300
		ELSE
			C = 1000 + (ABL:サドっ気 * 150)
		ENDIF
	ENDIF
	;好感度はカット
