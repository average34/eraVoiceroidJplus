;---------------------------------------------------------
;調合経験取得
;---------------------------------------------------------
@TYOGO_EXP
;雑草、薬草はレア度０
;小数点以下切捨て
P = 0
REPEAT 100
	IF TFLAG:(900 + COUNT)
		P += ITEMPRICE:(900 + COUNT)
	ENDIF
REND

;---------------------------------------------------------
;レア度・必要な素材数取得
;Aの調合アイテムの必要な素材数とレア度を取得する
;Cが必要な素材数
;Pがレア度(計算の関係で不正確ですがレシピの断片ということで)
;---------------------------------------------------------
@TYOGO_CHECK
;使用する素材を取得し、レア度の合計と必要な素材数を取得。
CALL TYOGO_USE_SOZAI
C = 0
P = 0
REPEAT 100
	;COUNT == 0は飛ばす（A:0はレシピの番号）
	SIF COUNT == 0
		CONTINUE
	;A:COUNTが0だったらそれ以上必要な素材はないので終了する
	SIF A:COUNT == 0
		BREAK
	
	;レア度(調合経験取得に合わせてP)
	P += ITEMPRICE:(A:COUNT)
	;必要な素材数
	C += 1
REND

;---------------------------------------------------------
;レシピの断片
;---------------------------------------------------------
@RECIPE_FRAGMENT
ITEM:800 -= 1
;レシピ名登録
CALL RECIPE_NAME
;調合ランク判定
B = EXP:TARGET:調合経験
;習得早い場合は経験値1.5倍、遅い場合は2/3倍
IF TALENT:TARGET:習得早い
	B *= 3
	B /= 2
ELSEIF TALENT:TARGET:習得遅い
	B *= 2
	B /= 3
ENDIF
IF B >= 5000
	B = 120
ELSEIF B >= 3000
	B = 20 + (B - 3000)/25
ELSEIF B >= 1500
	B = 10 + (B - 1500)/75
ELSEIF B >= 300
	B = 5 + (B - 300)/240
ELSEIF B >= 200
	B = 4
ELSEIF B >= 100
	B = 3
ELSEIF B >= 50
	B = 2
ELSE
	B = 1
ENDIF
;調教者が調合知識もしくは禁断の知識を持っている場合、ランクは最低でも5になる
SIF (TALENT:PLAYER:調合知識 || TALENT:PLAYER:禁断の知識) && B <= 5
	B = 5
;調教対象が調合知識もしくは禁断の知識を持っている場合、ランクは最低でも10になる
SIF (TALENT:TARGET:調合知識 || TALENT:TARGET:禁断の知識) && B <= 10
	B = 10
;ふたりとも禁断の知識を持っていない場合は1%の確率で、片方が持っている場合は10%で、二人とも持っている場合20%で、ランクMAXになる
IF TALENT:PLAYER:禁断の知識 && TALENT:TARGET:禁断の知識
	SIF RAND:20 == 0
		B = 120
ELSEIF TALENT:TARGET:禁断の知識
	SIF RAND:10 == 0
		B = 120
ELSEIF TALENT:PLAYER:禁断の知識
	SIF RAND:10 == 0
		B = 120
ELSE
	SIF RAND:100 == 0
		B = 120
ENDIF

A = 0
N = 0
G = 0
;解明可能レシピ候補選定。不可能なのをすべてはじいて、可能なのをF列に記録していく
REPEAT 300
	A = COUNT + 2000

	;2000と失敗作は除外(2185混沌の渦も特殊なので除外)
	SIF A == 2001 || A == 2000 || A == 2185
		CONTINUE
	;作成済みレシピと中身がすべて判明しているレシピは除外
	SIF (FLAG:A & 1) || (FLAG:A & 2)
		CONTINUE
	
	;レアリティー、必要素材数を取得
	CALL TYOGO_CHECK
	;多重REPEATなのでAを利用してCOUNT復元。
	COUNT = A - 2000
	
	;必要素材数が０（C = 0）なら除外
	SIF C == 0
		CONTINUE
	
	;調合ランクが不足しているレシピは蹴る
	SIF B < P
		CONTINUE

	;ここまで残ったレシピが解明レシピ候補
	L:N = A
	N += 1
REND
;取得出来るレシピがなかった
IF N == 0
	PRINTFORMW 残念ながら、レシピの断片から新しいレシピを読み取ることはできなかった…
ELSE
	;取得するレシピの確定
	G = RAND:N
	A = L:G
	;必要素材数を取得
	CALL TYOGO_CHECK
	;調合知識・禁断の知識があるときは名前フラグも立てておく
	SIF TALENT:PLAYER:調合知識 == 1 || TALENT:TARGET:調合知識 == 1 || TALENT:PLAYER:禁断の知識 == 1 || TALENT:TARGET:禁断の知識 == 1
		FLAG:A |= 128
	;未取得のレシピの位置を選定
	N = 0
	;1個目
	IF (FLAG:A & 4) == 0
		L:N = 1
		N += 1
	ENDIF
	;2個目
	IF (FLAG:A & 8) == 0 && C >= 2
		L:N = 2
		N += 1
	ENDIF
	;3個目
	IF (FLAG:A & 16) == 0 && C >= 3
		L:N = 3
		N += 1
	ENDIF
	;4個目
	IF (FLAG:A & 32) == 0 && C >= 4
		L:N = 4
		N += 1
	ENDIF
	;5個目
	IF (FLAG:A & 64) == 0 && C >= 5
		L:N = 5
		N += 1
	ENDIF
	;名前
	IF (FLAG:A & 128) == 0
		L:N = 6
		N += 1
	ENDIF
	;一応Nが0のときの対策を立てとく
	SIF N == 0
		N = 1
	G = RAND:N
	IF L:G == 1
		FLAG:A |= 4
	ELSEIF L:G == 2
		FLAG:A |= 8
	ELSEIF L:G == 3
		FLAG:A |= 16
	ELSEIF L:G == 4
		FLAG:A |= 32
	ELSEIF L:G == 5
		FLAG:A |= 64
	ELSEIF L:G == 6
		FLAG:A |= 128
	ENDIF
	CALL RECIPE_BOOK_LIST_NAME
	PRINT 以上のレシピで
	;調合知識か禁断の知識があるか、名前をレシピで拾ってるか
	IF (FLAG:A & 128) || TALENT:PLAYER:調合知識 == 1 || TALENT:TARGET:調合知識 == 1 || TALENT:PLAYER:禁断の知識 == 1 || TALENT:TARGET:禁断の知識 == 1
		PRINTFORM [{A - 2000}][%STR:A%]が
	ELSE
		PRINTFORM 何かが
	ENDIF
	PRINTW できるようだ
	;この追加で全部うまった場合、レシピ判明済みにする
	SIF N == 1
		FLAG:A = 2
ENDIF


