$INPUT_LOOPB
INPUT

;FLAG:2001～のため変数A使用
A = RESULT + 2000

IF RESULT == 0
	;前のページ　0ページ以下にはならないようにする
	FLAG:2000 -= 1
	SIF FLAG:2000 <= 0
		FLAG:2000 = 1
	GOTO RECIPE_BOOK_SHOW
ELSEIF RESULT == 300
	;後のページ 最終ページ以上にはならないようにする
	FLAG:2000 += 1
	SIF FLAG:2000 >= LOCAL + 1
		FLAG:2000 = LOCAL
	GOTO RECIPE_BOOK_SHOW
ELSEIF RESULT == 1000 || RESULT == -1
	;@COM310に戻り素材表示
ELSEIF RESULT >= 1001 && RESULT <= 1000 + LOCAL
	;各ページへのショートカット
	FLAG:2000 = RESULT - 1000
	GOTO RECIPE_BOOK_SHOW
ELSEIF RESULT >= 8000
	;配列変数FLAGの第１引数 配列の範囲外防止
	GOTO INPUT_LOOPB
ELSEIF (RESULT == 1 || RESULT == 666) && FLAG:A
	PRINTFORMW 所持している素材を全種類追加します。
	G = 0
	REPEAT 100
		G = COUNT + 900
		IF TFLAG:G == 0 && ITEM:G > 0
			TFLAG:G = 1
			ITEM:G -= 1
		ENDIF
	REND
	F = 1
ELSEIF (RESULT >= 2 && RESULT <= LOCAL:1) && FLAG:A == 1 || FLAG:A == 2
	;レシピから素材を追加した場合F = 1にし、@COM310に戻った後$INPUT_MADAに移動するように指定
	F = 1
	
	;既に素材を追加していた場合、その素材はTFLAG:9○○ = 2として保存し、素材の所持数も戻す
	;レシピの素材を追加した後に2ならTFLAG:9○○ = 1に戻し、素材の所持数も減らす（レシピの素材として選択されていた場合1になっている）
	G = 0
	REPEAT 100
		G = COUNT + 900
		IF TFLAG:G == 1
			TFLAG:G = 2
			ITEM:G += 1
		ENDIF
	REND
	
	;2185（全種類追加）だけ別処理にし、後は@TYOGO_USE_SOZAIを利用して素材追加
	IF RESULT == 185
		;GはCOUNT + 900用、LOCAL:2は現在所持している素材の種類数
		;LOCAL:3は登録されている素材数。ここでは全種類持っているか判定する。
		G = 0
		LOCAL:2 = 0
		LOCAL:3 = 0
		REPEAT 100
			G = COUNT + 900
			;現在所持している素材の種類数をカウント
			IF TFLAG:G || ITEM:G
				LOCAL:2 += 1
			ENDIF
			;素材が設定されていないITEMを所持している場合は所持数0に変更。設定されていたらカウント
			STRLENS ITEMNAME:G
			IF RESULT
				LOCAL:3 += 1
			ENDIF
		REND
		;現在所持している素材の種類数が素材全種類と同じ数なら素材追加
		IF LOCAL:2 == LOCAL:3
			G = 0
			REPEAT 100
				G = COUNT + 900
				IF TFLAG:G == 0 && ITEM:G > 0
					TFLAG:G = 1
					ITEM:G -= 1
				ENDIF
			REND
		ELSE
			;全種類なかったら素材追加しない(F = 0にし、後でGOTO RECIPE_BOOK_SHOW)
			F = 0
		ENDIF
	ELSE
		;上のA = RESULT + 2000で指定したレシピに必要な素材を取得
		CALL TYOGO_USE_SOZAI
		;素材が足りているかチェック。Gは所持している素材数、Hは必要素材数のカウントに使用
		G = 0
		H = 0
		REPEAT 100
			;COUNT == 0は飛ばす（A:0はレシピの番号）
			SIF COUNT == 0
				CONTINUE
			;A:COUNTが0だったらそれ以上必要な素材はないので終了する
			SIF A:COUNT == 0
				BREAK
			
			SIF ITEM:(A:COUNT) > 0
				G += 1
			H += 1
		REND
		
		;素材が足りている場合TFLAGとITEM変更。
		IF G == H
			REPEAT 100
				;COUNT == 0は飛ばす（A:0はレシピの番号）
				SIF COUNT == 0
					CONTINUE
				;A:COUNTが0だったらそれ以上必要な素材はないので終了する
				SIF A:COUNT == 0
					BREAK
				
				IF ITEM:(A:COUNT) > 0
					TFLAG:(A:COUNT) = 1
					ITEM:(A:COUNT) -= 1
				ENDIF
			REND
		ELSE
			;素材が足りない場合F = 0にし、後でGOTO RECIPE_BOOK_SHOW
			F = 0
		ENDIF
	ENDIF
	;レシピの素材を追加した後に2ならTFLAG:9○○ = 1に戻し、素材の所持数も減らす（レシピの素材として選択されていた場合1になっている）
	G = 0
	REPEAT 100
		G = COUNT + 900
		IF TFLAG:G == 2
			TFLAG:G = 1
			ITEM:G -= 1
		ENDIF
	REND
	
	;素材を追加した(F = 1)場合@COM310に戻った後$INPUT_MADAに移動。追加しなかった場合(F = 0)はGOTO RECIPE_BOOK_SHOW
	IF F
		PRINTFORMW [%STR:A%]の素材を追加します。
	ELSE
		PRINTFORMW 素材が足りませんでした。
		GOTO RECIPE_BOOK_SHOW
	ENDIF
ELSEIF (RESULT >= 1 && RESULT <= LOCAL:1) || RESULT == 666
	PRINTFORMW そのレシピの素材はまだ判明していません。
	GOTO RECIPE_BOOK_SHOW
ELSE
	GOTO INPUT_LOOPB
ENDIF

