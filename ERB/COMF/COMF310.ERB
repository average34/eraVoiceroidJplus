	PRINTFORML [0]砥石に使ってみよう
	PRINTFORML [1]アクセサリーにしよう
	PRINTFORML [2]武器に利用してみようか
	$INPUT_LOOPA
	INPUT
	SIF !INRANGE(RESULT, 0, 2)
		GOTO INPUT_LOOPA
	TFLAG:91 = RESULT
	PRINTFORML 
	CALL TRAIN_MESSAGE_B
	PRINTFORML 
	SELECTCASE TFLAG:91
	;砥石
	CASE 0
		U += 15
		PRINTFORMW 家事経験+{U}
		EXP:家事経験 += U
	;アクセサリー
	CASE 1
		CLEARLINE 1
		;調合のソースとの衝突を避けるため変数に変換（SOURCE:歓楽をCに、SOURCE:受動をDに、SOURCE:征服をEに）
		;固定で獲得するソース
		C = 200
		;ABL:従順をみる
		IF ABL:親密 <= 1
			C += (ABL:親密 * 60)
		ELSEIF ABL:親密 <= 3
			C += 200 + (ABL:親密 * 60)
		ELSEIF ABL:親密 <= 5
			C += 500 + (ABL:親密 * 60)
		ELSEIF ABL:親密 <= 8
			C += 750 + (ABL:親密 * 75)
		ELSEIF ABL:親密 <= 11
			C += 1000 + (ABL:親密 * 75)
		ELSE
			C += 2000 + (ABL:親密 * 30)
		ENDIF

		;好感度をみる
		IF CFLAG:2 <= 500
			C += CFLAG:2
		ELSEIF CFLAG:2 <= 5000
			C += 500 + (CFLAG:2 - 500) / 3
		ELSE
			C += 2000 + (CFLAG:2 - 5000) / 5
		ENDIF
		SIF C < 0
			C = 0

		;主導権をチェック
		IF TFLAG:45 == 0
			;主導権が主人公側
			;ABL:従順をみる
			IF ABL:親密 == 0
				D = 200
			ELSEIF ABL:親密 == 1
				D = 300
			ELSEIF ABL:親密 == 2
				D = 400
			ELSEIF ABL:親密 == 3
				D = 500
			ELSEIF ABL:親密 == 4
				D = 600
			ELSE
				D = 500 + (ABL:親密 * 50)
			ENDIF
		ELSE
			;主導権が相手側
			;ABL:サドっ気をみる
			IF ABL:サドっ気 == 0
				E = 300
			ELSEIF ABL:サドっ気 == 1
				E = 400
			ELSEIF ABL:サドっ気 == 2
				E = 500
			ELSEIF ABL:サドっ気 == 3
				E = 600
			ELSEIF ABL:サドっ気 == 4
				E = 700
			ELSE
				E = 300 + (ABL:サドっ気 * 100)
			ENDIF
		ENDIF
		;このプレゼントは好感度が一定値以下でも効果減少しない
		;好感度が一定値以上なら効果増加
		IF CFLAG:2 > 1000
			C = C * 7
			E = E * 7
			D = D * 7
			A = RAND:8 + 6
			TFLAG:49 = 2
		ELSE
			C = C * 5
			E = E * 5
			D = D * 5
			A = RAND:6 + 4
		ENDIF
		;戻してソースを加算する
		C += S
		D += S
		E += S
		A += U
		;アダマンタイトをアクセサリーにして喜ぶようなキャラってソフトウェアトーク界隈に居たっけ……？
		;IF NO:TARGET == 40
		IF 0
			TIMES C , 1.20
			TIMES E , 1.20
			TIMES D , 1.20
			A += RAND:8 + 6
		ENDIF
		SOURCE:歓楽 += C
		SOURCE:征服 += E
		SOURCE:受動 += D

		;好感度変化
		TFLAG:30 += A
	;武器
	CASE 2
		;技師ボーナス
		CALL TYOGO_BONUS

		U += 30
		PRINTFORMW 戦闘経験+{U}
		EXP:戦闘経験 += U
	ENDSELECT
;売却
CASE -1
	M += 10000
	CALL COM310_SELL
ENDSELECT

