;---------------------------------------------------------
;[触手ブラ]
;---------------------------------------------------------
@COM310_247
SELECTCASE SIGN(TFLAG:17)
;使用
CASE 1
	;LOCAL:0 FORの10回行動に使用,（1 C絶頂強度, 2 V絶頂強度, 3 A絶頂強度,） 4 B絶頂強度,（5 M絶頂,） 6 射精判定, 7 噴乳判定
	;LOCAL:10 絶頂したかどうか, 11:恐怖の貯まり具合
	
	TFLAG:91 = 1
	VARSET LOCAL, 0
	
	TFLAG:94 = 0
	CALL TRAIN_MESSAGE_B
	
	LOCAL:11 = PALAM:恐怖
	FOR LOCAL, 0, 10
		SOURCE:快Ｃ = 0
		IF LOCAL > 0
			REPEAT 10
				A:COUNT = LOCAL:COUNT
			REND
			TFLAG:94 = 1
			CALL TRAIN_MESSAGE_B
		ENDIF
		
		PRINTL 異常経験+1
		EXP:異常経験 += 1
		PRINTL 触手経験+1
		EXP:触手経験 += 1
		
		;ABL:Ｂ感覚をみる
		IF ABL:Ｂ感覚 == 0
			SOURCE:快Ｂ = 50
		ELSEIF ABL:Ｂ感覚 == 1
			SOURCE:快Ｂ = 100
		ELSEIF ABL:Ｂ感覚 == 2
			SOURCE:快Ｂ = 250
		ELSEIF ABL:Ｂ感覚 == 3
			SOURCE:快Ｂ = 600
		ELSEIF ABL:Ｂ感覚 == 4
			SOURCE:快Ｂ = 1000
		ELSE
			SOURCE:快Ｂ = 400 + (ABL:Ｂ感覚 * 200)
		ENDIF
		;5回目から絶頂してない場合触手がちょっとやる気出す
		IF LOCAL:0 > 4 && LOCAL:10 == 0
			TFLAG:94 = 2
			CALL TRAIN_MESSAGE_B
			SOURCE:快Ｂ += 150
			SOURCE:快Ｂ += RAND:100
		ENDIF
		;付加価値を見る
		IF (S + U) < 1
			S = 1
			U = 1
		ENDIF
		SOURCE:快Ｂ += S + U
		SOURCE:快Ｂ *= (S + U + 100) / 100
		;巨乳だと快感が増えます（触れる面積が増えるため）
		IF TALENT:巨乳
			TIMES SOURCE:快Ｂ, 1.50
		;貧乳だと減ります（触れる面積が…）
		ELSEIF TALENT:貧乳
			TIMES SOURCE:快Ｂ, 0.70
		ENDIF
		;B絶頂、噴乳で倍増
		SOURCE:快Ｂ *= (LOCAL:4 + LOCAL:6 + 1)
		
		;触手経験による恐怖パラ
		UP:恐怖 += 60
		IF EXP:触手経験 < EXPLV:1
			TIMES UP:恐怖 , 1.80
		ELSEIF EXP:触手経験 < EXPLV:2
			TIMES UP:恐怖 , 1.75
		ELSEIF EXP:触手経験 < EXPLV:3
			TIMES UP:恐怖 , 1.60
		ELSEIF EXP:触手経験 < EXPLV:4
			TIMES UP:恐怖 , 0.90
		ELSEIF EXP:触手経験 < EXPLV:5
			TIMES UP:恐怖 , 0.80
		ELSE
			TIMES UP:恐怖 , 0.70
		ENDIF
		UP:恐怖 *= 10

		;B快
		CALL CALC_SOURCE17
		A = UP:快Ｂ
		;液体追加
		IF A > 100 && TALENT:オトコ == 0
			A /= 5
			;濡れやすいと濡れにくいはここで処理
			SIF TALENT:濡れやすい
				TIMES A , 3.00
			SIF TALENT:濡れにくい
				TIMES A , 0.40
			SOURCE:液体追加 += A
		ENDIF
		CALL CALC_SOURCE10
		;絶頂処理
		CALL ORGASM_PROCESS
		REPEAT 5
			LOCAL:(COUNT + 1) = RESULT:COUNT
		REND
		;射精強度を記録
		CALL TARGET_EJAC_CHECK
		LOCAL:6 = RESULT
		;噴乳強度を記録
		CALL TARGET_MILK_CHECK
		LOCAL:7 = RESULT
		
		REPEAT 10
			A:COUNT = LOCAL:COUNT
		REND
		TFLAG:94 = 3
		CALL TRAIN_MESSAGE_B
		
		UPCHECK
		VARSET SOURCE, 0
		;絶頂回数記録
		LOCAL:10 += LOCAL:4
		LOCAL:10 += LOCAL:7
		PRINTFORML 
	NEXT
	
	VARSET SOURCE, 0
	
	A:10 = LOCAL:10
	A:11 = LOCAL:11
	TFLAG:94 = 4
	CALL TRAIN_MESSAGE_B
	
	LOSEBASE:体力 += 100
	LOSEBASE:気力 += 150
	
	TFLAG:91 = 2
;売却
CASE -1
	M += 12000
	CALL COM310_SELL
ENDSELECT

