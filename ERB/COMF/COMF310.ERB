	;@TYOGO_USE_SOZAIの関係で不正確なレシピが表示されますが、不完全なレシピということで
	CALL TYOGO_USE_SOZAI
	REPEAT 30
		;COUNT == 0は飛ばす（A:0はレシピの番号）
		SIF COUNT == 0
			CONTINUE
		;A:COUNTが0だったらそれ以上必要な素材はないので終了する
		SIF A:COUNT == 0
			BREAK
		
		;2個目の素材からは素材名の前に＋を表示
		SIF COUNT >= 2
			PRINTFORM ＋
		
		;eramakerでITEM:(A:COUNT)がアウトなのでITEM:Bに変更
		B = A:COUNT
		
		;1(+5の倍数)個目
		IF COUNT % 5 == 1 && (FLAG:A & 4)
			PRINTFORM %ITEMNAME:B%
		;2(+5の倍数)個目
		ELSEIF COUNT % 5 == 2 && (FLAG:A & 8)
			PRINTFORM %ITEMNAME:B%
		;3(+5の倍数)個目
		ELSEIF COUNT % 5 == 3 && (FLAG:A & 16)
			PRINTFORM %ITEMNAME:B%
		;4(+5の倍数)個目
		ELSEIF COUNT % 5 == 4 && (FLAG:A & 32)
			PRINTFORM %ITEMNAME:B%
		;5(+5の倍数)個目
		ELSEIF COUNT % 5 == 0 && (FLAG:A & 64)
			PRINTFORM %ITEMNAME:B%
		ELSE
			STRLENS ITEMNAME:B
			VARSET LOCALS, ""
			LOCALS += "？"*(RESULT/2)
			PRINTFORM %LOCALS%
		ENDIF
		
		;素材が多いレシピは改行処理
		IF (A == 2002 || A == 2181) && (COUNT % 5 == 0)
			PRINTL 
			PRINT 　　
		ENDIF
	REND
	PRINTL 
ENDIF

