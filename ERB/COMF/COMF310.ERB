		C += 500 + (ABL:親密 * 80)
	ELSEIF ABL:親密 <= 8
		C += 750 + (ABL:親密 * 90)
	ELSEIF ABL:親密 <= 11
		C += 1000 + (ABL:親密 * 90)
	ELSE
		C += 2000 + (ABL:親密 * 40)
	ENDIF

	;好感度をみる
	IF CFLAG:2 <= 500
		C += CFLAG:2
	ELSEIF CFLAG:2 <= 5000
		C += 500 + (CFLAG:2 - 500) / 3
	ELSE
		C += 2000 + (CFLAG:2 - 5000) / 5
	ENDIF
	SIF C < 0
		C = 0

	;主導権をチェック
	IF TFLAG:45 == 0
		;主導権が主人公側
		;ABL:従順をみる
		IF ABL:親密 == 0
			E = 250
		ELSEIF ABL:親密 == 1
			E = 350
		ELSEIF ABL:親密 == 2
			E = 500
		ELSEIF ABL:親密 == 3
			E = 600
		ELSEIF ABL:親密 == 4
			E = 800
		ELSE
			E = 700 + (ABL:親密 * 50)
		ENDIF
	ELSE
		;主導権が相手側
		;ABL:サドっ気をみる
		IF ABL:サドっ気 == 0
			D = 350
		ELSEIF ABL:サドっ気 == 1
			D = 450
		ELSEIF ABL:サドっ気 == 2
			D = 600
		ELSEIF ABL:サドっ気 == 3
			D = 700
		ELSEIF ABL:サドっ気 == 4
			D = 800
		ELSE
			D = 400 + (ABL:サドっ気 * 100)
		ENDIF
	ENDIF

	;アクセサリ　割と仲良くなっておかないと変に思われる。
	;好感度が一定値以下なら効果減少と共に懐疑のソースが入る
	IF CFLAG:2 < 400
		TIMES C , 0.70
		TIMES D , 9.00
		TIMES E , 0.70
		SOURCE:懐疑 = 1800
		A -= RAND:10 + 5
		TFLAG:49 = 1
	;好感度が一定値以上なら効果増加
	ELSEIF CFLAG:2 > 1000
		TIMES C , 7.00
		TIMES D , 7.00
		TIMES E , 7.00
		A = RAND:8 + 6
		TFLAG:49 = 2
	ELSE
		TIMES C , 5.00
		TIMES D , 5.00
		TIMES E , 5.00
		A = RAND:6 + 4
	ENDIF
	A += U
	;悪要素～付加価値の補正
	IF TFLAG:976
		TIMES C , 0.20
		TIMES D , 0.50
		TIMES E , 0.50
		TFLAG:30 -= 5
		TFLAG:37 -= 5
	ELSEIF TFLAG:977
		TIMES C , 2.00
		TIMES D , 2.00
		TIMES E , 2.00
		SIF TFLAG:30 <= 0
			TFLAG:30 = 1
		TFLAG:30 += 2
		TFLAG:37 += 10 + A
	ELSE
		TIMES C , 1.00
		TIMES D , 1.00
		TIMES E , 1.00
		TFLAG:37 += A
	ENDIF
	C += S
	D += S
	E += S
	SOURCE:歓楽 += C
	SOURCE:征服 += D
	SOURCE:受動 += E
	;好感度変化
	TFLAG:30 += A
;売却
CASE -1
	M += 20000
	CALL COM310_SELL
ENDSELECT

