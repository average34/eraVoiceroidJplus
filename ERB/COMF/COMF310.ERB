;---------------------------------------------------------
;[リボルバーローター]
;---------------------------------------------------------
@COM310_244
#DIM LCOUNT, 2
SELECTCASE SIGN(TFLAG:17)
;使用
CASE 1
	;LOCAL:1～4 CVAB絶頂判定に使用, 5 射精判定、6 噴乳判定
	
	VARSET LOCAL
	FOR LCOUNT:0, 1, 3
		A = LCOUNT:0
		TFLAG:94 = 0
		CALL TRAIN_MESSAGE_B
		
		FOR LCOUNT:1, 0, 6
			SOURCE:快Ｃ = 0
			IF LCOUNT:1 > 0
				A = RAND:6
				B = RAND:9
				TFLAG:94 = 1
				CALL TRAIN_MESSAGE_B
				;時々反応を返します
				IF A < 2
					;強気だと若干出にくい
					IF (TFLAG:45 == 0 && B < 4) || (TFLAG:45 == 1 && B < 2)
						SOURCE:快Ｃ += 300
					;弱気だと若干出にくい
					ELSEIF B < 6
						SOURCE:快Ｃ += 100
					ELSE
						SOURCE:快Ｃ += 200
					ENDIF
				ENDIF
			ENDIF
			
			;ABL:Ｃ感覚をみる
			IF ABL:Ｃ感覚 == 0
				SOURCE:快Ｃ += 200
			ELSEIF ABL:Ｃ感覚 == 1
				SOURCE:快Ｃ += 400
			ELSEIF ABL:Ｃ感覚 == 2
				SOURCE:快Ｃ += 900
			ELSEIF ABL:Ｃ感覚 == 3
				SOURCE:快Ｃ += 1600
			ELSEIF ABL:Ｃ感覚 == 4
				SOURCE:快Ｃ += 2400
			ELSE
				SOURCE:快Ｃ += 1500 + (ABL:Ｃ感覚 * 300)
			ENDIF
			;付加価値を見る
			SOURCE:快Ｃ *= (S + U + 100) / 100
			;C絶頂済で倍
			SIF LOCAL:2 > 0
				SOURCE:快Ｃ *= (LOCAL:2 + 1)
			;噴乳で2割増
			SIF LOCAL:4 > 0
				TIMES SOURCE:快Ｃ, 1.20
			;快楽
			CALL CALC_SOURCE0
			A = UP:快Ｃ
			;液体追加
			IF A > 100 && TALENT:オトコ == 0
				A /= 5
				;濡れやすいと濡れにくいはここで処理
				SIF TALENT:濡れやすい
					TIMES A , 3.00
				SIF TALENT:濡れにくい
					TIMES A , 0.40
				SOURCE:液体追加 += A
			ENDIF
			CALL CALC_SOURCE10
			
			;絶頂処理
			CALL ORGASM_PROCESS
			REPEAT 4
				LOCAL:(COUNT + 1) = RESULT:COUNT
			REND
			;射精強度を記録
			CALL TARGET_EJAC_CHECK
			LOCAL:5 = RESULT
			;噴乳強度を記録
			CALL TARGET_MILK_CHECK
			LOCAL:6 = RESULT

			CALL CALC_SOURCE12
			CALL CALC_SOURCE13
			UPCHECK
			
			REPEAT 7
				A:COUNT = LOCAL:COUNT
			REND
			TFLAG:94 = 2
			CALL TRAIN_MESSAGE_B
			SIF LCOUNT:1 < 5
				PRINTFORMW ----残弾{5 - LCOUNT:1}----
		NEXT
		
		VARSET SOURCE, 0
		LOSEBASE:体力 += 10
		LOSEBASE:気力 += 80
		
		IF LCOUNT:0 == 1 && ITEM:955 > 6
			PRINTFORML 
			PRINTFORML 銃弾を6個消費してリロードできます(現在{ITEM:955}個)
			PRINTFORML リロードしますか？
			PRINTL [0]はい
			PRINTL [1]いいえ
			$INPUT_REVOLVE
			INPUT
			SELECTCASE RESULT
			CASE 0
				ITEM:955 -= 6
				PRINTFORMW ----残弾{6}----
				CONTINUE
			CASE 1
				BREAK
			CASEELSE
				GOTO INPUT_REVOLVE
			ENDSELECT
		ENDIF
	NEXT
	;合意がないと好感度が下がる
	IF TALENT:合意 == 0
		SOURCE:反感追加 += 400 * LCOUNT:0
		PRINTL 
		PRINTFORML 好感度-{20*LCOUNT:0}
		CFLAG:2 -= 20 * LCOUNT:0
	ENDIF
;売却
CASE -1
	M += 18000
	CALL COM310_SELL
ENDSELECT

