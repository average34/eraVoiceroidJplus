;---------------------------------------------------------
;調合
;このファイルの中に入っている関数一覧
;@COM_ABLE310
;@COM310　使用している変数 B,D,E,F,G,P
;@TRAIN_MESSAGE_COM310
;@COM310_X
;@TRAIN_MESSAGE_COM310_X
;@SOZAI　使用している変数 A,B 　CALL SOZAIの前にE = 1が有るとアイテム数表示。無いと選択番号表示。
;@RECIPE　使用している変数 A,B,C,D,E,F,K,M,S,U
;	;CPMで使用する場合、;CALL MONEY_FORM,MをCALL MONEY_FORM,Mに置換、${M}を%RESULTS%に置換でCPMの金額表示と同期
;@RECIPE_BOOK 使用している変数 A,F,G
;@RECIPE_BOOK_LIST 使用している変数 A
;@TSUIKA_KOKA 使用している変数 K,M,S,U,H,I,J
;@TSUIKA_KOKA2
;@TSUIKA_KOKA_NO_1 使用している変数 H
;@TSUIKA_KOKA_NO_2 使用している変数 H
;@TYOGO_EXP 使用している変数 P
;@RECIPE_NAME
;@RECIPE_BOOK_LIST_NAME
;@RECIPE_FRAGMENT 使用している変数 A,B,C,D,G,L:1~,N
;@TYOGO_CHECK 使用している変数 C,D
;@TYOGO_USE_SOZAI 使用している変数 A,A:1～
;@TYOGO_BONUS 使用している変数 C,S,U
;	キャラ毎のボーナスまとめ
;---------------------------------------------------------
@COM_ABLE310
;調合実行判定
;バグのもとなので実行不可
RETURN 0

;COM_ABLE共通判定
CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中は無理
SIF TFLAG:44 == 1
	RETURN 0
;デート中も無理
SIF TEQUIP:2
	RETURN 0
;素材が１つもないと無理
REPEAT 100
	SIF ITEM:(900 + COUNT)
		LOCAL = 1
REND
SIF LOCAL == 0
	RETURN 0
RETURN 1

@COM310
;調合
PRINTL 調合
STR:0 = 調合

;素材所持数の補正、カウント
;LOCALで現在所持している素材の種類数
;LOCAL:1で現在登録されている素材の種類数をカウントし、LOCALとLOCAL:1が同じ場合全種類所持になる
G = 0
VARSET LOCAL, 0
REPEAT 100
	G = COUNT + 900
	;所持数99以上の素材は所持数99に変更。
	SIF ITEM:G >= 99
		ITEM:G = 99
	;現在所持している素材の種類数をカウント
	SIF ITEM:G
		LOCAL += 1
	;素材が設定されていないITEMを所持している場合は所持数0に変更。設定されていたらカウント
	STRLENS ITEMNAME:G
	IF RESULT == 0
		ITEM:G = 0
	ELSE
		LOCAL:1 += 1
	ENDIF
	;TFLAG初期化
	TFLAG:G = 0
REND

;混沌の渦は特殊なのではじめからレシピ判明
SIF FLAG:2185 != 1
	FLAG:2185 = 2

;奇跡の薬は特殊なのではじめからレシピ判明
SIF FLAG:2666 != 1
	FLAG:2666 = 2

;TFLAG初期化
TFLAG:800 = 0

;参考書があるか判断
LOCAL:3 = 0
REPEAT 10
	;レシピの断片はCONTINUE
	SIF COUNT == 0
		CONTINUE
	SIF ITEM:(COUNT + 800)
		LOCAL:3 = 1
REND

$INPUT_MADA
PRINTFORML 
PRINTFORML 何を加えますか？
PRINTFORML 既に加えている素材を選ぶとその素材をキャンセルできます
CALL RECIPE_CHECK
PRINTFORML (調合済みレシピ数：{RESULT:1}/{RESULT:2})
RESULT:1 = 0
RESULT:2 = 0
PRINTFORML 
;全種類所持していたら表示有
IF LOCAL == LOCAL:1
	PRINTFORML 現在所有している素材（★全種類所持中）
ELSE
	PRINTFORML 現在所有している素材
ENDIF
CALL SOZAI_LIST
DRAWLINE
PRINTFORM 調合素材：
B = 0
REPEAT 100
	G = COUNT + 900
	IF TFLAG:G
		PRINTFORM [%ITEMNAME:G%]
		B += 1
	ENDIF
REND
SIF B == 0
	PRINTFORML なし

PRINTFORML 
PRINTFORML 

SIF B >= 2
	PRINTFORML [101]調合を開始する
PRINTFORML [102]調合をやめる
PRINTFORML [103]レシピ図鑑を見る
SIF ITEM:800
	PRINTFORML [104]レシピの断片を読む（あと{ITEM:800}枚）
SIF LOCAL:3
	PRINTFORML [105] 参考書を読む
PRINTFORML [106]まとめて素材を加える
INPUT
IF RESULT >= 0 && RESULT < 100
	;TFLAGとITEMに変数G使用
	G = RESULT + 900
	IF TFLAG:G == 0 && ITEM:G > 0
		PRINTFORMW %ITEMNAME:G%を加えました
		TFLAG:G = 1
		ITEM:G -= 1
		;素材が追加されているかの判定。上のWAITの有無に関係します
		B = 1
	ELSEIF TFLAG:G == 1
		PRINTFORMW %ITEMNAME:G%を加えるのをやめます
		TFLAG:G = 0
		ITEM:G += 1
	ELSE
		PRINTFORMW 値が間違っているか、既に加えている素材です
	ENDIF
	GOTO INPUT_MADA
;調合をやめる
ELSEIF RESULT == 102
	G = 0
	REPEAT 100
		G = COUNT + 900
		IF TFLAG:G >= 1
			ITEM:G += TFLAG:G
			TFLAG:G = 0
		ENDIF
	REND
	PRINTFORMW 調合を行うのを止めました
	RETURN 0
;レシピ図鑑を見る
ELSEIF RESULT == 103
	CALL RECIPE_BOOK
	GOTO INPUT_MADA
;レシピの断片を読む
ELSEIF RESULT == 104 && ITEM:800
	CALL RECIPE_FRAGMENT
	GOTO INPUT_MADA
;参考書を読む　COMF309.ERBに関数有
ELSEIF RESULT == 105 && LOCAL:3
	CALL REFERENCE_BOOK
	GOTO INPUT_MADA
;まとめて素材を加える
ELSEIF RESULT == 106
	PRINTFORML 何を加えますか？
	FOR LOCAL, 0, 6
		IF LOCAL == 0
			PRINTFORMLC [{LOCAL}] 雑草・薬草
		ELSEIF LOCAL == 1
			PRINTFORMLC [{LOCAL}] 低ランク素材
		ELSEIF LOCAL == 2
			PRINTFORMLC [{LOCAL}] 中ランク素材
		ELSEIF LOCAL == 3
			PRINTFORMLC [{LOCAL}] 高ランク素材
		ELSEIF LOCAL == 4
			PRINTFORMLC [{LOCAL}] 最高ランク素材
		ELSEIF LOCAL == 5
			PRINTFORMLC [{LOCAL}] 全種類
		ENDIF
	NEXT
	$INPUT_ALL
	INPUT
	IF RESULT >= 0 && RESULT <= 5
		FOR LOCAL, 900, 1000
			SIF RESULT != 5 && ITEMPRICE:LOCAL != RESULT
				CONTINUE
			IF ITEM:LOCAL > 0 && TFLAG:LOCAL == 0
				TFLAG:LOCAL = 1
				ITEM:LOCAL -= 1
			ENDIF
		NEXT
		PRINTW 素材をまとめて追加しました
		;素材が追加されているかの判定。何を加えますか？表示時のWAITの有無に関係します
		B = 1
	ELSE
		PRINTFORMW 正しい数値を選択してください
		GOTO INPUT_ALL
	ENDIF
	GOTO INPUT_MADA
;調合を開始する
ELSEIF RESULT == 101
	SIF B < 2
		GOTO INPUT_MADA
	;体力回復系の調合結果があるため、気力消費のみここで行う
	LOSEBASE:気力 += 150
	;恋慕・恋人・調合知識があるとそれぞれ軽減
	SIF TALENT:恋慕
		LOSEBASE:気力 -= 20
	SIF TALENT:恋人
		LOSEBASE:気力 -= 30
	SIF TALENT:調合知識
		LOSEBASE:気力 -= 60
	;成否フラグ
	;失敗作ができたときのみTFLAG:18 = -1 に
	TFLAG:18 = 0
	
	TFLAG:17 = 0
	;調合開始時（TFLAG:17 == 0）の地の文
	CALL TRAIN_MESSAGE_B
	
	;調合実行
	CALL RECIPE
	PRINTFORML 調合してできたものをどうしますか？
	PRINTFORML 
	PRINTFORML [0]使用する
	PRINTFORML [1]売る
	$INPUT_LOOP_SELLOREAT
	INPUT
	SELECTCASE RESULT
	CASE 0
		;TFLAG:17 = TFLAG:17
	CASE 1
		TFLAG:17 = -TFLAG:17
	CASEELSE
		GOTO INPUT_LOOP_SELLOREAT
	ENDSELECT
	
	VARSET A
	VARSET B
	VARSET C
	VARSET D
	VARSET E
	VARSET LOCAL
	;調合結果別（TFLAG:17 != 0）の地の文とソース処理
	TRYCCALLFORM COM310_K{NO:TARGET}
	CATCH
		CALLFORM COM310_{ABS(TFLAG:17)}
	ENDCATCH
	
	;固定で獲得するソース
	SOURCE:歓楽 += 10
	SOURCE:家務 += 100
	;成功していると達成感も
	SIF TFLAG:18 > -1
		SOURCE:達成感 += 150
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:歓楽 += (ABL:親密 * 5)
		SOURCE:家務 += (ABL:親密 * 20)
		SOURCE:達成感 += (ABL:親密 * 30)
	ELSEIF ABL:親密 <= 3
		SOURCE:歓楽 += 50 + (ABL:親密 * 5)
		SOURCE:家務 += 100 + (ABL:親密 * 30)
		SOURCE:達成感 += 100 + (ABL:親密 * 40)
	ELSEIF ABL:親密 <= 5
		SOURCE:歓楽 += 100 + (ABL:親密 * 5)
		SOURCE:家務 += 200 + (ABL:親密 * 30)
		SOURCE:達成感 += 150 + (ABL:親密 * 40)
	ELSEIF ABL:親密 <= 8
		SOURCE:歓楽 += 150 + (ABL:親密 * 10)
		SOURCE:家務 += 250 + (ABL:親密 * 40)
		SOURCE:達成感 += 200 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 11
		SOURCE:歓楽 += 200 + (ABL:親密 * 10)
		SOURCE:家務 += 300 + (ABL:親密 * 50)
		SOURCE:達成感 += 300 + (ABL:親密 * 50)
	ELSE
		SOURCE:歓楽 += 300 + (ABL:親密 * 10)
		SOURCE:家務 += 400 + (ABL:親密 * 60)
		SOURCE:達成感 += 500 + (ABL:親密 * 60)
	ENDIF
	;好感度をみる
	IF CFLAG:2 <= 300
		SOURCE:歓楽 += CFLAG:2 / 3
	ELSEIF CFLAG:2 <= 5000
		SOURCE:歓楽 += 400 + (CFLAG:2 - 500) / 3
	ELSE
		SOURCE:歓楽 += 800 + (CFLAG:2 - 5000) / 5
	ENDIF
	SIF SOURCE:歓楽 < 0
		SOURCE:歓楽 = 0

	;ABL:技巧をみる
	SOURCE:家務 = SOURCE:家務 * (ABL:技巧 * 5 + 100) / 100
	SOURCE:達成感 = SOURCE:達成感 * (ABL:技巧 * 5 + 100) / 100
	;ABL:料理技術をみる
	SOURCE:歓楽 += ABL:料理技能 * 10
	SOURCE:家務 = SOURCE:家務 * (ABL:料理技能 * 10 + 100) / 100
	SOURCE:達成感 = SOURCE:達成感 * (ABL:料理技能 * 20 + 100) / 100
	;調合知識を持っていると効果上昇
	IF TALENT:TARGET:調合知識
		TIMES SOURCE:家務 , 2.00
		TIMES SOURCE:達成感 , 2.00
	ENDIF
	;主導権をチェック
	IF TFLAG:45 == 0
	;主導権が主人公側
		;ABL:従順をみる
		IF ABL:親密 <= 1
			SOURCE:受動 += 100 + (ABL:親密 * 20)
		ELSEIF ABL:親密 <= 3
			SOURCE:受動 += 200 + (ABL:親密 * 30)
		ELSEIF ABL:親密 <= 5
			SOURCE:受動 += 300 + (ABL:親密 * 40)
		ELSEIF ABL:親密 <= 8
			SOURCE:受動 += 400 + (ABL:親密 * 50)
		ELSEIF ABL:親密 <= 11
			SOURCE:受動 += 500 + (ABL:親密 * 60)
		ELSE
			SOURCE:受動 += 600 + (ABL:親密 * 70)
		ENDIF
	ELSE
	;主導権が相手
		;ABL:サドっ気をみる
		IF ABL:サドっ気 == 0
			SOURCE:征服 += 200
		ELSEIF ABL:サドっ気 == 1
			SOURCE:征服 += 300
		ELSEIF ABL:サドっ気 == 2
			SOURCE:征服 += 400
		ELSEIF ABL:サドっ気 == 3
			SOURCE:征服 += 550
		ELSEIF ABL:サドっ気 == 4
			SOURCE:征服 += 700
		ELSE
			SOURCE:征服 += 300 + (ABL:サドっ気 * 100)
		ENDIF
	ENDIF

	;好感度ランダム変化
	TFLAG:30 += RAND:5 - 2
	;	B = 1
	IF TFLAG:18 == -1
		TIMES SOURCE:歓楽 , 0.10
		TIMES SOURCE:家務 , 0.50
		TFLAG:30 -= 5
		TFLAG:37 -= 2
	ELSEIF TFLAG:18 == 0
		TIMES SOURCE:歓楽 , 1.00
		TIMES SOURCE:家務 , 1.00
		TIMES SOURCE:達成感 , 1.00
		TFLAG:37 += 1
	ELSE
		TIMES SOURCE:歓楽 , 2.00
		TIMES SOURCE:家務 , 2.00
		TIMES SOURCE:達成感 , 2.00
		TFLAG:30 += 2
		;ELSEに分岐することはないようですが変数Aを使用しているため一応コメントアウトし代わりを設定
		TFLAG:37 += 2
		;TFLAG:37 += 2 + A 
	;	B += 1
	ENDIF
	;黒い霧使用時、獲得ソースを0に
	SIF TFLAG:17 == 226
		VARSET SOURCE
	;調合経験プラス
	;蓬莱の薬か奇跡の薬でプラス1999
	IF GROUPMATCH(ABS(TFLAG:17), 2, 666)
		EXP:TARGET:調合経験 += 1999
		PRINTFORML 調合経験+1999
	;失敗作は一律プラス１
	ELSEIF TFLAG:18 < 0
		EXP:TARGET:調合経験 += 1
		PRINTFORML 調合経験+1
	;その他は使用素材のレア度と付加価値に依存
	ELSE
		CALL TYOGO_EXP
		P += 1
		EXP:TARGET:調合経験 += P
		PRINTFORML 調合経験+{P}
	ENDIF
	;調合知識なし
	IF TALENT:TARGET:調合知識 == 0 
		;習得早い
		IF TALENT:TARGET:習得早い == 1
			A = 1500
		;習得遅い
		ELSEIF TALENT:TARGET:習得遅い == 1
			A = 3000
		;それ以外
		ELSE
			A = 2000
		ENDIF
		;習得早いと調合経験1500以上で、習得遅いと調合経験3000以上で、それ以外は調合経験2000以上で、調合知識入手
		IF EXP:TARGET:調合経験 >= A
			PRINTFORML 
			PRINTFORMW %CALLNAME:TARGET%は調合師として板についてきたようだ…
			PRINTFORMW %CALLNAME:TARGET%は[調合知識]を得ました
			TALENT:TARGET:調合知識 = 1
		ENDIF
	ENDIF
	;TFLAG初期化
	FOR LOCAL, 900, 1000
		TFLAG:LOCAL = 0
	NEXT
ELSE
	PRINTFORMW どれかを選択してください
	GOTO INPUT_MADA
ENDIF

RETURN 1

;---------------------------------------------------------
;素材リスト表示
;---------------------------------------------------------
@SOZAI_LIST
;素材入手フラグ
FOR LOCAL, 0, 100
	SIF ITEM:(900+LOCAL)
		FLAG:(3000+LOCAL) = 1
NEXT

;変数Aにはアイテム番号を代入
A = 900
VARSET LOCAL, 0
REPEAT 128
	;表示位置判定。1～4列目にそれぞれランク1～4が表示されるよう判定
	LOCAL = (COUNT % 4) + 1
	IF COUNT == 0 || COUNT == 1
		;雑草と薬草は例外
		LOCALS = [{A-900,2}]%ITEMNAME:A%({ITEM:A})
	;表示予定素材のランクと表示列の位置が違う場合―を表示
	ELSEIF LOCAL != ITEMPRICE:A
		LOCALS = 　　　　 ― 
	ELSEIF ITEM:A > 0 || TFLAG:A
		LOCALS = [{A-900,2}]%ITEMNAME:A%({ITEM:A})
	ELSE
		;素材を所持していない＆既に選択済みの場合、空欄表示
		LOCALS = 　　　　
	ENDIF
	
	;LOCAL:1は名前の最大文字数
	;1列目
	IF LOCAL == 1
		LOCAL:1 = 22
	;2列目
	ELSEIF LOCAL == 2
		LOCAL:1 = 20
	;3列目
	ELSEIF LOCAL == 3
		LOCAL:1 = 22
	;4列目
	ELSE
		LOCAL:1 = 20
	ENDIF
	;選択済みの素材はグレー表示
	SIF TFLAG:A
		SETCOLOR 105, 105, 105
	PRINTFORM %LOCALS,(LOCAL:1),LEFT%
	RESETCOLOR
	
	;―を表示しなかった場合はA += 1（Aが次に表示する素材の番号）
	SIF LOCAL == ITEMPRICE:A
		A += 1
	;雑草と薬草は例外
	SIF COUNT == 0 || COUNT == 1
		A += 1
	;4列おきに改行
	SIF COUNT % 4 == 3
		PRINTL 
REND

