	ELSE
		TFLAG:47 += 10
	ENDIF
	SOURCE:歓楽 += A
	SOURCE:家務 += B
	SOURCE:征服 += C
	SOURCE:受動 += D
	;以上「宴会に参加」ソース
;売却
CASE -1
	M += 3000
	CALL COM310_SELL
ENDSELECT

