		SOURCE:歓楽 += CFLAG:2
	ELSEIF CFLAG:2 <= 5000
		SOURCE:歓楽 += 600 + (CFLAG:2 - 500) / 3
	ELSE
		SOURCE:歓楽 += 2200 + (CFLAG:2 - 5000) / 5
	ENDIF
	SIF SOURCE:歓楽 < 0
		SOURCE:歓楽 = 0
	;アクセサリ　割と仲良くなっておかないと変に思われる。かなりの効果を発揮
	;好感度が一定値以下なら効果減少と共に懐疑のソースが入る
	IF CFLAG:2 < 400
		TIMES SOURCE:歓楽 , 0.70
		TIMES SOURCE:征服 , 9.00
		TIMES SOURCE:受動 , 0.70
		SOURCE:懐疑 = 1800
		A -= RAND:10 + 5
		TFLAG:49 = 1
	;好感度が一定値以上なら効果増加
	ELSEIF CFLAG:2 > 1000
		TIMES SOURCE:歓楽 , 7.00
		TIMES SOURCE:征服 , 7.00
		TIMES SOURCE:受動 , 7.00
		A = RAND:8 + 6
		TFLAG:49 = 2
	ELSE
		TIMES SOURCE:歓楽 , 5.00
		TIMES SOURCE:征服 , 5.00
		TIMES SOURCE:受動 , 5.00
		A = RAND:6 + 4
	ENDIF
	;あかり効果上昇
	IF NO:TARGET == 11
		PRINTFORML 流星ボーナス＋
		TIMES SOURCE:歓楽 , 1.20
		TIMES SOURCE:征服 , 1.20
		TIMES SOURCE:受動 , 1.20
		U += 5
	ENDIF

	SOURCE:歓楽 += S
	SOURCE:征服 += S
	SOURCE:受動 += S
	;好感度変化
	TFLAG:30 += A
	U += 10
	PRINTFORMW 好感度+{U}
	CFLAG:TARGET:2 += U
;売却
CASE -1
	M += 15000
	CALL COM310_SELL
