;[ヴォジャノーイの舌]
;---------------------------------------------------------
@COM310_246
SELECTCASE SIGN(TFLAG:17)
;使用
CASE 1
	CALL TRAIN_MESSAGE_B
	SOURCE:快Ａ += 175 + (ABL:Ａ感覚 * 100)
	SOURCE:屈従 = 150 + (ABL:Ａ感覚 * 100)
	SOURCE:露出 = 250
	SOURCE:逸脱 = 400
	;EXP:Ａ経験をみる
	IF EXP:Ａ経験 < EXPLV:1
		SOURCE:痛み = 700
	ELSEIF EXP:Ａ経験 < EXPLV:2
		SOURCE:痛み = 600
	ELSEIF EXP:Ａ経験 < EXPLV:3
		SOURCE:痛み = 500
	ELSEIF EXP:Ａ経験 < EXPLV:4
		TIMES SOURCE:快Ａ , 1.20
		TIMES SOURCE:屈従, 1.20
		SOURCE:痛み = 400
	ELSEIF EXP:Ａ経験 < EXPLV:5
		TIMES SOURCE:快Ａ , 1.60
		TIMES SOURCE:屈従, 1.60
		SOURCE:痛み = 200
	ELSE
		TIMES SOURCE:快Ａ , 1.80
		TIMES SOURCE:屈従, 1.80
		SOURCE:痛み = 100
	ENDIF
	;PALAM:欲情をみる
	IF PALAM:欲情 < PALAMLV:3
		TIMES SOURCE:快Ａ , 1.00
		TIMES SOURCE:屈従, 1.00
	ELSEIF PALAM:欲情 < PALAMLV:4
		TIMES SOURCE:快Ａ , 1.30
		TIMES SOURCE:屈従, 1.30
	ELSEIF PALAM:欲情 >= PALAMLV:4
		TIMES SOURCE:快Ａ , 1.60
		TIMES SOURCE:屈従, 1.60
	ENDIF
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:受動 += 150 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:受動 += 300 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:受動 += 400 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:受動 += 500 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:受動 += 600 + (ABL:親密 * 160)
	ELSE
		SOURCE:受動 += 750 + (ABL:親密 * 180)
	ENDIF
	;Ａ敏感、鈍感をみる
	;快Ａ自体のチェックは後でまとめてやる
	IF TALENT:Ａ鈍感
		TIMES SOURCE:痛み , 1.50
		TIMES SOURCE:屈従 , 1.50
		TIMES SOURCE:逸脱 , 1.50
	ELSEIF TALENT:Ａ敏感
		TIMES SOURCE:痛み , 0.60
		TIMES SOURCE:屈従 , 0.60
		TIMES SOURCE:逸脱 , 0.60
	ENDIF
	EXP:Ａ経験 += 1
	PRINTL Ａ経験+1
	EXP:異常経験 += 1
	PRINTFORML %EXPNAME:50%+1
	PRINTFORML 
	;合意がないと好感度が下がる
	IF TALENT:合意 == 0
		SOURCE:反感追加 += 400
		PRINTFORMW 好感度-20
		CFLAG:2 -= 20
	ELSE
		CLEARLINE 1
	ENDIF
	;痛覚ソース変換
	CALL CALC_SOURCE6
	;苦痛を潤滑に変える
	UP:潤滑 += UP:苦痛
	UP:苦痛 = 0
	SOURCE:痛み = 0
;売却
CASE -1
	M += 7000
	CALL COM310_SELL
ENDSELECT

