@COM310_168
SELECTCASE SIGN(TFLAG:17)
;使用
CASE 1
	CALL TRAIN_MESSAGE_B
	REPEAT CHARANUM
		A = 0
		;MASTER除く
		SIF COUNT == 0
			CONTINUE
		;パートナーは除く
		SIF COUNT == TARGET
			CONTINUE
		A = CFLAG:COUNT:2
		SIF TALENT:COUNT:恋人
			A *= 2
		SIF TALENT:COUNT:恋慕
			A *= 2
		SIF TALENT:COUNT:親愛
			A *= 2
		S += A
	REND
	;親密をみる
	IF ABL:親密 <= 3
		S /= 100
	ELSEIF ABL:親密 <= 6
		S /= 10
	ELSEIF ABL:親密 <= 9
		S /= 5
	ENDIF
	;恋人ないと半減
	SIF TALENT:恋人 == 0
		S /= 2
	;恋慕無いと半減
	SIF TALENT:恋慕 == 0
		S /= 2
	;嫉妬あり
	SIF TALENT:嫉妬
		S *= 2

	SIF S < 0
		S = 0
	;欲情の上昇値の半分反感が上がる
	U = S / 2

	SOURCE:欲情追加 += S
	SOURCE:反感追加 += U
;売却
CASE -1
	M += 5000
	CALL COM310_SELL
ENDSELECT

