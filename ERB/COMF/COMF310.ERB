;---------------------------------------------------------
@COM310_100
SELECTCASE SIGN(TFLAG:17)
;使用
CASE 1
	CALL TRAIN_MESSAGE_B
	;固定で獲得するソース
	SOURCE:歓楽 += 500
	SOURCE:家務 += 100
	
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:歓楽 += (ABL:親密 * 100)
		SOURCE:家務 += (ABL:親密 * 30)
	ELSEIF ABL:親密 <= 3
		SOURCE:歓楽 += 300 + (ABL:親密 * 100)
		SOURCE:家務 += 200 + (ABL:親密 * 30)
	ELSEIF ABL:親密 <= 5
		SOURCE:歓楽 += 750 + (ABL:親密 * 100)
		SOURCE:家務 += 400 + (ABL:親密 * 30)
	ELSEIF ABL:親密 <= 8
		SOURCE:歓楽 += 1000 + (ABL:親密 * 120)
		SOURCE:家務 += 600 + (ABL:親密 * 40)
	ELSEIF ABL:親密 <= 11
		SOURCE:歓楽 += 1500 + (ABL:親密 * 120)
		SOURCE:家務 += 800 + (ABL:親密 * 40)
	ELSE
		SOURCE:歓楽 += 3000 + (ABL:親密 * 60)
		SOURCE:家務 += 1800 + (ABL:親密 * 20)
	ENDIF
	
	;好感度をみる
	IF CFLAG:2 <= 500
		SOURCE:歓楽 += CFLAG:2
	ELSEIF CFLAG:2 <= 5000
		SOURCE:歓楽 += 500 + (CFLAG:2 - 500) / 3
	ELSE
		SOURCE:歓楽 += 2000 + (CFLAG:2 - 5000) / 5
	ENDIF
	;夜の時効果倍増
	IF TIME == 1
		TIMES SOURCE:歓楽 , 2.00
		TIMES SOURCE:家務 , 2.00
	ENDIF
	SIF SOURCE:歓楽 < 0
		SOURCE:歓楽 = 0
	SOURCE:歓楽 += S
	
	;恋慕キャラ全員の好感度上昇
	;好感度上昇値の基本設定
	A = 1
	;夜の場合2倍
	SIF TIME == 1
		A *= 2
	REPEAT CHARANUM
		;主人公は除外
		SIF COUNT == 0
			CONTINUE
		;別れ中も除外
		SIF CFLAG:COUNT:21
			CONTINUE
		;恋慕キャラは好感度を上げる
		SIF TALENT:COUNT:恋慕
			CFLAG:COUNT:2 += A
	REND
	
	;ムード上昇
	TFLAG:37 += A
;売却
CASE -1
	M += 10000
	CALL COM310_SELL
ENDSELECT

