	STR:0 = 蜘蛛脚愛撫
	Y = 0
ENDIF

;蜘蛛脚愛撫
;たくさんある脚で全身いたる所を撫でまわします
IF Y == 0 
	LOSEBASE:0 += 50
	LOSEBASE:1 += 120


	SOURCE:4 = 120
	SOURCE:8 = 50
	SOURCE:12 = 950
	SOURCE:14 = 1000

	;ABL:Ｃ感覚をみる
	IF ABL:Ｃ感覚 == 0
