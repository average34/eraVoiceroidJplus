		SOURCE:13 = 300
	ELSEIF ABL:Ａ感覚 == 1
		SOURCE:2 = 75
		SOURCE:13 = 350
	ELSEIF ABL:Ａ感覚 == 2
		SOURCE:2 = 300
		SOURCE:13 = 400
	ELSEIF ABL:Ａ感覚 == 3
		SOURCE:2 = 700
		SOURCE:13 = 650
	ELSEIF ABL:Ａ感覚 == 4
		SOURCE:2 = 1100
		SOURCE:13 = 1000
	ELSE
		SOURCE:2 = 1500
		SOURCE:13 = 1500
	ENDIF

	;ABL:マゾっ気をみる
	IF ABL:マゾっ気 == 0
		SOURCE:3 = 0
		TIMES SOURCE:14, 2.50
		A = 3000
	ELSEIF ABL:マゾっ気 == 1
		SOURCE:3 = 60
		TIMES SOURCE:14, 2.00
		A = 2500
	ELSEIF ABL:マゾっ気 == 2
		SOURCE:3 = 120
		TIMES SOURCE:14, 1.40
		A = 2000
	ELSEIF ABL:マゾっ気 == 3
		SOURCE:3 = 180
		TIMES SOURCE:14, 1.00
		A = 1500
	ELSEIF ABL:マゾっ気 == 4
		SOURCE:3 = 240
		TIMES SOURCE:14, 0.70
		A = 1000
	ELSE
		SOURCE:8 = 300
		TIMES SOURCE:14, 0.30
		A = 500
	ENDIF


	;EXP:Ａ経験をみる
	IF EXP:1 < EXPLV:1
		TIMES SOURCE:2 , 0.20
		TIMES SOURCE:13, 0.20
		SOURCE:6 = 500
	ELSEIF EXP:1 < EXPLV:2
		TIMES SOURCE:2 , 0.50
		TIMES SOURCE:13, 0.50
		SOURCE:6 = 400
	ELSEIF EXP:1 < EXPLV:3
		TIMES SOURCE:2 , 1.00
		TIMES SOURCE:13, 1.00
		SOURCE:6 = 300
	ELSEIF EXP:1 < EXPLV:4
		TIMES SOURCE:2 , 1.20
		TIMES SOURCE:13, 1.20
		SOURCE:6 = 200
	ELSEIF EXP:1 < EXPLV:5
		TIMES SOURCE:2 , 1.60
		TIMES SOURCE:13, 1.60
		SOURCE:6 = 100
	ELSE
		TIMES SOURCE:2 , 1.80
		TIMES SOURCE:13, 1.80
		SOURCE:6 = 50
	ENDIF

	;PALAM:潤滑をみる
	IF PALAM:3 < PALAMLV:1
		TIMES SOURCE:0 , 0.10
		TIMES SOURCE:2 , 0.10
		TIMES SOURCE:14 , 0.10
		TIMES SOURCE:13 , 0.10
		TIMES SOURCE:6 , 3.00
	ELSEIF PALAM:3 < PALAMLV:2
		TIMES SOURCE:0 , 0.20
		TIMES SOURCE:2 , 0.20
		TIMES SOURCE:14 , 0.20
		TIMES SOURCE:13 , 0.20
		TIMES SOURCE:6 , 2.00
	ELSEIF PALAM:3 < PALAMLV:3
		TIMES SOURCE:0 , 0.60
		TIMES SOURCE:2 , 0.60
		TIMES SOURCE:14 , 0.60
		TIMES SOURCE:13 , 0.60
		TIMES SOURCE:6 , 1.00
	ELSEIF PALAM:3 < PALAMLV:4
		TIMES SOURCE:0 , 1.00
		TIMES SOURCE:2 , 1.00
		TIMES SOURCE:14 , 1.00
		TIMES SOURCE:13 , 1.00
		TIMES SOURCE:6 , 0.50
	ELSEIF PALAM:3 >= PALAMLV:4
		TIMES SOURCE:0 , 2.00
		TIMES SOURCE:2 , 2.00
		TIMES SOURCE:14 , 2.00
		TIMES SOURCE:13 , 2.00
		TIMES SOURCE:6 , 0.10
	ENDIF

	;PALAM:欲情をみる
	IF PALAM:5 < PALAMLV:1
		TIMES SOURCE:0 , 0.30
		TIMES SOURCE:2 , 0.30
		TIMES SOURCE:13 , 0.30
		TIMES SOURCE:14 , 0.30
	ELSEIF PALAM:5 < PALAMLV:2
		TIMES SOURCE:0 , 0.60
		TIMES SOURCE:2 , 0.60
		TIMES SOURCE:13 , 0.60
		TIMES SOURCE:14 , 0.60
	ELSEIF PALAM:5 < PALAMLV:3
		TIMES SOURCE:0 , 1.00
		TIMES SOURCE:2 , 1.00
		TIMES SOURCE:13, 1.00
		TIMES SOURCE:14, 1.00
	ELSEIF PALAM:5 < PALAMLV:4
		TIMES SOURCE:0 , 1.30
		TIMES SOURCE:2 , 1.30
		TIMES SOURCE:13 , 1.30
		TIMES SOURCE:14 , 1.30
	ELSEIF PALAM:5 >= PALAMLV:4
		TIMES SOURCE:0 , 1.60
		TIMES SOURCE:2 , 1.60
		TIMES SOURCE:13 , 1.60
		TIMES SOURCE:14 , 1.60
	ENDIF

	;処女で貞操観念
	IF EXP:0 == 0 && TALENT:30
		SOURCE:13 /= 3
	ENDIF

	CALL TRAIN_MESSAGE_B

	;レズ中毒ならソース加算
	IF ABL:PLAYER:レズ中毒 > 0
		TIMES SOURCE:3 , 1.20
		TIMES SOURCE:4 , 1.20
	ENDIF

	W = 21 + RAND:10
	TFLAG:68 -= W
	IF TFLAG:68 <= 0
		TFLAG:68 = 0
		PRINTL 何度ももがくうちに糸は切れてしまった
		PRINTFORML 糸の断面から粘り気のある液体が滲み出て%CALLNAME:TARGET%の全身に付着した
		STAIN:TARGET:0 |= 1
		STAIN:TARGET:1 |= 1
		STAIN:TARGET:2 |= 1
		STAIN:TARGET:3 |= 1
		STAIN:TARGET:4 |= 1
	ENDIF

	UP:10 += A
	EXP:1 += 1
	PRINTL Ａ経験＋１
;蜘蛛脚六点責め
;たくさんある脚をフルに活用させ、全身の穴に突っ込ませます
;処女喪失を伴うので注意
ELSE
	LOSEBASE:0 += 250
	LOSEBASE:1 += 700

	SOURCE:3 = 90
	SOURCE:4 = 60
	SOURCE:6 = 200
	SOURCE:8 = 20
	SOURCE:12 = 700
	SOURCE:13 = 1500
	SOURCE:14 = 400
	SOURCE:16 = 500

	;ABL:Ａ感覚をみる
	IF ABL:Ａ感覚 == 0
		SOURCE:2 = 10
		SOURCE:13 += 100
	ELSEIF ABL:Ａ感覚 == 1
		SOURCE:2 = 30
		SOURCE:13 += 700
	ELSEIF ABL:Ａ感覚 == 2
		SOURCE:2 = 500
		SOURCE:13 += 1500
	ELSEIF ABL:Ａ感覚 == 3
		SOURCE:2 = 1000
		SOURCE:13 += 3000
	ELSEIF ABL:Ａ感覚 == 4
		SOURCE:2 = 1700
		SOURCE:13 += 5000
	ELSE
		SOURCE:2 = 2200
		SOURCE:13 += 8000
	ENDIF

	;ABL:Ｃ感覚をみる
	IF ABL:Ｃ感覚 == 0
		SOURCE:0 = 200
	ELSEIF ABL:Ｃ感覚 == 1
		SOURCE:0 = 400
	ELSEIF ABL:Ｃ感覚 == 2
		SOURCE:0 = 900
	ELSEIF ABL:Ｃ感覚 == 3
		SOURCE:0 = 1600
	ELSEIF ABL:Ｃ感覚 == 4
		SOURCE:0 = 2400
	ELSE
		SOURCE:0 = 3000
	ENDIF

	;ABL:Ｂ感覚をみる
	IF ABL:Ｂ感覚 == 0
		SOURCE:17 = 20
	ELSEIF ABL:Ｂ感覚 == 1
		SOURCE:17 = 100
	ELSEIF ABL:Ｂ感覚 == 2
		SOURCE:17 = 500
	ELSEIF ABL:Ｂ感覚 == 3
		SOURCE:17 = 1200
	ELSEIF ABL:Ｂ感覚 == 4
		SOURCE:17 = 2000
	ELSE
		SOURCE:17 = 2800
	ENDIF

	;ABL:Ｖ感覚をみる
	IF ABL:Ｖ感覚 == 0
		SOURCE:1 = 40
	ELSEIF ABL:Ｖ感覚 == 1
		SOURCE:1 = 150
	ELSEIF ABL:Ｖ感覚 == 2
		SOURCE:1 = 400
	ELSEIF ABL:Ｖ感覚 == 3
		SOURCE:1 = 1000
	ELSEIF ABL:Ｖ感覚 == 4
		SOURCE:1 = 1700
	ELSE
		SOURCE:1 = 2200
	ENDIF


	;EXP:Ａ経験をみる
	IF EXP:1 < EXPLV:1
		TIMES SOURCE:2 , 0.10
		SOURCE:6 += 20000
	ELSEIF EXP:1 < EXPLV:2
		TIMES SOURCE:2 , 0.30
		SOURCE:6 += 12000
	ELSEIF EXP:1 < EXPLV:3
		TIMES SOURCE:2 , 0.50
		SOURCE:6 += 5000
	ELSEIF EXP:1 < EXPLV:4
		TIMES SOURCE:2 , 1.00
		SOURCE:6 += 1800
	ELSEIF EXP:1 < EXPLV:5
		TIMES SOURCE:2 , 1.40
		SOURCE:6 += 1000
	ELSE
		TIMES SOURCE:2 , 1.60
		SOURCE:6 += 600
	ENDIF

	;EXP:Ｖ経験をみる
	IF EXP:0 < EXPLV:1
		TIMES SOURCE:1 , 0.20
		SOURCE:6 = 5000
		IF ASSIPLAY && TALENT:PLAYER:122 == 0
			EXP:50 += 1
			PRINTFORML %EXPNAME:50%＋１
		ENDIF
	ELSEIF EXP:0 < EXPLV:2
		TIMES SOURCE:1 , 0.60
		SOURCE:6 += 220
	ELSEIF EXP:0 < EXPLV:3
		TIMES SOURCE:1 , 1.00
		SOURCE:6 += 30
	ELSEIF EXP:0 < EXPLV:4
		TIMES SOURCE:1 , 1.20
		SOURCE:6 += 5
	ELSEIF EXP:0 < EXPLV:5
		TIMES SOURCE:1 , 1.30
		SOURCE:6 += 0
	ELSE
		TIMES SOURCE:1 , 1.80
		SOURCE:6 += 0
	ENDIF

	;PALAM:潤滑をみる
	IF PALAM:3 < PALAMLV:1
		TIMES SOURCE:1 , 0.10
		TIMES SOURCE:2 , 0.40
		SOURCE:6 += 10000
		TIMES SOURCE:6 , 3.00
	ELSEIF PALAM:3 < PALAMLV:2
		TIMES SOURCE:1 , 0.40
		TIMES SOURCE:2 , 0.80
		SOURCE:6 += 3500
		TIMES SOURCE:6 , 1.00
	ELSEIF PALAM:3 < PALAMLV:3
		TIMES SOURCE:1 , 1.00
		TIMES SOURCE:2 , 1.00
		SOURCE:6 += 1200
		TIMES SOURCE:6 , 0.50
	ELSEIF PALAM:3 < PALAMLV:4
		TIMES SOURCE:1 , 1.40
		TIMES SOURCE:2 , 1.40
		SOURCE:6 += 200
		TIMES SOURCE:6 , 0.20
	ELSEIF PALAM:3 >= PALAMLV:4
		TIMES SOURCE:1 , 1.80
		TIMES SOURCE:2 , 1.80
		SOURCE:6 += 100
		TIMES SOURCE:6 , 0.10
	ENDIF

	;PALAM:欲情をみる
	IF PALAM:5 < PALAMLV:1
		TIMES SOURCE:1 , 0.60
		TIMES SOURCE:3 , 0.30
		TIMES SOURCE:2 , 0.60
		TIMES SOURCE:13 , 0.60
	ELSEIF PALAM:5 < PALAMLV:2
		TIMES SOURCE:1 , 0.80
		TIMES SOURCE:3 , 0.60
		TIMES SOURCE:2 , 0.80
		TIMES SOURCE:13 , 0.80
	ELSEIF PALAM:5 < PALAMLV:3
		TIMES SOURCE:1 , 1.00
		TIMES SOURCE:3 , 1.00
		TIMES SOURCE:2 , 1.00
		TIMES SOURCE:13 , 1.00
	ELSEIF PALAM:5 < PALAMLV:4
		TIMES SOURCE:1 , 1.20
		TIMES SOURCE:3 , 1.50
		TIMES SOURCE:2 , 1.20
		TIMES SOURCE:13 , 1.20
	ELSEIF PALAM:5 >= PALAMLV:4
		TIMES SOURCE:1 , 1.50
		TIMES SOURCE:3 , 1.80
		TIMES SOURCE:2 , 1.40
		TIMES SOURCE:13 , 1.40
	ENDIF

	;ABL:従順をみる
	IF ABL:親密 == 0
		TIMES SOURCE:1 , 0.50
		TIMES SOURCE:3 , 0.60
		TIMES SOURCE:15 , 2.00
	ELSEIF ABL:親密 == 1
		TIMES SOURCE:1 , 0.80
		TIMES SOURCE:3 , 0.80
		TIMES SOURCE:15 , 1.50
	ELSEIF ABL:親密 == 2
		TIMES SOURCE:1 , 1.00
		TIMES SOURCE:3 , 1.00
		TIMES SOURCE:15 , 1.00
	ELSEIF ABL:親密 == 3
		TIMES SOURCE:1 , 1.30
		TIMES SOURCE:3 , 1.20
		TIMES SOURCE:15 , 0.80
	ELSEIF ABL:親密 == 4
		TIMES SOURCE:1 , 1.60
		TIMES SOURCE:3 , 1.40
		TIMES SOURCE:15 , 0.60
	ELSE
		TIMES SOURCE:1 , 2.00
		TIMES SOURCE:3 , 1.60
		TIMES SOURCE:15 , 0.30
	ENDIF

	SOURCE:8 = 500

	;ABL:奉仕精神をみる
	IF ABL:奉仕精神 == 0
		SOURCE:4 = 420
		SOURCE:5 = 150
		TIMES SOURCE:8 , 4.00
	ELSEIF ABL:奉仕精神 == 1
		SOURCE:4 = 500
		SOURCE:5 = 300
		TIMES SOURCE:8 , 2.50
	ELSEIF ABL:奉仕精神 == 2
		SOURCE:4 = 580
		SOURCE:5 = 600
		TIMES SOURCE:8 , 1.50
	ELSEIF ABL:奉仕精神 == 3
		SOURCE:4 = 660
		SOURCE:5 = 900
		TIMES SOURCE:8 , 1.00
	ELSEIF ABL:奉仕精神 == 4
		SOURCE:4 = 740
		SOURCE:5 = 1500
		TIMES SOURCE:8 , 0.50
	ELSE
		SOURCE:4 = 820
		SOURCE:5 = 2200
		TIMES SOURCE:8 , 0.10
	ENDIF

	;小柄体形
	SIF TALENT:100
		TIMES SOURCE:6 , 2.00

	;Ａ敏感、鈍感をみる
	;快Ａ自体のチェックは後でまとめてやる
	IF TALENT:105
		TIMES SOURCE:6 , 1.50
		TIMES SOURCE:13 , 1.50
		TIMES SOURCE:14 , 1.50
	ELSEIF TALENT:106
		TIMES SOURCE:6 , 0.60
		TIMES SOURCE:13 , 0.60
		TIMES SOURCE:14 , 0.60
	ENDIF

	;貞操観念
	IF TALENT:30
		IF EXP:0 == 0
			TIMES SOURCE:3 , 0.60
			SOURCE:15 = 10000
			SOURCE:13 /= 3
		ELSE
			TIMES SOURCE:3 , 0.60
			SOURCE:15 = 1000
		ENDIF
	;貞操無頓着
	ELSEIF TALENT:31
		IF EXP:0 == 0
			TIMES SOURCE:3 , 0.60
			SOURCE:15 = 300
		ENDIF
	ELSE
		IF EXP:0 == 0
			SOURCE:15 = 3000
		ENDIF
	ENDIF

	;V経験を伴うコマンドのフラグ(処女膜再生対応)
	TFLAG:19 = 1
	SIF TALENT:85 && ASSIPLAY == 0 && EXP:0 == 0
		TFLAG:20 = 1

	CALL TRAIN_MESSAGE_B

	W = 46 + RAND:10
	TFLAG:68 -= W
	IF TFLAG:68 <= 0
		TFLAG:68 = 0
		PRINTL 何度ももがくうちに糸は切れてしまった
		PRINTFORML 糸の断面から粘り気のある液体が滲み出て%CALLNAME:TARGET%の全身に付着した
		STAIN:TARGET:0 |= 1
		STAIN:TARGET:1 |= 1
		STAIN:TARGET:2 |= 1
		STAIN:TARGET:3 |= 1
		STAIN:TARGET:4 |= 1
	ENDIF

	EXP:0 += 1
	EXP:1 += 5
	PRINTL Ｖ経験＋１
	PRINTL Ａ経験＋５
ENDIF

IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +2
	EXP:40 += 2
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +2
	EXP:41 += 2
ENDIF

SIF ASSIPLAY == 0 && ABL:マゾっ気 >= 2
	TFLAG:30 += 1

RETURN 1

@TRAIN_MESSAGE_COM159
IF STR:0 == "蜘蛛脚六点責め"
	;蜘蛛脚六点責め
	PRINTFORM %CALLNAME:PLAYER%の無数の脚が%CALLNAME:TARGET%の
	SIF PALAM:3 >= PALAMLV:5
		PRINT たっぷりと
	SIF PALAM:3 >= PALAMLV:3
		PRINT 粘液をたたえた
	PRINTL ありとあらゆる穴の中へと潜り込んだ…

	PRINTFORM %CALLNAME:TARGET%は
	IF TALENT:0 && TALENT:76 && ABL:TARGET:レズ中毒 > 0 && ASSIPLAY == 0
		PRINTFORML 自分が処女であることを気にした様子もなく、%CALLNAME:PLAYER%の脚が膣に入っていく感触に幸福感を感じているようだ…
	ELSEIF TALENT:0 && TALENT:76
		PRINTL 自分が処女であった事も忘れ、前後の穴と口を同時に責められている感触を楽しんでいる…
	ELSEIF TALENT:0
		PRINTL 破瓜の痛みに絶叫をあげていたがすぐさま口も塞がれ、なすすべもなく蹂躙されている……
	ELSEIF TALENT:85 && TALENT:76 && ASSIPLAY
		PRINTL 初めやや困惑したような様子だったが、両の乳首に快感を与えられた瞬間から何もかも忘れたかの様に嬌声をあげ続けている…
	ELSEIF TALENT:85 && ASSIPLAY == 0
		PRINTFORML なにか訴えるかのように口を開けたが%CALLNAME:PLAYER%の脚が容赦なくそこに襲いかかった
	ELSEIF TALENT:76
		PRINTFORML 体全体で%CALLNAME:PLAYER%の脚から与えられる快感を貪るかのように何度も体を震わせた
	ELSE
		PRINTL 不浄の穴に深々と入っていく脚の感触に悲鳴を上げようとしたが絶妙なタイミングで陰核を弾かれ、声をあげることなく口を大きく開いた
	ENDIF
ELSE
	;蜘蛛脚愛撫
	PRINTFORM %CALLNAME:PLAYER%は
	SIF TALENT:PLAYER:83
		PRINT 嗜虐的な笑みを浮かべ、
	PRINTFORML 無数の脚で%CALLNAME:TARGET%の体を撫でまわした
	IF ABL:PLAYER:レズ中毒 > 0
		PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%の頭を持ち上げると貪るかのように口づけを始めた
		PRINTL ピチャピチャと唾液が混ざりあう音が響き、時折聞こえる嬌声や衣ずれの音と共に卑猥な協奏曲を奏でている……
		TIMES SOURCE:3 , 1.20
		TIMES SOURCE:4 , 1.20
	ENDIF
ENDIF

