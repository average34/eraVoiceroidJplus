@COM_ABLE159
;蜘蛛の脚実行判定
;COM_ABLE共通判定
CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中じゃないと無理
SIF TFLAG:44 == 0
	RETURN 0
;調教者が蜘蛛でないとダメ
SIF NAME_CSV(NO:PLAYER, 0) != "アルケニー" && NAME_CSV(NO:PLAYER, 0) != "ヤマメ"
	RETURN 0
;異常経験フィルタ
SIF ((FLAG:61 & 1) == 0) && (EXP:0 < EXPLV:1) && (PREVCOM == 159 && TEQUIP:11 == 0 && TEQUIP:13 == 0 && TEQUIP:14 == 0 && TEQUIP:15 == 0 && TEQUIP:16 == 0 && TEQUIP:19 == 0 && TEQUIP:46 == 0 && TEQUIP:47 == 0 && TEQUIP:48 == 0 && TEQUIP:49 == 0 && TEQUIP:61 == 0 && TEQUIP:70 == 0 && TEQUIP:71 == 0 && TEQUIP:79 == 0)
	RETURN 0
;グルグル巻きにしないとダメ
SIF TEQUIP:73 == 0
	RETURN 0
;触手調教中はダメ
SIF TEQUIP:90
	RETURN 0
;ボールギャグ装着中はダメ
SIF TEQUIP:45
	RETURN 0
;お風呂場プレイ中はダメ
SIF TEQUIP:58
	RETURN 0
;新妻プレイ中はダメ
SIF TEQUIP:59
	RETURN 0
;調教対象が小人体型の場合は主人が禁断の知識持ちじゃないとダメ
SIF TALENT:149 && TALENT:MASTER:125 == 0
	RETURN 0
RETURN 1

@COM159
;蜘蛛の脚
;普段は愛撫だが連続で蜘蛛の脚を使った場合、六点責めに効果が変わる
IF PREVCOM == 159 && TEQUIP:11 == 0 && TEQUIP:13 == 0 && TEQUIP:14 == 0 && TEQUIP:15 == 0 && TEQUIP:16 == 0 && TEQUIP:19 == 0 && TEQUIP:46 == 0 && TEQUIP:47 == 0 && TEQUIP:48 == 0 && TEQUIP:49 == 0 && TEQUIP:61 == 0 && TEQUIP:70 == 0 && TEQUIP:71 == 0 && TEQUIP:79 == 0
