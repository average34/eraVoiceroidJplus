		SOURCE:3 = 100
	ELSEIF ABL:Ｃ感覚 == 2
		SOURCE:0 = 500
		SOURCE:3 = 160
	ELSEIF ABL:Ｃ感覚 == 3
		SOURCE:0 = 1200
		SOURCE:3 = 200
	ELSEIF ABL:Ｃ感覚 == 4
		SOURCE:0 = 2000
		SOURCE:3 = 230
	ELSE
		SOURCE:0 = 2800
		SOURCE:3 = 250
	ENDIF

	;ABL:Ｂ感覚をみる
	IF ABL:Ｂ感覚 == 0
		SOURCE:17 = 25
	ELSEIF ABL:Ｂ感覚 == 1
		SOURCE:17 = 125
	ELSEIF ABL:Ｂ感覚 == 2
		SOURCE:17 = 650
	ELSEIF ABL:Ｂ感覚 == 3
		SOURCE:17 = 1500
	ELSEIF ABL:Ｂ感覚 == 4
		SOURCE:17 = 2300
	ELSE
		SOURCE:17 = 3200
