;-------------------------------------------------
;実行決定
;-------------------------------------------------
LOSEBASE:0 += 5
LOSEBASE:1 += 50

SOURCE:14 = 400
A = 0
B = 0
C = 0
D = 0

IF TEQUIP:53
	SOURCE:10 = 50
	SOURCE:11 = 100
ENDIF

;ABL:Ｃ感覚をみる
IF ABL:3 == 0
	SOURCE:0 = 15
	SOURCE:12 = 2000
	SOURCE:13 = 500
ELSEIF ABL:3 == 1
	SOURCE:0 = 50
	SOURCE:12 = 2300
	SOURCE:13 = 800
ELSEIF ABL:3 == 2
	SOURCE:0 = 300
	SOURCE:12 = 2600
	SOURCE:13 = 1200
ELSEIF ABL:3 == 3
	SOURCE:0 = 700
	SOURCE:12 = 2900
	SOURCE:13 = 1900
ELSEIF ABL:3 == 4
	SOURCE:0 = 1100
	SOURCE:12 = 3200
	SOURCE:13 = 2500
ELSE
	SOURCE:0 = 350 + (ABL:3 * 250)
	SOURCE:12 = 2750 + (ABL:3 * 150)
	SOURCE:13 = 1750 + (ABL:3 * 250)
ENDIF

;ABL:Ｂ感覚をみる
IF ABL:14 == 0
	SOURCE:17 = 15
ELSEIF ABL:14 == 1
	SOURCE:17 = 50
ELSEIF ABL:14 == 2
	SOURCE:17 = 300
ELSEIF ABL:14 == 3
	SOURCE:17 = 700
ELSEIF ABL:14 == 4
	SOURCE:17 = 1100
ELSE
	SOURCE:17 = 350 + (ABL:14 * 250)
ENDIF

;バイブ挿入中を確認
IF TEQUIP:11
	;ABL:Ｖ感覚をみる
	IF ABL:4 == 0
		A += 40
		D += 150
	ELSEIF ABL:4 == 1
		A += 120
		D += 400
	ELSEIF ABL:4 == 2
		A += 300
		D += 700
	ELSEIF ABL:4 == 3
		A += 500
		D += 900
	ELSEIF ABL:4 == 4
		A += 650
		D += 1000
	ELSE
		A += 350 + (ABL:4 * 100)
		D += 700 + (ABL:4 * 100)
	ENDIF

	;EXP:Ｖ経験をみる
	;処女はありえない
	IF EXP:0 < EXPLV:2
		TIMES A , 0.60
		C += 150
	ELSEIF EXP:0 < EXPLV:3
		TIMES A , 1.00
		C += 20
	ELSEIF EXP:0 < EXPLV:4
		TIMES A , 1.20
		C += 0
	ELSEIF EXP:0 < EXPLV:5
		TIMES A , 1.40
		C += 0
	ELSE
		TIMES A , 1.60
		C += 0
	ENDIF
	;Ｖ敏感、鈍感をみる
	IF TALENT:103
		TIMES C , 1.50
		TIMES D , 1.50
	ELSEIF TALENT:104
		TIMES C , 0.60
		TIMES D , 0.60
	ENDIF
	;一度単独で計算
	SOURCE:13 += D
ENDIF

;アナルバイブ挿入中を確認
IF TEQUIP:13
	LOSEBASE:0 += 30
	LOSEBASE:1 += 80

	;ABL:Ａ感覚をみる
	IF ABL:5 == 0
		B += 40
		D += 150
	ELSEIF ABL:5 == 1
		B += 120
		D += 400
	ELSEIF ABL:5 == 2
		B += 300
		D += 700
	ELSEIF ABL:5 == 3
		B += 500
		D += 900
	ELSEIF ABL:5 == 4
		B += 650
		D += 1000
	ELSE
		B += 350 + (ABL:5 * 100)
		D += 700 + (ABL:5 * 100)
	ENDIF

	;EXP:Ａ経験をみる
	IF EXP:1 < EXPLV:1
		TIMES B , 0.50
		C += 1000
	ELSEIF EXP:1 < EXPLV:2
		TIMES B , 1.00
		C += 150
	ELSEIF EXP:1 < EXPLV:3
		TIMES B , 1.10
		C += 20
	ELSEIF EXP:1 < EXPLV:4
		TIMES B , 1.20
		C += 0
	ELSEIF EXP:1 < EXPLV:5
		TIMES B , 1.40
		C += 0
	ELSE
		TIMES B , 1.60
		C += 0
	ENDIF

	;Ａ敏感、鈍感をみる
	IF TALENT:105
		TIMES C , 1.50
		TIMES D , 1.50
	ELSEIF TALENT:106
		TIMES C , 0.60
		TIMES D , 0.60
	ENDIF
	;一度単独で計算
	SOURCE:13 += D
ENDIF

;シャワー使用中を確認
IF TEQUIP:18
	;ABL:Ｃ感覚をみる
	IF ABL:3 == 0
		SOURCE:0 = 150
		SOURCE:12 = 1000
		SOURCE:13 = 50
	ELSEIF ABL:3 == 1
		SOURCE:0 = 400
		SOURCE:12 = 1300
		SOURCE:13 = 80
	ELSEIF ABL:3 == 2
		SOURCE:0 = 800
		SOURCE:12 = 1600
		SOURCE:13 = 120
	ELSEIF ABL:3 == 3
		SOURCE:0 = 1200
		SOURCE:12 = 1900
		SOURCE:13 = 190
	ELSEIF ABL:3 == 4
		SOURCE:0 = 1500
		SOURCE:12 = 2200
		SOURCE:13 = 250
	ELSE
		SOURCE:0 = 1050 + (ABL:3 * 150)
		SOURCE:12 = 1750 + (ABL:3 * 150)
		SOURCE:13 = 175 + (ABL:3 * 25)
	ENDIF
ENDIF

;ＶかＡが上昇するとき、上昇に従ってSOURCE:0、SOURCE:17を減らす
IF TEQUIP:11 || TEQUIP:13
	E = ABL:4 + ABL:5
	IF E <= 1
		TIMES SOURCE:0 , 1.00
		TIMES SOURCE:17 , 1.00
	ELSEIF E <= 3
		TIMES SOURCE:0 , 0.90
		TIMES SOURCE:17 , 0.90
	ELSEIF E <= 5
		TIMES SOURCE:0 , 0.80
		TIMES SOURCE:17 , 0.80
	ELSEIF E <= 7
		TIMES SOURCE:0 , 0.70
		TIMES SOURCE:17 , 0.70
	ELSEIF E <= 9
		TIMES SOURCE:0 , 0.60
		TIMES SOURCE:17 , 0.60
	ELSE
		TIMES SOURCE:0 , 0.50
		TIMES SOURCE:17 , 0.50
	ENDIF
ENDIF

;バイブ、アナルバイブだけ先に計算
IF TEQUIP:11 || TEQUIP:13
	;PALAM:潤滑をみる
	IF PALAM:3 < PALAMLV:1
		TIMES A , 0.40
		TIMES B , 0.40
		C += 800
	ELSEIF PALAM:3 < PALAMLV:2
		TIMES A , 0.80
		TIMES B , 0.80
		C += 500
	ELSEIF PALAM:3 < PALAMLV:3
		TIMES A , 1.00
		TIMES B , 1.00
		C += 300
	ELSEIF PALAM:3 < PALAMLV:4
		TIMES A , 1.40
		TIMES B , 1.40
		C += 120
	ELSEIF PALAM:3 >= PALAMLV:4
		TIMES A , 1.80
		TIMES B , 1.80
		C += 100
	ENDIF

	;PALAM:欲情をみる
	IF PALAM:5 < PALAMLV:1
		TIMES A , 0.80
		TIMES B , 0.80
	ELSEIF PALAM:5 < PALAMLV:2
		TIMES A , 0.90
		TIMES B , 0.90
	ELSEIF PALAM:5 < PALAMLV:3
		TIMES A , 1.00
		TIMES B , 1.00
	ELSEIF PALAM:5 < PALAMLV:4
		TIMES A , 1.10
		TIMES B , 1.10
	ELSEIF PALAM:5 >= PALAMLV:4
		TIMES A , 1.20
		TIMES B , 1.20
	ENDIF

	;ABL:従順をみる
	IF ABL:0 == 0
		TIMES A , 0.80
		TIMES B , 0.80
	ELSEIF ABL:0 == 1
		TIMES A , 0.90
		TIMES B , 0.90
	ELSEIF ABL:0 == 2
		TIMES A , 1.00
		TIMES B , 1.00
	ELSEIF ABL:0 == 3
		TIMES A , 1.10
		TIMES B , 1.10
	ELSEIF ABL:0 == 4
		TIMES A , 1.20
		TIMES B , 1.20
	ELSE
		A = A * (ABL:0 * 5 + 105) / 100
		B = B * (ABL:0 * 5 + 105) / 100
	ENDIF

	;小柄体形
	SIF TALENT:100
		TIMES C , 2.00

	;貞操観念
	IF TALENT:30
		TIMES C, 3.00
	ENDIF

	SOURCE:1 = A
	SOURCE:2 = B
	SOURCE:6 = C

ENDIF

;ABL:技巧をみる
IF ABL:2 == 0
	SOURCE:4 = 100
	TIMES SOURCE:0 , 0.30
	TIMES SOURCE:17 , 0.30
	TIMES SOURCE:1 , 0.30
	TIMES SOURCE:2 , 0.30
ELSEIF ABL:2 == 1
	SOURCE:4 = 160
	TIMES SOURCE:0 , 0.70
	TIMES SOURCE:17 , 0.70
	TIMES SOURCE:1 , 0.70
	TIMES SOURCE:2 , 0.70
ELSEIF ABL:2 == 2
	SOURCE:4 = 220
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:17 , 1.00
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:2 , 1.00
