	TIMES SOURCE:2 , 1.50
ENDIF

;公開のときは、ABL:露出癖をみる
IF TEQUIP:53
	IF ABL:7 == 0
		SOURCE:7 += 0
		TIMES SOURCE:0 , 1.00
		TIMES SOURCE:17 , 1.00
		TIMES SOURCE:1 , 1.00
		TIMES SOURCE:2 , 1.00
		TIMES SOURCE:12, 1.00
	ELSEIF ABL:7 == 1
		SOURCE:7 += 100
		TIMES SOURCE:0 , 1.10
		TIMES SOURCE:17 , 1.10
		TIMES SOURCE:1 , 1.10
		TIMES SOURCE:2 , 1.10
		TIMES SOURCE:12, 1.20
	ELSEIF ABL:7 == 2
		SOURCE:7 += 300
		TIMES SOURCE:0 , 1.20
		TIMES SOURCE:17 , 1.20
		TIMES SOURCE:1 , 1.20
		TIMES SOURCE:2 , 1.20
		TIMES SOURCE:12, 1.40
	ELSEIF ABL:7 == 3
		SOURCE:7 += 800
		TIMES SOURCE:0 , 1.30
		TIMES SOURCE:17 , 1.30
		TIMES SOURCE:1 , 1.30
		TIMES SOURCE:2 , 1.30
		TIMES SOURCE:12, 1.60
	ELSEIF ABL:7 == 4
		SOURCE:7 += 1500
		TIMES SOURCE:0 , 1.50
		TIMES SOURCE:17 , 1.50
		TIMES SOURCE:1 , 1.50
		TIMES SOURCE:2 , 1.50
		TIMES SOURCE:12, 2.00
	ELSE
		SOURCE:7 += ABL:7 * 500
		SOURCE:0 = SOURCE:0 * (ABL:7 * 10 + 120) / 100
		SOURCE:17 = SOURCE:17 * (ABL:7 * 10 + 120) / 100
		SOURCE:1 = SOURCE:1 * (ABL:7 * 10 + 120) / 100
		SOURCE:2 = SOURCE:2 * (ABL:7 * 10 + 120) / 100
		SOURCE:12 = SOURCE:12 * (ABL:7 * 50 + 50) / 100
	ENDIF
ENDIF

;自慰経験増加。基本値は1
LOCAL = 1

;自慰中毒が高いと経験にプラス
SIF ABL:10 >= 5
	LOCAL += ABL:10 / 5
;上限64
SIF LOCAL >= 64
