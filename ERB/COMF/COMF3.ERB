	SOURCE:0 = SOURCE:0 * (ABL:2 * 10 + 110) / 100
	SOURCE:17 = SOURCE:17 * (ABL:2 * 10 + 110) / 100
	SOURCE:1 = SOURCE:1 * (ABL:2 * 10 + 110) / 100
	SOURCE:2 = SOURCE:2 * (ABL:2 * 10 + 110) / 100
ENDIF

;ABL:自慰中毒をみる
IF ABL:10 == 0
	SOURCE:7 = 0
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:17 , 1.00
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:2 , 1.00
ELSEIF ABL:10 == 1
	SOURCE:7 = 100
	TIMES SOURCE:0 , 1.10
	TIMES SOURCE:17 , 1.10
	TIMES SOURCE:1 , 1.10
	TIMES SOURCE:2 , 1.10
ELSEIF ABL:10 == 2
	SOURCE:7 = 300
	TIMES SOURCE:0 , 1.20
	TIMES SOURCE:17 , 1.20
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:2 , 1.20
ELSEIF ABL:10 == 3
	SOURCE:7 = 800
	TIMES SOURCE:0 , 1.30
	TIMES SOURCE:17 , 1.30
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:2 , 1.30
ELSEIF ABL:10 == 4
	SOURCE:7 = 1500
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:17 , 1.50
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:2 , 1.50
ELSE
	SOURCE:7 = ABL:10 * 500
	SOURCE:0 = SOURCE:0 * (ABL:10 * 10 + 120) / 100
	SOURCE:17 = SOURCE:17 * (ABL:10 * 10 + 120) / 100
