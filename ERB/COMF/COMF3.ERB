@COM3
;自慰
;バイブ、アナルバイブ、ビデオ撮影の時に内容変化

;ビデオ撮影中なら公開オナニー
;シャワー使用中ならシャワーオナニー
;バイブ、アナルバイブ、或いは両方装備でバイブオナニー
;オナホ装着でセンズリ
SIF TEQUIP:53
	PRINT 公開
SIF TEQUIP:18
	PRINT シャワー
IF TEQUIP:11 && TEQUIP:13
	PRINT 二穴バイブ
ELSEIF TEQUIP:11
	PRINT バイブ
ELSEIF TEQUIP:13
	PRINT アナルバイブ
ELSEIF TEQUIP:17
	PRINT センズリ
ENDIF

IF TEQUIP:53 || TEQUIP:11 || TEQUIP:13 || TEQUIP:18
	PRINTL オナニー
ELSEIF TEQUIP:17
	PRINTL 
ELSE
	PRINTL 自慰
ENDIF

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = 自慰
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
LOSEBASE:体力 += 5
LOSEBASE:気力 += 50

SOURCE:逸脱 = 400
A = 0
B = 0
C = 0
D = 0

IF TEQUIP:53
	SOURCE:液体追加 = 50
	SOURCE:欲情追加 = 100
ENDIF

;ABL:Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	SOURCE:快Ｃ = 15
	SOURCE:露出 = 2000
	SOURCE:屈従 = 500
ELSEIF ABL:Ｃ感覚 == 1
	SOURCE:快Ｃ = 50
	SOURCE:露出 = 2300
	SOURCE:屈従 = 800
ELSEIF ABL:Ｃ感覚 == 2
	SOURCE:快Ｃ = 300
	SOURCE:露出 = 2600
	SOURCE:屈従 = 1200
ELSEIF ABL:Ｃ感覚 == 3
	SOURCE:快Ｃ = 700
	SOURCE:露出 = 2900
	SOURCE:屈従 = 1900
ELSEIF ABL:Ｃ感覚 == 4
	SOURCE:快Ｃ = 1100
	SOURCE:露出 = 3200
	SOURCE:屈従 = 2500
ELSE
	SOURCE:快Ｃ = 350 + (ABL:Ｃ感覚 * 250)
	SOURCE:露出 = 2750 + (ABL:Ｃ感覚 * 150)
	SOURCE:屈従 = 1750 + (ABL:Ｃ感覚 * 250)
ENDIF

;ABL:Ｂ感覚をみる
IF ABL:Ｂ感覚 == 0
	SOURCE:快Ｂ = 15
ELSEIF ABL:Ｂ感覚 == 1
	SOURCE:快Ｂ = 50
ELSEIF ABL:Ｂ感覚 == 2
	SOURCE:快Ｂ = 300
ELSEIF ABL:Ｂ感覚 == 3
	SOURCE:快Ｂ = 700
ELSEIF ABL:Ｂ感覚 == 4
	SOURCE:快Ｂ = 1100
ELSE
	SOURCE:快Ｂ = 350 + (ABL:Ｂ感覚 * 250)
ENDIF

;バイブ挿入中を確認
IF TEQUIP:11
	;ABL:Ｖ感覚をみる
	IF ABL:Ｖ感覚 == 0
		A += 40
		D += 150
	ELSEIF ABL:Ｖ感覚 == 1
		A += 120
		D += 400
	ELSEIF ABL:Ｖ感覚 == 2
		A += 300
		D += 700
	ELSEIF ABL:Ｖ感覚 == 3
		A += 500
		D += 900
	ELSEIF ABL:Ｖ感覚 == 4
		A += 650
		D += 1000
	ELSE
		A += 350 + (ABL:Ｖ感覚 * 100)
		D += 700 + (ABL:Ｖ感覚 * 100)
	ENDIF

	;EXP:Ｖ経験をみる
	;処女はありえない
	IF EXP:Ｖ経験 < EXPLV:2
		TIMES A , 0.60
		C += 150
	ELSEIF EXP:Ｖ経験 < EXPLV:3
		TIMES A , 1.00
		C += 20
	ELSEIF EXP:Ｖ経験 < EXPLV:4
		TIMES A , 1.20
		C += 0
	ELSEIF EXP:Ｖ経験 < EXPLV:5
		TIMES A , 1.40
		C += 0
	ELSE
		TIMES A , 1.60
		C += 0
	ENDIF
	;Ｖ敏感、鈍感をみる
	IF TALENT:Ｖ鈍感
		TIMES C , 1.50
		TIMES D , 1.50
	ELSEIF TALENT:Ｖ敏感
		TIMES C , 0.60
		TIMES D , 0.60
	ENDIF
	;一度単独で計算
	SOURCE:屈従 += D
ENDIF

;アナルバイブ挿入中を確認
IF TEQUIP:13
	LOSEBASE:体力 += 30
	LOSEBASE:気力 += 80

	;ABL:Ａ感覚をみる
	IF ABL:Ａ感覚 == 0
		B += 40
		D += 150
	ELSEIF ABL:Ａ感覚 == 1
		B += 120
		D += 400
	ELSEIF ABL:Ａ感覚 == 2
		B += 300
		D += 700
	ELSEIF ABL:Ａ感覚 == 3
		B += 500
		D += 900
	ELSEIF ABL:Ａ感覚 == 4
		B += 650
		D += 1000
	ELSE
		B += 350 + (ABL:Ａ感覚 * 100)
		D += 700 + (ABL:Ａ感覚 * 100)
	ENDIF

	;EXP:Ａ経験をみる
	IF EXP:Ａ経験 < EXPLV:1
		TIMES B , 0.50
		C += 1000
	ELSEIF EXP:Ａ経験 < EXPLV:2
		TIMES B , 1.00
		C += 150
	ELSEIF EXP:Ａ経験 < EXPLV:3
		TIMES B , 1.10
		C += 20
	ELSEIF EXP:Ａ経験 < EXPLV:4
		TIMES B , 1.20
		C += 0
	ELSEIF EXP:Ａ経験 < EXPLV:5
		TIMES B , 1.40
		C += 0
	ELSE
		TIMES B , 1.60
		C += 0
	ENDIF

	;Ａ敏感、鈍感をみる
	IF TALENT:Ａ鈍感
		TIMES C , 1.50
		TIMES D , 1.50
	ELSEIF TALENT:Ａ敏感
		TIMES C , 0.60
		TIMES D , 0.60
	ENDIF
	;一度単独で計算
	SOURCE:屈従 += D
ENDIF

;シャワー使用中を確認
IF TEQUIP:18
	;ABL:Ｃ感覚をみる
	IF ABL:Ｃ感覚 == 0
		SOURCE:快Ｃ = 150
		SOURCE:露出 = 1000
		SOURCE:屈従 = 50
	ELSEIF ABL:Ｃ感覚 == 1
		SOURCE:快Ｃ = 400
		SOURCE:露出 = 1300
		SOURCE:屈従 = 80
	ELSEIF ABL:Ｃ感覚 == 2
		SOURCE:快Ｃ = 800
		SOURCE:露出 = 1600
		SOURCE:屈従 = 120
	ELSEIF ABL:Ｃ感覚 == 3
		SOURCE:快Ｃ = 1200
		SOURCE:露出 = 1900
		SOURCE:屈従 = 190
	ELSEIF ABL:Ｃ感覚 == 4
		SOURCE:快Ｃ = 1500
		SOURCE:露出 = 2200
		SOURCE:屈従 = 250
	ELSE
		SOURCE:快Ｃ = 1050 + (ABL:Ｃ感覚 * 150)
		SOURCE:露出 = 1750 + (ABL:Ｃ感覚 * 150)
		SOURCE:屈従 = 175 + (ABL:Ｃ感覚 * 25)
	ENDIF
ENDIF

;ＶかＡが上昇するとき、上昇に従ってSOURCE:快Ｃ、SOURCE:快Ｂを減らす
IF TEQUIP:11 || TEQUIP:13
	E = ABL:Ｖ感覚 + ABL:Ａ感覚
	IF E <= 1
		TIMES SOURCE:快Ｃ , 1.00
		TIMES SOURCE:快Ｂ , 1.00
	ELSEIF E <= 3
		TIMES SOURCE:快Ｃ , 0.90
		TIMES SOURCE:快Ｂ , 0.90
	ELSEIF E <= 5
		TIMES SOURCE:快Ｃ , 0.80
		TIMES SOURCE:快Ｂ , 0.80
	ELSEIF E <= 7
		TIMES SOURCE:快Ｃ , 0.70
		TIMES SOURCE:快Ｂ , 0.70
	ELSEIF E <= 9
		TIMES SOURCE:快Ｃ , 0.60
		TIMES SOURCE:快Ｂ , 0.60
	ELSE
		TIMES SOURCE:快Ｃ , 0.50
		TIMES SOURCE:快Ｂ , 0.50
	ENDIF
ENDIF

;バイブ、アナルバイブだけ先に計算
IF TEQUIP:11 || TEQUIP:13
	;PALAM:潤滑をみる
	IF PALAM:潤滑 < PALAMLV:1
		TIMES A , 0.40
		TIMES B , 0.40
		C += 800
	ELSEIF PALAM:潤滑 < PALAMLV:2
		TIMES A , 0.80
		TIMES B , 0.80
		C += 500
	ELSEIF PALAM:潤滑 < PALAMLV:3
		TIMES A , 1.00
		TIMES B , 1.00
		C += 300
	ELSEIF PALAM:潤滑 < PALAMLV:4
		TIMES A , 1.40
		TIMES B , 1.40
		C += 120
	ELSEIF PALAM:潤滑 >= PALAMLV:4
		TIMES A , 1.80
		TIMES B , 1.80
		C += 100
	ENDIF

	;PALAM:欲情をみる
	IF PALAM:欲情 < PALAMLV:1
		TIMES A , 0.80
		TIMES B , 0.80
	ELSEIF PALAM:欲情 < PALAMLV:2
		TIMES A , 0.90
		TIMES B , 0.90
	ELSEIF PALAM:欲情 < PALAMLV:3
		TIMES A , 1.00
		TIMES B , 1.00
	ELSEIF PALAM:欲情 < PALAMLV:4
		TIMES A , 1.10
		TIMES B , 1.10
	ELSEIF PALAM:欲情 >= PALAMLV:4
		TIMES A , 1.20
		TIMES B , 1.20
	ENDIF

	;ABL:従順をみる
	IF ABL:親密 == 0
		TIMES A , 0.80
		TIMES B , 0.80
	ELSEIF ABL:親密 == 1
		TIMES A , 0.90
		TIMES B , 0.90
	ELSEIF ABL:親密 == 2
		TIMES A , 1.00
		TIMES B , 1.00
	ELSEIF ABL:親密 == 3
		TIMES A , 1.10
		TIMES B , 1.10
	ELSEIF ABL:親密 == 4
		TIMES A , 1.20
		TIMES B , 1.20
	ELSE
		A = A * (ABL:親密 * 5 + 105) / 100
		B = B * (ABL:親密 * 5 + 105) / 100
	ENDIF

	;小柄体形
	SIF TALENT:小柄体型
		TIMES C , 2.00

	;貞操観念
	IF TALENT:貞操観念
		TIMES C, 3.00
	ENDIF

	SOURCE:快Ｖ = A
	SOURCE:快Ａ = B
	SOURCE:痛み = C

ENDIF

;ABL:技巧をみる
IF ABL:技巧 == 0
	SOURCE:性行動 = 100
	TIMES SOURCE:快Ｃ , 0.30
	TIMES SOURCE:快Ｂ , 0.30
	TIMES SOURCE:快Ｖ , 0.30
	TIMES SOURCE:快Ａ , 0.30
ELSEIF ABL:技巧 == 1
	SOURCE:性行動 = 160
	TIMES SOURCE:快Ｃ , 0.70
	TIMES SOURCE:快Ｂ , 0.70
	TIMES SOURCE:快Ｖ , 0.70
	TIMES SOURCE:快Ａ , 0.70
ELSEIF ABL:技巧 == 2
	SOURCE:性行動 = 220
	TIMES SOURCE:快Ｃ , 1.00
	TIMES SOURCE:快Ｂ , 1.00
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:快Ａ , 1.00
ELSEIF ABL:技巧 == 3
	SOURCE:性行動 = 280
	TIMES SOURCE:快Ｃ , 1.20
	TIMES SOURCE:快Ｂ , 1.20
	TIMES SOURCE:快Ｖ , 1.20
	TIMES SOURCE:快Ａ , 1.20
ELSEIF ABL:技巧 == 4
	SOURCE:性行動 = 340
	TIMES SOURCE:快Ｃ , 1.40
	TIMES SOURCE:快Ｂ , 1.40
	TIMES SOURCE:快Ｖ , 1.40
	TIMES SOURCE:快Ａ , 1.40
ELSE
	SOURCE:性行動 = 250 + (ABL:技巧 * 30)
	SOURCE:快Ｃ = SOURCE:快Ｃ * (ABL:技巧 * 10 + 110) / 100
	SOURCE:快Ｂ = SOURCE:快Ｂ * (ABL:技巧 * 10 + 110) / 100
	SOURCE:快Ｖ = SOURCE:快Ｖ * (ABL:技巧 * 10 + 110) / 100
	SOURCE:快Ａ = SOURCE:快Ａ * (ABL:技巧 * 10 + 110) / 100
ENDIF

;ABL:自慰中毒をみる
IF ABL:自慰中毒 == 0
	SOURCE:中毒充足 = 0
	TIMES SOURCE:快Ｃ , 1.00
	TIMES SOURCE:快Ｂ , 1.00
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:快Ａ , 1.00
ELSEIF ABL:自慰中毒 == 1
	SOURCE:中毒充足 = 100
	TIMES SOURCE:快Ｃ , 1.10
	TIMES SOURCE:快Ｂ , 1.10
	TIMES SOURCE:快Ｖ , 1.10
	TIMES SOURCE:快Ａ , 1.10
ELSEIF ABL:自慰中毒 == 2
	SOURCE:中毒充足 = 300
	TIMES SOURCE:快Ｃ , 1.20
	TIMES SOURCE:快Ｂ , 1.20
	TIMES SOURCE:快Ｖ , 1.20
	TIMES SOURCE:快Ａ , 1.20
ELSEIF ABL:自慰中毒 == 3
	SOURCE:中毒充足 = 800
	TIMES SOURCE:快Ｃ , 1.30
	TIMES SOURCE:快Ｂ , 1.30
	TIMES SOURCE:快Ｖ , 1.30
	TIMES SOURCE:快Ａ , 1.30
ELSEIF ABL:自慰中毒 == 4
	SOURCE:中毒充足 = 1500
	TIMES SOURCE:快Ｃ , 1.50
	TIMES SOURCE:快Ｂ , 1.50
	TIMES SOURCE:快Ｖ , 1.50
	TIMES SOURCE:快Ａ , 1.50
ELSE
	SOURCE:中毒充足 = ABL:自慰中毒 * 500
	SOURCE:快Ｃ = SOURCE:快Ｃ * (ABL:自慰中毒 * 10 + 120) / 100
	SOURCE:快Ｂ = SOURCE:快Ｂ * (ABL:自慰中毒 * 10 + 120) / 100
	TIMES SOURCE:快Ｖ , 1.50
	TIMES SOURCE:快Ａ , 1.50
ENDIF

;公開のときは、ABL:露出癖をみる
IF TEQUIP:53
	IF ABL:露出癖 == 0
		SOURCE:中毒充足 += 0
		TIMES SOURCE:快Ｃ , 1.00
		TIMES SOURCE:快Ｂ , 1.00
		TIMES SOURCE:快Ｖ , 1.00
		TIMES SOURCE:快Ａ , 1.00
		TIMES SOURCE:露出, 1.00
	ELSEIF ABL:露出癖 == 1
		SOURCE:中毒充足 += 100
		TIMES SOURCE:快Ｃ , 1.10
		TIMES SOURCE:快Ｂ , 1.10
		TIMES SOURCE:快Ｖ , 1.10
		TIMES SOURCE:快Ａ , 1.10
		TIMES SOURCE:露出, 1.20
	ELSEIF ABL:露出癖 == 2
		SOURCE:中毒充足 += 300
		TIMES SOURCE:快Ｃ , 1.20
		TIMES SOURCE:快Ｂ , 1.20
		TIMES SOURCE:快Ｖ , 1.20
		TIMES SOURCE:快Ａ , 1.20
		TIMES SOURCE:露出, 1.40
	ELSEIF ABL:露出癖 == 3
		SOURCE:中毒充足 += 800
		TIMES SOURCE:快Ｃ , 1.30
		TIMES SOURCE:快Ｂ , 1.30
		TIMES SOURCE:快Ｖ , 1.30
		TIMES SOURCE:快Ａ , 1.30
		TIMES SOURCE:露出, 1.60
	ELSEIF ABL:露出癖 == 4
		SOURCE:中毒充足 += 1500
		TIMES SOURCE:快Ｃ , 1.50
		TIMES SOURCE:快Ｂ , 1.50
		TIMES SOURCE:快Ｖ , 1.50
		TIMES SOURCE:快Ａ , 1.50
		TIMES SOURCE:露出, 2.00
	ELSE
		SOURCE:中毒充足 += ABL:露出癖 * 500
		SOURCE:快Ｃ = SOURCE:快Ｃ * (ABL:露出癖 * 10 + 120) / 100
		SOURCE:快Ｂ = SOURCE:快Ｂ * (ABL:露出癖 * 10 + 120) / 100
		SOURCE:快Ｖ = SOURCE:快Ｖ * (ABL:露出癖 * 10 + 120) / 100
		SOURCE:快Ａ = SOURCE:快Ａ * (ABL:露出癖 * 10 + 120) / 100
		SOURCE:露出 = SOURCE:露出 * (ABL:露出癖 * 50 + 50) / 100
	ENDIF
ENDIF

;自慰経験増加。基本値は1
LOCAL = 1

;自慰中毒が高いと経験にプラス
SIF ABL:自慰中毒 >= 5
	LOCAL += ABL:自慰中毒 / 5
;上限64
SIF LOCAL >= 64
	LOCAL = 64

;ビデオ撮影時は経験にプラス
SIF TEQUIP:53
	LOCAL += 1

EXP:自慰経験 += LOCAL
PRINTFORML 自慰経験＋{LOCAL}
EXP:強制自慰経験 += LOCAL
PRINTFORML 強制自慰経験＋{LOCAL}

;ビデオ撮影時は異常経験にプラス
IF TEQUIP:53
	EXP:異常経験 += 1
	PRINTL 異常経験＋1
ENDIF

;奴隷の指⇔奴隷のＢの汚れが移動
STAIN:手 |= STAIN:胸
STAIN:胸 |= STAIN:手

;奴隷の指⇔奴隷のＶの汚れが移動
STAIN:手 |= STAIN:ヴァギナ
STAIN:ヴァギナ |= STAIN:手


;シャワーオナニーの場合汚れをリセット、潤滑更に半分。
IF TEQUIP:18 == 1
	STAIN:手 = 0
	STAIN:ペニス = 2
	STAIN:ヴァギナ = 1
	STAIN:アナル = 8
	PALAM:潤滑 /= 2
ENDIF


;既成事実２に相当
TFLAG:200 = 2

;撮影時の調教内容チェック
IF TEQUIP:11 && TEQUIP:13
	TFLAG:90 = 993
ELSEIF TEQUIP:11
	TFLAG:90 = 991
ELSEIF TEQUIP:13
	TFLAG:90 = 992
ELSEIF TEQUIP:18
	TFLAG:90 = 994
ENDIF

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +3
	EXP:レズ経験 += 3
	EXP:PLAYER:レズ経験 += 3
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +3
	EXP:ＢＬ経験 += 3
	EXP:PLAYER:ＢＬ経験 += 3
ENDIF


;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:受動 += 300 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:受動 += 600 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:受動 += 800 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:受動 += 1000 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:受動 += 1200 + (ABL:親密 * 160)
	ELSE
		SOURCE:受動 += 1500 + (ABL:親密 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:征服 = 600
	ELSEIF ABL:サドっ気 == 1
		SOURCE:征服 = 900
	ELSEIF ABL:サドっ気 == 2
		SOURCE:征服 = 1200
	ELSEIF ABL:サドっ気 == 3
		SOURCE:征服 = 1600
	ELSEIF ABL:サドっ気 == 4
		SOURCE:征服 = 2000
	ELSE
		SOURCE:征服 = 1000 + (ABL:サドっ気 * 300)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
;合意がないと好感度が下がる
IF TALENT:合意 == 0 && TFLAG:48 == 0
	A -= 8
	SOURCE:反感追加 += 300
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2


RETURN 1

