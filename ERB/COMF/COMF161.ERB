@COM_ABLE161
;鈴蘭の毒の薬実行判定
;メディスンはいません
RETURN 0

;COM_ABLE共通判定
CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中じゃないと無理
SIF TFLAG:44 == 0
	RETURN 0
;調教者がメディスンじゃないとダメ
SIF NO:PLAYER != 33
	RETURN 0
;使いすぎるとダメ
SIF CFLAG:10 >= 1000
	RETURN 0
;薬毒耐性だとダメ。ただし上海人形、蓬莱人形は除く
SIF TALENT:薬毒耐性 && NO:TARGET != 88 && NO:TARGET != 89
	RETURN 0
;鈴蘭の毒効果発揮中は不可
SIF TEQUIP:74
	RETURN 0
;触手調教中はダメ
SIF TEQUIP:90
	RETURN 0
;体力が1000未満では使用不可
SIF BASE:体力 < 1000 && TEQUIP:75 == 0
	RETURN 0
RETURN 1

@TRAIN_MESSAGE_COM161
PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%に鈴蘭の毒を浴びせた
IF A == 0 && CFLAG:10 == 0
	CALL POISON_EFFECT_MESSAGE
ELSEIF A == 0 && CFLAG:10 > 0
	PRINTFORML %CALLNAME:TARGET%は毒で体力を消耗している……
ELSEIF A >= 1 && A < 4
	PRINTFORML %CALLNAME:TARGET%は虚ろな目つきで%CALLNAME:PLAYER%を見上げている……
	PRINTFORML その瞳からは反抗する意思が感じられない
ELSEIF A >= 4 && A < 8
	PRINTFORML 毒の影響か%CALLNAME:TARGET%は顔を紅潮させ、息を荒げている……
ELSEIF A >= 8 && A < 12
	PRINTFORML %CALLNAME:PLAYER%と目が合うと%CALLNAME:TARGET%は急に怯えた様子で逃げ出そうとした……
ELSE
	PRINTFORML %CALLNAME:TARGET%は気分が悪いの顔面蒼白になり、口元に手を当てている……
ENDIF

@COM161
;鈴蘭の毒
PRINTL 鈴蘭の毒
STR:0 = 鈴蘭の毒

A = RAND:16
B = CFLAG:10

LOSEBASE:体力 += 50 + (50 * (16-A) / (B+1))
LOSEBASE:気力 += 30 + (30 * (16-A) / (B+1))

SOURCE:痛み = 3000
SOURCE:逸脱 = 8000

SIF TEQUIP:74 == 0
	TEQUIP:74 = 1

;POISON_EFFECTの分岐用
C = RAND:10
D = RAND:2

CALL TRAIN_MESSAGE_B

IF A == 0 && CFLAG:10 == 0
	CALL POISON_EFFECT
ELSEIF A == 0 && CFLAG:10 > 0
	SIF BASE:体力 > 10
		BASE:体力 = 10
ELSEIF A >= 1 && A < 4
	PALAM:恭順 += 5000
	PALAM:屈服 += 5000
ELSEIF A >= 4 && A < 8
	PALAM:潤滑 += 5000
	PALAM:欲情 += 5000
	PALAM:恥情 += 5000
ELSEIF A >= 8 && A < 12
	PALAM:恐怖 += 5000
ELSE
	PALAM:苦痛 += 5000
	PALAM:不快 += 5000
ENDIF

CFLAG:10 += 10 + RAND:11

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +1
	EXP:レズ経験 += 1
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +1
	EXP:ＢＬ経験 += 1
ENDIF

RETURN 1

@EQUIP_COM161
;鈴蘭

PRINTL ＜鈴蘭の毒効果発揮中＞

LOSEBASE:体力 += 100
LOSEBASE:気力 += 90

A = (CFLAG:10 / 10) + 1
SOURCE:痛み += 2500 / A

CFLAG:10 -= RAND:5

B = 100
;幽霊、蓬莱人は早めに耐性が付く
SIF NO:TARGET == 19 || NO:TARGET == 20 || NO:TARGET == 29 || NO:TARGET == 30 || NO:TARGET == 31 || NO:TARGET == 51 || NO:TARGET == 87
	TIMES B , 0.50
;死神、裁判長、厄神も同じく耐性が早めに付く
SIF NO:TARGET == 35 || NO:TARGET == 36 || NO:TARGET == 39
	TIMES B , 0.66
;異常経験が多いと同じく早めに耐性が付く
SIF EXP:異常経験 >= 5
	TIMES B , 0.75
;中毒しやすいだと耐性が付きにくい
SIF TALENT:中毒しやすい
	TIMES B , 2.00

C = RAND:8
IF CFLAG:10 >= B && C == 0 && TALENT:薬毒耐性 == 0
	PRINTFORMW %CALLNAME:TARGET%は毒や薬に対する抵抗力を持つようになった
	PRINTFORML %CALLNAME:TARGET%は【薬毒耐性】を得た
