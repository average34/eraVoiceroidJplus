CALL COM_ORDER

;男嫌い
IF TALENT:82 && TALENT:MASTER:122
	A -= 7
ENDIF
;恥じらい
IF TALENT:35
	A -= 5
ENDIF
;献身的
IF TALENT:63
	A += 6
ENDIF
;恋慕
IF TALENT:85
	A += 5
ENDIF

