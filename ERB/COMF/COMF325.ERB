	;ビデオテープ、カメラがある場合、撮影開始
	IF K > 0
		;消費量は最大で10
		SIF K >= 10
			K = 10
		A += 1
		;テープの消費と経験の増加
		;ITEM:28 -= K
		EXP:ASSI:撮影経験 += K
		EXP:MASTER:被写経験 += K

		;弱味ゲット(相手は身に覚えがない事なので弱味にはならない)
;		IF TALENT:37 == 0
;			TALENT:37 = 1
;		PRINTFORML %CALLNAME:ASSI%は%CALLNAME:TARGET%の【%TALENTNAME:37%】を得た
;		ENDIF
	ENDIF

	;霊夢様のサドっ気変動
	IF ABL:ASSI:サドっ気 <= 4
		ABL:ASSI:サドっ気 = 5
	ELSEIF ABL:ASSI:サドっ気 < 15
		ABL:ASSI:サドっ気 += 1
	ENDIF
ENDIF
;異常経験取得あり
IF A > 0
	EXP:MASTER:異常経験 += A
ENDIF
