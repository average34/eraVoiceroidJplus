@COM_ABLE185
;口移し
;直前にフェラするで射精させていないとダメ
;もしくはコンドーム精飲で精液を口に含んでいるのが条件
;CFLAG108
;COM_ABLE共通判定
CALL COM_ABLE_COMMON, SELECTCOM
SIF RESULT == 0
	RETURN 0
;ウフフ中じゃないと無理
SIF TFLAG:44 == 0
	RETURN 0
;触手調教中はダメ
SIF TEQUIP:90
	RETURN 0
;直前がフェラかコンドーム精飲(助手/マスター)じゃないとダメ
SIF (PREVCOM != 4) && (PREVCOM != 183) && (PREVCOM != 184)
	RETURN 0
;直前のコマンドがフェラの場合
IF PREVCOM == 4
	;直前のフェラするで射精してないとダメ
	SIF TFLAG:10 < 1
		RETURN 0
ELSEIF PREVCOM == 183
	;コンドーム精飲直後にレズっ気3以上の助手が調教者でないとダメ
	SIF (ASSIPLAY == 0) || (ABL:ASSI:レズっ気 < 3)
		RETURN 0
ENDIF
RETURN 1

@COM185
;口移し

PRINTL 口移し


;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0

STR:0 = 口移し
CALL TRAIN_MESSAGE_B
;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;射精ゲージチェック（ふたなり・オトコの場合）
;-------------------------------------------------
SIF TALENT:PLAYER:半人半妖 || TALENT:PLAYER:オトコ  || TALENT:PLAYER:ふたなり
	BASE:PLAYER:射精 += CALC_EJAC(SELECTCOM, PLAYER)

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 0
LOSEBASE:気力 += 80

SOURCE:屈従 = 1500
SOURCE:逸脱 = 500

;上のほうで計算した汚れデータ
SOURCE:不潔 = Y*80 + 100

;Ｍ感覚を設定している場合快Ｍソース等取得
SIF FLAG:62 & 4194304
	CALL SOURCE_M_COM185

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:性行動 = 420
	SOURCE:達成感 = 150
	TIMES SOURCE:不潔 , 4.00
ELSEIF ABL:奉仕精神 == 1
	SOURCE:性行動 = 500
	SOURCE:達成感 = 300
	TIMES SOURCE:不潔 , 2.50
ELSEIF ABL:奉仕精神 == 2
	SOURCE:性行動 = 580
	SOURCE:達成感 = 600
	TIMES SOURCE:不潔 , 1.50
ELSEIF ABL:奉仕精神 == 3
	SOURCE:性行動 = 660
	SOURCE:達成感 = 900
	TIMES SOURCE:不潔 , 1.00
ELSEIF ABL:奉仕精神 == 4
	SOURCE:性行動 = 740
	SOURCE:達成感 = 1500
	TIMES SOURCE:不潔 , 0.50
ELSE
	SOURCE:性行動 = 820
	SOURCE:達成感 = 2200
	TIMES SOURCE:不潔 , 0.10
ENDIF

PRINTFORML %EXPNAME:96%＋１
EXP:キス経験 += 1
EXP:PLAYER:キス経験 += 1
IF TALENT:PLAYER:キス未経験
	PRINTFORMW %CALLNAME:PLAYER%は【キス未経験】を失いました
	TALENT:PLAYER:キス未経験 = 0
ENDIF

IF TALENT:キス未経験
	TFLAG:600 = 1
	CALL KOJO_JUN
	TALENT:キス未経験 = 0
	PRINTFORMW %CALLNAME:TARGET%は【キス未経験】を失いました
	TFLAG:38 = 1
ENDIF

;主導権をチェック
IF TFLAG:45 == 0
	;主導権が主人公側
	;ABL:従順をみる
	IF ABL:親密 <= 1
		SOURCE:受動 += 300 + (ABL:親密 * 50)
	ELSEIF ABL:親密 <= 3
		SOURCE:受動 += 600 + (ABL:親密 * 100)
	ELSEIF ABL:親密 <= 5
		SOURCE:受動 += 800 + (ABL:親密 * 120)
	ELSEIF ABL:親密 <= 8
		SOURCE:受動 += 1000 + (ABL:親密 * 140)
	ELSEIF ABL:親密 <= 11
		SOURCE:受動 += 1200 + (ABL:親密 * 160)
	ELSE
		SOURCE:受動 += 1500 + (ABL:親密 * 180)
	ENDIF
ELSE
	;主導権が相手側
	;ABL:サドっ気をみる
	IF ABL:サドっ気 == 0
		SOURCE:征服 = 600
	ELSEIF ABL:サドっ気 == 1
		SOURCE:征服 = 900
	ELSEIF ABL:サドっ気 == 2
		SOURCE:征服 = 1200
	ELSEIF ABL:サドっ気 == 3
		SOURCE:征服 = 1600
	ELSEIF ABL:サドっ気 == 4
		SOURCE:征服 = 2000
	ELSE
		SOURCE:征服 = 1000 + (ABL:サドっ気 * 300)
	ENDIF
ENDIF

;好感度ランダム変化
A = RAND:10 + 5
;合意がないと好感度が下がる
IF TALENT:合意 == 0 && TFLAG:48 == 0
	A -= 3
	SOURCE:反感追加 += 100
ENDIF
TFLAG:30 += A
TFLAG:37 += A / 2

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK
;S = BASE:PLAYER:射精
;EJAC = MAXBASE:PLAYER:射精
;
;IF  S > EJAC * 2
;	E = 2
;ELSEIF S > EJAC
;	E = 1
;ELSE
;	E = 0
;ENDIF

IF E
;射精している
	TIMES SOURCE:性行動 , 3.00

	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		SOURCE:中毒充足 = 0
		TIMES SOURCE:達成感 , 2.00
		TIMES SOURCE:屈従, 2.00
	ELSEIF ABL:精液中毒 == 1
		SOURCE:中毒充足 = 100
		TIMES SOURCE:達成感 , 2.50
		TIMES SOURCE:屈従, 1.60
	ELSEIF ABL:精液中毒 == 2
		SOURCE:中毒充足 = 300
		TIMES SOURCE:達成感 , 3.00
		TIMES SOURCE:屈従, 1.00
	ELSEIF ABL:精液中毒 == 3
		SOURCE:中毒充足 = 700
		TIMES SOURCE:達成感 , 4.00
		TIMES SOURCE:屈従, 0.70
	ELSEIF ABL:精液中毒 == 4
		SOURCE:中毒充足 = 1500
		TIMES SOURCE:達成感 , 5.00
		TIMES SOURCE:屈従, 0.40
	ELSE
		SOURCE:中毒充足 = (ABL:精液中毒 * 2000) - 4000
		SOURCE:達成感 = SOURCE:達成感 * (ABL:精液中毒 * 50 + 350) / 100
		TIMES SOURCE:屈従, 0.10
	ENDIF

	;射精先タイプの設定
	TFLAG:703 = 11
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;;大量射精
;IF E == 2
;	TIMES SOURCE:中毒充足 , 2.00
;	TIMES SOURCE:達成感 , 1.50
;
;	EXP:PLAYER:射精経験 += 2
;	EXP:精液経験 += 1
;	PRINTL 大量射精
;	PRINTL 精液経験＋１
;	;精液汚れ
;	STAIN:PLAYER:ペニス |= 4
;
;	BASE:PLAYER:射精 -= EJAC*2
;	SIF BASE:PLAYER:射精 >= EJAC
;		BASE:PLAYER:射精 = EJAC-1
;
;	;キスで射精させたフラグ
;	TFLAG:4 = 2
;;通常の射精
;ELSEIF E == 1
;	EXP:PLAYER:射精経験 += 1
;	PRINTL 射精
;	;精液汚れ
;	STAIN:PLAYER:ペニス |= 4
;
;	BASE:PLAYER:射精 -= EJAC
;	SIF BASE:PLAYER:射精 >= EJAC
;		BASE:PLAYER:射精 = EJAC-1
;
;	;キスで射精させたフラグ
;	TFLAG:4 = 1
;ENDIF


;調教者の口の汚れと奴隷の口の汚れを処理
S = STAIN:PLAYER:口
STAIN:PLAYER:口 |= STAIN:口
STAIN:口 |= S

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +3
	EXP:レズ経験 += 3
	EXP:PLAYER:レズ経験 += 3
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +3
	EXP:ＢＬ経験 += 3
	EXP:PLAYER:ＢＬ経験 += 3
ENDIF

;既成事実２に相当
TFLAG:200 = 2

RETURN 1

;Ｍ感覚を設定している場合快Ｍソース等取得
@SOURCE_M_COM185
;ABL:Ｍ感覚をみる
SOURCE:快Ｍ = 100 + (25 * ABL:Ｍ感覚)

;EXP:キス経験をみる
IF EXP:キス経験 < EXPLV:1
	TIMES SOURCE:快Ｍ , 1.00
ELSEIF EXP:キス経験 < EXPLV:2
	TIMES SOURCE:逸脱 , 0.95
	TIMES SOURCE:快Ｍ , 1.05
ELSEIF EXP:キス経験 < EXPLV:3
	TIMES SOURCE:逸脱 , 0.90
	TIMES SOURCE:快Ｍ , 1.10
ELSEIF EXP:キス経験 < EXPLV:4
	TIMES SOURCE:逸脱 , 0.85
	TIMES SOURCE:快Ｍ , 1.15
ELSEIF EXP:キス経験 < EXPLV:5
	TIMES SOURCE:逸脱 , 0.80
	TIMES SOURCE:快Ｍ , 1.20
ELSE
	TIMES SOURCE:逸脱 , 0.75
	TIMES SOURCE:快Ｍ , 1.20
ENDIF

;ABL:精液中毒をみる
IF ABL:精液中毒 == 0
	TIMES SOURCE:快Ｍ , 0.80
ELSEIF ABL:精液中毒 == 1
	TIMES SOURCE:快Ｍ , 1.00
ELSEIF ABL:精液中毒 == 2
	TIMES SOURCE:快Ｍ , 1.20
ELSEIF ABL:精液中毒 == 3
	TIMES SOURCE:快Ｍ , 1.40
ELSEIF ABL:精液中毒 == 4
	TIMES SOURCE:快Ｍ , 1.60
ELSE
	TIMES SOURCE:快Ｍ , 1.80
ENDIF

;口移し
@TRAIN_MESSAGE_COM185
PRINTFORMW %CALLNAME:PLAYER%は口に含んだ精液を唾液と混ぜ、キスで口移しした……

;恋慕で精液中毒LV3以上でマスターの精液をマスターが飲ませる
IF TALENT:恋慕 && (CFLAG:PLAYER:108 == 1) && (ABL:TARGET:精液中毒 >= 3 ) && (PLAYER == MASTER)
	PRINTFORMW %CALLNAME:TARGET%は嬉しそうに%CALLNAME:MASTER%の精液を味わっている…
	PRINTFORMW そして、名残惜しそうに、%CALLNAME:TARGET%と舌を絡め、口内を舐め回している…
;精液中毒5以上
ELSEIF ABL:TARGET:精液中毒 >= 5
	PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:PLAYER%の唾液ごと貪欲に吸引し、
	PRINTFORMW さらに%CALLNAME:PLAYER%の口内を舐め回して残り香を愉しんでいる…
;精液中毒3以上
ELSEIF ABL:TARGET:精液中毒 >= 3
	PRINTFORM %CALLNAME:TARGET%は恍惚としながら、
	IF CFLAG:108 == 1
		PRINTS CALLNAME:MASTER
	ELSEIF CFLAG:108 == 2
		PRINTS CALLNAME:ASSI
	ELSE
		PRINT 己
	ENDIF
	PRINTFORMW の子種を%CALLNAME:PLAYER%の舌と絡め合っている…
;精液中毒1以上
ELSEIF ABL:TARGET:精液中毒 >= 1
	PRINTFORM %CALLNAME:TARGET%は
	IF CFLAG:108 == 1
		PRINTS CALLNAME:MASTER
	ELSEIF CFLAG:108 == 2
		PRINTS CALLNAME:ASSI
	ELSE
		PRINT 自分
	ENDIF
	PRINTFORM の精液を飲まされ、
	IF TALENT:恋慕 && (CFLAG:108 == 1)
		PRINTFORMW 少し顔を赤らめている…
	ELSE
		PRINTFORMW 屈辱に満ちた表情をしている…
	ENDIF
;精液中毒なし
ELSE
	PRINTFORMW %CALLNAME:TARGET%は涙目で逃れようとする…
	PRINTFORMW しかし%CALLNAME:PLAYER%に追い詰められ、諦めたような表情で口内に精液を流し込まれている…
	PRINTFORM %CALLNAME:TARGET%は口内に溜まった精液を咽せながら
	SIF TALENT:恋慕 && (CFLAG:108 == 1) && (PLAYER == MASTER)
		PRINT も懸命に
	PRINTFORMW 嚥下していく…
ENDIF

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_185
;;PLAYERが無理矢理するので実行はいつでも可能
;適当にキス辺りの処理を持ってくる

A = 0
S = 0
V = 85
;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;男嫌い
IF TALENT:男嫌い && TALENT:PLAYER:オトコ
	A -= 7
ENDIF

;ABL:欲望
IF ABL:欲望
	SIF S
	A += ABL:欲望 * 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	SIF S
	A += ABL:奉仕精神 * 4
ENDIF

IF MARK:快楽依存
	SIF S
	A += MARK:快楽依存 * 2
ENDIF


;PALAM:欲情
IF PALAM:欲情 < PALAMLV:1
	L = 0
ELSEIF PALAM:欲情 < PALAMLV:2
	L = 1
ELSEIF PALAM:欲情 < PALAMLV:3
	L = 2
ELSEIF PALAM:欲情 < PALAMLV:4
	L = 3
ELSEIF PALAM:欲情 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
	A += L * 1
ENDIF

;恥じらい
IF TALENT:恥じらい
	A -= 5
ENDIF
;汚臭鈍感
IF TALENT:汚臭鈍感
	SIF S
	A += 1
ENDIF
;汚臭敏感
IF TALENT:汚臭敏感
	A -= 3
ENDIF
;献身的
IF TALENT:献身的
	SIF S
	A += 6
ENDIF
;快感の否定
IF TALENT:快感の否定
	A -= 1
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	SIF S
	A += 5
	S = 5
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:口 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:口 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:口 & 8
	Y += 7
SIF STAIN:PLAYER:口 & 16
	Y += 15

SIF TALENT:汚臭鈍感
	Y /= 3
SIF TALENT:汚臭敏感
	Y *= 2

;キスでは汚れがそれほど影響しない
Y /= 2

;汚れあり
IF Y
	A -= Y
	;汚臭鈍感
	IF TALENT:汚臭鈍感
	ELSEIF TALENT:汚臭敏感
	ELSE
	ENDIF
ENDIF



;キス未経験
IF TALENT:キス未経験
	A -= 30
ENDIF

;一線越えない（キス未経験時のみ）
IF TALENT:キス未経験 && TALENT:一線越えない
	A -= 15
ENDIF


;合意
IF TALENT:合意 == 0
	A -= 10
ENDIF


;好感度
IF CFLAG:2 < 500
	B = 0
	IF CFLAG:2 < 100
		B = 20
	ELSEIF CFLAG:2 < 300
		B = 10
	ELSE
		B = 5
	ENDIF
	A -= B
ENDIF

;好感度
IF CFLAG:2 >= 700
	B = 0
	SIF S
	B = CFLAG:2 / 80
	A += B
ENDIF

IF ((FLAG:23 & 1p27) == 0) && FLAG:2 != 1
	;男嫌い
	IF TALENT:男嫌い && TALENT:PLAYER:オトコ
		PRINT  - 
		PRINTS TALENTNAME:82
		PRINTV '(,7,')
		S = 1
	ENDIF

	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
		S = 1
	ENDIF
	;ABL:奉仕精神
	IF ABL:奉仕精神
		SIF S
			PRINT  + 
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
		S = 1
	ENDIF

	IF MARK:快楽依存
		SIF S
			PRINT  + 
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:快楽依存,'(,MARK:快楽依存 * 2,')
		S = 1
	ENDIF


	;PALAM:欲情
	IF PALAM:欲情 < PALAMLV:1
		L = 0
	ELSEIF PALAM:欲情 < PALAMLV:2
		L = 1
	ELSEIF PALAM:欲情 < PALAMLV:3
		L = 2
	ELSEIF PALAM:欲情 < PALAMLV:4
		L = 3
	ELSEIF PALAM:欲情 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		PRINTS PALAMNAME:5
		PRINTV 'LV,L,'(,L * 1,')
		S = 1
	ENDIF

	;恥じらい
	IF TALENT:恥じらい
		PRINT  - 
		PRINTS TALENTNAME:35
		PRINTV '(,5,')
		S = 1
	ENDIF
	;汚臭鈍感
	IF TALENT:汚臭鈍感
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:61
		PRINTV '(,1,')
		S = 1
	ENDIF
	;汚臭敏感
	IF TALENT:汚臭敏感
		PRINT  - 
		PRINTS TALENTNAME:62
		PRINTV '(,3,')
		S = 1
	ENDIF
	;献身的
	IF TALENT:献身的
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:63
		PRINTV '(,6,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:快感の否定
		PRINT  - 
		PRINTS TALENTNAME:71
		PRINTV '(,1,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:恋慕 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		PRINTS TALENTNAME:85
		PRINTV '(,5,')
		S = 5
	ENDIF

	Y = 0
	;愛液の汚れ
	SIF STAIN:PLAYER:口 & 1
		Y += 1
	;精液の汚れ
	SIF STAIN:PLAYER:口 & 4
		Y += 3
	;アナルの汚れ
	SIF STAIN:PLAYER:口 & 8
		Y += 7
	SIF STAIN:PLAYER:口 & 16
		Y += 15

	SIF TALENT:汚臭鈍感
		Y /= 3
	SIF TALENT:汚臭敏感
		Y *= 2

	;キスでは汚れがそれほど影響しない
	Y /= 2

	;汚れあり
	IF Y
		PRINT  - 
		;汚臭鈍感
		IF TALENT:汚臭鈍感
			PRINT 汚れあり、
			PRINTS TALENTNAME:61
		ELSEIF TALENT:汚臭敏感
			PRINT 汚れあり、
			PRINTS TALENTNAME:62
		ELSE
			PRINT 汚れあり
		ENDIF
		PRINTV '(,Y,')
		S = 1
	ENDIF



	;キス未経験
	IF TALENT:キス未経験
		PRINT  - 
		PRINTS TALENTNAME:151
		PRINTV '(,30,')
		S = 1
	ENDIF

	;一線越えない（キス未経験時のみ）
	IF TALENT:キス未経験 && TALENT:一線越えない
		PRINT  - 
		PRINTS TALENTNAME:27
		PRINTV '(,15,')
		S = 1
	ENDIF


	;合意
	IF TALENT:合意 == 0
		PRINT  - 
		PRINTFORM %TALENTNAME:152%無し
		PRINTV '(,10,')
		S = 1
	ENDIF


	;好感度
	IF CFLAG:2 < 500
		B = 0
		PRINT  - 
		IF CFLAG:2 < 100
			B = 20
		ELSEIF CFLAG:2 < 300
			B = 10
		ELSE
			B = 5
		ENDIF
		PRINT 好感度不足
		PRINTV '(,B,')
		S = 1
	ENDIF

	;好感度
	IF CFLAG:2 >= 700
		B = 0
		SIF S
			PRINT  + 
		B = CFLAG:2 / 80
		PRINT 好感度
		PRINTV '(,B,')
		S = 1
	ENDIF


	;合計を表示(85以上で実行)
	PRINT  = 
	PRINTV A


	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V
	WAIT
ENDIF

