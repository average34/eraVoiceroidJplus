ELSEIF ABL:Ａ感覚 == 3
	SOURCE:快Ａ = 450
	SOURCE:情愛 = 180
	SOURCE:屈従 += 1500
	TIMES SOURCE:性行動 , 1.20
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:快Ａ = 850
	SOURCE:情愛 = 300
	SOURCE:屈従 += 2600
	TIMES SOURCE:性行動 , 1.50
ELSE
	SOURCE:快Ａ = 175 + (ABL:Ａ感覚 * 225)
	SOURCE:情愛 = 75 + (ABL:Ａ感覚 * 75)
	SOURCE:屈従 += 500 + (ABL:Ａ感覚 * 700)
	SOURCE:性行動 = SOURCE:性行動 * (ABL:Ａ感覚 * 25 + 75) / 100
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:快Ａ , 0.10
	SOURCE:痛み = 18000
	LOSEBASE:体力 += 70
	LOSEBASE:気力 += 80
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:快Ａ , 0.30
	SOURCE:痛み = 10000
	LOSEBASE:体力 += 60
	LOSEBASE:気力 += 70
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:快Ａ , 0.50
	SOURCE:痛み = 4500
	LOSEBASE:体力 += 45
	LOSEBASE:気力 += 55
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:快Ａ , 1.00
	SOURCE:痛み = 1500
	LOSEBASE:体力 += 25
	LOSEBASE:気力 += 35
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:快Ａ , 1.40
	SOURCE:痛み = 700
	LOSEBASE:体力 += 10
	LOSEBASE:気力 += 15
ELSE
	TIMES SOURCE:快Ａ , 1.60
	SOURCE:痛み = 300
	LOSEBASE:体力 += 0
	LOSEBASE:気力 += 0
ENDIF


;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.40
