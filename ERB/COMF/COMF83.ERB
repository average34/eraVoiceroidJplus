@COM83
;両穴フィスト
PRINTL 両穴フィスト
STR:0 = 両穴フィスト

;V経験を伴うコマンドのフラグ(処女膜再生対応)
TFLAG:19 = 1

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 800
LOSEBASE:1 += 500

SOURCE:6 = 1800
SOURCE:10 = 2800
SOURCE:11 = 1500
SOURCE:12 = 2500
SOURCE:13 = 2500
SOURCE:14 = 2500
SOURCE:16 = 2500

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:1 = 40
	SOURCE:3 = 50
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:1 = 150
	SOURCE:3 = 150
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:1 = 400
	SOURCE:3 = 250
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:1 = 1000
	SOURCE:3 = 350
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:1 = 1700
	SOURCE:3 = 600
ELSE
	SOURCE:1 = 950 + (ABL:Ｖ感覚 * 250)
	SOURCE:3 = 225 + (ABL:Ｖ感覚 * 125)
ENDIF


;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:2 = 10
	SOURCE:3 = 10
	SOURCE:13 = 100
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:2 = 30
	SOURCE:3 = 30
	SOURCE:13 = 700
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:2 = 500
	SOURCE:3 = 100
	SOURCE:13 = 1500
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:2 = 1000
	SOURCE:3 = 200
	SOURCE:13 = 3000
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:2 = 1700
	SOURCE:3 = 450
	SOURCE:13 = 5000
ELSE
	SOURCE:2 = 950 + (ABL:Ａ感覚 * 250)
	SOURCE:3 = ABL:Ａ感覚 * 150
	SOURCE:13 = 500 + (ABL:Ａ感覚 * 1500)
ENDIF

;ABL:サドっ気をみる
IF ABL:PLAYER:サドっ気 == 0
	TIMES SOURCE:6 , 1.00
ELSEIF ABL:PLAYER:サドっ気 == 1
	TIMES SOURCE:6 , 1.05
ELSEIF ABL:PLAYER:サドっ気 == 2
	TIMES SOURCE:6 , 1.10
ELSEIF ABL:PLAYER:サドっ気 == 3
	TIMES SOURCE:6 , 1.20
ELSEIF ABL:PLAYER:サドっ気 == 4
	TIMES SOURCE:6 , 1.30
ELSE
	SOURCE:6 = SOURCE:6 * (ABL:PLAYER:サドっ気 * 5 + 125) / 100
ENDIF

;EXP:Ｖ経験をみる
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 += 20000
	SOURCE:11 += 2000

	IF TALENT:PLAYER:122 == 0
		EXP:50 += 1
		PRINTFORML %EXPNAME:50%＋１
	ENDIF
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 += 300
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:6 += 50
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:6 += 10
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.40
	SOURCE:6 += 0
ELSE
	TIMES SOURCE:1 , 1.60
	SOURCE:6 = 0
ENDIF


;EXP:Ａ経験をみる
IF EXP:1 < EXPLV:1
	TIMES SOURCE:2 , 0.10
	SOURCE:6 += 5000
	SOURCE:11 += 1000
ELSEIF EXP:1 < EXPLV:2
	TIMES SOURCE:2 , 0.30
	SOURCE:6 += 2000
	SOURCE:11 += 1000
ELSEIF EXP:1 < EXPLV:3
	TIMES SOURCE:2 , 0.50
	SOURCE:6 += 2000
	SOURCE:11 += 1000
ELSEIF EXP:1 < EXPLV:4
	TIMES SOURCE:2 , 1.00
	SOURCE:6 += 2000
	SOURCE:11 += 1000
ELSEIF EXP:1 < EXPLV:5
	TIMES SOURCE:2 , 1.40
	SOURCE:6 += 1000
	SOURCE:11 += 200
ELSE
	TIMES SOURCE:1 , 1.60
	SOURCE:6 += 600
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:1 , 0.20
	TIMES SOURCE:2 , 0.20
	SOURCE:11 += 1000

	SOURCE:6 += 1900
	TIMES SOURCE:6 , 9.00
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:2 , 0.40
	SOURCE:11 += 800

	SOURCE:6 += 1250
	TIMES SOURCE:6 , 3.00
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:2 , 0.60
	SOURCE:11 += 600

	SOURCE:6 += 1000
	TIMES SOURCE:6 , 1.50
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:2 , 1.00
	SOURCE:11 += 200

	SOURCE:6 += 200
	TIMES SOURCE:6 , 0.30
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:2 , 1.30

	TIMES SOURCE:6 , 0.10
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:2 , 0.60
	TIMES SOURCE:13 , 0.60
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:2 , 0.80
	TIMES SOURCE:13 , 0.80
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:13 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:2 , 1.20
	TIMES SOURCE:13 , 1.20
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:2 , 1.40
	TIMES SOURCE:13 , 1.40
ENDIF

;ABL:従順をみる
IF ABL:親密 == 0
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:2 , 0.70
	TIMES SOURCE:3 , 0.60
	TIMES SOURCE:11, 2.00
ELSEIF ABL:親密 == 1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:2 , 0.90
	TIMES SOURCE:3 , 0.80
	TIMES SOURCE:11, 1.20
ELSEIF ABL:親密 == 2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:11, 1.00
ELSEIF ABL:親密 == 3
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:2 , 1.10
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:11, 0.60
ELSEIF ABL:親密 == 4
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:2 , 1.20
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:11, 0.30
ELSE
	SOURCE:1 = SOURCE:1 * (ABL:親密 * 15 + 95) / 100
	SOURCE:2 = SOURCE:2 * (ABL:親密 * 5 + 105) / 100
	SOURCE:3 = SOURCE:3 * (ABL:親密 * 10 + 110) / 100
	TIMES SOURCE:11, 0.10
ENDIF

;小柄体形
SIF TALENT:100
	TIMES SOURCE:6 , 3.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:105
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:11 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:106
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:11 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

EXP:1 += 25
PRINTL Ａ経験＋２５

;貞操観念
IF TALENT:30
	IF EXP:0 == 0
		TIMES SOURCE:3 , 0.60
		TIMES SOURCE:11, 5.00
	ELSE
		TIMES SOURCE:3 , 0.60
		TIMES SOURCE:11, 1.80
	ENDIF
;貞操無頓着
ELSEIF TALENT:31
	IF EXP:0 == 0
		TIMES SOURCE:11, 0.50
	ELSE
		TIMES SOURCE:11, 0.30
	ENDIF
ENDIF

EXP:0 += 25
PRINTL Ｖ経験＋２５

CALL EVENTKAKUTYOU_VA

RETURN 1

