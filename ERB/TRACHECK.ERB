;-------------------------------------------------
;調教対象の射精チェック
;-------------------------------------------------
@TARGET_EJAC_CHECK
SIF TALENT:ふたなり == 0 && TALENT:オトコ == 0
	RETURN 0

;オーバーフローチェック。
;本来の処理はA = UP:快Ｃ + UP:快Ｖ + UP:快Ａ + UP:快Ｂ
IF UP:快Ｃ / 10 >= 10000000
	A = 100000000
ELSEIF UP:快Ｃ / 10 + UP:快Ｖ / 10 >= 10000000
	A = 100000000
ELSEIF UP:快Ｃ / 10 + UP:快Ｖ / 10 + UP:快Ａ / 10 >= 10000000
	A = 100000000
ELSEIF UP:快Ｃ / 10 + UP:快Ｖ / 10 + UP:快Ａ / 10 + UP:快Ｂ / 10>= 10000000
	A = 100000000
ELSE
	A = UP:快Ｃ + UP:快Ｖ + UP:快Ａ + UP:快Ｂ +UP:快Ｍ
ENDIF

;自制心
SIF TALENT:自制心
	A /= 2

;快感に素直
SIF TALENT:快感に素直
	TIMES A , 1.20

;淫乱化
SIF TALENT:淫乱
	TIMES A , 1.10

;快感の否定
SIF TALENT:快感の否定
	TIMES A , 0.80

;媚薬
SIF TEQUIP:21
	A *= 2

;利尿剤
SIF TEQUIP:22
	A /= 2

;淫蟲
SIF TEQUIP:92 == 1
	A *= 2
	
IF TFLAG:44
	A = 500 + A / 2
ELSE
	A /= 2
ENDIF
BASE:射精 += A

S = BASE:射精
EJAC = MAXBASE:射精

IF S > EJAC * 4
	E = 4
ELSEIF S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
	RETURN 0
ENDIF

;超大量射精
IF E == 4
	LOSEBASE:体力 += 40
	LOSEBASE:気力 += 200

	IF EXP:射精経験 < EXPLV:1
		SOURCE:露出 += 40000
		SOURCE:屈従 += 20000
	ELSEIF EXP:射精経験 < EXPLV:2
		SOURCE:露出 += 20000
		SOURCE:屈従 += 16000
	ELSEIF EXP:射精経験 < EXPLV:3
		SOURCE:露出 += 14000
		SOURCE:屈従 += 12000
	ELSEIF EXP:射精経験 < EXPLV:4
		SOURCE:露出 += 10000
		SOURCE:屈従 += 8000
	ELSEIF EXP:射精経験 < EXPLV:5
		SOURCE:露出 += 6000
		SOURCE:屈従 += 4000
	ELSE
		SOURCE:露出 += 3600
		SOURCE:屈従 += 2400
	ENDIF

	PRINTS NAME:TARGET
	PRINTL 超大量射精
	PRINTL 精液経験＋2
	PRINTL 射精経験＋4

	EXP:精液経験 += 2
	EXP:射精経験 += 4
;大量射精
ELSEIF E == 2
	LOSEBASE:体力 += 20
	LOSEBASE:気力 += 100

	IF EXP:射精経験 < EXPLV:1
		SOURCE:露出 += 20000
		SOURCE:屈従 += 10000
	ELSEIF EXP:射精経験 < EXPLV:2
		SOURCE:露出 += 10000
		SOURCE:屈従 += 8000
	ELSEIF EXP:射精経験 < EXPLV:3
		SOURCE:露出 += 7000
		SOURCE:屈従 += 6000
	ELSEIF EXP:射精経験 < EXPLV:4
		SOURCE:露出 += 5000
		SOURCE:屈従 += 4000
	ELSEIF EXP:射精経験 < EXPLV:5
		SOURCE:露出 += 3000
		SOURCE:屈従 += 2000
	ELSE
		SOURCE:露出 += 1800
		SOURCE:屈従 += 1200
	ENDIF

	PRINTS NAME:TARGET
	PRINTL 大量射精
	PRINTL 精液経験＋1
	PRINTL 射精経験＋2

	EXP:精液経験 += 1
	EXP:射精経験 += 2
;通常の射精
ELSEIF E == 1
	LOSEBASE:気力 += 40

	IF EXP:射精経験 < EXPLV:1
		SOURCE:露出 += 10000
		SOURCE:屈従 += 5000
	ELSEIF EXP:射精経験 < EXPLV:2
		SOURCE:露出 += 5000
		SOURCE:屈従 += 4000
	ELSEIF EXP:射精経験 < EXPLV:3
		SOURCE:露出 += 2500
		SOURCE:屈従 += 2000
	ELSEIF EXP:射精経験 < EXPLV:4
		SOURCE:露出 += 1600
		SOURCE:屈従 += 1400
	ELSEIF EXP:射精経験 < EXPLV:5
		SOURCE:露出 += 800
		SOURCE:屈従 += 500
	ELSE
		SOURCE:露出 += 200
		SOURCE:屈従 += 250
	ENDIF

	PRINTS NAME:TARGET
	PRINTL 射精
	PRINTL 射精経験＋1
	EXP:射精経験 += 1
ENDIF

;射精経験0かつオトコ、不思議な根、具現を持ってない場合は射精すると異常経験+5
IF EXP:射精経験 == 0 && TALENT:オトコ == 0 && TALENT:不思議な根 == 0 && TALENT:具現 == 0
	EXP:異常経験 += 5
	PRINTL 異常経験＋5
ENDIF

;パートナーの各コマンドでの中田氏チェック
CALL IN_VAGINA_CHECK_T

;Ｐに精液汚れ
STAIN:ペニス |= 4

BASE:射精 -= EJAC
SIF BASE:射精 >= EJAC
	BASE:射精 = EJAC-1

;調教対象が射精フラグ
TFLAG:10 = 1
NOWEX:射精 += 1
EX:射精 += 1

